    <melody>Heb dij niet gehoord lieve Teunis</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">HEbdy niet gehoort, lieven <rhyme label="a" type="f">Teunis</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Van Myntjens <rhyme label="b" type="m">Bruyloft-feest</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Hoe dat <rhyme label="c" type="m">Tan</rhyme></l>
        <l id="s1:l4" met="2" type="+-"><rhyme label="c" type="m" corresp="#s1:l3">Alle-man</rhyme>,,</l>
        <l id="s1:l5" met="3" type="-+">Met haren lieven <rhyme label="c" type="m" corresp="#s1:l3">Jan</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Was vrolyck om het <rhyme label="b" type="m" corresp="#s1:l2">meest</rhyme>:</l>
        <l id="s1:l7" met="2" type="+-">Heyntjen den <rhyme label="d" type="m">baes</rhyme> //</l>
        <l id="s1:l8" met="2" type="+-">en kreupelen <rhyme label="e" type="m">Niel</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">En schemelen <rhyme label="d" type="m" corresp="#s1:l7">Claes</rhyme> ,,</l>
        <l id="s1:l10" met="2" type="+-">en scheeven <rhyme label="e" type="m" corresp="#s1:l8">Michiel</rhyme>,</l>
        <l id="s1:l11" met="1" type="+-">Die <rhyme label="f" type="f">schoncken</rhyme> //</l>
        <l id="s1:l12" met="1" type="-+">en <rhyme label="f" type="f" corresp="#s1:l11">droncken</rhyme></l>
        <l id="s1:l13" met="1" type="-+">En <rhyme label="g" type="f">songhen</rhyme> //</l>
        <l id="s1:l14" met="1" type="-+">en <rhyme label="g" type="f" corresp="#s1:l13">klonghen</rhyme></l>
        <l id="s1:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s1:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s1:l15">Bruyloft-feest</rhyme>.</l>
        <l id="s1:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s1:l15">Bruyloft-feest</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Haddy eens ghesien al de <rhyme label="a" type="f">wyven</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die hadden 't daer soo <rhyme label="b" type="m">breedt</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Dat sy <rhyme label="c" type="m">mans</rhyme> //</l>
        <l id="s2:l4" met="2" type="+-">wel <rhyme label="c" type="m" corresp="#s2:l3">bykans</rhyme> //</l>
        <l id="s2:l5" met="3" type="-+">beschaemden door den <rhyme label="c" type="m" corresp="#s2:l3">dans</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Was dat niet fraey <rhyme label="b" type="m" corresp="#s2:l2">bescheedt</rhyme>?</l>
        <l id="s2:l7" met="2" type="+-">Meyntjen de <rhyme label="d" type="m">Bruyt</rhyme> //</l>
        <l id="s2:l8" met="2" type="+-">en Beyken <rhyme label="e" type="m">Was-kranck</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">En Lincken <rhyme label="d" type="m" corresp="#s2:l7">Lap-uyt</rhyme> //</l>
        <l id="s2:l10" met="2" type="+-">en Lysken de <rhyme label="e" type="m" corresp="#s2:l8">manck</rhyme>,</l>
        <l id="s2:l11" met="1" type="+-">Die <rhyme label="f" type="f">riepen</rhyme> //</l>
        <l id="s2:l12" met="1" type="-+">en <rhyme label="f" type="f" corresp="#s2:l11">liepen</rhyme></l>
        <l id="s2:l13" met="1" type="-+"><rhyme label="g" type="f">Couranten</rhyme> //</l>
        <l id="s2:l14" met="1" type="-+">
          <rhyme label="g" type="f" corresp="#s2:l13">servanten</rhyme>
        </l>
        <l id="s2:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s2:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s2:l15">Bruyloft-feest</rhyme>?</l>
        <l id="s2:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s2:l15">Bruyloft-feest</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Job Licht-voet, en Hennen in 't <rhyme label="a" type="f">Wambas</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Sprongh over Stoel en <rhyme label="b" type="m">banck</rhyme>;</l>
        <l id="s3:l3" met="2" type="+-">Maer <rhyme label="c" type="m">Cousyn</rhyme> //</l>
        <l id="s3:l4" met="2" type="+-"><rhyme label="c" type="m" corresp="#s3:l3">Severyn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">die ded' het alsoo <rhyme label="c" type="m" corresp="#s3:l3">fyn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat hy oock t'samen <rhyme label="b" type="m" corresp="#s3:l2">sanck</rhyme>;</l>
        <l id="s3:l7" met="2" type="+-">Magheren <rhyme label="d" type="m">Lauvv</rhyme>,,</l>
        <l id="s3:l8" met="2" type="+-">en kalen <rhyme label="e" type="m">Matthys</rhyme>.</l>
        <l id="s3:l9" met="2" type="+-">En Seppen den <rhyme label="d" type="m" corresp="#s3:l7">Blauw</rhyme> //</l>
        <l id="s3:l10" met="2" type="+-">en simpelen <rhyme label="e" type="m" corresp="#s3:l8">Gys</rhyme>,</l>
        <l id="s3:l11" met="1" type="+-">Die <rhyme label="f" type="f">draeyden</rhyme> //</l>
        <l id="s3:l12" met="1" type="-+">en <rhyme label="f" type="f" corresp="#s3:l11">kraeyden</rhyme></l>
        <l id="s3:l13" met="1" type="-+"><rhyme label="g" type="f">Almande</rhyme> //</l>
        <l id="s3:l14" met="1" type="-+">
          <rhyme label="g" type="f" corresp="#s3:l13">Pavande</rhyme>
        </l>
        <l id="s3:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s3:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s3:l15">Bruyloft-feest</rhyme>?</l>
        <l id="s3:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s3:l15">Bruyloft-feest</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Daniel de Vos met syn <rhyme label="a" type="f">Mutsken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En A'bram <rhyme label="b" type="m">Doncker-vliedt</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Had een <rhyme label="c" type="m">pack</rhyme> //</l>
        <l id="s4:l4" met="2" type="+-">met <rhyme label="c" type="m" corresp="#s4:l3">toeback</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">ghesteken in den <rhyme label="c" type="m" corresp="#s4:l3">sack</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Maer Lys en wist dat <rhyme label="b" type="m" corresp="#s4:l2">niet</rhyme>,</l>
        <l id="s4:l7" met="2" type="+-">Scheel <rhyme label="d" type="m">Jackemyn</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">en Claerken <rhyme label="e" type="m">Petiet</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">En Langhe <rhyme label="d" type="m" corresp="#s4:l7">Pieryn</rhyme>,,</l>
        <l id="s4:l10" met="2" type="+-">en Lossche <rhyme label="e" type="m" corresp="#s4:l8">Margriet</rhyme>,</l>
        <l id="s4:l11" met="1" type="+-">Die <rhyme label="f" type="f">heygden</rhyme> // //</l>
        <l id="s4:l12" met="1" type="-+">en <rhyme label="f" type="f" corresp="#s4:l11">neyghden</rhyme>,</l>
        <l id="s4:l13" met="1" type="-+">En <rhyme label="g" type="f">lachten</rhyme> //</l>
        <l id="s4:l14" met="1" type="-+">en <rhyme label="g" type="f" corresp="#s4:l13">schachten</rhyme></l>
        <l id="s4:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s4:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s4:l15">Bruyloft-feest</rhyme>?</l>
        <l id="s4:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s4:l15">Bruyloft-feest</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Onder al dees Draeyers en <rhyme label="a" type="f">Danssers</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Was Slinger-been <rhyme label="b" type="m">Passchier</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">Die wou <rhyme label="c" type="m">ras</rhyme> //</l>
        <l id="s5:l4" met="2" type="+-">met 't <rhyme label="c" type="m" corresp="#s5:l3">ghelas</rhyme> //</l>
        <l id="s5:l5" met="3" type="-+">gaen danssen <rhyme label="c" type="m" corresp="#s5:l3">slincke-pas</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maer viel met glas vol <rhyme label="b" type="m" corresp="#s5:l2">bier</rhyme>;</l>
        <l id="s5:l7" met="2" type="+-">Sy riepen al <rhyme label="d" type="m">Jau</rhyme> //</l>
        <l id="s5:l8" met="2" type="+-">en trocken wel <rhyme label="e" type="m">styf</rhyme>,</l>
        <l id="s5:l9" met="2" type="+-">En scheurden syn <rhyme label="d" type="m" corresp="#s5:l7">mauw</rhyme> //</l>
        <l id="s5:l10" met="2" type="+-">en vielen op 't <rhyme label="e" type="m" corresp="#s5:l8">lyf</rhyme>,</l>
        <l id="s5:l11" met="1" type="+-">Daer <rhyme label="f" type="f">wroetten</rhyme> //</l>
        <l id="s5:l12" met="1" type="-+">de <rhyme label="f" type="f" corresp="#s5:l11">voeten</rhyme>,</l>
        <l id="s5:l13" met="1" type="-+">En <rhyme label="g" type="f">rolden</rhyme> //</l>
        <l id="s5:l14" met="1" type="-+">en <rhyme label="g" type="f" corresp="#s5:l13">dolden</rhyme></l>
        <l id="s5:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s5:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s5:l15">Bruyloft-feest</rhyme>?</l>
        <l id="s5:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s5:l15">Bruyloft-feest</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Dierick in de Cat // nam het <rhyme label="a" type="f">Sout-vat</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Op 't hooft voor een <rhyme label="b" type="m">kasket</rhyme>;</l>
        <l id="s6:l3" met="2" type="+-">Maer <rhyme label="c" type="m">Renier</rhyme> ,,</l>
        <l id="s6:l4" met="2" type="+-">nam van 't <rhyme label="c" type="m" corresp="#s6:l3">vier</rhyme> //</l>
        <l id="s6:l5" met="3" type="-+">den spit voor een <rhyme label="c" type="m" corresp="#s6:l3">rapier</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Voor schildt de pan met <rhyme label="b" type="m" corresp="#s6:l2">vet</rhyme>;</l>
        <l id="s6:l7" met="2" type="+-">Dries <rhyme label="d" type="m">Capiteyn</rhyme> //</l>
        <l id="s6:l8" met="2" type="+-">en Thys <rhyme label="e" type="m">Lieutenant</rhyme>,</l>
        <l id="s6:l9" met="2" type="+-"><rhyme label="d" type="m" corresp="#s6:l7">AlpherisQuintyn</rhyme> ,,</l>
        <l id="s6:l10" met="2" type="+-">en Balten <rhyme label="e" type="m" corresp="#s6:l8">Sergeant</rhyme>,</l>
        <l id="s6:l11" met="1" type="+-">Die <rhyme label="f" type="f">wouden</rhyme> //</l>
        <l id="s6:l12" met="1" type="-+">en <rhyme label="f" type="f" corresp="#s6:l11">souden</rhyme></l>
        <l id="s6:l13" met="1" type="-+"><rhyme label="g" type="f">Vernielen</rhyme> //</l>
        <l id="s6:l14" met="1" type="-+">die <rhyme label="g" type="f" corresp="#s6:l13">fielen</rhyme> //</l>
        <l id="s6:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s6:l16" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s6:l15">Bruyloft-feest</rhyme>?</l>
        <l id="s6:l17" met="4" type="+-">Was dat niet een <rhyme label="h" type="m" corresp="#s6:l15">Bruyloft-feest</rhyme>?</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Joos den Speel-man begon te <rhyme label="a" type="f">roncken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En saeghden op den <rhyme label="b" type="m">Bas</rhyme>,</l>
        <l id="s7:l3" met="2" type="+-">Maer <rhyme label="c" type="m">Alfons</rhyme> ,,</l>
        <l id="s7:l4" met="2" type="+-"><rhyme label="c" type="m" corresp="#s7:l3">Violons</rhyme> //</l>
        <l id="s7:l5" met="3" type="-+">die kloncker oock <rhyme label="c" type="m" corresp="#s7:l3">terstonts</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Dat we'er al vrolyck <rhyme label="b" type="m" corresp="#s7:l2">was</rhyme>;</l>
        <l id="s7:l7" met="2" type="+-">Jaecq nam den <rhyme label="d" type="m">hael</rhyme> //</l>
        <l id="s7:l8" met="2" type="+-">en Lieven de <rhyme label="e" type="m">tangh</rhyme>,</l>
        <l id="s7:l9" met="2" type="+-">De schup Jan de <rhyme label="d" type="m" corresp="#s7:l7">Wael</rhyme> //</l>
        <l id="s7:l10" met="2" type="+-">en Rommel nam den <rhyme label="e" type="m" corresp="#s7:l8">pan</rhyme>;</l>
        <l id="s7:l11" met="1" type="+-">Dus <rhyme label="f" type="f">roeren</rhyme> //</l>
        <l id="s7:l12" met="1" type="-+">de <rhyme label="f" type="f" corresp="#s7:l11">Boeren</rhyme>,</l>
        <l id="s7:l13" met="1" type="-+">En <rhyme label="g" type="f">schreeeuwen</rhyme> //</l>
        <l id="s7:l14" met="1" type="-+">als <rhyme label="g" type="f" corresp="#s7:l13">Leeuwen</rhyme> //</l>
        <l id="s7:l15" met="2" type="+-">om het <rhyme label="h" type="m">meest</rhyme>,</l>
        <l id="s7:l16" met="4" type="+-">Als sy houden <rhyme label="h" type="m" corresp="#s7:l15">Bruyloft-feest</rhyme>,</l>
        <l id="s7:l17" met="4" type="+-">Als sy houden <rhyme label="h" type="m" corresp="#s7:l15">Bruyloft-feest</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
