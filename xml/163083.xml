<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>163083</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>De tijd is hier</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">WAeckt op ghy Christens <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Waeckt op, wilt u <rhyme label="b" type="f">verblijden</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Ghy die in 't aerdsche <rhyme label="a" type="m" corresp="#s1:l1">dal</rhyme></l>
        <l id="s1:l4" met="3" type="-+">U Bruydegom <rhyme label="c" type="m">verwacht</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Men hoort met bly <rhyme label="a" type="m" corresp="#s1:l1">geschal</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Veel vreught aen alle <rhyme label="b" type="f" corresp="#s1:l2">zijden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Veel Eng'len in <rhyme label="a" type="m" corresp="#s1:l1">getal</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Verschijnen in de <rhyme label="c" type="m" corresp="#s1:l4">nacht</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En singen <rhyme label="b" type="f" corresp="#s1:l2">bly</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Met Hemels <rhyme label="c" type="m" corresp="#s1:l4">melody</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Tot lof van Godes lieve <rhyme label="d" type="m">Soon</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Eere zy Godt in d' hooge <rhyme label="d" type="m" corresp="#s1:l11">throon</rhyme>,</l>
        <l id="s1:l13" met="3" type="+-">En vreed op aerd hier <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Al in dit <rhyme label="e" type="m" corresp="#s1:l13">Nieuwe-Iaer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Een licht is <rhyme label="a" type="m">op-gegaen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Een kint is ons <rhyme label="b" type="f">geboren</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Een Star is <rhyme label="a" type="m" corresp="#s2:l1">op-gestaen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Gelijck Gods Woort ons <rhyme label="c" type="m">leert</rhyme>:</l>
        <l id="s2:l5" met="2" type="-+">De Heer neemt nu weer <rhyme label="a" type="m" corresp="#s2:l1">aen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Sijn Bruyt, sijn <rhyme label="b" type="f" corresp="#s2:l2">uytverkoren</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En maeckt dat zy <rhyme label="a" type="m" corresp="#s2:l1">voortaen</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Weer na den Hemel <rhyme label="c" type="m" corresp="#s2:l4">gaet</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">O <rhyme label="b" type="f" corresp="#s2:l2">Bethlehem</rhyme>!</l>
        <l id="s2:l10" met="3" type="-+">O soet en blijde <rhyme label="c" type="m" corresp="#s2:l4">stem</rhyme>!</l>
        <l id="s2:l11" met="4" type="+-">Die d' Harders hoorden op dat <rhyme label="d" type="m">pas</rhyme>,</l>
        <l id="s2:l12" met="4" type="+-">Dat heden haer geboren <rhyme label="d" type="m" corresp="#s2:l11">was</rhyme></l>
        <l id="s2:l13" met="3" type="+-">De Salichmaker <rhyme label="e" type="m">klaer</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Al in dit <rhyme label="e" type="m" corresp="#s2:l13">Nieuwe-Iaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Sy hebben in der <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Tot Bethlem in de <rhyme label="b" type="f">Stede</rhyme></l>
        <l id="s3:l3" met="2" type="-+">Gesocht, en <rhyme label="a" type="m" corresp="#s3:l1">overdacht</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Al wat haer was <rhyme label="c" type="m">geseyt</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Het woort dat is <rhyme label="a" type="m" corresp="#s3:l1">volbracht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Sy vonden daer in <rhyme label="b" type="f" corresp="#s3:l2">vrede</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Een kint seer kleyn <rhyme label="a" type="m" corresp="#s3:l1">geacht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">In een kribbe neer <rhyme label="c" type="m" corresp="#s3:l4">geleyt</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Slecht van <rhyme label="b" type="f" corresp="#s3:l2">gewaet</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Van 's werelts pronck en <rhyme label="c" type="m" corresp="#s3:l4">staet</rhyme>.</l>
        <l id="s3:l11" met="4" type="+-">O Christen-zieltje! denckt hier <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s3:l12" met="4" type="+-">En wilt tot Bethlem binnen <rhyme label="d" type="m" corresp="#s3:l11">gaen</rhyme>,</l>
        <l id="s3:l13" met="3" type="+-">Laet 's werelts pronck <rhyme label="e" type="m">voorwaer</rhyme>,</l>
        <l id="s3:l14" met="3" type="-+">Al in dit <rhyme label="e" type="m" corresp="#s3:l13">Nieuwe-Iaer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Het woort dat is <rhyme label="a" type="m">verbreyt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">De Wijsen zijn <rhyme label="b" type="f">gekomen</rhyme>,</l>
        <l id="s4:l3" met="2" type="-+">Die hebben oock <rhyme label="a" type="m" corresp="#s4:l1">geseyt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat haer van 't Oosten <rhyme label="c" type="m">klaer</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Een Starre heeft <rhyme label="a" type="m" corresp="#s4:l1">geleyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die hebben zy <rhyme label="b" type="f" corresp="#s4:l2">vernomen</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En sochten recht <rhyme label="a" type="m" corresp="#s4:l1">bescheyt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Wie dese Koninck <rhyme label="c" type="m" corresp="#s4:l4">waer</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">De Starre <rhyme label="b" type="f" corresp="#s4:l2">fijn</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Die wees haer 't <rhyme label="c" type="m" corresp="#s4:l4">Kindekijn</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Sy vonden dese Koninck <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s4:l12" met="4" type="+-">En schoncken hem haer schatten <rhyme label="d" type="m" corresp="#s4:l11">goet</rhyme>,</l>
        <l id="s4:l13" met="3" type="+-">Aenbaden hem <rhyme label="e" type="m">eenpaer</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Al in dit <rhyme label="e" type="m" corresp="#s4:l13">Nieuw-Iaer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Staet op, o soete <rhyme label="a" type="m">Bruyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Gaet uyt nu te <rhyme label="b" type="f">gemoete</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Met Sangh en soet <rhyme label="a" type="m" corresp="#s5:l1">geluyt</rhyme></l>
        <l id="s5:l4" met="3" type="-+">U lieve <rhyme label="c" type="m">Bruydegom</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Van bloem en Ieughdigh <rhyme label="a" type="m" corresp="#s5:l1">kruyt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Vlecht kransjes, wilt hem <rhyme label="b" type="f" corresp="#s5:l2">groeten</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Met weer-liefd' tot <rhyme label="a" type="m" corresp="#s5:l1">besluyt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En hiet hem <rhyme label="c" type="m" corresp="#s5:l4">wellekoom</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Niet door de <rhyme label="b" type="f" corresp="#s5:l2">kracht</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Van 's werelts staet en <rhyme label="c" type="m" corresp="#s5:l4">pracht</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Maer met een nedrig hert en <rhyme label="d" type="m">geest</rhyme>,</l>
        <l id="s5:l12" met="4" type="+-">In Liefd', Geloove <rhyme label="d" type="m" corresp="#s5:l11">aldermeest</rhyme>,</l>
        <l id="s5:l13" met="3" type="+-">Dat salight ons hier <rhyme label="e" type="m">naer</rhyme>,</l>
        <l id="s5:l14" met="3" type="-+">Al in dit <rhyme label="e" type="m" corresp="#s5:l13">Nieuwe-Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
