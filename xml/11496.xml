      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love complaint</tag>
    </tags>
    <melody>Engelse klokkedans</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ACh pluymighe Diertjens <rhyme label="a" type="m">teer</rhyme>!</l>
        <l id="s1:l2" met="6" type="+-">Staeckt u soet geluyt, daelt op u wieckjens <rhyme label="a" type="m" corresp="#s1:l1">neer</rhyme></l>
        <l id="s1:l3" met="4" type="+-">By mijn Bruyt, Schiet dan <rhyme label="b" type="m">uyt</rhyme></l>
        <l id="s1:l4" met="2" type="-+">V kropjens <rhyme label="c" type="m">bly</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Doet haer verstaen waer dat ick <rhyme label="c" type="m" corresp="#s1:l4">sy</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En suyght dan uyt de <rhyme label="a" type="m">tip</rhyme></l>
        <l id="s2:l2" met="6" type="+-">'t Honingh-soete sap van haer corale <rhyme label="a" type="m" corresp="#s2:l1">lip</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Drinckt u sat: Brenght van 't <rhyme label="b" type="m">nat</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Een druppel of <rhyme label="c" type="m">twee</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Voor my in u beckjens <rhyme label="c" type="m" corresp="#s2:l4">mee</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo haest ghy hebt <rhyme label="a" type="m">ghedaen</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Ach! kleyn beckjen trouw, vanght dan u reys weer <rhyme label="a" type="m" corresp="#s3:l1">aen</rhyme>.</l>
        <l id="s3:l3" met="4" type="+-">Aen mijn sy, Vertaelt aen <rhyme label="b" type="m">my</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Waer sy haer <rhyme label="c" type="m">onthout</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">In het dicht beladen <rhyme label="c" type="m" corresp="#s3:l4">Wout</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dan sal ick ras voort <rhyme label="a" type="m">vlien</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Tot ick het beeld der Maeghden weer kan <rhyme label="a" type="m" corresp="#s4:l1">sien</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Daer ick moet, Haer tot <rhyme label="b" type="m">boet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Met kusjens <rhyme label="c" type="m">dry</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Betalen aen haer kaeckjens <rhyme label="c" type="m" corresp="#s4:l4">bey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dees boet sy heeft <rhyme label="a" type="m">gestelt</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Als ick met haer sat in 't <rhyme label="a" type="m" corresp="#s5:l1">klaver-velt</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Daer met vreughd, En <rhyme label="b" type="m">geneughd</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Wy queelden <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Dit Lied, dat my uyt-teeren <rhyme label="c" type="m" corresp="#s5:l4">doet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
