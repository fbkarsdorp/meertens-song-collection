<song>
  <meta>
    <id>36484</id>
    <date>
      <year-start>1655</year-start>
      <year-end>1655</year-end>
    </date>
    <location>
      <orginal-name>gent</orginal-name>
      <standard-name>Gent, Belgium</standard-name>
      <coordinates>
        <lon>51.0538286</lon>
        <lat>3.7250121</lat>
      </coordinates>
    </location>
