<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160544</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Femme Gerbrantsz Drieduym</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ICk moet een deuntje <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Indien 't u niet <rhyme label="b" type="m">verveelt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Doch van geen vreemde <rhyme label="a" type="f" corresp="#s1:l1">dingen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maer Goetschen <rhyme label="b" type="m" corresp="#s1:l2">Haringh-teelt</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ick sal 't u dan <rhyme label="c" type="f">verklaren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Op dat de vreemde <rhyme label="d" type="m">Man</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">In kennis <rhyme label="c" type="f" corresp="#s1:l5">on-ervaren</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Daer mee van spreecken <rhyme label="d" type="m" corresp="#s1:l6">kan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy sien dan tot <rhyme label="a" type="f">Enchuysen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ons' Evers komen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sy komen vaeck <rhyme label="a" type="f" corresp="#s2:l1">aen-bruysen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Tot sinckens toe <rhyme label="b" type="m" corresp="#s2:l2">gelaen</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Noch komen hier aen <rhyme label="c" type="f">sacken</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Soo Urck als <rhyme label="d" type="m">Harderwijck</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met Schuyten, Kagen, <rhyme label="c" type="f" corresp="#s2:l5">Smacken</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vol Haringh <rhyme label="d" type="m" corresp="#s2:l6">al-gelijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Iongers strack aen 't <rhyme label="a" type="f">loopen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En roepen met <rhyme label="b" type="m">bescheyt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wil yemandt Haringh <rhyme label="a" type="f" corresp="#s3:l1">koopen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Want sy aen d'Of-slach <rhyme label="b" type="m" corresp="#s3:l2">leyt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Een ander roept oock <rhyme label="c" type="f">wacker</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Oom, moet ick tellen, <rhyme label="d" type="m">Oom</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Ick ben u Vrient en <rhyme label="c" type="f" corresp="#s3:l5">Macker</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Helpt my doch uyt den <rhyme label="d" type="m" corresp="#s3:l6">droom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De Visscher selfs moet <rhyme label="a" type="f">vragen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De Drooghsters aen de <rhyme label="b" type="m">Wal</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En 't staet aen haer <rhyme label="a" type="f" corresp="#s4:l1">behagen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Hoe duur hy of-slaen <rhyme label="b" type="m" corresp="#s4:l2">sal</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Het Tal loof ick vier <rhyme label="c" type="f">blancken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">(Seyt hy) een Oortjen <rhyme label="d" type="m">min</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">'Is mijn, ick moet u <rhyme label="c" type="f" corresp="#s4:l5">dancken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want het gaet nae ons <rhyme label="d" type="m" corresp="#s4:l6">sin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Stracks wortse dan <rhyme label="a" type="f">gesouten</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Te midden op de <rhyme label="b" type="m">straet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Die alder-best kan <rhyme label="a" type="f" corresp="#s5:l1">kouten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maeckt dan de meeste <rhyme label="b" type="m" corresp="#s5:l2">praet</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Een sweetjen kan men <rhyme label="c" type="f">halen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Soo veel is daer aen <rhyme label="d" type="m">vast</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Want effen vijftigh <rhyme label="c" type="f" corresp="#s5:l5">talen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Die maecken eerst een <rhyme label="d" type="m" corresp="#s5:l6">Last</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Men roept de Steecksters <rhyme label="a" type="f">t'samen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Komt Vrou-luy maeckje <rhyme label="b" type="m">ree</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Men roeptse by haer <rhyme label="a" type="f" corresp="#s6:l1">namen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Elck neem sijn Tobje <rhyme label="b" type="m" corresp="#s6:l2">mee</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Dit bed op, dan aen 't <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Treck met een moed daer <rhyme label="d" type="m">an</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Singh vroolick op met <rhyme label="c" type="f" corresp="#s6:l5">lusten</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dat men het hooren <rhyme label="d" type="m" corresp="#s6:l6">kan</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dan spoelt men af de <rhyme label="a" type="f">schobben</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En daer mee in de <rhyme label="b" type="m">Hangh</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dit maeckt een poesich <rhyme label="a" type="f" corresp="#s7:l1">slobben</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En kladdigh is de <rhyme label="b" type="m" corresp="#s7:l2">gangh</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Die klimmen om te <rhyme label="c" type="f">scheeren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">En tast men niet licht <rhyme label="d" type="m">aen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Een Proncker met sijn <rhyme label="c" type="f" corresp="#s7:l5">kleeren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Mach dan wel elders <rhyme label="d" type="m" corresp="#s7:l6">gaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">De Meyden stracks aen 't <rhyme label="a" type="f">stoocken</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met vyertjes hier en <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">En maecken door het <rhyme label="a" type="f" corresp="#s8:l1">smoocken</rhyme></l>
        <l id="s8:l4" met="3" type="-+">De Bocken droogh en <rhyme label="b" type="m" corresp="#s8:l2">klaer</rhyme>;</l>
        <l id="s8:l5" met="3" type="-+">Dit doet men alle <rhyme label="c" type="f">uren</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Gedurigh nacht en <rhyme label="d" type="m">dagh</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Op datse langh mach <rhyme label="c" type="f" corresp="#s8:l5">duren</rhyme></l>
        <l id="s8:l8" met="3" type="-+">De Keur oock uyt staen <rhyme label="d" type="m" corresp="#s8:l6">mach</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Men mach geen tijdt <rhyme label="a" type="f">verliesen</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Komt morgen ochten <rhyme label="b" type="m">vroo</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Men moet de Bocken <rhyme label="a" type="f" corresp="#s9:l1">miesen</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En packen in het <rhyme label="b" type="m" corresp="#s9:l2">Stroo</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Om die stracks te <rhyme label="c" type="f">verscheepen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dan raecken wy aen <rhyme label="d" type="m">gelt</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Want anders geeftse <rhyme label="c" type="f" corresp="#s9:l5">neepen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Dat vaeck de Buydel <rhyme label="d" type="m" corresp="#s9:l6">quelt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Dees Bocken werdt <rhyme label="a" type="f">gesonden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Door d'Oost en Wester <rhyme label="b" type="m">Zee</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Nae Vranckrijck en nae <rhyme label="a" type="f" corresp="#s10:l1">Londen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Hamburgh en Bremen <rhyme label="b" type="m" corresp="#s10:l2">mee</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">De Rus kan daer van <rhyme label="c" type="f">roemen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">De Noor-man, en de <rhyme label="d" type="m">Straet</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Ick weet het niet te <rhyme label="c" type="f" corresp="#s10:l5">noemen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Waer sy al henen <rhyme label="d" type="m" corresp="#s10:l6">gaet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Hoe moet een mensch al <rhyme label="a" type="f">slooven</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Eer hy aen 't kosje <rhyme label="b" type="m">raeckt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Men soud' het niet <rhyme label="a" type="f" corresp="#s11:l1">gelooven</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Voor die't met eere <rhyme label="b" type="m" corresp="#s11:l2">maeckt</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Denckt hy sijn tijdt <rhyme label="c" type="f">verleden</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Die was met veel <rhyme label="d" type="m">onlust</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">En op den dagh van <rhyme label="c" type="f" corresp="#s11:l5">heden</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Heeft hy noch weynigh <rhyme label="d" type="m" corresp="#s11:l6">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
