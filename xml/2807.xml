  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt u nu al <rhyme label="a" type="f">voerblyden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">leeuen met alle <rhyme label="b" type="m">vlyt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">den heer tot allen <rhyme label="a" type="f" corresp="#s1:l1">tyden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Want hy ons heeft <rhyme label="b" type="m" corresp="#s1:l2">bevryt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">van dees wintersche <rhyme label="c" type="f">daegen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">bedryft toch nu <rhyme label="d" type="m">genuecht</rhyme></l>
        <l id="s1:l7" met="3" type="-+">geen lieuer tyt wy <rhyme label="c" type="f" corresp="#s1:l5">saegen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">die mey diet al <rhyme label="d" type="m" corresp="#s1:l6">voerhuecht</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die blomkens syn <rhyme label="a" type="f">ontloeken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">het wout hem schoen <rhyme label="b" type="m">versiert</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die vogelkens Ioelyt <rhyme label="a" type="f" corresp="#s2:l1">gebruiken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Die waeren In <rhyme label="b" type="m" corresp="#s2:l2">dantgier</rhyme></l>
        <l id="s2:l5" met="3" type="-+">al doer dees winters <rhyme label="c" type="f">krachten</rhyme></l>
        <l id="s2:l6" met="3" type="-+">gebrueckt nu uwe <rhyme label="d" type="m">Iuecht</rhyme></l>
        <l id="s2:l7" met="3" type="-+">wilt alle druck <rhyme label="c" type="f" corresp="#s2:l5">voorpachten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Die mey diet al <rhyme label="d" type="m" corresp="#s2:l6">voorhuecht</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Tgoudts blomken schoen voer <rhyme label="a" type="f">heeuen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Dat hem doer den dach <rhyme label="b" type="m">ondoet</rhyme></l>
        <l id="s3:l3" met="3" type="-+">geen schoender blom <rhyme label="a" type="f" corresp="#s3:l1">beseouen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tansien voerhuecht den <rhyme label="b" type="m" corresp="#s3:l2">meet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">het staet nu Int <rhyme label="c" type="f">saesoen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">voersiert als gy sien <rhyme label="d" type="m">muecht</rhyme></l>
        <l id="s3:l7" met="3" type="-+">wilt alle druck <rhyme label="c" type="f" corresp="#s3:l5">voerpachten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Door den mey diet al <rhyme label="d" type="m" corresp="#s3:l6">reetIeert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gy prinse wilt <rhyme label="a" type="f">ontfonken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">doer deesen meye tyt <rhyme label="b" type="m">plaesant</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wt liefde heeft u <rhyme label="a" type="f" corresp="#s4:l1">geschonken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Reetoerykae <rhyme label="b" type="m" corresp="#s4:l2">faeliant</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die schoen eedele <rhyme label="c" type="f">vrouwe</rhyme></l>
        <l id="s4:l6" met="3" type="-+">versiert met eedele <rhyme label="d" type="m">duecht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">gy geeste van der <rhyme label="c" type="f" corresp="#s4:l5">goude</rhyme></l>
        <l id="s4:l8" met="3" type="-+">syt duer den mey <rhyme label="d" type="m" corresp="#s4:l6">voerhuecht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
