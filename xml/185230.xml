  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Myn lief myn <rhyme label="a" type="f">uitgelezen</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">O voorwerp van myn <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ei wild zoo wreed niet <rhyme label="a" type="f" corresp="#s1:l1">wezen</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">Gy zyt staat in myn <rhyme label="b" type="m" corresp="#s1:l2">zin</rhyme>?</l>
        <l id="s1:l5" met="4" type="-+">Verdriet en eindeloze <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Kon gy in my <rhyme label="a" type="f" corresp="#s1:l1">genezen</rhyme>;</l>
        <l id="s1:l7" met="4" type="-+">Maar weet dat ik u wysheid <rhyme label="c" type="m" corresp="#s1:l5">tart</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Daar min vervult myn <rhyme label="c" type="m" corresp="#s1:l5">hart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Waarom komt mond en <rhyme label="a" type="f">zinnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Te zaam niet over <rhyme label="b" type="m">een</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Wy zweeren te <rhyme label="a" type="f" corresp="#s2:l1">beminnen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met mond, maar 't hert meend <rhyme label="b" type="m" corresp="#s2:l2">neen</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Helaas die Eed werd niet <rhyme label="c" type="m">volbragt</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Hoe los waard gy van <rhyme label="a" type="f" corresp="#s2:l1">zinnen</rhyme>?</l>
        <l id="s2:l7" met="4" type="-+">Dog schoon de liefde word <rhyme label="c" type="m" corresp="#s2:l5">veragt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ik nogtans naar uw <rhyme label="c" type="m" corresp="#s2:l5">tragt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En schoon gy zyt <rhyme label="a" type="f">bewogen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In al myn ziels <rhyme label="b" type="m">verdriet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wild myn weermin <rhyme label="a" type="f" corresp="#s3:l1">gedogen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dan is myn smert tot <rhyme label="b" type="m" corresp="#s3:l2">niet</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Ach dat u hart eens werd <rhyme label="c" type="m">geraakt</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En toonden <rhyme label="a" type="f" corresp="#s3:l1">mededoogen</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dan is myn heil op aard <rhyme label="c" type="m" corresp="#s3:l5">volmaakt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Daar reeds myn ziel na <rhyme label="c" type="m" corresp="#s3:l5">haakt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
