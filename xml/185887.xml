<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185887</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1790</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ton humeur est Catherine</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Duyzent kussies <rhyme label="a" type="f">duyzentmalen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Een die zoo veel <rhyme label="b" type="m">duyzentmael</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">Als 'er flonkerend Ligjes <rhyme label="a" type="f" corresp="#s1:l1">pralen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Aen de blaeuwe <rhyme label="b" type="m" corresp="#s1:l2">Hemel-zael</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Geven het genoegen <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Dat een hert, een hert <rhyme label="c" type="m" corresp="#s1:l5">aenbiet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Als een Minnaers hert komt <rhyme label="a" type="f">zugten</rhyme>!</l>
        <l id="s2:l2" met="4" type="+-">Na de gunst van zyn <rhyme label="b" type="m">vrindin</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">En onophoudelyk gaet <rhyme label="a" type="f" corresp="#s2:l1">dugten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Om te krygen <rhyme label="b" type="m" corresp="#s2:l2">wedermin</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Kan dat vergenoeging <rhyme label="c" type="m">zyn</rhyme>?</l>
        <l id="s2:l6" met="4" type="+-">Neen, Ã´ neen! het is maer <rhyme label="c" type="m" corresp="#s2:l5">pyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hoe zugt Kloris onder 't <rhyme label="a" type="f">lommer</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Van Ebbe- of <rhyme label="b" type="m">Lindegroen</rhyme>?</l>
        <l id="s3:l3" met="4" type="-+">Wat voor vreugt kan dog zyn <rhyme label="a" type="f" corresp="#s3:l1">kommer</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Aen het zielgenoegen <rhyme label="b" type="m" corresp="#s3:l2">doen</rhyme>?</l>
        <l id="s3:l5" met="4" type="+-">Hy haakt na dien blyden <rhyme label="c" type="m">stond</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Om te kussen Roosies <rhyme label="c" type="m" corresp="#s3:l5">mond</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Kloris in de groene <rhyme label="a" type="f">Elzen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Loert al op zyn <rhyme label="b" type="m">Ziels-vrindin</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Om haar staadig te <rhyme label="a" type="f" corresp="#s4:l1">omhelzen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Boezemt haar zyn liefde <rhyme label="b" type="m" corresp="#s4:l2">in</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Door zyn zugte die 't <rhyme label="c" type="m">gemoed</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Troost in hunne <rhyme label="c" type="m" corresp="#s4:l5">tegenspoed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Zugten die het hert doet <rhyme label="a" type="f">beven</rhyme>!</l>
        <l id="s5:l2" met="4" type="+-">Tranen die men stadig <rhyme label="b" type="m">schreid</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">Kunnen geen genoegen <rhyme label="a" type="f" corresp="#s5:l1">geven</rhyme>;</l>
        <l id="s5:l4" met="4" type="+-">Want het hert staet niet <rhyme label="b" type="m" corresp="#s5:l2">bereid</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Om met zuyvere ziel en <rhyme label="c" type="m">zin</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">'t Zoet te maalen van de <rhyme label="c" type="m" corresp="#s5:l5">Min</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Dus wat baet al 't ydel <rhyme label="a" type="f">poogen</rhyme>?</l>
        <l id="s6:l2" met="4" type="+-">Traanen, zugten en <rhyme label="b" type="m">geween</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Voldoene niet aen het <rhyme label="a" type="f" corresp="#s6:l1">vermogen</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat zoo yverig werd <rhyme label="b" type="m" corresp="#s6:l2">bestreen</rhyme>!</l>
        <l id="s6:l5" met="4" type="+-">Dog de deugt vind steeds haer <rhyme label="c" type="m">bron</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">O trouwe deugt! o vreugde <rhyme label="c" type="m" corresp="#s6:l5">zon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
