  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn Suyk’re Iaepje <rhyme label="a" type="m">slurf</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Als ick de stroop mag <rhyme label="b" type="f">licken</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Wt jou confijte <rhyme label="a" type="m" corresp="#s1:l1">Murf</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">So dangst men hart op <rhyme label="b" type="f" corresp="#s1:l2">kricken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">’k Sweer by mijn droge <rhyme label="a" type="m">keel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En al de <rhyme label="b" type="f">Schelementen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Iou zelschop is soo <rhyme label="a" type="m" corresp="#s2:l1">eel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Soo eel als kool met <rhyme label="b" type="f" corresp="#s2:l2">krenten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als Stremlis uyt een <rhyme label="a" type="m">nap</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Als bolle veur een <rhyme label="b" type="f">Slapert</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als botter in de <rhyme label="a" type="m" corresp="#s3:l1">Pap</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Als Beuling in de <rhyme label="b" type="f" corresp="#s3:l2">Gapert</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn daget uyt den <rhyme label="a" type="m">Oost</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Slaet lammert oom niet heel <rhyme label="b" type="f">of</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Want laetj’ hem <rhyme label="a" type="m" corresp="#s4:l1">ongetroost</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hy bijt hem selfs de keel <rhyme label="b" type="f" corresp="#s4:l2">of</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
