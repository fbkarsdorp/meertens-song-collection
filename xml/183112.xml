<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183112</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Al is ons prinsje nog zo klein</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Ons Prinsje is een dappere held, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s1:l2" met="6" type="+-">Ons Prinsje is een dappere held, ha, <rhyme label="a" type="m" corresp="#s1:l1">ha</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ons Prinsje is een dappere <rhyme label="b" type="m">held</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En Pruisen trekt voor hem te <rhyme label="b" type="m" corresp="#s1:l3">veld</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s1:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Ons Prinsje die is op de jagt, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s2:l2" met="6" type="+-">Ons Prinsje die is op de jagt, ha, <rhyme label="a" type="m" corresp="#s2:l1">ha</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ons Prinsje die is op de <rhyme label="b" type="m">jagt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Hy heeft Utrecht te regt <rhyme label="b" type="m" corresp="#s2:l3">gebragt</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s2:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Als Prinsje by nagt te slapen lag, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s3:l2" met="6" type="+-">Als Prinsje by nagt te slaapen lag, ha, <rhyme label="a" type="m" corresp="#s3:l1">ha</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Als Prinsje by nagt te slaapen <rhyme label="b" type="m">lag</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Zo kwam 'er de Vyand <rhyme label="b" type="m" corresp="#s3:l3">onverwagt</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s3:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Zy jaagde de Vyand op de vlugt, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s4:l2" met="6" type="+-">Zy jaagde de Vyand op de vlugt, ha, <rhyme label="a" type="m" corresp="#s4:l1">ha</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy jaagde de Vyand op de <rhyme label="b" type="m">vlugt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Al by de Bilt, met groot <rhyme label="b" type="m" corresp="#s4:l3">genugt</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s4:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="+-">Toen zy te Soestdyk kwame aan, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s5:l2" met="6" type="+-">Toen zy te Soestdyk kwamen aan, ha, <rhyme label="a" type="m" corresp="#s5:l1">ha</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Toen zy te Soestdyk kwamen <rhyme label="b" type="m">aan</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Daar moesten zy met schande <rhyme label="b" type="m" corresp="#s5:l3">gaan</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s5:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="+-">Hy zy, myn volk stap dapper voort, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s6:l2" met="6" type="+-">Hy zy, myn volk stap dapper voort, ha, <rhyme label="a" type="m" corresp="#s6:l1">ha</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hy zy, myn volk stap dapper <rhyme label="b" type="m">voort</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Ontzie nu maar geen kruit of <rhyme label="b" type="m" corresp="#s6:l3">loot</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s6:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="+-">Hy zy, myn volk behou de moed, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s7:l2" met="6" type="+-">Hy zy, myn volk behou de moed, ha, <rhyme label="a" type="m" corresp="#s7:l1">ha</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Hy zy, myn volk behou de <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Terwyl de kans voor ons is <rhyme label="b" type="m" corresp="#s7:l3">goed</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s7:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="+-">Ons Prinsje is weer in den Haag, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s8:l2" met="6" type="+-">Ons Prinsje is weer in den Haag, ha, <rhyme label="a" type="m" corresp="#s8:l1">ha</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ons Prinsje is weer in den <rhyme label="b" type="m">Haag</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Daar draagt hy een <rhyme label="b" type="m" corresp="#s8:l3">Oranje-kraag</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s8:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="+-">De Burgers waren daar vol moed, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s9:l2" met="6" type="+-">De Burgers waren daar vol moed, ha, <rhyme label="a" type="m" corresp="#s9:l1">ha</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">De Burgers waren daar vol <rhyme label="b" type="m">moed</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Om dat zy zagen 's Prinsen <rhyme label="b" type="m" corresp="#s9:l3">Bloed</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s9:l1">falderala</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="+-">Al die dit liedje heeft gedigt, ha, <rhyme label="a" type="m">ha</rhyme>,</l>
        <l id="s10:l2" met="6" type="+-">Al die dit liedje heeft gedigt, ha, <rhyme label="a" type="m" corresp="#s10:l1">ha</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Al die dit liedje heeft <rhyme label="b" type="m">gedigt</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Was een Prinsman, zyn hertje was <rhyme label="b" type="m" corresp="#s10:l3">ligt</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">ha, ha, van <rhyme label="a" type="m" corresp="#s10:l1">falderala</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
