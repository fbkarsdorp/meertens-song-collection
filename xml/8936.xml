    <melody>Durch Adams Fall ist ganz verderbt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Heer die is mijn Herder <rhyme label="a" type="m">goet</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">My en sal niet <rhyme label="b" type="f">ghebreken</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Hy weydt my op valleyen <rhyme label="a" type="m" corresp="#s1:l1">soet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Tot verssche water <rhyme label="b" type="f" corresp="#s1:l2">beken</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Hy my beschickt// En hy <rhyme label="c" type="m">verquickt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Mijn ziel: Op rechter <rhyme label="d" type="f">straten</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Voert hy my stil// Om syns Naems <rhyme label="e" type="m">wil</rhyme>:</l>
        <l id="s1:l8" met="3" type="-+">Hy komt my ras te <rhyme label="d" type="f" corresp="#s1:l6">baten</rhyme>.</l>
      </lg>
    </lg>
  </text>
