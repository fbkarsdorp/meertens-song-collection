    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Wilder dan wild</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Anna Moeder wy u <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">En groeten u met diep <rhyme label="b" type="m">ootmoet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Wilt ons ghebedt tot Iesum <rhyme label="a" type="f" corresp="#s1:l1">Keeren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Al met u lieve dochter <rhyme label="b" type="m" corresp="#s1:l2">soet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghy waert van Godt daer toe <rhyme label="a" type="f">verkoren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">En voorsien vander <rhyme label="b" type="m">eeuwicheydt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Op dat van u soud' worden <rhyme label="a" type="f" corresp="#s2:l1">gheboren</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Godts lieve Moeder langh <rhyme label="b" type="m" corresp="#s2:l2">Verbeydt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die ghy ghebaert hebt met <rhyme label="a" type="f">verblijden</rhyme></l>
        <l id="s3:l2" met="4" type="-+">En oock met admiratie <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want het was in u oude <rhyme label="a" type="f" corresp="#s3:l1">tijden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En alle u hope was te <rhyme label="b" type="m" corresp="#s3:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Och met wat vreught en <rhyme label="a" type="f">ghenuchte</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Offerden ghy dit saligh <rhyme label="b" type="m">kindt</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Het was voorwaer een soete <rhyme label="a" type="f" corresp="#s4:l1">vruchte</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Die Godt van eeuwigheydt had <rhyme label="b" type="m" corresp="#s4:l2">bemindt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">O weerdige vrouw' seer hoogh <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Al met u lieve dochter <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Bidt toch voor ons op'dat wy <rhyme label="a" type="f" corresp="#s5:l1">wesen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Eens mochten indes hemels <rhyme label="b" type="m" corresp="#s5:l2">throon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
