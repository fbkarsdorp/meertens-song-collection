<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3885</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="mystical song">mystiek lied</tag>
    </tags>
    <melody>Een ridder en een meisje jong</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Had ick eenen getrouwen <rhyme label="a" type="m">bode</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En die waer hoghe van <rhyme label="b" type="f">prise</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Ick souden seynden in mijns vaders <rhyme label="c" type="m">lant</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mi en lust hier niet langher te <rhyme label="b" type="f" corresp="#s1:l2">bliuuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick heb gheleeft so menigen <rhyme label="a" type="m">dach</rhyme></l>
        <l id="s2:l2" met="3" type="-+">In also groter <rhyme label="b" type="f">qualen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Ic sou so gaerne ontbonden <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Woude my mijn vader thuyswaert <rhyme label="b" type="f" corresp="#s2:l2">halen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ic de werelt wel <rhyme label="a" type="m">ouersie</rhyme></l>
        <l id="s3:l2" met="3" type="-+">So wort mijn herte so <rhyme label="b" type="f">bange</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Want ick so menighen verborghen strick <rhyme label="c" type="m">sie</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Al voer mijn ooghen <rhyme label="b" type="f" corresp="#s3:l2">hanghen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ten is voerwaer gheen wonder <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Al wort mijn hertken dickwils <rhyme label="b" type="f">banghe</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Daer wort soe menighe <rhyme label="c" type="m">duechdelijck</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Met desen stricken <rhyme label="b" type="f" corresp="#s4:l2">gheuanghen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Al had mi al dese werelt <rhyme label="a" type="m">lief</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met herten <rhyme label="b" type="f">wtuercoren</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Had ick Ihesum in mijn herte <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">So waert toch altemael <rhyme label="b" type="f" corresp="#s5:l2">verloren</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als ic dese werelt al <rhyme label="a" type="m">ouersie</rhyme></l>
        <l id="s6:l2" met="3" type="-+">So en vinde ic nergens <rhyme label="b" type="f">ruste</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Waer ic mi keere oft <rhyme label="c" type="m">wende</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Ic ligghe in swaren <rhyme label="b" type="f" corresp="#s6:l2">suchten</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ic wil gaen alde werelt <rhyme label="a" type="m">loos</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Met haren stricken laten <rhyme label="b" type="f">varen</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Ende seynden eenen trouwen <rhyme label="c" type="m">bode</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Tot God minen hemelscen <rhyme label="b" type="f" corresp="#s7:l2">vader</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Als hi mijn druckelike stemme <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s8:l2" met="3" type="-+">In sinen ooren <rhyme label="b" type="f">luyden</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Hi heeft mi so herteliken <rhyme label="c" type="m">lief</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Hi sal mi wel haest <rhyme label="b" type="f" corresp="#s8:l2">verbliden</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die bode liep seer <rhyme label="a" type="m">haestelijc</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Tot God haren hemelscen <rhyme label="b" type="f">vader</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Hi badt hem also <rhyme label="c" type="m">vriendelijck</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Dat hi sijn kint woude thuyswaerts <rhyme label="b" type="f" corresp="#s9:l2">halen</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Segt minen kinde seer <rhyme label="a" type="m">haestelijc</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Si moet een luttel <rhyme label="b" type="f">verbeyden</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Mijn sone heeft op <rhyme label="c" type="m">aertrijcke</rhyme></l>
        <l id="s10:l4" met="3" type="-+">So veel om haer <rhyme label="b" type="f" corresp="#s10:l2">gheleden</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Hy heeft haer costelijck hemels <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Tot haren troost <rhyme label="b" type="f">ghelaten</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Nochtans en was si niet te <rhyme label="c" type="m">vreden</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Si en wilde noch hoogher dingen <rhyme label="b" type="f" corresp="#s11:l2">smaken</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Segt haer si moet de herde <rhyme label="a" type="m">not</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Der bitter doot ierst <rhyme label="b" type="f">craken</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Eer si die kerne der <rhyme label="c" type="m">soeticheyt</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Van mijnre godheyt mach <rhyme label="b" type="f" corresp="#s12:l2">smaken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Segt haer de bruloft is al <rhyme label="a" type="m">bereet</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Dat broot dat is <rhyme label="b" type="f">ghebacken</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Die croon die ooc seer blinckende <rhyme label="c" type="m">steet</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Den wijn die salmen <rhyme label="b" type="f" corresp="#s13:l2">tappen</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Die camer daer si in wonen <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Die is so lieflijck den <rhyme label="b" type="f">ooghen</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Daer sal si sien dat Paulus <rhyme label="c" type="m">sach</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Doen hi was op <rhyme label="b" type="f" corresp="#s14:l2">ghetogen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Hier mede troost mi mijn beminde <rhyme label="a" type="m">bruyt</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Ende segt haer dat si op mi wil <rhyme label="b" type="f">dincken</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Ic wil altoos haer stierman <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Als haer haer scipken wil <rhyme label="b" type="f" corresp="#s15:l2">ontsincken</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Die ons dit liedeken eerstwerf <rhyme label="a" type="m">sanc</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Met swaren druck is hi <rhyme label="b" type="f">beuanghen</rhyme></l>
        <l id="s16:l3" met="4" type="-+">Om te comen daermen heer Iesum <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Daer na staet al sijn <rhyme label="b" type="f" corresp="#s16:l2">verlangen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
