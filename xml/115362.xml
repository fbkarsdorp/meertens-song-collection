    <melody>Mijn allerliefste verheven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">T Huw’lijc wil ic gaen <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Holla, siet wat ghy <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ghy spreeckt als de <rhyme label="a" type="f" corresp="#s1:l1">onwijsen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En mist den rechten <rhyme label="b" type="m" corresp="#s1:l2">voet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Paulus seydt ‘t is niet <rhyme label="b" type="m" corresp="#s1:l2">goedt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Den mensch een vrouwe te <rhyme label="c" type="f">raken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Ghy gaet t’onrechte <rhyme label="c" type="f" corresp="#s1:l6">laken</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Godes instellingh <rhyme label="b" type="m" corresp="#s1:l2">vroet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Kondy tot dien <rhyme label="a" type="f">bestiere</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Bewijsen dat ick <rhyme label="b" type="m">gis</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ja na raders <rhyme label="a" type="f" corresp="#s2:l1">maniere</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Soo slady hier oock <rhyme label="b" type="m" corresp="#s2:l2">mis</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Maer bey dat gaet <rhyme label="b" type="m" corresp="#s2:l2">gewis</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">De maeghden-staet <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Verdient het eeuwigh <rhyme label="c" type="f" corresp="#s2:l6">leven</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Bewijst hoe waer dat <rhyme label="b" type="m" corresp="#s2:l2">is</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Let op d’ontelbaer <rhyme label="a" type="f">Maegden</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die ‘t Lam al volgen <rhyme label="b" type="m">na</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Maer ofmen u af <rhyme label="a" type="f" corresp="#s3:l1">vraeghden</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Of niet Abram <rhyme label="b" type="m" corresp="#s3:l2">Sara</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En Petrus <rhyme label="b" type="m" corresp="#s3:l2">Lidia</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En veel salige <rhyme label="c" type="f">paren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Geest’lijke Maeghden <rhyme label="c" type="f" corresp="#s3:l6">waren</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Soudt ghy niet seggen <rhyme label="b" type="m" corresp="#s3:l2">Ja</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Paulus in sijne <rhyme label="a" type="f">brieven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Verheft den Maeghden <rhyme label="b" type="m">staet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy acht nae u <rhyme label="a" type="f" corresp="#s4:l1">believen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer ‘t Houwlijc niet voor <rhyme label="b" type="m" corresp="#s4:l2">quaet</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Hy geeft de Maeghden <rhyme label="b" type="m" corresp="#s4:l2">raet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Reyn ongehouwt te <rhyme label="c" type="f">blijven</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Ick wil niet met u <rhyme label="c" type="f" corresp="#s4:l6">kijven</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Want ghy ‘t u niet <rhyme label="b" type="m" corresp="#s4:l2">verstaet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Princen Princessen <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Paulus tot onser <rhyme label="b" type="m">baet</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Leert hoe elck kan wel <rhyme label="a" type="f" corresp="#s5:l1">leven</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Geest’lijck in bey den <rhyme label="b" type="m" corresp="#s5:l2">staet</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Blijfdy soo ‘t is niet <rhyme label="b" type="m" corresp="#s5:l2">quaedt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Of komdy ‘t Huw’lijcx <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dient in reynheydt den <rhyme label="c" type="f" corresp="#s5:l6">Heere</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Onkuysheyt hy <rhyme label="b" type="m" corresp="#s5:l2">versmaet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
