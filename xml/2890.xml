  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Oe schoene goddinne Ick u <rhyme label="a" type="f">beminne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">boeven al die daer <rhyme label="b" type="f">leeven</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Want In mijn sinne de <rhyme label="a" type="f" corresp="#s1:l1">koeninInne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">hoe kuent gij mijn <rhyme label="b" type="f" corresp="#s1:l2">begeeven</rhyme></l>
        <l id="s1:l5" met="4" type="-+">duer u liefelick <rhyme label="c" type="m">aenschin</rhyme></l>
        <l id="s1:l6" met="3" type="-+">mijn pin mijn droetheit heel <rhyme label="d" type="m">voergaet</rhyme></l>
        <l id="s1:l7" met="4" type="+-">wilt mijnder genaedich sijn seer <rhyme label="c" type="m" corresp="#s1:l5">fijn</rhyme></l>
        <l id="s1:l8" met="3" type="-+">oerein maechdelick <rhyme label="d" type="m" corresp="#s1:l6">saet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">u gout geel haer dat krult <rhyme label="a" type="f">voorwaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">op u voerhoeft <rhyme label="b" type="f">voerheeven</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Schoenlief eenpaer mocht Ick <rhyme label="a" type="f" corresp="#s2:l1">aldaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">met u In vruechde <rhyme label="b" type="f" corresp="#s2:l2">leeven</rhyme></l>
        <l id="s2:l5" met="4" type="-+">wij souden een meeloedij seer <rhyme label="c" type="m">blij</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Den tijt meest brengen <rhyme label="d" type="m">duer</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Ick stelder geheel aen sijn van <rhyme label="c" type="m" corresp="#s2:l5">mij</rhyme></l>
        <l id="s2:l8" met="3" type="-+">droufheit en alle <rhyme label="d" type="m" corresp="#s2:l6">getruer</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Oe schoene prinsesse, wserdich <rhyme label="a" type="f">maetresse</rhyme></l>
        <l id="s3:l2" met="3" type="-+">weygert mijn geen <rhyme label="b" type="f">genaede</rhyme></l>
        <l id="s3:l3" met="4" type="-+">gij sulter exppresse sijn mijn <rhyme label="a" type="f" corresp="#s3:l1">voechdesse</rhyme></l>
        <l id="s3:l4" met="3" type="-+">wilt u daer toe <rhyme label="b" type="f" corresp="#s3:l2">beraede</rhyme></l>
        <l id="s3:l5" met="4" type="-+">want gij sijter mijn liefie konstant <rhyme label="c" type="m">samblant</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Int hert wel schoene <rhyme label="d" type="m">Jonckfrou</rhyme></l>
        <l id="s3:l7" met="4" type="+-">reyckt mijnder u soete hant <rhyme label="c" type="m" corresp="#s3:l5">samblant</rhyme></l>
        <l id="s3:l8" met="3" type="-+">soe veer draech Ick miJn <rhyme label="d" type="m" corresp="#s3:l6">rou</rhyme>.</l>
      </lg>
    </lg>
  </text>
