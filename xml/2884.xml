    <melody>Verheug u maak jolijt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Verhuecht u maeckt <rhyme label="a" type="m">Joelijt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Aldoer Des meyen <rhyme label="a" type="m" corresp="#s1:l1">tijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die des winters <rhyme label="b" type="m">gewelt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">verdrijft met al haer <rhyme label="c" type="m">macht</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Des waeters vloet <rhyme label="a" type="m" corresp="#s1:l1">suebijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">haegel en snee Dat <rhyme label="a" type="m" corresp="#s1:l1">slijt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Door de son heel <rhyme label="b" type="m" corresp="#s1:l3">versmelt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Den tijt die baert haer <rhyme label="c" type="m" corresp="#s1:l4">kracht</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Die blomkens <rhyme label="d" type="m">neenichderley</rhyme></l>
        <l id="s1:l10" met="3" type="-+">al op de Droege <rhyme label="d" type="m" corresp="#s1:l9">hey</rhyme></l>
        <l id="s1:l11" met="3" type="+-">bloeyen soemen <rhyme label="e" type="m">aenschout</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Die winter <rhyme label="e" type="m" corresp="#s1:l11">kout</rhyme></l>
        <l id="s1:l13" met="3" type="-+">hielt bedwongen Inde <rhyme label="d" type="m" corresp="#s1:l9">wey</rhyme></l>
        <l id="s1:l14" met="3" type="-+">maer tlof nu schoen <rhyme label="e" type="m" corresp="#s1:l11">bedout</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">het wout seer schoen <rhyme label="a" type="m">omdaen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">(me)t lof en kruit <rhyme label="a" type="m" corresp="#s2:l1">belaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">(tw)elck tsmenschen hert <rhyme label="b" type="m">voerhuecht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">stoet nu seer <rhyme label="c" type="m">tryonfant</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Den vyge boem haer <rhyme label="a" type="m" corresp="#s2:l1">blaen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Des meys dou heeft <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dies met lust hem voor <rhyme label="b" type="m" corresp="#s2:l3">huecht</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Int hof vol blomme <rhyme label="c" type="m" corresp="#s2:l4">plaesant</rhyme></l>
        <l id="s2:l9" met="3" type="-+">en u Daer in voor <rhyme label="d" type="m">Jubyleert</rhyme></l>
        <l id="s2:l10" met="3" type="-+">pluckt dat u hart <rhyme label="d" type="m" corresp="#s2:l9">begeert</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Den egelantier <rhyme label="e" type="m">ontluickt</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Den leely sijn kracht <rhyme label="e" type="m" corresp="#s2:l11">bruickt</rhyme></l>
        <l id="s2:l13" met="3" type="-+">En den dooren haech <rhyme label="d" type="m" corresp="#s2:l9">geeert</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Met bloeysem twelck soet <rhyme label="e" type="m" corresp="#s2:l11">ruickt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer hoert men Den <rhyme label="a" type="m">nachtegael</rhyme></l>
        <l id="s3:l2" met="3" type="-+">met noch meenich vogel <rhyme label="a" type="m" corresp="#s3:l1">tael</rhyme></l>
        <l id="s3:l3" met="3" type="-+">versiert met pluimen <rhyme label="b" type="m">schoen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Elck sijn naeyuer brenckt <rhyme label="c" type="m">voert</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Des tortelduif <rhyme label="a" type="m" corresp="#s3:l1">voorhael</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Den leewerick <rhyme label="a" type="m" corresp="#s3:l1">geenerael</rhyme></l>
        <l id="s3:l7" met="3" type="-+">zinckt met vruechDen sijn <rhyme label="b" type="m" corresp="#s3:l3">t(oen</rhyme>)</l>
        <l id="s3:l8" met="3" type="-+">met lof seer soet <rhyme label="c" type="m" corresp="#s3:l4">ackoo(rt</rhyme>)</l>
        <l id="s3:l9" met="3" type="-+">Boeven Der snaeren <rhyme label="d" type="m">geklanck</rhyme></l>
        <l id="s3:l10" met="3" type="-+">elck sinckt sijn scheppers <rhyme label="d" type="m" corresp="#s3:l9">danck</rhyme></l>
        <l id="s3:l11" met="3" type="+-">geen schoender dinck men <rhyme label="e" type="m">vint</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Elck dier sijn tijt <rhyme label="e" type="m" corresp="#s3:l11">bemint</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Dan den menschen van sinnen <rhyme label="d" type="m" corresp="#s3:l9">kranck</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Wert dickmaels oever <rhyme label="e" type="m" corresp="#s3:l11">rint</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Lof godt den prinsen <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">hij is den wijn stock <rhyme label="a" type="m" corresp="#s4:l1">zoet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Doer hem komt alt <rhyme label="b" type="m">sieraet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat nu voor doegen <rhyme label="c" type="m">blickt</rhyme></l>
        <l id="s4:l5" met="3" type="-+">hij is die ons <rhyme label="a" type="m" corresp="#s4:l1">behoet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">en al dat leeft hij <rhyme label="a" type="m" corresp="#s4:l1">voet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">.............een getrouwe <rhyme label="b" type="m" corresp="#s4:l3">toeverlaet</rhyme></l>
        <l id="s4:l8" met="3" type="-+">..........  Dee(s)? mey niet af en <rhyme label="c" type="m" corresp="#s4:l4">gaet</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Wilt hem doch eere <rhyme label="d" type="m">bien</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Want hem b(uige) alle <rhyme label="d" type="m" corresp="#s4:l9">knien</rhyme></l>
        <l id="s4:l11" met="3" type="+-">stiert pays voor een <rhyme label="e" type="m">present</rhyme></l>
        <l id="s4:l12" met="3" type="-+">dat elck u (kr) acht <rhyme label="e" type="m" corresp="#s4:l11">kent</rhyme></l>
        <l id="s4:l13" met="3" type="-+">u woort hier laet <rhyme label="d" type="m" corresp="#s4:l9">geschien</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Door Liefde Ist <rhyme label="e" type="m" corresp="#s4:l11">tfondaement</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
