  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wie sall ich fre dtt <rhyme label="a" type="f">beginnen</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">ich bin bedro ett <rhyme label="b" type="m">gar</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">mein lieb hatt mich <rhyme label="a" type="f" corresp="#s1:l1">begeben</rhyme></l>
        <l id="s1:l4" met="3" type="-+">des bin ich worden <rhyme label="b" type="m" corresp="#s1:l2">gewar</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">Jr fre ndtschafft will ich mir von mir <rhyme label="c" type="f">stellen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">die sei mir plach z  th <rhyme label="d" type="m">n</rhyme> /</l>
        <l id="s1:l7" met="3" type="-+">deß mochtt ehr werdenn re <rhyme label="c" type="f" corresp="#s1:l5">wenn</rhyme> /</l>
        <l id="s1:l8" met="3" type="-+">das ellendtt moß ich ba <rhyme label="c" type="f" corresp="#s1:l5">wenn</rhyme> /</l>
        <l id="s1:l9" met="3" type="-+">ich moß farhenn <rhyme label="d" type="m" corresp="#s1:l6">lann</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">N  hin ich moß mich <rhyme label="a" type="f">leidenn</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">ich m ß sei farenn <rhyme label="b" type="m">lassenn</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Jch will mich selbers <rhyme label="a" type="f" corresp="#s2:l1">verbleidenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">daß bestenn das ich <rhyme label="b" type="m" corresp="#s2:l2">kan</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Jch finden keinen trost a ff <rhyme label="c" type="f">erdenn</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">das sag ich sunder <rhyme label="d" type="m">wann</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Jn tre wenn will ich <rhyme label="c" type="f" corresp="#s2:l5">felherdenn</rhyme></l>
        <l id="s2:l8" met="3" type="-+">eß magh noch besser <rhyme label="c" type="f" corresp="#s2:l5">werdenn</rhyme> /</l>
        <l id="s2:l9" met="3" type="-+">der eß erwartenn <rhyme label="d" type="m" corresp="#s2:l6">kan</rhyme>.</l>
      </lg>
    </lg>
