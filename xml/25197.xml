<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25197</id>
    <date>
      <year-start>1260</year-start>
      <year-end>1280</year-end>
    </date>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mijn doghen willic met vruechden <rhyme label="a" type="f">draghen</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Up aventueren watter <rhyme label="b" type="m">ghesciet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">want al wildic mi <rhyme label="a" type="f" corresp="#s1:l1">vercnaghen</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Mi ne ware dies te beter <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">de sulke sout sien hi soude mi <rhyme label="a" type="f" corresp="#s1:l1">claghen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">nochtanne ware lief hem mijn <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ic wille mi blidelike <rhyme label="a" type="f">ghelaten</rhyme></l>
        <l id="s2:l2" met="4" type="-+">al legghic nu onder <rhyme label="b" type="m">voet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Si sullens verdrouven die mi <rhyme label="a" type="f" corresp="#s2:l1">haten</rhyme></l>
        <l id="s2:l4" met="4" type="-+">want ic pense in minen <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Ic sals comen te goeder <rhyme label="a" type="f" corresp="#s2:l1">baten</rhyme></l>
        <l id="s2:l6" met="4" type="-+">al hebbic noch cranken <rhyme label="b" type="m" corresp="#s2:l2">spoet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
