  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wiewoll ich arm vnnd ellendt <rhyme label="a" type="m">bin</rhyme> /</l>
        <l id="s1:l2" met="4" type="-+">So hab ich doch einenn stättigenn <rhyme label="a" type="m" corresp="#s1:l1">Syn</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">Hoffn ng thuitt mich <rhyme label="b" type="f">ernerenn</rhyme> /</l>
        <l id="s1:l4" met="4" type="-+">Das mir vonn gott bescherett <rhyme label="c" type="m">iß</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">kan mir kein kleffer nitt <rhyme label="b" type="f" corresp="#s1:l3">werhen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Der falschenn z ngenn seintt so <rhyme label="a" type="m">fill</rhyme> /</l>
        <l id="s2:l2" met="4" type="-+">Jch hoff eß soll jnn helffenn <rhyme label="a" type="m" corresp="#s2:l1">nitt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Gott is noch voller g <rhyme label="b" type="f">ette</rhyme> /</l>
        <l id="s2:l4" met="4" type="-+">Dem ich mich gantz unnd gar ergebenn <rhyme label="c" type="m">hab</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Der wirtt mich woll <rhyme label="b" type="f" corresp="#s2:l3">behuten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Mein hertz ist mir bek mmertt <rhyme label="a" type="m">sehr</rhyme></l>
        <l id="s3:l2" met="4" type="-+">gantz  will aldinck z m bestenn <rhyme label="a" type="m" corresp="#s3:l1">kerenn</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">Jch far darhin mitt <rhyme label="b" type="f">schmertzenn</rhyme> /</l>
        <l id="s3:l4" met="4" type="-+">ich sehenn das ichs nitt kan <rhyme label="c" type="m">abelan</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">gott trost all bedrubte <rhyme label="b" type="f" corresp="#s3:l3">herzenn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Ferst  darher vnnd ließ <rhyme label="a" type="m">mich</rhyme></l>
        <l id="s4:l2" met="4" type="-+">was lest  mir zur letzenn <rhyme label="a" type="m" corresp="#s4:l1">hier</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">das ich meins leidtz <rhyme label="b" type="f">vergesse</rhyme> /</l>
        <l id="s4:l4" met="4" type="-+">Stett lieb / trew vnnd <rhyme label="c" type="m">stattigkeitt</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">Las ich dir z  der letzenn <rhyme label="b" type="f" corresp="#s4:l3">heir</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Der  vnß dies newe leidtlein <rhyme label="a" type="m">sanck</rhyme> /</l>
        <l id="s5:l2" met="4" type="-+">ein freier Schreibentt was ehr <rhyme label="a" type="m" corresp="#s5:l1">gnantt</rhyme> /</l>
        <l id="s5:l3" met="3" type="-+">ehr hatz gar frei ges <rhyme label="b" type="f">ngenn</rhyme> /</l>
        <l id="s5:l4" met="4" type="-+">ehr hatt ein feins mägdlein jm hertzenn <rhyme label="c" type="m">lieb</rhyme></l>
        <l id="s5:l5" met="3" type="-+">z  ihr kundt  ehr woll <rhyme label="b" type="f" corresp="#s5:l3">komen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
