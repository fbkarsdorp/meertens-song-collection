<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9160</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Gij die mij met uw braaf gelaat</melody>
  <author>E. Pels de Jonge</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Cupido die mijn hart steets <rhyme label="a" type="m">plaecht</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Met de minnen <rhyme label="b" type="m">brant</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Comt by de gheen die my <rhyme label="a" type="m" corresp="#s1:l1">behaecht</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Of gheeft haer <rhyme label="b" type="m" corresp="#s1:l2">'tverstant</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dat sy my <rhyme label="c" type="m">mint</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Of my <rhyme label="c" type="m" corresp="#s1:l5">ontbint</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Van dese pijn die my <rhyme label="c" type="m" corresp="#s1:l5">verslint</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En die my doet <rhyme label="d" type="f">treure</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Of mijn hart sal <rhyme label="d" type="f" corresp="#s1:l8">scheure</rhyme>,</l>
        <l id="s1:l10" met="2" type="-+">Soo my niet <rhyme label="d" type="f" corresp="#s1:l8">ghebeure</rhyme>,</l>
        <l id="s1:l11" met="2" type="+-">Die my <rhyme label="c" type="m" corresp="#s1:l5">verwint</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat heb ick u Ã´ Godt <rhyme label="a" type="m">misdaen</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Int minst of int <rhyme label="b" type="m">meest</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat ick dees straffe moet <rhyme label="a" type="m" corresp="#s2:l1">ontfaen</rhyme>.</l>
        <l id="s2:l4" met="2" type="-+">Ick heb u <rhyme label="b" type="m" corresp="#s2:l2">ghevreest</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Meer als <rhyme label="c" type="m">Iupijn</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Wiens blixems <rhyme label="c" type="m" corresp="#s2:l5">schijn</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Yder onderdanich moet <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Maer ick gae u <rhyme label="d" type="f">eere</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Cupido veel <rhyme label="d" type="f" corresp="#s2:l8">meere</rhyme>,</l>
        <l id="s2:l10" met="2" type="-+">Waerom comt ghy <rhyme label="d" type="f" corresp="#s2:l8">deere</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">En plaghen <rhyme label="c" type="m" corresp="#s2:l5">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet eens versachten <rhyme label="a" type="m">Galathees</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Verharde <rhyme label="b" type="m">ghemoet</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Maeckt dat sy voor u heeft meer <rhyme label="a" type="m" corresp="#s3:l1">vrees</rhyme></l>
        <l id="s3:l4" met="2" type="-+">En haer als my <rhyme label="b" type="m" corresp="#s3:l2">doet</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Ghevoele <rhyme label="c" type="m">'tverdriet</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Op dat sy <rhyme label="c" type="m" corresp="#s3:l5">siet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Wiens smart sy can bevroede <rhyme label="c" type="m" corresp="#s3:l5">niet</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Schiet een schicht der <rhyme label="d" type="f">minne</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Galathees hart <rhyme label="d" type="f" corresp="#s3:l8">inne</rhyme>,</l>
        <l id="s3:l10" met="2" type="-+">Op dat sy met <rhyme label="d" type="f" corresp="#s3:l8">sinne</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Mijn weermin <rhyme label="c" type="m" corresp="#s3:l5">biet</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Als ick haer eens claech van mijn <rhyme label="a" type="m">smart</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Die ick om haer <rhyme label="b" type="m">ly</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Och vriendt ghy zijt verdoolt te <rhyme label="a" type="m" corresp="#s4:l1">vart</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Sayt sy teghens <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Dat ghy dies <rhyme label="c" type="m">claecht</rhyme></l>
        <l id="s4:l6" met="2" type="-+">En my van haer <rhyme label="c" type="m" corresp="#s4:l5">jaecht</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En is het niet wel wreet <rhyme label="c" type="m" corresp="#s4:l5">gheplaecht</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Hoe dat ick my <rhyme label="d" type="f">sinne</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Op haer legh ick <rhyme label="d" type="f" corresp="#s4:l8">winne</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Niet met het <rhyme label="d" type="f" corresp="#s4:l8">minne</rhyme></l>
        <l id="s4:l11" met="2" type="+-">Haer my niet <rhyme label="c" type="m" corresp="#s4:l5">haecht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Prins Cupido seer groot van <rhyme label="a" type="m">macht</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Verhoort mijn <rhyme label="b" type="m">gheclach</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Toont Galathe doch eens u <rhyme label="a" type="m" corresp="#s5:l1">cracht</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Op dat ick eens <rhyme label="b" type="m" corresp="#s5:l2">mach</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Wesen <rhyme label="c" type="m">bevrijt</rhyme></l>
        <l id="s5:l6" met="2" type="-+">Van desen <rhyme label="c" type="m" corresp="#s5:l5">strijt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En die het binnenst myns Hart <rhyme label="c" type="m" corresp="#s5:l5">doorsnijt</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Laet Galathee <rhyme label="d" type="f">smaecke</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">De minne en <rhyme label="d" type="f" corresp="#s5:l8">maecke</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Wilt dat ick <rhyme label="d" type="f" corresp="#s5:l8">gheraecke</rhyme>,</l>
        <l id="s5:l11" met="2" type="+-">Wt pijn 'tis <rhyme label="c" type="m" corresp="#s5:l5">tijdt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
