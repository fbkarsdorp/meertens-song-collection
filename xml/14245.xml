    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="heiligenlied" translation="saint song">heiligenlied</tag>
    </tags>
    <melody>O Venus' band o vurige brand</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">¶ Van sinte <rhyme label="a" type="m">Katherinen</rhyme>.</l>
        <l id="s1:l2" met="2" type="-+">Op die wise o venus <rhyme label="a" type="m" corresp="#s1:l1">bant</rhyme></l>
        <l id="s1:l3" met="4" type="-+">O Godlike <rhyme label="a" type="m" corresp="#s1:l1">cracht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">van hogher <rhyme label="b" type="f">macht</rhyme></l>
        <l id="s1:l5" met="4" type="-+">wat groter wijsheyt hebdi <rhyme label="a" type="m" corresp="#s1:l1">gewracht</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">al aen een maghet <rhyme label="a" type="m" corresp="#s1:l1">ionghe</rhyme></l>
        <l id="s1:l7" met="2" type="+-">doen ghi haer dat vingerlinc <rhyme label="b" type="f" corresp="#s1:l4">bracht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">want si v diende dach ende <rhyme label="b" type="f" corresp="#s1:l4">nacht</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">tot <rhyme label="a" type="m">oorconden</rhyme>.</l>
        <l id="s2:l2" met="2" type="-+">dat ghi haer trou hebt <rhyme label="a" type="m" corresp="#s2:l1">vonden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">¶ Haer wijsheyt <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme></l>
        <l id="s2:l4" met="3" type="-+">was sonder <rhyme label="b" type="f">ghenoot</rhyme></l>
        <l id="s2:l5" met="4" type="-+">die heylige geest haer hert <rhyme label="a" type="m" corresp="#s2:l1">doerscoot</rhyme></l>
        <l id="s2:l6" met="4" type="-+">teghen die <rhyme label="a" type="m" corresp="#s2:l1">sarazijne</rhyme>.</l>
        <l id="s2:l7" met="2" type="+-">vijftich wise meesters <rhyme label="b" type="f" corresp="#s2:l4">groot</rhyme></l>
        <l id="s2:l8" met="3" type="-+">heeft si met reden al <rhyme label="b" type="f" corresp="#s2:l4">versmoort</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">sonder <rhyme label="a" type="m">pijne</rhyme></l>
        <l id="s3:l2" met="2" type="-+">die reyne maghet <rhyme label="a" type="m" corresp="#s3:l1">katherine</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">¶ Des keysers <rhyme label="a" type="m" corresp="#s3:l1">goet</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">ende hoghen <rhyme label="b" type="f">moet</rhyme></l>
        <l id="s3:l5" met="4" type="-+">hebdi versmaet wt sinnen <rhyme label="a" type="m" corresp="#s3:l1">vroet</rhyme></l>
        <l id="s3:l6" met="4" type="-+">en koost den doot daer <rhyme label="a" type="m" corresp="#s3:l1">vuere</rhyme>.</l>
        <l id="s3:l7" met="2" type="+-">ghi boot den hals wt minnen <rhyme label="b" type="f" corresp="#s3:l4">soet</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">daer wt ran melc voer roode <rhyme label="b" type="f" corresp="#s3:l4">bloet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">tot dier <rhyme label="a" type="m">vren</rhyme></l>
        <l id="s4:l2" met="2" type="-+">bekeerden veel <rhyme label="a" type="m" corresp="#s4:l1">creatueren</rhyme></l>
        <l id="s4:l3" met="4" type="-+">¶ O edel martelaerse <rhyme label="a" type="m" corresp="#s4:l1">vry</rhyme></l>
        <l id="s4:l4" met="3" type="-+">al opten berch van <rhyme label="b" type="f">synay</rhyme></l>
        <l id="s4:l5" met="4" type="-+">deden v die enghelen ter <rhyme label="a" type="m" corresp="#s4:l1">aerden</rhyme></l>
        <l id="s4:l6" met="4" type="-+">daer v so menich ridder <rhyme label="a" type="m" corresp="#s4:l1">vry</rhyme></l>
        <l id="s4:l7" met="2" type="+-">versoect wt edelre <rhyme label="b" type="f" corresp="#s4:l4">heerscapi</rhyme></l>
        <l id="s4:l8" met="3" type="-+">met <rhyme label="b" type="f" corresp="#s4:l4">weerden</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">ende oly van v <rhyme label="a" type="m">begheerden</rhyme></l>
        <l id="s5:l2" met="2" type="-+">¶ Ick bidde v patronersse <rhyme label="a" type="m" corresp="#s5:l1">vroet</rhyme></l>
        <l id="s5:l3" met="4" type="-+">neemt ons allen in v <rhyme label="a" type="m" corresp="#s5:l1">behoet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">ende wilt ons wijsheyt <rhyme label="b" type="f">gheuen</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">in vreden te leuen in minne <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme></l>
        <l id="s5:l6" met="4" type="-+">ende helpt ons aen dat eewighe <rhyme label="a" type="m" corresp="#s5:l1">goet</rhyme></l>
        <l id="s5:l7" met="2" type="+-">sonder <rhyme label="b" type="f" corresp="#s5:l4">sneuen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">hier na met v te <rhyme label="b" type="f" corresp="#s5:l4">leuen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
