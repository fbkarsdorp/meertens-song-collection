  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ioachims ende sinte annen <rhyme label="a" type="m">bloet</rhyme>.</l>
        <l id="s1:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Heeft gedaen ons allen <rhyme label="a" type="m" corresp="#s1:l1">goet</rhyme>.</l>
        <l id="s1:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s1:l2">plena</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s1:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Doen dat kint ter werelt <rhyme label="a" type="m">quam</rhyme>.</l>
        <l id="s2:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s2:l3" met="4" type="-+">Die hemel was vroe die helle wert <rhyme label="a" type="m" corresp="#s2:l1">gram</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s2:l2">plena</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s2:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die maecht wies in doechden <rhyme label="a" type="m">voert</rhyme>.</l>
        <l id="s3:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s3:l3" met="4" type="-+">Gebenedijt si haer <rhyme label="a" type="m" corresp="#s3:l1">geboert</rhyme>.</l>
        <l id="s3:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s3:l2">plena</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s3:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Die maecht lach in <rhyme label="a" type="m">orisoen</rhyme>.</l>
        <l id="s4:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s4:l3" met="4" type="-+">Die ingel quam wt shemels <rhyme label="a" type="m" corresp="#s4:l1">broen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s4:l2">plena</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Geloeft si di <rhyme label="b" type="m" corresp="#s4:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hi sprac god gruetu reyne <rhyme label="a" type="m">wijf</rhyme>.</l>
        <l id="s5:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Al vol gracien es dijn <rhyme label="a" type="m" corresp="#s5:l1">lijf</rhyme>.</l>
        <l id="s5:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s5:l2">plena</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s5:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Du sult ontfaen dat eewege <rhyme label="a" type="m">woert</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s6:l3" met="4" type="-+">Die maget bleef hier af <rhyme label="a" type="m" corresp="#s6:l1">acoert</rhyme>.</l>
        <l id="s6:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s6:l2">plena</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s6:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tote gabriel soe sprac <rhyme label="a" type="m">si</rhyme>.</l>
        <l id="s7:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s7:l3" met="4" type="-+">Een kint tontfane verwondert <rhyme label="a" type="m" corresp="#s7:l1">my</rhyme>.</l>
        <l id="s7:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s7:l2">plena</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s7:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Want ic en kinde nye <rhyme label="a" type="m">man</rhyme>.</l>
        <l id="s8:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s8:l3" met="4" type="-+">Die vrucht ane mine liue <rhyme label="a" type="m" corresp="#s8:l1">wan</rhyme>.</l>
        <l id="s8:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s8:l2">plena</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s8:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die ingel sprac blijft <rhyme label="a" type="m">onbevreest</rhyme>.</l>
        <l id="s9:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s9:l3" met="4" type="-+">In di soe comt die heilege <rhyme label="a" type="m" corresp="#s9:l1">geest</rhyme>.</l>
        <l id="s9:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s9:l2">plena</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s9:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Des heilichs geests cracht <rhyme label="a" type="m">saldi</rhyme>.</l>
        <l id="s10:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s10:l3" met="4" type="-+">Verlichten reyne maget <rhyme label="a" type="m" corresp="#s10:l1">vri</rhyme>.</l>
        <l id="s10:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s10:l2">plena</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s10:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Siet hier die deerne ons heren <rhyme label="a" type="m">gi</rhyme>.</l>
        <l id="s11:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s11:l3" met="4" type="-+">Na dinen woerden gescie <rhyme label="a" type="m" corresp="#s11:l1">my</rhyme>.</l>
        <l id="s11:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s11:l2">plena</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s11:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Te hant ter stede si <rhyme label="a" type="m">ontfinc</rhyme>.</l>
        <l id="s12:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s12:l3" met="4" type="-+">Ihesum kerst den <rhyme label="a" type="m" corresp="#s12:l1">iongelinc</rhyme>.</l>
        <l id="s12:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s12:l2">plena</rhyme>.</l>
        <l id="s12:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s12:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Nochtan bleef si reyne <rhyme label="a" type="m">maecht</rhyme>.</l>
        <l id="s13:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s13:l3" met="4" type="-+">Dat wonder die natuere <rhyme label="a" type="m" corresp="#s13:l1">claecht</rhyme>.</l>
        <l id="s13:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s13:l2">plena</rhyme>.</l>
        <l id="s13:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s13:l2">maria</rhyme>,</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Dgelas doerscijnt die sonne <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s14:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s14:l3" met="4" type="-+">Ongebroken <rhyme label="a" type="m" corresp="#s14:l1">openbaer</rhyme>.</l>
        <l id="s14:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s14:l2">plena</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s14:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Alsoe geheel ende niet <rhyme label="a" type="m">som</rhyme>.</l>
        <l id="s15:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s15:l3" met="4" type="-+">Behielt si haren <rhyme label="a" type="m" corresp="#s15:l1">magedom</rhyme>.</l>
        <l id="s15:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s15:l2">plena</rhyme>.</l>
        <l id="s15:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s15:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Si droegen ionc si baerden <rhyme label="a" type="m">out</rhyme>.</l>
        <l id="s16:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s16:l3" met="4" type="-+">In eenen winters nachte <rhyme label="a" type="m" corresp="#s16:l1">cout</rhyme>.</l>
        <l id="s16:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s16:l2">plena</rhyme>.</l>
        <l id="s16:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s16:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Ongequets ende sonder <rhyme label="a" type="m">wee</rhyme>.</l>
        <l id="s17:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s17:l3" met="4" type="-+">Dat wonder en valt <rhyme label="a" type="m" corresp="#s17:l1">nemmermeer</rhyme>.</l>
        <l id="s17:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s17:l2">plena</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s17:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Edel teeder was dat reyne <rhyme label="a" type="m">bloet</rhyme>.</l>
        <l id="s18:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s18:l3" met="4" type="-+">Dat ihesus ane die maget <rhyme label="a" type="m" corresp="#s18:l1">loet</rhyme>.</l>
        <l id="s18:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s18:l2">plena</rhyme>.</l>
        <l id="s18:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s18:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Gebenedijt soe si die <rhyme label="a" type="m">nacht</rhyme>.</l>
        <l id="s19:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s19:l3" met="4" type="-+">Dat ihesus was ter werelt <rhyme label="a" type="m" corresp="#s19:l1">bracht</rhyme>.</l>
        <l id="s19:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s19:l2">plena</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s19:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Nie en wert ter werelt <rhyme label="a" type="m">kint</rhyme>.</l>
        <l id="s20:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme></l>
        <l id="s20:l3" met="4" type="-+">Van moeder herte soe seere <rhyme label="a" type="m" corresp="#s20:l1">gemint</rhyme></l>
        <l id="s20:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s20:l2">plena</rhyme>.</l>
        <l id="s20:l5" met="3" type="-+">Bidt vor ons <rhyme label="b" type="m" corresp="#s20:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Want doen hi ane den cruce <rhyme label="a" type="m">hinc</rhyme>.</l>
        <l id="s21:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme></l>
        <l id="s21:l3" met="4" type="-+">Een sweert al doer haer herte <rhyme label="a" type="m" corresp="#s21:l1">ginc</rhyme>.</l>
        <l id="s21:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s21:l2">plena</rhyme>.</l>
        <l id="s21:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s21:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Dat was van meneger pinen <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s22:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s22:l3" met="4" type="-+">Ende bouen haers kints <rhyme label="a" type="m" corresp="#s22:l1">doots</rhyme>.</l>
        <l id="s22:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s22:l2">plena</rhyme>.</l>
        <l id="s22:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s22:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Ewangeliste sint <rhyme label="a" type="m">ian</rhyme>.</l>
        <l id="s23:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s23:l3" met="4" type="-+">Sprac der droeuer moeder <rhyme label="a" type="m" corresp="#s23:l1">ian</rhyme>.</l>
        <l id="s23:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s23:l2">plena</rhyme>.</l>
        <l id="s23:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s23:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Troest v lieue moeder <rhyme label="a" type="m">mijn</rhyme>.</l>
        <l id="s24:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s24:l3" met="4" type="-+">Veruult moet die scriftuere <rhyme label="a" type="m" corresp="#s24:l1">sijn</rhyme>.</l>
        <l id="s24:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s24:l2">plena</rhyme>.</l>
        <l id="s24:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s24:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Want ghi sult scouwen sonder <rhyme label="a" type="m">waen</rhyme></l>
        <l id="s25:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s25:l3" met="4" type="-+">Den soeten ihesus op <rhyme label="a" type="m" corresp="#s25:l1">verstaen</rhyme>.</l>
        <l id="s25:l4" met="2" type="-+">Gratia <rhyme label="b" type="m" corresp="#s25:l2">plena</rhyme>.</l>
        <l id="s25:l5" met="3" type="-+">Nv bidt vor ons <rhyme label="b" type="m" corresp="#s25:l2">maria</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Mettien sheilichs vader <rhyme label="a" type="m">cracht</rhyme>.</l>
        <l id="s26:l2" met="2" type="-+">Aue <rhyme label="b" type="m">maria</rhyme>.</l>
        <l id="s26:l3" met="4" type="-+">Haer droeue herte wert <rhyme label="a" type="m" corresp="#s26:l1">versacht</rhyme>.</l>
        <l id="s26:l4" met="2" type="-+">Gracia <rhyme label="b" type="m" corresp="#s26:l2">plena</rhyme>.</l>
        <l id="s26:l5" met="3" type="-+">Geloeft sidi <rhyme label="b" type="m" corresp="#s26:l2">maria</rhyme>.</l>
      </lg>
    </lg>
