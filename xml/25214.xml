  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wij bidden v o heijlich <rhyme label="a" type="m">geest</rhyme></l>
        <l id="s1:l2" met="4" type="-+">die met den vader ende <rhyme label="b" type="m">soen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">altijt sijt een goet godt <rhyme label="a" type="m" corresp="#s1:l1">geweest</rhyme></l>
        <l id="s1:l4" met="4" type="-+">stort, v, in onse herten <rhyme label="b" type="m" corresp="#s1:l2">coen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ons mont, ons tong, ons herten <rhyme label="a" type="m">cracht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">laet spreken v belijdig <rhyme label="b" type="m">groet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">de lieft laet branden door v <rhyme label="a" type="m" corresp="#s2:l1">macht</rhyme></l>
        <l id="s2:l4" met="4" type="-+">dat t'vier onsteeckt ons naestens <rhyme label="b" type="m" corresp="#s2:l2">moet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dit gunt ons vader <rhyme label="a" type="m">goedertier</rhyme></l>
        <l id="s3:l2" met="4" type="-+">oeck ghij dit sijt den vaer <rhyme label="b" type="m">gelijck</rhyme></l>
        <l id="s3:l3" met="4" type="-+">met den geest heijlich een <rhyme label="a" type="m" corresp="#s3:l1">banier</rhyme></l>
        <l id="s3:l4" met="4" type="-+">regnerende drie <rhyme label="b" type="m" corresp="#s3:l2">ewelijck</rhyme>.</l>
      </lg>
    </lg>
  </text>
