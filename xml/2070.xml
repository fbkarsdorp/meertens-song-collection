<song>
  <meta>
    <id>2070</id>
    <date>
      <year-start>1674</year-start>
      <year-end>1674</year-end>
    </date>
    <location>
      <orginal-name>brussel</orginal-name>
      <standard-name>Brussels, Belgium</standard-name>
      <coordinates>
        <lon>50.8465573</lon>
        <lat>4.351697</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
      <tag standard-name="danklied" translation="gratitude song">danklied</tag>
    </tags>
    <melody>Ik heb menig pint en kan</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Daniel Bellemans</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k HEbb' lest-mael op de <rhyme label="a" type="m">Luyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Bevalligh hooren <rhyme label="b" type="m">slaen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick wirt daer door <rhyme label="c" type="f">ontsteken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Ick sagh te vensteren <rhyme label="a" type="m" corresp="#s1:l1">uyt</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En 't stondt my soo wel <rhyme label="b" type="m" corresp="#s1:l2">aen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Dat ick begonst te <rhyme label="c" type="f" corresp="#s1:l3">spreken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wie is hier Meester <rhyme label="d" type="m">van</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wie toetst dit <rhyme label="e" type="m">instrument</rhyme>.</l>
        <l id="s1:l9" met="3" type="-+">Door 't speuren naer den <rhyme label="d" type="m" corresp="#s1:l7">Man</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Heb ick den Heer <rhyme label="e" type="m" corresp="#s1:l8">ghekent</rhyme>.</l>
        <l id="s1:l11" met="3" type="+-">Heb ick den Heer <rhyme label="e" type="m" corresp="#s1:l8">ghekent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">'t Is maer een <rhyme label="a" type="m">sinne-beldt</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Om myn verflouwt <rhyme label="b" type="m">ghemoet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Wat voorts omhoog te <rhyme label="c" type="f">sturen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Godt selver sit en <rhyme label="a" type="m" corresp="#s2:l1">spelt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Als hy hem kennen <rhyme label="b" type="m" corresp="#s2:l2">doet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In syne <rhyme label="c" type="f" corresp="#s2:l3">Creaturen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Hy spelt, en slaet de <rhyme label="d" type="m">Luyt</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Met sulcken soeten <rhyme label="e" type="m">slagh</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Dat ieder uyt 't <rhyme label="d" type="m" corresp="#s2:l7">gheluydt</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Den Schepper kennen <rhyme label="e" type="m" corresp="#s2:l8">magh</rhyme>.</l>
        <l id="s2:l11" met="3" type="+-">Den Schepper kennen <rhyme label="e" type="m" corresp="#s2:l8">magh</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Slaen ick myn <rhyme label="a" type="m">oogh-ghemerck</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Op 't ghen' ter Wereldt <rhyme label="b" type="m">is</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ghevoeght in sulcken <rhyme label="c" type="f">orden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat onbegryp'lyck <rhyme label="a" type="m" corresp="#s3:l1">Werck</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Wiêns schoon <rhyme label="b" type="m" corresp="#s3:l2">ghesteltenis</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Niet kan verbetert <rhyme label="c" type="f" corresp="#s3:l3">worden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wat oordeelt myn <rhyme label="d" type="m">verstandt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Wat Sleutel tot dit <rhyme label="e" type="m">slot</rhyme>?</l>
        <l id="s3:l9" met="3" type="-+">Dat ick aen alle <rhyme label="d" type="m" corresp="#s3:l7">kant</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Kan thoonen mynen <rhyme label="e" type="m" corresp="#s3:l8">Godt</rhyme>.</l>
        <l id="s3:l11" met="3" type="+-">Kan thoonen mynen <rhyme label="e" type="m" corresp="#s3:l8">Godt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Spreeck ick de Boomen <rhyme label="a" type="m">toe</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De Kruyden en het <rhyme label="b" type="m">Gras</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De Vruchten, en de <rhyme label="c" type="f">Blommen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En vraegh' ick hun eens <rhyme label="a" type="m" corresp="#s4:l1">hoe</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat aen-ghenaem <rhyme label="b" type="m" corresp="#s4:l2">Ghewas</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Is voor den dagh <rhyme label="c" type="f" corresp="#s4:l3">gekommen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Het minste Kruyd'ken <rhyme label="d" type="m">gheeft</rhyme></l>
        <l id="s4:l8" met="3" type="-+">My antwoordt sonder <rhyme label="e" type="m">stem</rhyme>:</l>
        <l id="s4:l9" met="3" type="-+">Door Godt die eeuwigh <rhyme label="d" type="m" corresp="#s4:l7">leeft</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Door niemant als door <rhyme label="e" type="m" corresp="#s4:l8">hem</rhyme>.</l>
        <l id="s4:l11" met="3" type="+-">Door niemant als door <rhyme label="e" type="m" corresp="#s4:l8">hem</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'t Ghediert' en all' het <rhyme label="a" type="m">Vêe</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Beesten groot en <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Voghels die daer <rhyme label="c" type="f">swieren</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">De Visschen in de <rhyme label="a" type="m" corresp="#s5:l1">Zee</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soo wonder aerdigh <rhyme label="b" type="m" corresp="#s5:l2">zyn</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In leven en <rhyme label="c" type="f" corresp="#s5:l3">manieren</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat oock het minste <rhyme label="d" type="m">Dier</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Het welck uyt 't Ey <rhyme label="e" type="m">geraeckt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Alwaer 't de kleynste <rhyme label="d" type="m" corresp="#s5:l7">Mier</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Den Schepper kenbaer <rhyme label="e" type="m" corresp="#s5:l8">maeckt</rhyme>.</l>
        <l id="s5:l11" met="3" type="+-">Den Schepper kenbaer <rhyme label="e" type="m" corresp="#s5:l8">maeckt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Den kostelycken <rhyme label="a" type="m">pandt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Waer van de Wereldt <rhyme label="b" type="m">houdt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Zyn Perels en <rhyme label="c" type="f">Robynen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sapphier en <rhyme label="a" type="m" corresp="#s6:l1">Diamant</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Het Silver en het <rhyme label="b" type="m" corresp="#s6:l2">Goudt</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Der grondeloose <rhyme label="c" type="f" corresp="#s6:l3">Mynen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die stellen my ten <rhyme label="d" type="m">thoon</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hoe machtigh, milt en <rhyme label="e" type="m">goet</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Hoe ryck, hoe eêl en <rhyme label="d" type="m" corresp="#s6:l7">schoon</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Den Schepper wesen <rhyme label="e" type="m" corresp="#s6:l8">moet</rhyme>.</l>
        <l id="s6:l11" met="3" type="+-">Den Schepper wesen <rhyme label="e" type="m" corresp="#s6:l8">moet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Den Aerden bol, de <rhyme label="a" type="m">Locht</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Het Water en het <rhyme label="b" type="m">Vier</rhyme></l>
        <l id="s7:l3" met="3" type="-+">En al de <rhyme label="c" type="f">Elementen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wie heeft-se <rhyme label="a" type="m" corresp="#s7:l1">voordts-ghebrocht</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Wie heeft het groot <rhyme label="b" type="m" corresp="#s7:l2">bestier</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Van Winter, Somer, <rhyme label="c" type="f" corresp="#s7:l3">Lenten</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wie draeyt den Hemel <rhyme label="d" type="m">om</rhyme>?</l>
        <l id="s7:l8" met="3" type="-+">Gheen Sterr' in 't <rhyme label="e" type="m">Firmament</rhyme></l>
        <l id="s7:l9" met="3" type="-+">Oft spreckt als is sy <rhyme label="d" type="m" corresp="#s7:l7">stom</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Dat s'haren Schepper <rhyme label="e" type="m" corresp="#s7:l8">kent</rhyme>.</l>
        <l id="s7:l11" met="3" type="+-">Dat s'haren Schepper <rhyme label="e" type="m" corresp="#s7:l8">kent</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ick vraegh u Son en <rhyme label="a" type="m">Maen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die ons soo bly <rhyme label="b" type="m">belacht</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ick vraegh u twelf <rhyme label="c" type="f">Planeten</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">U op en <rhyme label="a" type="m" corresp="#s8:l1">onder-gaen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En uw' verborghen <rhyme label="b" type="m" corresp="#s8:l2">kracht</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Die souwick gherne <rhyme label="c" type="f" corresp="#s8:l3">weten</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Wie is hier oorsaeck <rhyme label="d" type="m">van</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Wie schonck u sulcken <rhyme label="e" type="m">eer</rhyme>?</l>
        <l id="s8:l9" met="3" type="-+">'k En hoor niet anders <rhyme label="d" type="m" corresp="#s8:l7">dan</rhyme>,</l>
        <l id="s8:l10" met="3" type="-+">Den alder-hooghsten <rhyme label="e" type="m" corresp="#s8:l8">Heer</rhyme>.</l>
        <l id="s8:l11" met="3" type="+-">Den alder-hooghsten <rhyme label="e" type="m" corresp="#s8:l8">Heer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Al hadd' ick gheen <rhyme label="a" type="m">ghesicht</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Al waer ick stom en <rhyme label="b" type="m">doof</rhyme></l>
        <l id="s9:l3" met="3" type="-+">En al myn ouders <rhyme label="c" type="f">Heyden</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En noyt en hadt het <rhyme label="a" type="m" corresp="#s9:l1">Licht</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Ontfanghen van 't <rhyme label="b" type="m" corresp="#s9:l2">Gheloof</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">'k Moet even-wel <rhyme label="c" type="f" corresp="#s9:l3">belyden</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dat ick u grooten <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Uyt uwe Schepsels <rhyme label="e" type="m">ken</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Dat ick u lof en <rhyme label="d" type="m" corresp="#s9:l7">eer'</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">En liefde schuldigh <rhyme label="e" type="m" corresp="#s9:l8">ben</rhyme>.</l>
        <l id="s9:l11" met="3" type="+-">En liefde schuldigh <rhyme label="e" type="m" corresp="#s9:l8">ben</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
