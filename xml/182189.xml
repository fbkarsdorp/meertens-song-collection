      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Ieugt, Wat <rhyme label="a" type="m">vreugt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Geniet men hier op <rhyme label="b" type="f">d’Aarde</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">O Dag, Ik <rhyme label="c" type="m">mag</rhyme></l>
        <l id="s1:l4" met="3" type="-+">U roemen hoog van <rhyme label="b" type="f" corresp="#s1:l2">waarde</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dewyl my <rhyme label="d" type="m">Clorimeen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Gered heeft uit ‘t <rhyme label="d" type="m" corresp="#s1:l5">geween</rhyme>.</l>
