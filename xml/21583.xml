<song>
  <meta>
    <id>21583</id>
    <date>
      <year-start>1582</year-start>
      <year-end>1582</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
