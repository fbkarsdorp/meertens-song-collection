  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">NIets is zoo zoet, dan Wyn te <rhyme label="a" type="f">drinken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Want Venus, met haar <rhyme label="b" type="m">dartelheit</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Doet die haar volgen, maar <rhyme label="a" type="f" corresp="#s1:l1">verstinken</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Daar zy ons ten verderve <rhyme label="b" type="m" corresp="#s1:l2">leid</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Met Bachus wil ik steeds <rhyme label="a" type="f" corresp="#s1:l1">rinkinken</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Terwyl myn hart zelf voor hem <rhyme label="b" type="m" corresp="#s1:l2">pleit</rhyme>.</l>
      </lg>
    </lg>
  </text>
