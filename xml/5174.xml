<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>5174</id>
    <date>
      <year-start>1924</year-start>
      <year-end>1925</year-end>
    </date>
    <tags>
      <tag>beggar song</tag>
      <tag>proverb</tag>
    </tags>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wat seytmen nu van <rhyme label="a" type="f">Duckdalve</rhyme>:</l>
        <l id="s1:l2" met="3" type="-+">Sijn Boter ghelt vier en <rhyme label="a" type="f" corresp="#s1:l1">halve</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Is dat niet veel te <rhyme label="b" type="m">dier</rhyme>?</l>
        <l id="s1:l4" met="3" type="-+">Door den Prins van <rhyme label="c" type="f">Oraengien</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Moet hy na <rhyme label="c" type="f" corresp="#s1:l4">Spaengien</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Conste hy comen van <rhyme label="b" type="m" corresp="#s1:l3">hier</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat seytmen van die <rhyme label="a" type="f">Spechten</rhyme> ?</l>
        <l id="s2:l2" met="3" type="-+">Het zijn duckdalvens <rhyme label="a" type="f" corresp="#s2:l1">knechten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ruydich, rappich en <rhyme label="b" type="m">zeer</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Tegen donnoosel en <rhyme label="c" type="f">slechten</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Willen sy sterck gaen <rhyme label="c" type="f" corresp="#s2:l4">vechten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Schoorsteenveghers sonder <rhyme label="b" type="m" corresp="#s2:l3">leer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Met haer Fluweelen <rhyme label="a" type="f">Broecken</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Gaen sy de Meyskens <rhyme label="a" type="f" corresp="#s3:l1">soecken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maer wat daer onder <rhyme label="b" type="m">schuylt</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dat zijn Pockighe <rhyme label="c" type="f">doecken</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Om tGasthuys te <rhyme label="c" type="f" corresp="#s3:l4">vercloecken</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Want binnen zijn sy <rhyme label="b" type="m" corresp="#s3:l3">vervuylt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In Spaengien en zijnt maer <rhyme label="a" type="f">Fielen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer loopen sy met <rhyme label="a" type="f" corresp="#s4:l1">Kackhielen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al zijnse hier groot <rhyme label="b" type="m">gheacht</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Vol Luysen ist dat sy <rhyme label="c" type="f">crielen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Tzijn Moordenaers der <rhyme label="c" type="f" corresp="#s4:l4">Sielen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ghespuys omte vullen die <rhyme label="b" type="m" corresp="#s4:l3">Gracht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Vijchcorven connen sy <rhyme label="a" type="f">breyen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ranckskens connen sy <rhyme label="a" type="f" corresp="#s5:l1">leyen</rhyme>,,</l>
        <l id="s5:l3" met="3" type="-+">Wijngaert snijden onder <rhyme label="b" type="m">dEerd</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Dan lotert haer de <rhyme label="c" type="f">Keye</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">En sy gaen haer <rhyme label="c" type="f" corresp="#s5:l4">vermeyen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Spaceren op een <rhyme label="b" type="m" corresp="#s5:l3">Peert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy willen om ons <rhyme label="a" type="f">Gheloove</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dees Landen heel <rhyme label="a" type="f" corresp="#s6:l1">berooven</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Als Duckdalve haer doet <rhyme label="b" type="m">verstaen</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Maer Godt al van hier <rhyme label="c" type="f">boven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Diemen altijt moet <rhyme label="c" type="f" corresp="#s6:l4">loven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die salse noch <rhyme label="b" type="m" corresp="#s6:l3">verslaen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Gy oude Grijs <rhyme label="a" type="f">Duckdalve</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Hebdy nu niet meer <rhyme label="a" type="f" corresp="#s7:l1">Salve</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Of zijt ghy ghenoech <rhyme label="b" type="m">ghesmeert</rhyme>?</l>
        <l id="s7:l4" met="3" type="-+">Ghy zijt ten heelen noch ten <rhyme label="c" type="f">halven</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">U Koe wil niet meer <rhyme label="c" type="f" corresp="#s7:l4">Calven</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Twelck uwen Paus seer <rhyme label="b" type="m" corresp="#s7:l3">deert</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En ghy Duckdalven <rhyme label="a" type="f">sone</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die boosheyt zijn <rhyme label="a" type="f" corresp="#s8:l1">ghewoone</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Om dat ghy hebt <rhyme label="b" type="m">vermoort</rhyme></l>
        <l id="s8:l4" met="3" type="-+">So menighen <rhyme label="c" type="f">Persoone</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Daerom Godt uut den <rhyme label="c" type="f" corresp="#s8:l4">Throone</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Op u is seer <rhyme label="b" type="m" corresp="#s8:l3">ghestoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O Prince van <rhyme label="a" type="f">Orangien</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Edel Vorst uut <rhyme label="a" type="f" corresp="#s9:l1">Almaengien</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maect ons dees Tyrannen <rhyme label="b" type="m">quyt</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Sy doen ons groote <rhyme label="c" type="f">calaengien</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Jaechtse weder na <rhyme label="c" type="f" corresp="#s9:l4">Spaengien</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">So crijghen wy goeden <rhyme label="b" type="m" corresp="#s9:l3">tijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
