  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ei, wilt toch <rhyme label="a" type="f">zwygen</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">'k Verdiende lof noch <rhyme label="b" type="m">eer</rhyme></l>
        <l id="s1:l3" met="2" type="-+">Van u te <rhyme label="a" type="f" corresp="#s1:l1">krygen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Herhaal die taal nooit <rhyme label="b" type="m" corresp="#s1:l2">weÃªr</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Heb ik iets goeds <rhyme label="c" type="m">verricht</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">'t Was niet meer dan myn <rhyme label="c" type="m" corresp="#s1:l5">plicht</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">En ik zal verder <rhyme label="d" type="f">trachten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">U beiden, die ik <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Op 't hoogste te <rhyme label="d" type="f" corresp="#s1:l7">achten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">By gintsche <rhyme label="a" type="f">bomen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Daar 't beekje sachtjes <rhyme label="b" type="m">gleid</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Langs groene <rhyme label="a" type="f" corresp="#s2:l1">zomen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Zit Filo, nat <rhyme label="b" type="m" corresp="#s2:l2">beschreid</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Hy loost vast zucht op <rhyme label="c" type="m">zucht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wyl ieder hem <rhyme label="c" type="m" corresp="#s2:l5">ontvlucht</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Ik hoorden, met <rhyme label="d" type="f">behagen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hoe hy zyn <rhyme label="b" type="m" corresp="#s2:l2">trouwloosheid</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Nu moet <rhyme label="d" type="f" corresp="#s2:l7">beklagen</rhyme>.</l>
      </lg>
    </lg>
  </text>
