<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3920</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>new year song</tag>
      <tag>christmas song</tag>
    </tags>
    <melody>Ik wil mij gaan vertroosten in Jezus' lijden groot</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MEt desen nieuwen <rhyme label="a" type="f">iare</rhyme></l>
        <l id="s1:l2" met="3" type="-+">So willen wij vrolijck <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ons heeft een maghet <rhyme label="a" type="f" corresp="#s1:l1">clare</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghebaert een <rhyme label="b" type="m" corresp="#s1:l2">kindekijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Te Bethleem <rhyme label="c" type="f">wtuercoren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">So ons die scrifture <rhyme label="d" type="m">verclaert</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Is ons dat kint <rhyme label="c" type="f" corresp="#s1:l5">gheboren</rhyme>.</l>
        <l id="s1:l8" met="3" type="-+">Die maecht bleef <rhyme label="d" type="m" corresp="#s1:l6">onbeswaert</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hier bouen wten <rhyme label="a" type="f">throone</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is Gabriel <rhyme label="b" type="m">ghesant</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Tot eender maghet <rhyme label="a" type="f" corresp="#s2:l1">schoone</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Al in dat ioetsche <rhyme label="b" type="m" corresp="#s2:l2">lant</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Te Nazareth in die <rhyme label="c" type="f">stede</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Vant hi die maecht <rhyme label="d" type="m">alleyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Hi sprac / God gheue v <rhyme label="c" type="f" corresp="#s2:l5">vrede</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Met Gode sidy <rhyme label="d" type="m" corresp="#s2:l6">ghemeyn</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Bouen alle <rhyme label="a" type="f">vrouwen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Sidy <rhyme label="b" type="m">ghebenedijt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">By v sal sijn <rhyme label="a" type="f" corresp="#s3:l1">behouwen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat Adam heeft <rhyme label="b" type="m" corresp="#s3:l2">ontvrijt</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ghi sult in uwen <rhyme label="c" type="f">lichame</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Een edel vruchte <rhyme label="d" type="m">ontfaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En twifelt hier niet <rhyme label="c" type="f" corresp="#s3:l5">ane</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Die maecht wert seer <rhyme label="d" type="m" corresp="#s3:l6">ontdaen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">De maecht wert seer van <rhyme label="a" type="f">binnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Verscrict in haren <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">En dachte in haren <rhyme label="a" type="f" corresp="#s4:l1">sinnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Wat wesen mocht die <rhyme label="b" type="m" corresp="#s4:l2">groet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die haer die enghel <rhyme label="c" type="f">dede</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Want si en hadde niet <rhyme label="d" type="m">ghesien</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Si sprack ter seluer <rhyme label="c" type="f" corresp="#s4:l5">stede</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Hoe sal dat moghen <rhyme label="d" type="m" corresp="#s4:l6">gheschien</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die enghel sprack / o <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En weset niet <rhyme label="b" type="m">versaecht</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Ghi sult ontfaen <rhyme label="a" type="f" corresp="#s5:l1">alleyne</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ende bliuen suyuer <rhyme label="b" type="m" corresp="#s5:l2">maecht</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ende baren sonder <rhyme label="c" type="f">pine</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Dat licht / der enghelen <rhyme label="d" type="m">broot</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Des sondaers <rhyme label="c" type="f" corresp="#s5:l5">medicine</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Men vant noyt sijn <rhyme label="d" type="m" corresp="#s5:l6">ghenoot</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hoe soude ic kint <rhyme label="a" type="f">ghedragen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Ick en bekende gheenen <rhyme label="b" type="m">man</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Daer om wilt mi <rhyme label="a" type="f" corresp="#s6:l1">ghewagen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hoe soude dat wesen <rhyme label="b" type="m" corresp="#s6:l2">dan</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Het is bouen naturen <rhyme label="c" type="f">crachten</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Te wesen moeder en <rhyme label="d" type="m">maecht</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Dus truer ic in mijn <rhyme label="c" type="f" corresp="#s6:l5">gedachten</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Hier om ben ick <rhyme label="d" type="m" corresp="#s6:l6">versaecht</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O suyuer maecht van <rhyme label="a" type="f">prisen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Waer om sidy <rhyme label="b" type="m">versaecht</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Die heylighe gheest sal <rhyme label="a" type="f" corresp="#s7:l1">risen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">In v wel suyuer <rhyme label="b" type="m" corresp="#s7:l2">maecht</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Die Gods cracht <rhyme label="c" type="f">wtuercoren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Sal v ter stont <rhyme label="d" type="m">omuaen</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dat van v sal sijn <rhyme label="c" type="f" corresp="#s7:l5">gheboren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Sal Adams sonden af <rhyme label="d" type="m" corresp="#s7:l6">dwaen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Elyzabeth heeft <rhyme label="a" type="f">ontfangen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">In haerder ouder <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Al sijn haer dagen <rhyme label="a" type="f" corresp="#s8:l1">vergangen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Reyn maecht <rhyme label="b" type="m" corresp="#s8:l2">ghebenedijt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Het is al in Gods <rhyme label="c" type="f">vermoghen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Dat is / oft wesen <rhyme label="d" type="m">sal</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Dus weest in goeden <rhyme label="c" type="f" corresp="#s8:l5">hoghen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Vertroost die werelt <rhyme label="d" type="m" corresp="#s8:l6">al</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Adam ende sijn <rhyme label="a" type="f">ghesellen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Die so langhe laghen <rhyme label="b" type="m">gheuaen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Int voergheburcht der <rhyme label="a" type="f" corresp="#s9:l1">hellen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Om dat si hadden <rhyme label="b" type="m" corresp="#s9:l2">misdaen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Want Eua hadde <rhyme label="c" type="f">verloren</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Dat aertsche <rhyme label="d" type="m">paradijs</rhyme></l>
        <l id="s9:l7" met="3" type="-+">O suyuer maecht <rhyme label="c" type="f" corresp="#s9:l5">vercoren</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Lof si v ende <rhyme label="d" type="m" corresp="#s9:l6">prijs</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Die ootmoedighe maghet <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Viel neder op haer <rhyme label="b" type="m">knien</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Siet hier Gods deerne <rhyme label="a" type="f" corresp="#s10:l1">cleyne</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Nae uwen woerden moet my <rhyme label="b" type="m" corresp="#s10:l2">gheschien</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Al doen ter seluer <rhyme label="c" type="f">vren</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ontfinck die maghet <rhyme label="d" type="m">fijn</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Den sone Gods sonder <rhyme label="c" type="f" corresp="#s10:l5">trueren</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Ende baerde hem sonder <rhyme label="d" type="m" corresp="#s10:l6">pijn</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Te Bethleem <rhyme label="a" type="f">wtuercoren</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Als ons scrifture <rhyme label="b" type="m">ghewaecht</rhyme></l>
        <l id="s11:l3" met="3" type="-+">So wert dit kint <rhyme label="a" type="f" corresp="#s11:l1">gheboren</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Van eender suyuer <rhyme label="b" type="m" corresp="#s11:l2">maecht</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Laet ons hem alle <rhyme label="c" type="f">louen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Al met die moeder <rhyme label="d" type="m">sijn</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Op dat si ons brenghe daer <rhyme label="c" type="f" corresp="#s11:l5">bouen</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Daer eewighe vruecht is sonder <rhyme label="d" type="m" corresp="#s11:l6">pijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
