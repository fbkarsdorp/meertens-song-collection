  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">REyn maechdeken / schoon / <rhyme label="a" type="m">onbesmet</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">Ghi waert van edelen <rhyme label="b" type="f">bloede</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ghi woonde te <rhyme label="a" type="m" corresp="#s1:l1">Nazareth</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ghesproten wt Iesses <rhyme label="b" type="f" corresp="#s1:l2">roede</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Voersien had v die vader <rhyme label="c" type="m">wel</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Voersien had v die vader <rhyme label="c" type="m" corresp="#s1:l5">wel</rhyme></l>
        <l id="s1:l7" met="4" type="-+">Dies daelde die engel <rhyme label="c" type="m" corresp="#s1:l5">Gabriel</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Met woorden snel / na gods <rhyme label="c" type="m" corresp="#s1:l5">beuel</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ghegroet sijt edel maghet <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van Gode <rhyme label="b" type="f">wtuercoren</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Alleen doer v groote <rhyme label="a" type="m" corresp="#s2:l1">ootmoet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wert van v een kint <rhyme label="b" type="f" corresp="#s2:l2">gheboren</rhyme></l>
        <l id="s2:l5" met="4" type="-+">De helle sal dies hebben <rhyme label="c" type="m">nijt</rhyme></l>
        <l id="s2:l6" met="4" type="-+">De helle sal dies hebben <rhyme label="c" type="m" corresp="#s2:l5">nijt</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Ende tvoerburch wortter bi <rhyme label="c" type="m" corresp="#s2:l5">verblijt</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Gebenedijt dies maget <rhyme label="c" type="m" corresp="#s2:l5">sijt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Si sprac hoe sou dat <rhyme label="a" type="m">geschien</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ende wert versaecht in <rhyme label="b" type="f">desen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Si hadde groot wonder in <rhyme label="a" type="m" corresp="#s3:l1">dien</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Hoe dattet mochte <rhyme label="b" type="f" corresp="#s3:l2">wesen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Dies hadde dit een dencken <rhyme label="c" type="m">an</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Dies hadde dit een dencken <rhyme label="c" type="m" corresp="#s3:l5">an</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Want noyt erfsonde in haer <rhyme label="c" type="m" corresp="#s3:l5">quam</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Van eenich man / twas vreemde <rhyme label="c" type="m" corresp="#s3:l5">dan</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het beliefde die hooghe <rhyme label="a" type="m">drieuuldicheyt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Voer alle beghins <rhyme label="b" type="f">beghinnen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dat ghi doer v <rhyme label="a" type="m" corresp="#s4:l1">suyuerheyt</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Der engelen val sout <rhyme label="b" type="f" corresp="#s4:l2">verwinnen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Ende versoenen tsondich <rhyme label="c" type="m">leet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Ende versoenen tsondich <rhyme label="c" type="m" corresp="#s4:l5">leet</rhyme></l>
        <l id="s4:l7" met="4" type="-+">Dat Adam inden appel <rhyme label="c" type="m" corresp="#s4:l5">beet</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Wt liefde heet / si was <rhyme label="c" type="m" corresp="#s4:l5">bereet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">So sonder mans saet ter werelt <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Eua / eerst van <rhyme label="b" type="f">Adame</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Alsoe God menschelijcke natuere aen <rhyme label="a" type="m" corresp="#s5:l1">nam</rhyme></l>
        <l id="s5:l4" met="3" type="-+">In Marien suyuer <rhyme label="b" type="f" corresp="#s5:l2">lichame</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Si baerde Iesum <rhyme label="c" type="m">onuersaecht</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Si baerde Iesum <rhyme label="c" type="m" corresp="#s5:l5">onuersaecht</rhyme></l>
        <l id="s5:l7" met="4" type="-+">Ende bleef moeder ende <rhyme label="c" type="m" corresp="#s5:l5">maecht</rhyme></l>
        <l id="s5:l8" met="4" type="-+">Wie dat beclaecht / het heeft gode also <rhyme label="c" type="m" corresp="#s5:l5">behaecht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doe Maria haer kint <rhyme label="a" type="m">aensach</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Heeft si hem minnelijc aen <rhyme label="b" type="f">gebeden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Corts daer na den achtsten <rhyme label="a" type="m" corresp="#s6:l1">dach</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soe wert Iesus <rhyme label="b" type="f" corresp="#s6:l2">besneden</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Des vaders gramscap heeft si <rhyme label="c" type="m">ghestaect</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Des vaders gramscap heeft si <rhyme label="c" type="m" corresp="#s6:l5">ghestaect</rhyme></l>
        <l id="s6:l7" met="4" type="-+">Des menschen paeys heeft si <rhyme label="c" type="m" corresp="#s6:l5">ghemaect</rhyme></l>
        <l id="s6:l8" met="4" type="-+">Simeon naect / is troost <rhyme label="c" type="m" corresp="#s6:l5">gedaect</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Dri coningen hebben dit <rhyme label="a" type="m">verhoort</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Iesum si neerstich <rhyme label="b" type="f">sochten</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Hoochlijc eerende <rhyme label="a" type="m" corresp="#s7:l1">rechteuoort</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Hem / offerande sij <rhyme label="b" type="f" corresp="#s7:l2">brochten</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Te Bethleem quamen si <rhyme label="c" type="m">naer</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Te Bethleem quamen si <rhyme label="c" type="m" corresp="#s7:l5">naer</rhyme></l>
        <l id="s7:l7" met="4" type="-+">Iaspar / Melchior / <rhyme label="c" type="m" corresp="#s7:l5">Balthasaer</rhyme> /</l>
        <l id="s7:l8" met="4" type="-+">Een sterre claer / beleydese <rhyme label="c" type="m" corresp="#s7:l5">daer</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Si anbaden den here <rhyme label="a" type="m">menichfout</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Doende haer <rhyme label="b" type="f">offerande</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Met myrre / wieroock / ende <rhyme label="a" type="m" corresp="#s8:l1">gout</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ende keerden tot haren <rhyme label="b" type="f" corresp="#s8:l2">lande</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Mer doen Herodes dat <rhyme label="c" type="m">vernam</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Mer doen Herodes dat <rhyme label="c" type="m" corresp="#s8:l5">vernam</rhyme></l>
        <l id="s8:l7" met="4" type="-+">Daer na so menich onnosel <rhyme label="c" type="m" corresp="#s8:l5">lam</rhyme></l>
        <l id="s8:l8" met="4" type="-+">In liden quam / wt nide <rhyme label="c" type="m" corresp="#s8:l5">gram</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Gloriose princesse <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Veriaecht van ons alle <rhyme label="b" type="f">sneuen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Int wterste weest ons <rhyme label="a" type="m" corresp="#s9:l1">behoet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Daer onse siele sal <rhyme label="b" type="f" corresp="#s9:l2">beuen</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Opperste moeder <rhyme label="c" type="m">excellent</rhyme></l>
        <l id="s9:l6" met="4" type="-+">Opperste moeder <rhyme label="c" type="m" corresp="#s9:l5">excellent</rhyme></l>
        <l id="s9:l7" met="4" type="-+">Weest onser sielen dan <rhyme label="c" type="m" corresp="#s9:l5">ontrent</rhyme></l>
        <l id="s9:l8" met="4" type="-+">Int leste ent / schoon maget <rhyme label="c" type="m" corresp="#s9:l5">yent</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
