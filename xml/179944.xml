<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>179944</id>
    <date>
      <year-start>1757</year-start>
      <year-end>1757</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wie kwam mij te wekken zo ras</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ach! ach! 'k ben myn geldje <rhyme label="a" type="m">kwyt</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Is dat voor my niet een <rhyme label="a" type="m" corresp="#s1:l1">spyt</rhyme>!</l>
        <l id="s1:l3" met="4" type="+-">'k Zeg voortaan 't verbruide <rhyme label="b" type="m">spel</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Altoos <rhyme label="b" type="m" corresp="#s1:l3">vaarwel</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">'k Wouw dat Bord en <rhyme label="c" type="m">Dobbelsteen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">In Nephtunis plas <rhyme label="c" type="m" corresp="#s1:l5">verdween</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En dat de <rhyme label="d" type="m">Kaart</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Verbrande op Pluto's <rhyme label="d" type="m" corresp="#s1:l7">haart</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schoon ik Trokkedielje <rhyme label="a" type="m">speel</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Of Verkeer, 't is <rhyme label="a" type="m" corresp="#s2:l1">eevenveel</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Gooi ik Vaa, of <rhyme label="b" type="m">Passedies</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">'t Is al <rhyme label="b" type="m" corresp="#s2:l3">verlies</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">'t Zy ik Omber of <rhyme label="c" type="m">Piecquet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Of Quadrielje of wel <rhyme label="c" type="m" corresp="#s2:l5">Bazet</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Wische of <rhyme label="d" type="m">Lanterlu</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">'t Is voor en naa fou <rhyme label="d" type="m" corresp="#s2:l7">tu</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Speel ik 's morgens op <rhyme label="a" type="m">Biljare</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">'t Is of my de Tafel <rhyme label="a" type="m" corresp="#s3:l1">fard</rhyme>:</l>
        <l id="s3:l3" met="4" type="+-">Keu en Bielje, Stok en <rhyme label="b" type="m">Mars</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Zyn my steeds <rhyme label="b" type="m" corresp="#s3:l3">dwars</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Als ik Trok, myn geld raakt <rhyme label="c" type="m">voort</rhyme>:</l>
        <l id="s3:l6" met="4" type="+-">Zelden raak ik Pen of <rhyme label="c" type="m" corresp="#s3:l5">Poort</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">'k Vind geen <rhyme label="d" type="m">geluk</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">In Kolf baan, Bal of <rhyme label="d" type="m" corresp="#s3:l7">Stuk</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">'k Heb daar weêr de gantsche <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Met de Stenen <rhyme label="a" type="m" corresp="#s4:l1">doorgebracht</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En, daar 't Spel op 't beste <rhyme label="b" type="m">leek</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Kreeg ik die <rhyme label="b" type="m" corresp="#s4:l3">steek</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Viermaal vyven naa <rhyme label="c" type="m">malkaêr</rhyme>;</l>
        <l id="s4:l6" met="4" type="+-">'k Heb noch werk dat ik <rhyme label="c" type="m" corresp="#s4:l5">bedaar</rhyme>;</l>
        <l id="s4:l7" met="2" type="-+">Maar, 'k hoor de <rhyme label="d" type="m">Haan</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">Des wil ik naar Bed toe <rhyme label="d" type="m" corresp="#s4:l7">gaan</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
