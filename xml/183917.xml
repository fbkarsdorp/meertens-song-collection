<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183917</id>
    <date>
      <year-start>1806</year-start>
      <year-end>1806</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ohne Lieb und ohne Wein</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Juigcht, o Broedere! aan den <rhyme label="a" type="m">disch</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Metslaarij <rhyme label="b" type="f">geheiligd</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">En wilt in dit <rhyme label="c" type="m">oogenblik</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Voor den nijd <rhyme label="b" type="f" corresp="#s1:l2">beveiligd</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Vergenoegd en blijd van <rhyme label="d" type="m">geest</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Dankbaarlijk <rhyme label="e" type="f">gedenken</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Aan den Heilig van dit <rhyme label="f" type="m">Feest</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Hem den eerwijn <rhyme label="e" type="f" corresp="#s1:l6">schenken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hulde zijn <rhyme label="a" type="m">gedachtenis</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Brenge elks dankbaar <rhyme label="b" type="f">harte</rhyme>!</l>
        <l id="s2:l3" met="4" type="+-">Volgende zijn voorbeeld <rhyme label="c" type="m">na</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">Dat vooroordeel <rhyme label="b" type="f" corresp="#s2:l2">tartte</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Deugd, door wijsheid <rhyme label="d" type="m">voorgelicht</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Doe ons 't spoor <rhyme label="e" type="f">betreden</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Dat zij zelf heeft <rhyme label="f" type="m">ingericht</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Naar de lijn der <rhyme label="e" type="f" corresp="#s2:l6">reden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laat ons in deez' blijden <rhyme label="a" type="m">stond</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Gulle vreugde <rhyme label="b" type="f">smaaken</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Aan deez' disch door <rhyme label="c" type="m">matigheid</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Allen zien <rhyme label="b" type="f" corresp="#s3:l2">bewaaken</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Laat geen heilloos <rhyme label="d" type="m">naberouw</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Stof tot wroeging <rhyme label="e" type="f">geeven</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Altijd vloeije, in kalme <rhyme label="f" type="m">rust</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">'t Leven van ons <rhyme label="e" type="f" corresp="#s3:l6">leven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Lost, Ã´ Broedren! het <rhyme label="a" type="m">geschut</rhyme>;</l>
        <l id="s4:l2" met="3" type="+-">Op den bloei der <rhyme label="b" type="f">Orde</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Wenscht dat, door een wijs <rhyme label="c" type="m">bestuur</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Die bevorderd <rhyme label="b" type="f" corresp="#s4:l2">worde</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Dat deez' Broederschap, <rhyme label="d" type="m">getrouw</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Hier te samen <rhyme label="e" type="f">koome</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Elk den geest der Orde <rhyme label="f" type="m">kenn'</rhyme>,</l>
        <l id="s4:l8" met="3" type="+-">En haar heil hier <rhyme label="e" type="f" corresp="#s4:l6">stroome</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
