  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Heylige Paus van <rhyme label="a" type="f">Romen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Hoe sal ick 't nu <rhyme label="b" type="m">aengaen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">'T geen my is <rhyme label="a" type="f" corresp="#s1:l1">over-komen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Kost my soo menigen <rhyme label="b" type="m" corresp="#s1:l2">traen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Dat die Hollanders kloeck en <rhyme label="c" type="m">rat</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Ontnemen my soo grooten <rhyme label="c" type="m" corresp="#s1:l5">schat</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hoe kan ick doch vergeten <rhyme label="c" type="m" corresp="#s1:l5">dat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Koninck van <rhyme label="a" type="f">Hispangien</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Roept ghy den Paus hier <rhyme label="b" type="m">aen</rhyme>?</l>
        <l id="s2:l3" met="3" type="-+">Het edel bloed <rhyme label="a" type="f" corresp="#s2:l1">Orangien</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Dat heeft het u <rhyme label="b" type="m" corresp="#s2:l2">gedaen</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Bid God die u verleent de <rhyme label="c" type="m">straf</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Die ist die 't nam, en weder <rhyme label="c" type="m" corresp="#s2:l5">gaf</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Dat u vernielen sal gelijck als <rhyme label="c" type="m" corresp="#s2:l5">kaf</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Al ghy Ketters in 't <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Jck sal 't u loonen <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Millos Diablos <rhyme label="a" type="f" corresp="#s3:l1">even</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Sullen u halen <rhyme label="b" type="m" corresp="#s3:l2">teer</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Om dat ghy my nu neemt mijn <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat my de koortse krijgen <rhyme label="c" type="m" corresp="#s3:l5">doet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Daer ick mijn volck mee betalen <rhyme label="c" type="m" corresp="#s3:l5">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">'T sijn soo wel onse <rhyme label="a" type="f">Mijnen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van Silver en <rhyme label="b" type="m">Gout</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Alsse de uwe <rhyme label="a" type="f" corresp="#s4:l1">schijnen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Hierom sijn wy soo <rhyme label="b" type="m" corresp="#s4:l2">stout</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Met een oprecht Christen <rhyme label="c" type="m">gemoet</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Doch wy nu wagen vlees en <rhyme label="c" type="m" corresp="#s4:l5">bloet</rhyme>.</l>
        <l id="s4:l7" met="4" type="+-">Voor onse Patramony <rhyme label="c" type="m" corresp="#s4:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Madamme de <rhyme label="a" type="f">Lorette</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En ons lief Vrou tot <rhyme label="b" type="m">Hal</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Geusen wilt <rhyme label="a" type="f" corresp="#s5:l1">verpletten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Met hun gebroetsel <rhyme label="b" type="m" corresp="#s5:l2">al</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">O Sint Jacob laet ghy dat <rhyme label="c" type="m">toe</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Dat de Ketters alles <rhyme label="c" type="m" corresp="#s5:l5">doe</rhyme>?</l>
        <l id="s5:l7" met="4" type="+-">Die my flaeu-hertigh maeckt en <rhyme label="c" type="m" corresp="#s5:l5">moe</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Roept ghy aen houte <rhyme label="a" type="f">Santen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Die u niet en <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Wilt u geloove <rhyme label="a" type="f" corresp="#s6:l1">planten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Roept God den Heere <rhyme label="b" type="m" corresp="#s6:l2">aen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Die kan u helpen, anders <rhyme label="c" type="m">geen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Verlaet het hout en oock de <rhyme label="c" type="m" corresp="#s6:l5">steen</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Soo wort ghy Godes volck <rhyme label="c" type="m" corresp="#s6:l5">gemeen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Bisschoppen, <rhyme label="a" type="f">Cardinalen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">U Proven sent my <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Om mijn volck te <rhyme label="a" type="f" corresp="#s7:l1">betalen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dat haest muyteneren <rhyme label="b" type="m" corresp="#s7:l2">sal</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Nonnen, Bagijnen <rhyme label="c" type="m">ongetelt</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Stiert my doch al u <rhyme label="c" type="m" corresp="#s7:l5">offer-gelt</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">Want mijn Kantoren sijn <rhyme label="c" type="m" corresp="#s7:l5">ontstelt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ons Generael <rhyme label="a" type="f">kloeck-moedigh</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met sijnen <rhyme label="b" type="m">Luytenant</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Die hebben alsoo <rhyme label="a" type="f" corresp="#s8:l1">spoedigh</rhyme></l>
        <l id="s8:l4" met="3" type="-+">U Vloote <rhyme label="b" type="m" corresp="#s8:l2">aengerant</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Met Gout en Silver wel <rhyme label="c" type="m">gelaen</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Maer kostelijcke ware <rhyme label="c" type="m" corresp="#s8:l5">saen</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Die behouden sijn gekomen <rhyme label="c" type="m" corresp="#s8:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Draeck noch Cants in 't <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Namen noyt sulcken schat <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Als my nu heeft <rhyme label="a" type="f" corresp="#s9:l1">ontdreven</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Den loosen Pieter <rhyme label="b" type="m" corresp="#s9:l2">Heyn</rhyme></l>
        <l id="s9:l5" met="4" type="+-">Ons Gelt-kisten staen leegh int <rhyme label="c" type="m">slot</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Niemant wil my leenen, o <rhyme label="c" type="m" corresp="#s9:l5">God</rhyme></l>
        <l id="s9:l7" met="4" type="+-">Den Geus die helpt my <rhyme label="c" type="m" corresp="#s9:l5">banckerot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Een Koninckrijcks schat <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Wy krijgen in <rhyme label="b" type="m">Hollandt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Daertoe een Konincks <rhyme label="a" type="f" corresp="#s10:l1">Kroone</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Met meenigh <rhyme label="b" type="m" corresp="#s10:l2">Diamant</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Veel onwaerdeerde Waeren <rhyme label="c" type="m">snel</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Die Kroon ons Prins wel passen <rhyme label="c" type="m" corresp="#s10:l5">sel</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">Den Speck wy dus temteren <rhyme label="c" type="m" corresp="#s10:l5">fel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">O Keyser groot van <rhyme label="a" type="f">machten</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Hebt ghy noch gelt by <rhyme label="b" type="m">u</rhyme>?</l>
        <l id="s11:l3" met="3" type="-+">Jck Koninck klaegh mijn <rhyme label="a" type="f" corresp="#s11:l1">klachten</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Wilt my vertroosten <rhyme label="b" type="m" corresp="#s11:l2">nu</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Jck bid u my niet en <rhyme label="c" type="m">verlaet</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Of weet ghy selver genen <rhyme label="c" type="m" corresp="#s11:l5">raet</rhyme></l>
        <l id="s11:l7" met="4" type="+-">Soo sijn wy in soberen <rhyme label="c" type="m" corresp="#s11:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Wy sullen 't vyer u <rhyme label="a" type="f">bringen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Soo na by uwen <rhyme label="b" type="m">voet</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Dat ghy sult moeten <rhyme label="a" type="f" corresp="#s12:l1">singen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Mesericorde <rhyme label="b" type="m" corresp="#s12:l2">soet</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">En 't hooft te leggen in den <rhyme label="c" type="m">schoot</rhyme></l>
        <l id="s12:l6" met="4" type="+-">Dat ghy moet komen voor den <rhyme label="c" type="m" corresp="#s12:l5">noot</rhyme></l>
        <l id="s12:l7" met="4" type="+-">Te soecken vreed' of Treves <rhyme label="c" type="m" corresp="#s12:l5">bloot</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Brabant en <rhyme label="a" type="f">Henegouwe</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Arthoys en Vlaend'ren <rhyme label="b" type="m">mee</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Weest u Koninck <rhyme label="a" type="f" corresp="#s13:l1">getrouwe</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Gebied de Klosters <rhyme label="b" type="m" corresp="#s13:l2">ree</rhyme></l>
        <l id="s13:l5" met="4" type="+-">Dat Gout en Silver <rhyme label="c" type="m">ongetelt</rhyme></l>
        <l id="s13:l6" met="4" type="+-">Al tot ront schijven wert <rhyme label="c" type="m" corresp="#s13:l5">gesmelt</rhyme>,</l>
        <l id="s13:l7" met="4" type="+-">En stiert doch cito my het <rhyme label="c" type="m" corresp="#s13:l5">gelt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Paep Jan sent gelt met <rhyme label="a" type="f">hoopen</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Want ons See-helden <rhyme label="b" type="m">stout</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Die hoopent weer te <rhyme label="a" type="f" corresp="#s14:l1">stropen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">'T is Silver of root <rhyme label="b" type="m" corresp="#s14:l2">Gout</rhyme>,</l>
        <l id="s14:l5" met="4" type="+-">Sy kruyssen de See gins en <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Na Papen goet streckt hun <rhyme label="c" type="m" corresp="#s14:l5">begeer</rhyme>,</l>
        <l id="s14:l7" met="4" type="+-">Jn ons Pack-huysen mach wel <rhyme label="c" type="m" corresp="#s14:l5">meer</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Lief-vrou van <rhyme label="a" type="f">Scherpen-heuvel</rhyme></l>
        <l id="s15:l2" met="3" type="-+">U Offer-kist op <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Verset u Kap en <rhyme label="a" type="f" corresp="#s15:l1">Keuvel</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Want ghy mijn helpen <rhyme label="b" type="m" corresp="#s15:l2">moet</rhyme>,</l>
        <l id="s15:l5" met="4" type="+-">Jst oock dat ghy mede kael <rhyme label="c" type="m">sijt</rhyme></l>
        <l id="s15:l6" met="4" type="+-">Soo helpt my dan o <rhyme label="c" type="m" corresp="#s15:l5">Jesuwijt</rhyme>!</l>
        <l id="s15:l7" met="4" type="+-">Mijn macht is anders niet een <rhyme label="c" type="m" corresp="#s15:l5">mijt</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Ons Helden kloeck te <rhyme label="a" type="f">water</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Tot slaen sijn <rhyme label="b" type="m">toe-gerust</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Den Nederlandschen <rhyme label="a" type="f" corresp="#s16:l1">Pater</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Regneert oock langhs de <rhyme label="b" type="m" corresp="#s16:l2">kust</rhyme>,</l>
        <l id="s16:l5" met="4" type="+-">Hy kruyst de woeste ruyme <rhyme label="c" type="m">See</rhyme>,</l>
        <l id="s16:l6" met="4" type="+-">Doorsoeckt veel havens ende <rhyme label="c" type="m" corresp="#s16:l5">Ree</rhyme></l>
        <l id="s16:l7" met="4" type="+-">Hy hoopt goe buyt te brengen <rhyme label="c" type="m" corresp="#s16:l5">mee</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Hoe sal ick 't dan gaen <rhyme label="a" type="f">maken</rhyme>?</l>
        <l id="s17:l2" met="3" type="-+">Jck ben in grooten <rhyme label="b" type="m">noot</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Krijgen sy noch mijn <rhyme label="a" type="f" corresp="#s17:l1">Kraken</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Met al de schatten <rhyme label="b" type="m" corresp="#s17:l2">groot</rhyme>,</l>
        <l id="s17:l5" met="4" type="+-">So sterf ick, eer mijn sterf-dagh <rhyme label="c" type="m">komt</rhyme>,</l>
        <l id="s17:l6" met="4" type="+-">Het is verloren veel <rhyme label="c" type="m" corresp="#s17:l5">geromt</rhyme>,</l>
        <l id="s17:l7" met="4" type="+-">O Paus de Geusen toch <rhyme label="c" type="m" corresp="#s17:l5">verdomt</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wy vragen na geen <rhyme label="a" type="f">Santen</rhyme>,</l>
        <l id="s18:l2" met="3" type="-+">Maer na Silver en <rhyme label="b" type="m">Gout</rhyme>,</l>
        <l id="s18:l3" met="3" type="-+">Van sulcke fraye <rhyme label="a" type="f" corresp="#s18:l1">quanten</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Den Hollander veel <rhyme label="b" type="m" corresp="#s18:l2">hout</rhyme>,</l>
        <l id="s18:l5" met="4" type="+-">Die eeren wy tot uwer <rhyme label="c" type="m">spijt</rhyme>,</l>
        <l id="s18:l6" met="4" type="+-">U droefheyt maeckt ons herte <rhyme label="c" type="m" corresp="#s18:l5">blijt</rhyme></l>
        <l id="s18:l7" met="4" type="+-">Noch sijt ghy soo den Geus niet <rhyme label="c" type="m" corresp="#s18:l5">quijt</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Waer ick my keer of <rhyme label="a" type="f">wende</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Soo koom ick in de <rhyme label="b" type="m">ly</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Hier en aen allen <rhyme label="a" type="f" corresp="#s19:l1">ende</rhyme></l>
        <l id="s19:l4" met="3" type="-+">De Ketters plagen <rhyme label="b" type="m" corresp="#s19:l2">my</rhyme>,</l>
        <l id="s19:l5" met="4" type="+-">Jn 't Oost, in 't Weste, hier en <rhyme label="c" type="m">daer</rhyme></l>
        <l id="s19:l6" met="4" type="+-">Ontneemt men my mijn schatten <rhyme label="c" type="m" corresp="#s19:l5">klaer</rhyme>,</l>
        <l id="s19:l7" met="4" type="+-">Och die al wel gestorven <rhyme label="c" type="m" corresp="#s19:l5">waer</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Princelijcke Koninck <rhyme label="a" type="f">machtigh</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Noch en sijt ghy niet <rhyme label="b" type="m">vry</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Met een nieuwe Vloot <rhyme label="a" type="f" corresp="#s20:l1">krachtigh</rhyme></l>
        <l id="s20:l4" met="3" type="-+">So komt u Loncq weer <rhyme label="b" type="m" corresp="#s20:l2">by</rhyme>,</l>
        <l id="s20:l5" met="4" type="+-">Jndien hem God gesontheyt <rhyme label="c" type="m">spaer</rhyme></l>
        <l id="s20:l6" met="4" type="+-">Met ander Helden kloeck <rhyme label="c" type="m" corresp="#s20:l5">voorwaer</rhyme>,</l>
        <l id="s20:l7" met="4" type="+-">Al int aen-staende <rhyme label="c" type="m" corresp="#s20:l5">Nieuwe-jaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
