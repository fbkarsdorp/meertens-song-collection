    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik ben verwonnen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="+-">Ick ben ghecomen / sonder dromen / by een <rhyme label="a" type="m">bloem</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Daer al myn hart en sin op <rhyme label="b" type="m">staet</rhyme></l>
        <l id="s1:l3" met="6" type="+-">En wert ghenomen / als den vromen / sonder <rhyme label="a" type="m" corresp="#s1:l1">roem</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dwelck ick met minnen <rhyme label="b" type="m" corresp="#s1:l2">aenvaet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dus weest myn <rhyme label="b" type="m" corresp="#s1:l2">toeuerlaet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Maer en syt niet <rhyme label="b" type="m" corresp="#s1:l2">opstynaet</rhyme></l>
        <l id="s1:l7" met="6" type="+-">Door veel schromen / sou den lomen / syn <rhyme label="c" type="m">vervaert</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Het welck wel mocht werden <rhyme label="c" type="m" corresp="#s1:l7">ghespaert</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="+-">Om] te verwinnen sal beginnen met een <rhyme label="a" type="m">moet</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Te vryen een maechdeken <rhyme label="b" type="m">fyn</rhyme></l>
        <l id="s2:l3" met="6" type="+-">Met versinnen sal ick minnen metter <rhyme label="a" type="m" corresp="#s2:l1">spoet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Al hier op dit <rhyme label="b" type="m" corresp="#s2:l2">termyn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">En oock in dese <rhyme label="b" type="m" corresp="#s2:l2">schyn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">So wilt -doch vriendelyck <rhyme label="b" type="m" corresp="#s2:l2">syn</rhyme></l>
        <l id="s2:l7" met="6" type="+-">Om v goddinne te beminne nu ter <rhyme label="c" type="m">tyt</rhyme></l>
        <l id="s2:l8" met="4" type="+-">So spoor ick na v met <rhyme label="c" type="m" corresp="#s2:l7">jolyt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="+-">Mocht ick ydone tot myn lone winnen <rhyme label="a" type="m">haer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die edele getrouwe <rhyme label="b" type="m">fier</rhyme></l>
        <l id="s3:l3" met="6" type="+-">Sy die schone spant die crone doch <rhyme label="a" type="m" corresp="#s3:l1">voorwaer</rhyme></l>
        <l id="s3:l4" met="3" type="-+">En seer soet al van <rhyme label="b" type="m" corresp="#s3:l2">manier</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Mocht ick v winnen <rhyme label="b" type="m" corresp="#s3:l2">schier</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Schoon <rhyme label="b" type="m" corresp="#s3:l2">eghelantier</rhyme></l>
        <l id="s3:l7" met="6" type="+-">Ende mocht thonen oock te wonen al in <rhyme label="c" type="m">vruecht</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Dat waer voor myn een groot <rhyme label="c" type="m" corresp="#s3:l7">ghenuecht</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="+-">Prinsses verheuen wilt doch gheuen met een <rhyme label="a" type="m">sin</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een trostelyck woort <rhyme label="b" type="m">voortaen</rhyme></l>
        <l id="s4:l3" met="6" type="+-">Ghy syt myn leuen sonder sneuen schoon <rhyme label="a" type="m" corresp="#s4:l1">goddin</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer ick op bouwe <rhyme label="b" type="m" corresp="#s4:l2">saen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dus doet een goedt <rhyme label="b" type="m" corresp="#s4:l2">vermaen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Ten sal v oock niet <rhyme label="b" type="m" corresp="#s4:l2">schaen</rhyme></l>
        <l id="s4:l7" met="6" type="+-">Door v aencleuen wert bedreuen metter <rhyme label="c" type="m">daet</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Reyn lieffde die niet en <rhyme label="c" type="m" corresp="#s4:l7">vergaet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
