      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Cupido triomfant</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">AL scheent in <rhyme label="a" type="m">ongheval</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dat ick u had' <rhyme label="b" type="f">verlaten</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">'k Had' niet verstooten <rhyme label="a" type="m" corresp="#s1:l1">al</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Maer was tot uwer <rhyme label="b" type="f" corresp="#s1:l2">baten</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ghegaen in 't <rhyme label="c" type="m">Noten-Hof</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Te sien de groene <rhyme label="c" type="m" corresp="#s1:l5">stof</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Van der valeyen <rhyme label="c" type="m" corresp="#s1:l5">leege-lof</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick gingh eens om te <rhyme label="a" type="m">sien</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Of oock de Wijn-stock <rhyme label="b" type="f">bloeyde</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Of oock de teere <rhyme label="a" type="m" corresp="#s2:l1">lie'n</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">'t Arm-geestigh volckje, <rhyme label="b" type="f" corresp="#s2:l2">groeyde</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Of die <rhyme label="c" type="m">Granate-boom</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Uyt-bottede de <rhyme label="c" type="m" corresp="#s2:l5">room</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Van mijn <rhyme label="c" type="m" corresp="#s2:l5">genaden-rijcke-stroom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En eer dat ick het <rhyme label="a" type="m">wist</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Of u noch hoorde <rhyme label="b" type="f">klagen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dat ghy my hadt <rhyme label="a" type="m" corresp="#s3:l1">vermist</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Soo was ick op de <rhyme label="b" type="f" corresp="#s3:l2">wagen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Van 't willigh volck <rhyme label="c" type="m">geset</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Ick spoede, gheen <rhyme label="c" type="m" corresp="#s3:l5">verlet</rhyme></l>
        <l id="s3:l7" met="4" type="-+">My hield'. Vw heyl was my de <rhyme label="c" type="m" corresp="#s3:l5">Wet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu dan, mijn lieve <rhyme label="a" type="m">kom</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">O Sulamith! keer <rhyme label="b" type="f">weder</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Ghy vredighe <rhyme label="a" type="m" corresp="#s4:l1">keerom</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Keer tot u self, ghy <rhyme label="b" type="f" corresp="#s4:l2">teder</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ghy swacke wel <rhyme label="c" type="m">besiet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Dat ick u niet <rhyme label="c" type="m" corresp="#s4:l5">verliet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En wat u noch al we'er <rhyme label="c" type="m" corresp="#s4:l5">geschiet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dat ick en 't <rhyme label="a" type="m">Hemels-Hof</rhyme></l>
        <l id="s5:l2" met="3" type="-+">V kleyn en reyn <rhyme label="b" type="f">beschouwen</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Ghy lie'n van g'ringher <rhyme label="a" type="m" corresp="#s5:l1">stof</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wat siet ghy dese <rhyme label="b" type="f" corresp="#s5:l2">vrouwe</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Aensiet haer <rhyme label="c" type="m">heerlijckheyt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">S' is als een heyr <rhyme label="c" type="m" corresp="#s5:l5">geleydt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Volveerdigh tot den krijgh <rhyme label="c" type="m" corresp="#s5:l5">bereydt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
