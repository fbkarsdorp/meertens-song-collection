<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>15027</id>
    <date>
      <year-start>1626</year-start>
      <year-end>1626</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>praise</tag>
      <tag>secular song</tag>
    </tags>
    <melody>O Heer geef zo lang leeft</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Glyck't eel <rhyme label="a" type="m">gesteent</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Met gout <rhyme label="a" type="m" corresp="#s1:l1">vereent</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Verciert werd', en ons jeucht <rhyme label="b" type="m">verheugt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">So even <rhyme label="c" type="m">is</rhyme>,,</l>
        <l id="s1:l5" met="2" type="-+">Ons Prins <rhyme label="c" type="m" corresp="#s1:l4">gewis</rhyme>,,</l>
        <l id="s1:l6" met="3" type="-+">Verciert met ware <rhyme label="b" type="m" corresp="#s1:l3">deucht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Noyt sach m'hem <rhyme label="a" type="m">vlien</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Of oock <rhyme label="a" type="m" corresp="#s2:l1">ontsien</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">De sterckste van het <rhyme label="b" type="m">Christenryck</rhyme>;</l>
        <l id="s2:l4" met="2" type="-+">In <rhyme label="c" type="m">wackerheyt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">End' goet <rhyme label="c" type="m" corresp="#s2:l4">beleyt</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Is niemant zyns <rhyme label="b" type="m" corresp="#s2:l3">gelyck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Syn kloeck <rhyme label="a" type="m">bestaen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">End' dapper <rhyme label="a" type="m" corresp="#s3:l1">slaen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Doet ruymen 't Spaensch geweld,, Het <rhyme label="b" type="m">velt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Die daer <rhyme label="c" type="m">Bestand</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Soeckt 't zyner <rhyme label="c" type="m" corresp="#s3:l4">schand</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Aen desen vromen <rhyme label="b" type="m" corresp="#s3:l3">Held</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Dees' trouwen <rhyme label="a" type="m">Vorst</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Heeft noyt <rhyme label="a" type="m" corresp="#s4:l1">gedorst</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Naer synes vyands bloet, maer <rhyme label="b" type="m">doet</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Voor alle <rhyme label="c" type="m">quaet</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Oock hoe't hem <rhyme label="c" type="m" corresp="#s4:l4">gaet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hen altyt niet als <rhyme label="b" type="m" corresp="#s4:l3">goet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Als Mars nu <rhyme label="a" type="m">heen</rhyme></l>
        <l id="s5:l2" met="2" type="-+">Vertrock soo't <rhyme label="a" type="m" corresp="#s5:l1">scheen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">End' elck nu meynde hy,, was <rhyme label="b" type="m">vry</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Van't oorlogs <rhyme label="c" type="m">kop</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Soo staet hier <rhyme label="c" type="m" corresp="#s5:l4">op</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Inlantsche <rhyme label="b" type="m" corresp="#s5:l3">jalousy</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Doch desen <rhyme label="a" type="m">twist</rhyme></l>
        <l id="s6:l2" met="2" type="-+">Werd haest <rhyme label="a" type="m" corresp="#s6:l1">geslist</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Door 'sPrincen goeden raet,, en <rhyme label="b" type="m">daet</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">End' groot <rhyme label="c" type="m">verstand</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Die weer het <rhyme label="c" type="m" corresp="#s6:l4">Land</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Bracht in een goeden <rhyme label="b" type="m" corresp="#s6:l3">staet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
