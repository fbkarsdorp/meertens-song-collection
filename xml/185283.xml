<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185283</id>
    <date>
      <year-start>1631</year-start>
      <year-end>1631</year-end>
    </date>
    <location>
      <orginal-name>de rijp</orginal-name>
      <standard-name>De Rijp, The Netherlands</standard-name>
      <coordinates>
        <lon>52.5548999</lon>
        <lat>4.8440884</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>J'aimerai en ce village</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Die reyn liefde <rhyme label="a" type="f">vierigh</rhyme></l>
        <l id="s1:l2" met="3" type="+-">In sijn herte <rhyme label="b" type="m">draeght</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die daer leeft <rhyme label="a" type="f" corresp="#s1:l1">manierigh</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Ghelijckt Godt <rhyme label="b" type="m" corresp="#s1:l2">behaeght</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-">Die sal vry van <rhyme label="c" type="f">quale</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Schoon en vruchtbaer bloeyen <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Die sal der Sonnen <rhyme label="c" type="f" corresp="#s1:l5">strale</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Op den middagh lyden <rhyme label="d" type="m" corresp="#s1:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Die gheheel laet <rhyme label="a" type="f">varen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Der Godtloosen <rhyme label="b" type="m">raet</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Die op der <rhyme label="a" type="f" corresp="#s2:l1">sondaren</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Wech oock niet en <rhyme label="b" type="m" corresp="#s2:l2">gaet</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-">Die is wel <rhyme label="c" type="f">gheleken</rhyme></l>
        <l id="s2:l6" met="4" type="+-">By een groene vruchtbaer <rhyme label="d" type="m">boom</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die daer <rhyme label="c" type="f" corresp="#s2:l5">onbesweken</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Groeyt aen eenen <rhyme label="d" type="m" corresp="#s2:l6">water-stroom</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Die wel Godts <rhyme label="a" type="f">behaghen</rhyme></l>
        <l id="s3:l2" met="3" type="+-">Weet en niet en <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Die sal vele <rhyme label="a" type="f" corresp="#s3:l1">slaghen</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Lijden in den <rhyme label="b" type="m" corresp="#s3:l2">gloet</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-">Die eeuwigh sal <rhyme label="c" type="f">branden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Noch wert hy ghebonden // <rhyme label="d" type="m">met</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Die duystere <rhyme label="c" type="f" corresp="#s3:l5">banden</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Want hy ginck der sonden // <rhyme label="d" type="m" corresp="#s3:l6">tret</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Die loopt <rhyme label="a" type="f">onverholen</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Op weghen <rhyme label="b" type="m">ghemeen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Dit moet wel <rhyme label="a" type="f" corresp="#s4:l1">verdolen</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Want daer is maer <rhyme label="b" type="m" corresp="#s4:l2">een</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">Die daer tot den <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Is den wegh der waerheyt <rhyme label="d" type="m">plat</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Die daer gaet <rhyme label="c" type="f" corresp="#s4:l5">beneven</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Sal missen die schoone <rhyme label="d" type="m" corresp="#s4:l6">stadt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Die met Esaus <rhyme label="a" type="f">soorte</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Verkoopen <rhyme label="b" type="m">onwijs</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Die eerste <rhyme label="a" type="f" corresp="#s5:l1">gheboorte</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Voor een schotel <rhyme label="b" type="m" corresp="#s5:l2">spijs</rhyme>,</l>
        <l id="s5:l5" met="3" type="+-">Die gaet hier <rhyme label="c" type="f">verkiesen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">‘s Werelts tijdelijck <rhyme label="d" type="m">profijt</rhyme>,</l>
        <l id="s5:l7" met="3" type="+-">Die sullen <rhyme label="c" type="f" corresp="#s5:l5">verliesen</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">‘t Eeuwigh hemelsche <rhyme label="d" type="m" corresp="#s5:l6">jolijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Die wandelt <rhyme label="a" type="f">verstandigh</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Sijnde geen <rhyme label="b" type="m">sondaer</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">Die de dronckaerts <rhyme label="a" type="f" corresp="#s6:l1">schandigh</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Niet en volget <rhyme label="b" type="m" corresp="#s6:l2">naer</rhyme>,</l>
        <l id="s6:l5" met="3" type="+-">Die niet <rhyme label="c" type="f">Leugenachtigh</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Noch niet valsch is van <rhyme label="d" type="m">gemoet</rhyme>,</l>
        <l id="s6:l7" met="3" type="+-">Die sal noch <rhyme label="c" type="f" corresp="#s6:l5">deelachtigh</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Sijn, Godts edel rijcke <rhyme label="d" type="m" corresp="#s6:l6">soet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Die met sijnder <rhyme label="a" type="f">tonghen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Niet en smaet noch <rhyme label="b" type="m">schent</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Die wel heeft <rhyme label="a" type="f" corresp="#s7:l1">bedwonghen</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">Al sijn woorden <rhyme label="b" type="m" corresp="#s7:l2">jent</rhyme>,</l>
        <l id="s7:l5" met="3" type="+-">Die sijn hert noch <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Niet besmet om ‘t aertsche <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s7:l7" met="3" type="+-">Die sal vry van <rhyme label="c" type="f" corresp="#s7:l5">schanden</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Namaels hebben <rhyme label="d" type="m" corresp="#s7:l6">overvloet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="+-">Die niet is <rhyme label="a" type="f">wraack-gierigh</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Tegens sijn <rhyme label="b" type="m">vyandt</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Die hier <rhyme label="a" type="f" corresp="#s8:l1">goedertierigh</rhyme></l>
        <l id="s8:l4" met="3" type="+-">Leeft in leegen <rhyme label="b" type="m" corresp="#s8:l2">stant</rhyme>,</l>
        <l id="s8:l5" met="3" type="+-">Die ‘t bedroch der <rhyme label="c" type="f">sonden</rhyme></l>
        <l id="s8:l6" met="4" type="+-">Seer kloeckmoedigh <rhyme label="d" type="m">wederstaet</rhyme>,</l>
        <l id="s8:l7" met="3" type="+-">Die sal groot bevonden <rhyme label="c" type="f" corresp="#s8:l5">zijn</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Wanneer ‘t hier al <rhyme label="d" type="m" corresp="#s8:l6">vergaet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="+-">Die Liefde <rhyme label="a" type="f">bewijsen</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Aen Godts leden <rhyme label="b" type="m">fijn</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Die d’hong’rige <rhyme label="a" type="f" corresp="#s9:l1">spijsen</rhyme>,</l>
        <l id="s9:l4" met="3" type="+-">Oock die dorstigh <rhyme label="b" type="m" corresp="#s9:l2">sijn</rhyme>,</l>
        <l id="s9:l5" met="3" type="+-">Die weldaet <rhyme label="c" type="f">betoonen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Aen veel kranck en arme <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s9:l7" met="3" type="+-">Die sal Godt <rhyme label="c" type="f" corresp="#s9:l5">beloonen</rhyme></l>
        <l id="s9:l8" met="4" type="+-">Met een stroom die <rhyme label="d" type="m" corresp="#s9:l6">overvliet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="+-">Die heeft tot sijn <rhyme label="a" type="f">voordeel</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">Bermhertigheydt <rhyme label="b" type="m">an</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">Die haer tegen ‘t <rhyme label="a" type="f" corresp="#s10:l1">Oordeel</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">Wel beroemen <rhyme label="b" type="m" corresp="#s10:l2">can</rhyme>,</l>
        <l id="s10:l5" met="3" type="+-">Die in alle <rhyme label="c" type="f">stucken</rhyme></l>
        <l id="s10:l6" met="4" type="+-">Der Liefden volstandigh <rhyme label="d" type="m">blijft</rhyme>,</l>
        <l id="s10:l7" met="3" type="+-">Die sal ‘t wel <rhyme label="c" type="f" corresp="#s10:l5">ghelucken</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Eeuwigh hy in Godt <rhyme label="d" type="m" corresp="#s10:l6">beclijft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
