  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">FY Ioosten kreegh een nieuwe <rhyme label="a" type="m">vlam</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En riep met volle <rhyme label="b" type="f">snater</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Ier dat ic Melis Tysz <rhyme label="a" type="m" corresp="#s1:l1">nam</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ick sprongh veel liever in ’t <rhyme label="b" type="f" corresp="#s1:l2">water</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick bin versot op <rhyme label="c" type="f">Fransje</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ick ben versot op <rhyme label="c" type="f" corresp="#s1:l5">Fransje</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En Singh sijn <rhyme label="d" type="m">lof</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Van ainden <rhyme label="d" type="m" corresp="#s1:l7">of</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s1:l5">Iansje</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s1:l5">Iansje</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wat gingh ick met die Droogert <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die lompe Melis <rhyme label="b" type="f">krombien</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Maer Fransje Flore dat ’s een <rhyme label="a" type="m" corresp="#s2:l1">Haen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Die rijen ken en <rhyme label="b" type="f" corresp="#s2:l2">omsien</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Daerom bemin ick <rhyme label="c" type="f">Fransje</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daerom bemin ick <rhyme label="c" type="f" corresp="#s2:l5">Fransje</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En Singh sijn <rhyme label="d" type="m">lof</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Van ainden <rhyme label="d" type="m" corresp="#s2:l7">of</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s2:l5">Iansje</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s2:l5">Iansje</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ick, O suycker inde <rhyme label="a" type="m">Pan</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Met kus op kus mach <rhyme label="b" type="f">kaetsen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Mijn lippen tegens Fransjes <rhyme label="a" type="m" corresp="#s3:l1">an</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Soo rijdt mijn hert op <rhyme label="b" type="f" corresp="#s3:l2">schaetsen</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Daerom bemin ick <rhyme label="c" type="f">Fransje</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Daerom bemin ick <rhyme label="c" type="f" corresp="#s3:l5">Fransje</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">En Singh sijn <rhyme label="d" type="m">lof</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Van ainden <rhyme label="d" type="m" corresp="#s3:l7">of</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s3:l5">Iansje</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Het is een giestigh <rhyme label="c" type="f" corresp="#s3:l5">Iansje</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gien Keunincklijcke Schepter <rhyme label="a" type="m">swerck</rhyme></l>
        <l id="s4:l2" met="3" type="-+">En blinckt niet half soo <rhyme label="b" type="f">helder</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Als ’t rain eguurd betiele <rhyme label="a" type="m" corresp="#s4:l1">werck</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">In Frangsje Flores <rhyme label="b" type="f" corresp="#s4:l2">Kelder</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ick hout met Frangsje <rhyme label="c" type="f">Flore</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ick hout met Frangsje <rhyme label="c" type="f" corresp="#s4:l5">Flore</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En zingh sijn <rhyme label="d" type="m">lof</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Van ainden <rhyme label="d" type="m" corresp="#s4:l7">of</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Het is een giest met <rhyme label="c" type="f" corresp="#s4:l5">ooren</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Het is een giest met <rhyme label="c" type="f" corresp="#s4:l5">ooren</rhyme></l>
      </lg>
    </lg>
  </text>
