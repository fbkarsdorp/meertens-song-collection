  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NV laet ghy Heer <rhyme label="a" type="m">oprecht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghaen in vrede uwen <rhyme label="a" type="m" corresp="#s1:l1">Knecht</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Nae v beloft <rhyme label="b" type="f">ghestadich</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Na dat mijn Ooghen <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hebben ghesien <rhyme label="c" type="m" corresp="#s1:l4">voorwaer</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Vwen Heylandt <rhyme label="b" type="f" corresp="#s1:l3">ghenadich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">En Saelichmaeker <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Aller Volcken <rhyme label="a" type="m" corresp="#s2:l1">ghemeyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die aennemen sijn <rhyme label="b" type="f">leere</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Een licht schijnende <rhyme label="c" type="m">claer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Den Heydenen <rhyme label="c" type="m" corresp="#s2:l4">eenpaer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En ooc Israhels <rhyme label="b" type="f" corresp="#s2:l3">eere</rhyme>.</l>
      </lg>
    </lg>
