<song>
  <meta>
    <id>184694</id>
    <date>
      <year-start>1746</year-start>
      <year-end>1746</year-end>
    </date>
    <location>
      <orginal-name>den haag</orginal-name>
      <standard-name>The Hague, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0799838</lon>
        <lat>4.3113461</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Zullen dan mijn droeve klachten</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Lieflijk Bosje Silvere <rhyme label="a" type="f">Vlieten</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die onder de Telgen <rhyme label="b" type="m">schiet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">o Plaetsje daar ik kwam <rhyme label="a" type="f" corresp="#s1:l1">genieten</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Rosemont die bloozend <rhyme label="b" type="m" corresp="#s1:l2">ziet</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Strooit nu Blaaderen jonge <rhyme label="c" type="f">Boomen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Staakt uw loop vlietende <rhyme label="c" type="f" corresp="#s1:l5">stroomen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Hebt dog deel aan myn <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Want myn schoone, want myn <rhyme label="d" type="f">schoone</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Roosje die vind ik hier <rhyme label="b" type="m" corresp="#s1:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vlegt een krans Lief van <rhyme label="a" type="f">fioolen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Blust en blust myn jeugdig <rhyme label="b" type="m">vier</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">In dees haag’ daar ik ben <rhyme label="a" type="f" corresp="#s2:l1">verschoolen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Onder deezen <rhyme label="b" type="m" corresp="#s2:l2">Populier</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Als Venus eer door Liefd’ <rhyme label="c" type="f">gevangen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">klaag niet wil uw hert my <rhyme label="c" type="f" corresp="#s2:l5">langen</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Want ziet op en Berg en <rhyme label="b" type="m" corresp="#s2:l2">Dal</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Roosje schoone, Roosje <rhyme label="d" type="f">schoone</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Gy myn schaapes vinden <rhyme label="b" type="m" corresp="#s2:l2">zal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ganimeed’ ik kom uw <rhyme label="a" type="f">nooden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Met d’herders Nimphies op dit <rhyme label="b" type="m">pas</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Schenkt un Nektar aan de <rhyme label="a" type="f" corresp="#s3:l1">Gooden</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ontfangt dees krans van eel <rhyme label="b" type="m" corresp="#s3:l2">gewas</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Laat ik uw in ’t groen <rhyme label="c" type="f">omhelzen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Phaebus straalt reets door de <rhyme label="c" type="f" corresp="#s3:l5">Elzen</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Laat Floris smaaken ’t <rhyme label="b" type="m" corresp="#s3:l2">Ambrozyn</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Van uw Lippen, van uw <rhyme label="d" type="f">Lippen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Die veel zoeter is als <rhyme label="b" type="m" corresp="#s3:l2">wyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
