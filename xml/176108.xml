    <melody>Psalm 110 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">DE Heere heeft geseyt tot mijnen <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Set u gerust tot mijner <rhyme label="b" type="m">rechterhant</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Tot dat ick als een voetschabel <rhyme label="a" type="f" corresp="#s1:l1">verneere</rhyme></l>
        <l id="s1:l4" met="5" type="-+">Haest onder u die u doen <rhyme label="b" type="m" corresp="#s1:l2">wederstant</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">V scepter sterck sal senden God de <rhyme label="a" type="f">Heere</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Wt Sion over den aertbodem <rhyme label="b" type="m">breet</rhyme>:</l>
        <l id="s2:l3" met="5" type="-+">En hy sal tot u spreken; nu, <rhyme label="a" type="f" corresp="#s2:l1">regere</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Int midden van al uwe haters <rhyme label="b" type="m" corresp="#s2:l2">wreet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Een willich volck u dienen sal met <rhyme label="a" type="f">vreugde</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Ter tijt uws heyrs, in heylige <rhyme label="b" type="m">cieraet</rhyme>.</l>
        <l id="s3:l3" met="5" type="-+">Oneyndich is den dau van uwe <rhyme label="a" type="f" corresp="#s3:l1">jeugde</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Geboren uyt den vroegen <rhyme label="b" type="m" corresp="#s3:l2">dageraet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">De Heere heeft een hoogen eet <rhyme label="a" type="f">gesworen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">En die sal hem berouwen <rhyme label="b" type="m">nimmermeer</rhyme>;</l>
        <l id="s4:l3" met="5" type="-+">Ghy syt een eeuwich Priester <rhyme label="a" type="f" corresp="#s4:l1">uytvercoren</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Gelijckerwijs Melchizedech <rhyme label="b" type="m" corresp="#s4:l2">wel-eer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">De Heer (o God) ter rechterhant <rhyme label="a" type="f">geseten</rhyme></l>
        <l id="s5:l2" met="5" type="-+">Van uwe macht, sal coningen <rhyme label="b" type="m">verslaen</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">En haer den loon van haren trots <rhyme label="a" type="f" corresp="#s5:l1">uytmeten</rhyme></l>
        <l id="s5:l4" met="5" type="-+">Wanneer den brant syns torens aen sal <rhyme label="b" type="m" corresp="#s5:l2">gaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Hy sal het recht den heydenen <rhyme label="a" type="f">uytspreken</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">En vullen 't lant met dooden <rhyme label="b" type="m">ongetelt</rhyme>;</l>
        <l id="s6:l3" met="5" type="-+">Oock crachtichlijck het hooft in stucken <rhyme label="a" type="f" corresp="#s6:l1">breken</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Dat over een groot rijcke was <rhyme label="b" type="m" corresp="#s6:l2">gestelt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Iae 't sal geschien, dat uyt de <rhyme label="a" type="f">waterbeken</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Hy op den wech zal lesschen synen <rhyme label="b" type="m">dorst</rhyme></l>
        <l id="s7:l3" met="5" type="-+">Dies sal hy ooc het hooft om hooge <rhyme label="a" type="f" corresp="#s7:l1">steken</rhyme></l>
        <l id="s7:l4" met="5" type="-+">Als een vermaert en overwinnend' <rhyme label="b" type="m" corresp="#s7:l2">Vorst</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
