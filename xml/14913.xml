<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14913</id>
    <date>
      <year-start>1622</year-start>
      <year-end>1622</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Quand je vois son bel oeil</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy Rymers die in veersen <rhyme label="a" type="m">sucht</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Veul Jammer <rhyme label="a" type="m" corresp="#s1:l1">klucht</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En stroyt u arbeyt inde <rhyme label="b" type="m">windt</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Vol <rhyme label="c" type="f">ydelheden</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Daer ghy ons <rhyme label="c" type="f" corresp="#s1:l4">mede</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Als sotten <rhyme label="b" type="m" corresp="#s1:l3">bindt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">V vermaen ick dat ghy eens <rhyme label="a" type="m">staeckt</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">En spuwent <rhyme label="a" type="m" corresp="#s2:l1">braeckt</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Alle dees sotheyt uyt u <rhyme label="b" type="m">hooft</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Die met pronck <rhyme label="c" type="f">reden</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Ghy ons ginckt <rhyme label="c" type="f" corresp="#s2:l4">smeeden</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Van sin <rhyme label="b" type="m" corresp="#s2:l3">berooft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet vry u minne doot en <rhyme label="a" type="m">kout</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Die nu heel <rhyme label="a" type="m" corresp="#s3:l1">out</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Leyt gants verstorven onder <rhyme label="b" type="m">d’as</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Eens zijn <rhyme label="c" type="f">vergeten</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">En u waen <rhyme label="c" type="f" corresp="#s3:l4">weten</rhyme></l>
        <l id="s3:l6" met="2" type="-+">V vlieden <rhyme label="b" type="m" corresp="#s3:l3">ras</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoort nae d’oprechte Minnaers <rhyme label="a" type="m">klacht</rhyme></l>
        <l id="s4:l2" met="2" type="-+">En keurich <rhyme label="a" type="m" corresp="#s4:l1">acht</rhyme></l>
        <l id="s4:l3" met="4" type="-+">’T gheen haer de Liefde doet <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Soo suldy <rhyme label="c" type="f">mercken</rhyme></l>
        <l id="s4:l5" met="2" type="-+">Dat al u <rhyme label="c" type="f" corresp="#s4:l4">wercken</rhyme></l>
        <l id="s4:l6" met="2" type="-+">Is ydel <rhyme label="b" type="m" corresp="#s4:l3">waen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
