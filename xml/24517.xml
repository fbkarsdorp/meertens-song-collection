<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24517</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Ik weet een vrouwtje wel bereid</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ic weet heer Jhs wal <rhyme label="a" type="m">bereyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">hi heeft myn herte <rhyme label="b" type="m">omvaen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">mit synre sueter <rhyme label="a" type="m" corresp="#s1:l1">mynlicheit</rhyme></l>
        <l id="s1:l4" met="3" type="-+">ic en kan hem niet <rhyme label="b" type="m" corresp="#s1:l2">ontgaen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">ic moet syne guetheit <rhyme label="c" type="m">dragen</rhyme></l>
        <l id="s1:l6" met="4" type="-+">syn wesen dat is soe wal <rhyme label="d" type="m">gedaen</rhyme></l>
        <l id="s1:l7" met="3" type="-+">hi staet in mynen <rhyme label="c" type="m" corresp="#s1:l5">behagen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schoenre here en sach ick <rhyme label="a" type="m">nye</rhyme></l>
        <l id="s2:l2" met="3" type="-+">des wil ic my <rhyme label="b" type="m">vermeten</rhyme></l>
        <l id="s2:l3" met="4" type="-+">tys recht dat ic hem vrenscap <rhyme label="a" type="m" corresp="#s2:l1">doe</rhyme></l>
        <l id="s2:l4" met="3" type="-+">hi doet myn druck <rhyme label="b" type="m" corresp="#s2:l2">vergeten</rhyme></l>
        <l id="s2:l5" met="3" type="-+">hoe reyne is alle syne <rhyme label="c" type="m">mynne</rhyme></l>
        <l id="s2:l6" met="4" type="-+">my is te bet dat ic hem <rhyme label="d" type="m">sie</rhyme></l>
        <l id="s2:l7" met="3" type="-+">dat doet syne suete <rhyme label="c" type="m" corresp="#s2:l5">mynne</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Desen here en wil ic niet <rhyme label="a" type="m">begeuen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">hi staet soe vast in <rhyme label="b" type="m">my</rhyme></l>
        <l id="s3:l3" met="4" type="-+">bauen  alle die daer <rhyme label="a" type="m" corresp="#s3:l1">leuen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">tys recht daer is reden <rhyme label="b" type="m" corresp="#s3:l2">by</rhyme></l>
        <l id="s3:l5" met="3" type="-+">syne mynne is alsoe <rhyme label="c" type="m">breyt</rhyme></l>
        <l id="s3:l6" met="4" type="-+">als ic in synen synnen <rhyme label="d" type="m">stae</rhyme></l>
        <l id="s3:l7" met="3" type="-+">soe vergete ic alle myn <rhyme label="c" type="m" corresp="#s3:l5">leyt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Syn wesen is claer syn glorie is <rhyme label="a" type="m">schoen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">dat licht als een <rhyme label="b" type="m">crysstal</rhyme></l>
        <l id="s4:l3" met="4" type="-+">mocht ic hem spreken sonder <rhyme label="a" type="m" corresp="#s4:l1">vaen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">dat waer myn wonschen <rhyme label="b" type="m" corresp="#s4:l2">wal</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Och lacy neen ic <rhyme label="c" type="m">niet</rhyme></l>
        <l id="s4:l6" met="4" type="-+">dat doet myn vleysch ende alle myn <rhyme label="d" type="m">sunden</rhyme></l>
        <l id="s4:l7" met="3" type="-+">sy holden my int <rhyme label="c" type="m" corresp="#s4:l5">verdriet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Desen heer en is nyemant toe hoge <rhyme label="a" type="m">gebaren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">des wil ic my <rhyme label="b" type="m">verblyden</rhyme></l>
        <l id="s5:l3" met="4" type="-+">ic heb hem in myn herte <rhyme label="a" type="m" corresp="#s5:l1">vercaeren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">dat doet syne suete <rhyme label="b" type="m" corresp="#s5:l2">myn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Och lacy ic lyde <rhyme label="c" type="m">pyn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">dus moet ic vast in j hapen <rhyme label="d" type="m">leuen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">ic byn in druck <rhyme label="c" type="m" corresp="#s5:l5">alleyn</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Jhesus here siet aen myn <rhyme label="a" type="m">druck</rhyme></l>
        <l id="s6:l2" met="3" type="-+">doer alle myne <rhyme label="b" type="m">begeren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">al is dyt liden  in my <rhyme label="a" type="m" corresp="#s6:l1">gedicht</rhyme></l>
        <l id="s6:l4" met="3" type="-+">ic bid heer Jhesus <rhyme label="b" type="m" corresp="#s6:l2">seer</rhyme></l>
        <l id="s6:l5" met="3" type="-+">gy syts my wal <rhyme label="c" type="m">weert</rhyme></l>
        <l id="s6:l6" met="4" type="-+">och v te dienen alle myn <rhyme label="d" type="m">leuen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">waer gys here op my <rhyme label="c" type="m" corresp="#s6:l5">begeert</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
