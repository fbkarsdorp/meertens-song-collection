<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>175886</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>triumph song</tag>
      <tag>political song</tag>
      <tag>sailor song</tag>
      <tag>beggar song</tag>
    </tags>
    <melody>Te mei als alle vogels zingen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Looft God den Heer die 't alle <rhyme label="a" type="m">geeft</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Waer door Hemel en Aerde <rhyme label="a" type="m" corresp="#s1:l1">beeft</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ten spijt van ons <rhyme label="b" type="f">Vyanden</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die Triumphe gegeven <rhyme label="a" type="m" corresp="#s1:l1">heeft</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dees schoon-vereende <rhyme label="b" type="f" corresp="#s1:l3">Landen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O kloecke Batavieren <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Die minder als een loopent <rhyme label="a" type="m" corresp="#s2:l1">Jaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Vernielt het Rijck van <rhyme label="b" type="f">Spaengien</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dat wy nu singen <rhyme label="a" type="m" corresp="#s2:l1">openbaer</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Vive la Prins <rhyme label="b" type="f" corresp="#s2:l3">d'Oraengien</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">De Mogend' Staten <rhyme label="a" type="m">Generael</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ja Steden, Leden <rhyme label="a" type="m" corresp="#s3:l1">principael</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Door ons Velt-heer vol <rhyme label="b" type="f">trouwen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Toe-rusten Schepen <rhyme label="a" type="m" corresp="#s3:l1">altemael</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Tot lof van 't huys <rhyme label="b" type="f" corresp="#s3:l3">Nassouwen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Den E. Generael Hendrick <rhyme label="a" type="m">Loncq</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Uyt liefde dese reys <rhyme label="a" type="m" corresp="#s4:l1">aenvonck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Om sijn vyant te <rhyme label="b" type="f">krencken</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">De eer die hy hier in <rhyme label="a" type="m" corresp="#s4:l1">begonck</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Salmen seer langh <rhyme label="b" type="f" corresp="#s4:l3">gedencken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Den Kolonel van <rhyme label="a" type="m">Waerdenburgh</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Heeft hem gequeten sonder <rhyme label="a" type="m" corresp="#s5:l1">sorgh</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Als een Heldt boven <rhyme label="b" type="f">maten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Hy won Victory goet daer <rhyme label="a" type="m" corresp="#s5:l1">deur</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Met alle sijn <rhyme label="b" type="f" corresp="#s5:l3">Soldaten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Lof sy d'overste <rhyme label="a" type="m">Luytenants</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Eltsen, Steyncalen, Felt <rhyme label="a" type="m" corresp="#s6:l1">valjants</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Met heur Monjor in <rhyme label="b" type="f">vreden</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Die om dat lieve <rhyme label="a" type="m" corresp="#s6:l1">Vaderlant</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Manhaftig hebben <rhyme label="b" type="f" corresp="#s6:l3">gestreden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Lof sy Krijghs-helden over <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">En kloeck Matroosen int <rhyme label="a" type="m" corresp="#s7:l1">getal</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Die op 't strant moesten <rhyme label="b" type="f">waken</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Jnt natte, kout en <rhyme label="a" type="m" corresp="#s7:l1">ongeval</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Eer men de Stad kost <rhyme label="b" type="f" corresp="#s7:l3">naken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Door Rio Dulce een <rhyme label="a" type="m">Revier</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Passeerden sy met groot <rhyme label="a" type="m" corresp="#s8:l1">dangier</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Tot haer middel by <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Doen vanden sy aen elck <rhyme label="a" type="m" corresp="#s8:l1">quartier</rhyme></l>
        <l id="s8:l5" met="3" type="-+">De Stad beset te <rhyme label="b" type="f" corresp="#s8:l3">wesen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Aen alle oorden van de <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Speck-Jan hem wel betrencheet <rhyme label="a" type="m" corresp="#s9:l1">had</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En versien met <rhyme label="b" type="f">Soldaten</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Maer doen de bateviers <rhyme label="a" type="m" corresp="#s9:l1">aen-trad</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Mosten sy 't haest <rhyme label="b" type="f" corresp="#s9:l3">verlaten</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ons helden vielen dapper <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Met schieten, houwen ende <rhyme label="a" type="m" corresp="#s10:l1">slaen</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">En oock met Kanons <rhyme label="b" type="f">geruchten</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Soo dat sy mosten wijcken <rhyme label="a" type="m" corresp="#s10:l1">gaen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">By de bosschagien <rhyme label="b" type="f" corresp="#s10:l3">vluchten</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Kastelen, Forten <rhyme label="a" type="m">allegaer</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Jn min als veertien daegh daer <rhyme label="a" type="m" corresp="#s11:l1">naer</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Met accoort over <rhyme label="b" type="f">gingen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Duch mach men wel in 't <rhyme label="a" type="m" corresp="#s11:l1">openbaer</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Een Victory-lied <rhyme label="b" type="f" corresp="#s11:l3">singen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Olinda maeckt ons herten <rhyme label="a" type="m">vro</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">De Hooft-stad van <rhyme label="a" type="m" corresp="#s12:l1">Fernambuco</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Verovert met kleyn <rhyme label="b" type="f">schaden</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">De Batavieren vechten <rhyme label="a" type="m" corresp="#s12:l1">soo</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Alleen door Gods <rhyme label="b" type="f" corresp="#s12:l3">genaden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dit al geschiet door Godes <rhyme label="a" type="m">wil</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">Dat wy het lichaem van <rhyme label="a" type="m" corresp="#s13:l1">Brasil</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Vast soecken in te <rhyme label="b" type="f">boren</rhyme>,</l>
        <l id="s13:l4" met="4" type="-+">Daer Speck-Jan triumpheerde <rhyme label="a" type="m" corresp="#s13:l1">stil</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Komen wy hem <rhyme label="b" type="f" corresp="#s13:l3">verstoren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Soekt nu aflaet ghy Spaens <rhyme label="a" type="m">gekroont</rhyme></l>
        <l id="s14:l2" met="4" type="-+">Die noch in de Havana <rhyme label="a" type="m" corresp="#s14:l1">woont</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Wy u by tijts <rhyme label="b" type="f">verneeren</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Wie komt te laet, wort niet <rhyme label="a" type="m" corresp="#s14:l1">verschoont</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Pater sal 't <rhyme label="b" type="f" corresp="#s14:l3">absolveren</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Don Fredrick mocht wenschen <rhyme label="a" type="m">maer</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Dat hy gesont in Spanje <rhyme label="a" type="m" corresp="#s15:l1">waer</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Met Galjoens en <rhyme label="b" type="f">Galiassen</rhyme></l>
        <l id="s15:l4" met="4" type="-+">Want onse helden <rhyme label="a" type="m" corresp="#s15:l1">allegaer</rhyme></l>
        <l id="s15:l5" met="3" type="-+">Gestadigh daer op <rhyme label="b" type="f" corresp="#s15:l3">passen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Nu kruyst men daer de ruyme <rhyme label="a" type="m">See</rhyme>,</l>
        <l id="s16:l2" met="4" type="-+">Pater sal vinden een goed <rhyme label="a" type="m" corresp="#s16:l1">ree</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Jn Fernambuco <rhyme label="b" type="f">stercke</rhyme>,</l>
        <l id="s16:l4" met="4" type="-+">Mijn heer Loncq en Waerdenburg <rhyme label="a" type="m" corresp="#s16:l1">mee</rhyme></l>
        <l id="s16:l5" met="3" type="-+">Die wercken 't welvaerts <rhyme label="b" type="f" corresp="#s16:l3">wercke</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Dees Victory is meerder <rhyme label="a" type="m">waert</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Als vele schatten op der <rhyme label="a" type="m" corresp="#s17:l1">aert</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Elck een kan niet <rhyme label="b" type="f">bedenkcen</rhyme></l>
        <l id="s17:l4" met="4" type="-+">De plunderingh gaet sijnder <rhyme label="a" type="m" corresp="#s17:l1">vaert</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Maer Spanjen sal 't meest <rhyme label="b" type="f" corresp="#s17:l3">krencken</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Soldaten houdet goeden <rhyme label="a" type="m">moet</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Krijgs-helt te paerde en te <rhyme label="a" type="m" corresp="#s18:l1">voet</rhyme></l>
        <l id="s18:l3" met="3" type="-+">U Velt-heer blijft <rhyme label="b" type="f">getrouwe</rhyme>,</l>
        <l id="s18:l4" met="4" type="-+">Vernietight het Maraens <rhyme label="a" type="m" corresp="#s18:l1">gebroet</rhyme></l>
        <l id="s18:l5" met="3" type="-+">Tot lof van 't Huys <rhyme label="b" type="f" corresp="#s18:l3">Nassouwe</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">De Hoogmogende Heeren Staten <rhyme label="a" type="m">wijs</rhyme></l>
        <l id="s19:l2" met="4" type="-+">Vive Oranjen seer <rhyme label="a" type="m" corresp="#s19:l1">propijs</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Barons en <rhyme label="b" type="f">Kolonellen</rhyme>,</l>
        <l id="s19:l4" met="4" type="-+">Leydse o Heer na u <rhyme label="a" type="m" corresp="#s19:l1">advijs</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Om 't Land in rust te <rhyme label="b" type="f" corresp="#s19:l3">stellen</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Geoctroyeerde Heeren <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s20:l2" met="4" type="-+">Der West-indische <rhyme label="a" type="m" corresp="#s20:l1">Compagny</rhyme>,</l>
        <l id="s20:l3" met="3" type="-+">Geluck komt u toe <rhyme label="b" type="f">wenden</rhyme>,</l>
        <l id="s20:l4" met="4" type="-+">U helden maeckt nu herten <rhyme label="a" type="m" corresp="#s20:l1">bly</rhyme></l>
        <l id="s20:l5" met="3" type="-+">Secoers wiltse haest <rhyme label="b" type="f" corresp="#s20:l3">senden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
