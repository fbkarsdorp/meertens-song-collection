<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186891</id>
    <date>
      <year-start>1781</year-start>
      <year-end>1781</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Op de been Bataefsche <rhyme label="a" type="f">Helden</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Nu gy hoort den Trommel <rhyme label="b" type="m">slaan</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Wapend u en gaet te <rhyme label="a" type="f" corresp="#s1:l1">velden</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Om den Brit te <rhyme label="b" type="m" corresp="#s1:l2">wederstaen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Treft het woedend <rhyme label="c" type="m">Albion</rhyme>?</l>
        <l id="s1:l6" met="4" type="+-">Door 't gedonder van <rhyme label="c" type="m" corresp="#s1:l5">Kanon</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Zyn Hovaerdy moet zyn <rhyme label="d" type="m">geremd</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Daer hy uwe Koopvaerd <rhyme label="d" type="m" corresp="#s1:l7">stremd</rhyme>.</l>
        <l id="s1:l9" met="4" type="+-">En u rooft zo menig <rhyme label="e" type="m">Schip</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Of jaegt op zand of <rhyme label="e" type="m" corresp="#s1:l9">klip</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Onzen Prins en onze <rhyme label="a" type="f">Staaten</rhyme>;</l>
        <l id="s2:l2" met="4" type="+-">Nemen wapens in de <rhyme label="b" type="m">Hand</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Wyl zy zig niet binden <rhyme label="a" type="f" corresp="#s2:l1">laten</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Van den vuilen <rhyme label="b" type="m" corresp="#s2:l2">dwingeland</rhyme>?</l>
        <l id="s2:l5" met="4" type="+-">Men zal hem nu gaen te <rhyme label="c" type="m">keer</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Gelyk de Ruytrr deed wel <rhyme label="c" type="m" corresp="#s2:l5">eer</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">En hun Meesterschap op <rhyme label="d" type="m">Zee</rhyme>:</l>
        <l id="s2:l8" met="4" type="+-">Temmen tot op Chatams <rhyme label="d" type="m" corresp="#s2:l7">ree</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Leeve zal de <rhyme label="e" type="m">Batavier</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">In zyne <rhyme label="e" type="m" corresp="#s2:l9">Krygsbanier</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Hollandsch Leeuw laet zig niet <rhyme label="a" type="f">tergen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Van den Dog in schyn van <rhyme label="b" type="m">vriend</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Nooit zal hy zich laeten <rhyme label="a" type="f" corresp="#s3:l1">vergen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Iets dat tot ayn nadeel <rhyme label="b" type="m" corresp="#s3:l2">diend</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dien Hond heeft reeds lang <rhyme label="c" type="m">gesmagt</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Naer den breuk der Staeten <rhyme label="c" type="m" corresp="#s3:l5">magt</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Men zal den Brit zyn valsche <rhyme label="d" type="m">trouw</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Straffen met Bom en <rhyme label="d" type="m" corresp="#s3:l7">Kartouw</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">Smeden hun met Yz're <rhyme label="e" type="m">band</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Ter spyt van <rhyme label="e" type="m" corresp="#s3:l9">Engeland</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Loond men dus zyn braesste <rhyme label="a" type="f">vrinden</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">In het duivels <rhyme label="b" type="m">Engeland</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Kan men snoder voorbeeld <rhyme label="a" type="f" corresp="#s4:l1">vinden</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dan als wy zyn <rhyme label="b" type="m" corresp="#s4:l2">aengerand</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Onze Magten staeg <rhyme label="c" type="m">gehoond</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Zo als hun Memorie <rhyme label="c" type="m" corresp="#s4:l5">toont</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Gevuld met gal en bittere <rhyme label="d" type="m">haet</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Op de Amsterdamsche <rhyme label="d" type="m" corresp="#s4:l7">Magistraet</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Vaders van een goed <rhyme label="e" type="m">gedrag</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Waerd eerbied en <rhyme label="e" type="m" corresp="#s4:l9">ontzag</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maer Amsterdam zal hulp <rhyme label="a" type="f">geven</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Tot het heil van onzen <rhyme label="b" type="m">Staat</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Men zal wagen goed en <rhyme label="a" type="f" corresp="#s5:l1">leven</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Als het aen een vegten <rhyme label="b" type="m" corresp="#s5:l2">gaet</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Onzen Opper <rhyme label="c" type="m">Admirael</rhyme>;</l>
        <l id="s5:l6" met="4" type="+-">Die ontziet geen Vuur nog <rhyme label="c" type="m" corresp="#s5:l5">Staal</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Hartzing die is ook <rhyme label="d" type="m">gereed</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Om te wreeken al ons <rhyme label="d" type="m" corresp="#s5:l7">leed</rhyme>,</l>
        <l id="s5:l9" met="4" type="+-">Beiland en veel helden <rhyme label="e" type="m">meer</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Die stryden voor s'Lands <rhyme label="e" type="m" corresp="#s5:l9">Eer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Schoon 'er nu wat Scheepen <rhyme label="a" type="f">raeken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Onverhoeds in s'Vyands <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dit zal men zeer haestig <rhyme label="a" type="f" corresp="#s6:l1">staeken</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Door het onderling <rhyme label="b" type="m" corresp="#s6:l2">verband</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Met haer Russe <rhyme label="c" type="m">Majesteid</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Sweed en Deen die zyn <rhyme label="c" type="m" corresp="#s6:l5">bereid</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Alle om ons by te <rhyme label="d" type="m">staen</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">En de Brit te keer te <rhyme label="d" type="m" corresp="#s6:l7">gaen</rhyme>,</l>
        <l id="s6:l9" met="4" type="+-">Dan jaegt men uit het <rhyme label="e" type="m">pekelveld</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Den Dog en zyn <rhyme label="e" type="m" corresp="#s6:l9">geweld</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Hebt geduld <rhyme label="a" type="f">Negotianten</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Schoon uw Handel wort <rhyme label="b" type="m">gestaekt</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Men bereid zig aen alle <rhyme label="a" type="f" corresp="#s7:l1">kanten</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Haest ziet ge uw scha ten eind <rhyme label="b" type="m" corresp="#s7:l2">geraekt</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Den Motroos en den <rhyme label="c" type="m">Soldaet</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Wagten slegts als dat men <rhyme label="c" type="m" corresp="#s7:l5">slaet</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">En dan hun straft op Land en <rhyme label="d" type="m">Zee</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Tot dat men krygt gewenste <rhyme label="d" type="m" corresp="#s7:l7">Vree</rhyme>,</l>
        <l id="s7:l9" met="4" type="+-">Men zingt dan weer verheugt en <rhyme label="e" type="m">trouw</rhyme>,</l>
        <l id="s7:l10" met="3" type="-+">Wilbelmus van <rhyme label="e" type="m" corresp="#s7:l9">Nassouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
