  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MYn Vyzel is <rhyme label="a" type="f">Gestolen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Daer ik by Nagt in <rhyme label="b" type="m">stamp</rhyme>;</l>
        <l id="s1:l3" met="3" type="-+">Mijn Vyzel is <rhyme label="a" type="f" corresp="#s1:l1">Gestolen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De Dief blijft nooit <rhyme label="a" type="f" corresp="#s1:l1">verholen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ã´ Noodlot wat een <rhyme label="b" type="m" corresp="#s1:l2">ramp</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Den Dief dat is een <rhyme label="a" type="f">Snyer</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik kan hem wonder <rhyme label="b" type="m">wel</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Den Dief dat is een <rhyme label="a" type="f" corresp="#s2:l1">Snyer</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maekt hy mijn Vyzel <rhyme label="a" type="f" corresp="#s2:l1">wer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Haelt hem de Schout ligt <rhyme label="b" type="m" corresp="#s2:l2">snel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wat mag die Vent dog <rhyme label="a" type="f">schele</rhyme>?</l>
        <l id="s3:l2" met="3" type="-+">Hoe durft hy dat <rhyme label="b" type="m">bestaen</rhyme>?</l>
        <l id="s3:l3" met="3" type="-+">Wat mag die Vent dog <rhyme label="a" type="f" corresp="#s3:l1">schele</rhyme>?</l>
        <l id="s3:l4" met="3" type="-+">Noemt hy 't Vyzel <rhyme label="a" type="f" corresp="#s3:l1">stele</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zy is met hem <rhyme label="b" type="m" corresp="#s3:l2">gegaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dien Vyzel die had <rhyme label="a" type="f">Beenen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Noemt hy dat na de <rhyme label="b" type="m">kunst</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">Dien Vyzel die had <rhyme label="a" type="f" corresp="#s4:l1">Beenen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En scheen my wel te <rhyme label="a" type="f" corresp="#s4:l1">meenen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Betoonde my haer <rhyme label="b" type="m" corresp="#s4:l2">gunst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wel Dief had gy door <rhyme label="a" type="f">listen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Het stuk niet <rhyme label="b" type="m">aengeleit</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Wel Dief had gy door <rhyme label="a" type="f" corresp="#s5:l1">listen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Eer ik mijn Vyzel <rhyme label="a" type="f" corresp="#s5:l1">miste</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ik was hem nu niet <rhyme label="b" type="m" corresp="#s5:l2">queyt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wel 'k heb hem niet <rhyme label="a" type="f">Gestole</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ik heb hem maer <rhyme label="b" type="m">geleent</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Ik zal daer wat in <rhyme label="a" type="f" corresp="#s6:l1">Stampe</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En lossen zoo mijn <rhyme label="a" type="f" corresp="#s6:l1">dampe</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Soo heb ik het <rhyme label="b" type="m" corresp="#s6:l2">gemeent</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wilt doch u toorn <rhyme label="a" type="f">stille</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En stellen u <rhyme label="b" type="m">gerust</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Gy zult hem weder <rhyme label="a" type="f" corresp="#s7:l1">krijgen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wilt soo lang stille <rhyme label="a" type="f" corresp="#s7:l1">zwijgen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Tot dat ik aen u <rhyme label="b" type="m" corresp="#s7:l2">hust</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Daerom zijt maer te <rhyme label="a" type="f">vreden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">En schikt u na dees <rhyme label="b" type="m">tijd</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">'t Stampen in eige <rhyme label="a" type="f" corresp="#s8:l1">Beker</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Is voor u wel soo <rhyme label="a" type="f" corresp="#s8:l1">seker</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Voor my een groot <rhyme label="b" type="m" corresp="#s8:l2">verwyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maer wilt ook dan op <rhyme label="a" type="f">passe</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Dat gy wat beter <rhyme label="b" type="m">Let</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">En laet u niet <rhyme label="a" type="f" corresp="#s9:l1">verrasse</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Want die op Vyzels <rhyme label="a" type="f" corresp="#s9:l1">vlasse</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Die brengen u in 't <rhyme label="b" type="m" corresp="#s9:l2">Net</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
