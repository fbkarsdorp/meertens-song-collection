    <melody>Wil o windje zachtjes blazen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wilt dit heelryk Paar <rhyme label="a" type="f">vermaaken</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Liefde, die 't heil al <rhyme label="b" type="m">bestierd</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Doet hun door uw invloed <rhyme label="a" type="f" corresp="#s1:l1">blaaken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Daar de deugd hun hoofd <rhyme label="b" type="m" corresp="#s1:l2">Lauriert</rhyme>!</l>
        <l id="s1:l5" met="4" type="+-">Laat hun onafscheidbaar <rhyme label="c" type="f">Leeven</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">En tot hooger Jaaren <rhyme label="c" type="f" corresp="#s1:l5">streeven</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ryk van aardsche <rhyme label="d" type="m">Zaligheen</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Streel met <rhyme label="e" type="f">Kusjes</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Minne <rhyme label="e" type="f" corresp="#s1:l8">Lusjes</rhyme>,</l>
        <l id="s1:l10" met="4" type="+-">Lyris en zyn <rhyme label="d" type="m" corresp="#s1:l7">Clorimeen</rhyme>.</l>
      </lg>
    </lg>
  </text>
