  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">HYmen kom dees Trouw <rhyme label="a" type="f">versieren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Van den hooge <rhyme label="b" type="m">Helikon</rhyme>:</l>
        <l id="s1:l3" met="4" type="+-">Laat uw blonden Lokken <rhyme label="a" type="f" corresp="#s1:l1">zwieren</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">En hen Spiegelen in de <rhyme label="b" type="m" corresp="#s1:l2">Zon</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Gy kunt Herten zamen <rhyme label="c" type="f">knopen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Met een wettelyke <rhyme label="d" type="m">knoop</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Laat de wuste Minnaars <rhyme label="c" type="f" corresp="#s1:l5">loopen</rhyme>:</l>
        <l id="s1:l8" met="4" type="+-">Die doch Minnen zonder <rhyme label="d" type="m" corresp="#s1:l6">hoop</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Wy omringen uwen <rhyme label="a" type="f">Tempel</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Met Festoenen <rhyme label="b" type="m">opgestrikt</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Wy bestrooijen zynen <rhyme label="a" type="f" corresp="#s2:l1">Drempel</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Met Loof van dit Saisoen <rhyme label="b" type="m" corresp="#s2:l2">beschikt</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Treed nu met uw Goude <rhyme label="c" type="f">broosen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Voor uit naar de <rhyme label="d" type="m">Bruilofs-Zaal</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Nu de Bruid begint te <rhyme label="c" type="f" corresp="#s2:l5">blozen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Voor het MinnendÃ« <rhyme label="d" type="m" corresp="#s2:l6">onthaal</rhyme>.</l>
      </lg>
    </lg>
