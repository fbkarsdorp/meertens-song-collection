  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Maria <rhyme label="a" type="f">coninginne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">mijn troest mijn to <rhyme label="b" type="m">verlaet</rhyme></l>
        <l id="s1:l3" met="3" type="-+">vercrigt u kindes <rhyme label="a" type="f" corresp="#s1:l1">mynne</rhyme></l>
        <l id="s1:l4" met="3" type="-+">berou van mijn <rhyme label="b" type="m" corresp="#s1:l2">misdaet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">des wordic wel <rhyme label="a" type="f">geware</rhyme></l>
        <l id="s2:l2" met="3" type="-+">dat u kint myn is <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">want si kan wel hart <rhyme label="a" type="f" corresp="#s2:l1">verclaren</rhyme></l>
        <l id="s2:l4" met="3" type="-+">ende gheven goeden <rhyme label="b" type="m" corresp="#s2:l2">raet</rhyme></l>
      </lg>
    </lg>
  </text>
