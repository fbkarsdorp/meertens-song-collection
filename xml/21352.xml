<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21352</id>
    <date>
      <year-start>1591</year-start>
      <year-end>1592</year-end>
    </date>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
      <tag standard-name="vermaanlied" translation="admonitory song">vermaanlied</tag>
    </tags>
    <melody>De vogeltjes in de muiten</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Soetken Gerijts</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GHenade ende <rhyme label="a" type="f">vrede</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wensch ick v also <rhyme label="b" type="m">seer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Veel cracht des gheestes <rhyme label="a" type="f" corresp="#s1:l1">mede</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn broeder inden <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Om v leuen vroom wt te <rhyme label="c" type="f">voeren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Al tot des Heeren <rhyme label="d" type="m">prijs</rhyme></l>
        <l id="s1:l7" met="3" type="-+">In proeuing twist <rhyme label="c" type="f" corresp="#s1:l5">oproeren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Tot in v outheyt <rhyme label="d" type="m" corresp="#s1:l6">grijs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dit moet ick v noch <rhyme label="a" type="f">schencken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Al is mijn wijsheyt <rhyme label="b" type="m">cleyn</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Hier by moecht ghy <rhyme label="a" type="f" corresp="#s2:l1">bedencken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">My die wt liefden <rhyme label="b" type="m" corresp="#s2:l2">reyn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dit doe nae v <rhyme label="c" type="f">begheren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Eertijts aen my <rhyme label="d" type="m">gheschiet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Tmoet zijn tot prijs des <rhyme label="c" type="f" corresp="#s2:l5">heeren</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Verstaet nu wel mijn <rhyme label="d" type="m" corresp="#s2:l6">liet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ten eersten wilt <rhyme label="a" type="f">aenmercken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dat Godt den vader <rhyme label="b" type="m">al</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Den hemel met zijn <rhyme label="a" type="f" corresp="#s3:l1">percken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer toe dat aertsche <rhyme label="b" type="m" corresp="#s3:l2">dal</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Jae den mensch nae zijn <rhyme label="c" type="f">behaghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Met alle datmen <rhyme label="d" type="m">siet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Ghemaeckt heeft in ses <rhyme label="c" type="f" corresp="#s3:l5">daghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Bouen ons verstant wt <rhyme label="d" type="m" corresp="#s3:l6">niet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Na tbeelde gods wilt <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Is den mensch gheschapen <rhyme label="b" type="m">pleyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Duer Christum <rhyme label="a" type="f" corresp="#s4:l1">wtuercoren</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Naet opset Gods <rhyme label="b" type="m" corresp="#s4:l2">alleyn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dats gherechticheyt so wy <rhyme label="c" type="f">lesen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ende heerlijcheyt dees reden <rhyme label="d" type="m">vaet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dit was Gods wil <rhyme label="c" type="f" corresp="#s4:l5">ghepresen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Als daer gheschreuen <rhyme label="d" type="m" corresp="#s4:l6">staet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dit beelt heeft Adam <rhyme label="a" type="f">verlooren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Door <rhyme label="b" type="m">onghehoorsaemheyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Godt die heeft hem te <rhyme label="a" type="f" corresp="#s5:l1">vooren</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ghewaerschout en <rhyme label="b" type="m" corresp="#s5:l2">gheseyt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dat hy niet soude <rhyme label="c" type="f">eeten</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Vanden boom vroech nochte <rhyme label="d" type="m">laet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Die also was <rhyme label="c" type="f" corresp="#s5:l5">gheheten</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Des wetenschaps goet en <rhyme label="d" type="m" corresp="#s5:l6">quaet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Al door het <rhyme label="a" type="f">ouertreden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van Gods woort en <rhyme label="b" type="m">ghebot</rhyme></l>
        <l id="s6:l3" met="3" type="-+">So heeft Adam hoort <rhyme label="a" type="f" corresp="#s6:l1">reden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Hem ghescheyden af van <rhyme label="b" type="m" corresp="#s6:l2">Godt</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Inden doot is hy <rhyme label="c" type="f">ghecomen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Met al die ghebooren <rhyme label="d" type="m">zijn</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Van hem geen <rhyme label="c" type="f" corresp="#s6:l5">wtghenomen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Die oyt leefden verstaet wel <rhyme label="d" type="m" corresp="#s6:l6">mijn</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daerom gaet Christus <rhyme label="a" type="f">leeren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">In Matheus <rhyme label="b" type="m">openbaer</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Ghy moet v omme <rhyme label="a" type="f" corresp="#s7:l1">keeren</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Als een kint so staet <rhyme label="b" type="m" corresp="#s7:l2">daer</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Noch spreckt hy sonder <rhyme label="c" type="f">verstoren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Johannes drie <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Ghy moet zijn van nieus <rhyme label="c" type="f" corresp="#s7:l5">gebooren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Wt den water en geest dits <rhyme label="d" type="m" corresp="#s7:l6">claer</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Dese nieu gheboorte <rhyme label="a" type="f">crachtich</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Sijn wy wilt wel <rhyme label="b" type="m">verstaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Hier gheworden <rhyme label="a" type="f" corresp="#s8:l1">deelachtich</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Doen wy Christum namen <rhyme label="b" type="m" corresp="#s8:l2">aen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">En ons hebben <rhyme label="c" type="f">begheuen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Onder zijnen heylighe <rhyme label="d" type="m">woort</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Om hier daer nae te <rhyme label="c" type="f" corresp="#s8:l5">leuen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">In als so dat <rhyme label="d" type="m" corresp="#s8:l6">behoort</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Daerom moeten wy <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Godt ende maecken <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Hem altijt eer <rhyme label="a" type="f" corresp="#s9:l1">bewijsen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Want hy ons vanden <rhyme label="b" type="m" corresp="#s9:l2">doot</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Beurijt heeft door zijn <rhyme label="c" type="f">ghenaden</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dies moghen wy zijn <rhyme label="d" type="m">verblijt</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Hem dancken vroech en <rhyme label="c" type="f" corresp="#s9:l5">spaden</rhyme></l>
        <l id="s9:l8" met="3" type="-+">In desen quaden <rhyme label="d" type="m" corresp="#s9:l6">tijt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Veel drucx ende <rhyme label="a" type="f">elende</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Isser hier wel op <rhyme label="b" type="m">let</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Verdriet sal tot den <rhyme label="a" type="f" corresp="#s10:l1">ende</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Wt voeren nae Gods <rhyme label="b" type="m" corresp="#s10:l2">wet</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Daer wy toe zijn <rhyme label="c" type="f">gheroepen</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Van godt door zijn <rhyme label="d" type="m">goetheyt</rhyme></l>
        <l id="s10:l7" met="3" type="-+">Laet ons vast in hem <rhyme label="c" type="f" corresp="#s10:l5">hoopen</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Blijuen in <rhyme label="d" type="m" corresp="#s10:l6">ootmoedicheyt</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Op dat wijt niet <rhyme label="a" type="f">verliesen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Als Adam int <rhyme label="b" type="m">begin</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Door swerelts vrientschap <rhyme label="a" type="f" corresp="#s11:l1">kiesen</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Of comenschap handel <rhyme label="b" type="m" corresp="#s11:l2">ghewin</rhyme></l>
        <l id="s11:l5" met="3" type="-+">Want daer in is veel <rhyme label="c" type="f">gheleghen</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Veel perijckels dat weet ghy <rhyme label="d" type="m">wel</rhyme></l>
        <l id="s11:l7" met="3" type="-+">Dus wilt v toch te <rhyme label="c" type="f" corresp="#s11:l5">deghen</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Waernemen nae Gods <rhyme label="d" type="m" corresp="#s11:l6">beuel</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Wilt nemen oock voor <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Hoe Christus leeret <rhyme label="b" type="m">ient</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Dat een rijckman zijn <rhyme label="a" type="f" corresp="#s12:l1">pooghen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Niet baten mocht int <rhyme label="b" type="m" corresp="#s12:l2">ent</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Hoe veel hy heeft <rhyme label="c" type="f">vercoren</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Veel goets tot zijnen <rhyme label="d" type="m">lot</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Sijn doen was al <rhyme label="c" type="f" corresp="#s12:l5">verlooren</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Hy was niet rijck in <rhyme label="d" type="m" corresp="#s12:l6">Godt</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Hier mede wil ick v <rhyme label="a" type="f">beuelen</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Den Heer <rhyme label="b" type="m">ghebenedijt</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Die wil v mede <rhyme label="a" type="f" corresp="#s13:l1">deelen</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Sijn gheest in desen <rhyme label="b" type="m" corresp="#s13:l2">tijt</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Op dat ghy v moecht <rhyme label="c" type="f">wachten</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Voor dat v siele mach <rhyme label="d" type="m">schaen</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Neemt dit in v <rhyme label="c" type="f" corresp="#s13:l5">ghedachten</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Van mijn ten besten <rhyme label="d" type="m" corresp="#s13:l6">aen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
