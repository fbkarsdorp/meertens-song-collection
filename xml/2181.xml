<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>2181</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Gavotte d'Anjou</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MAg ik u dan niet <rhyme label="a" type="f">genieten</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Daar mijn Liefd' om heeft <rhyme label="b" type="m">geleen</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Soo veel angst en <rhyme label="b" type="m" corresp="#s1:l2">wrevelheen</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">MAg ik u dan niet <rhyme label="a" type="f" corresp="#s1:l1">genieten</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Daar mijn Liefd' om heeft <rhyme label="b" type="m" corresp="#s1:l2">geleen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Soo veel angst en <rhyme label="b" type="m" corresp="#s1:l2">wrevelheen</rhyme>?</l>
        <l id="s1:l7" met="3" type="+-">Quaamt gy dan van <rhyme label="c" type="f">Leewen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">In Saturnus <rhyme label="c" type="f" corresp="#s1:l7">eewen</rhyme>,</l>
        <l id="s1:l9" met="2" type="+-">Sonder <rhyme label="d" type="m">min</rhyme></l>
        <l id="s1:l10" met="2" type="+-">
          <rhyme label="d" type="m" corresp="#s1:l9">Engelin</rhyme>
        </l>
        <l id="s1:l11" met="2" type="+-">Dat gy <rhyme label="e" type="m">haat</rhyme>,</l>
        <l id="s1:l12" met="4" type="+-">Daar elk schepsel so na <rhyme label="e" type="m" corresp="#s1:l11">staat</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ik versoek maar om te <rhyme label="a" type="f">kussen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Uw twee lichten, daar het <rhyme label="b" type="m">licht</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Van het Sonne-licht voor <rhyme label="b" type="m" corresp="#s2:l2">swicht</rhyme>.</l>
        <l id="s2:l4" met="4" type="+-">Ik versoek maar om te <rhyme label="a" type="f" corresp="#s2:l1">kussen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Uw twee lichten, daar het <rhyme label="b" type="m" corresp="#s2:l2">licht</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van het Sonne-licht voor <rhyme label="b" type="m" corresp="#s2:l2">swicht</rhyme>.</l>
        <l id="s2:l7" met="3" type="+-">Na uw gulde <rhyme label="c" type="f">kronkklen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">By Sapphiers <rhyme label="c" type="f" corresp="#s2:l7">karbonkklen</rhyme></l>
        <l id="s2:l9" met="2" type="+-">Stuurt de <rhyme label="d" type="m">min</rhyme></l>
        <l id="s2:l10" met="2" type="+-">Mijne <rhyme label="d" type="m" corresp="#s2:l9">sin</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Om <rhyme label="e" type="m">gedood</rhyme></l>
        <l id="s2:l12" met="4" type="+-">Weg te treen in Charons <rhyme label="e" type="m" corresp="#s2:l11">boot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Soud gy my dat weygren <rhyme label="a" type="f">kunnen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Die uw Slaaf en Minnaar <rhyme label="b" type="m">heet</rhyme></l>
        <l id="s3:l3" met="4" type="+-">En sijn dienst aan u <rhyme label="b" type="m" corresp="#s3:l2">besteed</rhyme>?</l>
        <l id="s3:l4" met="4" type="+-">Soud gy my dat weygren <rhyme label="a" type="f" corresp="#s3:l1">kunnen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Die uw Slaaf en Minnaar <rhyme label="b" type="m" corresp="#s3:l2">heet</rhyme></l>
        <l id="s3:l6" met="4" type="+-">En sijn dienst aan u <rhyme label="b" type="m" corresp="#s3:l2">besteed</rhyme>?</l>
        <l id="s3:l7" met="3" type="+-">Ey so sal ik <rhyme label="c" type="f">waagen</rhyme></l>
        <l id="s3:l8" met="3" type="+-">'t Kussen sonder <rhyme label="c" type="f" corresp="#s3:l7">vraagen</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Of je <rhyme label="d" type="m">my</rhyme></l>
        <l id="s3:l10" met="2" type="+-">Stoot ter <rhyme label="d" type="m" corresp="#s3:l9">zy</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Dat 's maar <rhyme label="e" type="m">spel</rhyme>:</l>
        <l id="s3:l12" met="4" type="+-">Soo'n gestoole beet smaakt <rhyme label="e" type="m" corresp="#s3:l11">wel</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
