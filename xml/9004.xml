<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9004</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Aus tiefer Not schrei ich zu dir</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Goedig' Godt in <rhyme label="a" type="m">Eewicheyt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Onse Vader end <rhyme label="b" type="f">HEEre</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Tot dy schreyt de heel <rhyme label="a" type="m" corresp="#s1:l1">Christenheyt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dijn G'naed' niet van ons <rhyme label="b" type="f" corresp="#s1:l2">keere</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Laetse haest tot ons vlieten <rhyme label="c" type="m">daer</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Dat wy de Sonde nemen <rhyme label="c" type="m" corresp="#s1:l5">waer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">End recht voor dy <rhyme label="d" type="f">bekennen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wy hebben ons versondicht <rhyme label="a" type="m">veel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Met Misdaet <rhyme label="b" type="f">men'gerleye</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Met Hovaert, Wraeck end <rhyme label="a" type="m" corresp="#s2:l1">Overspeel</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Met Vloeck, Haet, <rhyme label="b" type="f" corresp="#s2:l2">Hoererije</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Met Achterclap end Ontrouw' <rhyme label="c" type="m">groot</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Met Giericheyt al boven <rhyme label="c" type="m" corresp="#s2:l5">Maet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met Onrecht aen die <rhyme label="d" type="f">Armen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dijn heylich Woort is niet <rhyme label="a" type="m">gheacht</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Noch bruycket nae <rhyme label="b" type="f">Betame</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Weynich is dijn Weldaet <rhyme label="a" type="m" corresp="#s3:l1">betracht</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dies over ons is <rhyme label="b" type="f" corresp="#s3:l2">comen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Die scherpe Straff der Roeden <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Daer met ghy HEEr Castijend' <rhyme label="c" type="m" corresp="#s3:l5">zijn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Die Schapen dijner <rhyme label="d" type="f">Weyde</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Met Cranckheyt veel aen men'gen <rhyme label="a" type="m">Oort</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daer door dat Volck moet <rhyme label="b" type="f">sterven</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Met duyre Tijden noyt <rhyme label="a" type="m" corresp="#s4:l1">ghehoort</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Daer door de Luyd' <rhyme label="b" type="f" corresp="#s4:l2">verderven</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">De Vyant Ancxtet ons so <rhyme label="c" type="m">seer</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ons overvalt syn bloedich <rhyme label="c" type="m" corresp="#s4:l5">Heyr</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dijn Christen te <rhyme label="d" type="f">vermoorden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Nochtans HEEr niemant seggen <rhyme label="a" type="m">can</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat ghy straffet t' <rhyme label="b" type="f">Onrechte</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Want wy die Straff verdienet <rhyme label="a" type="m" corresp="#s5:l1">haen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wy Arm sondich <rhyme label="b" type="f" corresp="#s5:l2">Gheslechte</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Dijn Ordeel al Rechtveerdich <rhyme label="c" type="m">gaet</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Want wy so lang' hebben <rhyme label="c" type="m" corresp="#s5:l5">versmaet</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">All' dijn ghetrouw' <rhyme label="d" type="f">Vermanen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Doch want ghy onse Vader <rhyme label="a" type="m">bist</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Sullen wy niet <rhyme label="b" type="f">vertzaghen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Maer onse schult end s'Vyants <rhyme label="a" type="m" corresp="#s6:l1">List</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">End alle Noot dy <rhyme label="b" type="f" corresp="#s6:l2">Claghen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">End dy gheloovich Roepen <rhyme label="c" type="m">aen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Ghy sult ons laten niet <rhyme label="c" type="m" corresp="#s6:l5">vergaen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Maer ons Ghebedt <rhyme label="d" type="f">verhooren</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Daerom Heer Godt der <rhyme label="a" type="m">goedicheyt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Wilt onser dy <rhyme label="b" type="f">Ontfarmen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Help dijn benaude <rhyme label="a" type="m" corresp="#s7:l1">Christenheyt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Ghedenck hoe wy syn <rhyme label="b" type="f" corresp="#s7:l2">Armen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Ons Wapen end Gheweer niet <rhyme label="c" type="m">gelt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">So ghy HEEr selffs niet helpen <rhyme label="c" type="m" corresp="#s7:l5">wilt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Die Vyand' te <rhyme label="d" type="f">Verjaghen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Ghy holpet HEEr so dickwils <rhyme label="a" type="m">vry</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Den Volck van <rhyme label="b" type="f">Israele</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Want sy Verthoornet hadden <rhyme label="a" type="m" corresp="#s8:l1">dy</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Met Sonden al te <rhyme label="b" type="f" corresp="#s8:l2">veele</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">So quam haer de Vyandt int <rhyme label="c" type="m">Landt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Verderffse seer met Rooff en <rhyme label="c" type="m" corresp="#s8:l5">Brandt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">End Worghen deerlijck <rhyme label="d" type="f">veele</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Dan keerdense weder tot <rhyme label="a" type="m">dy</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Als sy gheplaget <rhyme label="b" type="f">waren</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Aenriepen met Claecklijck <rhyme label="a" type="m" corresp="#s9:l1">gheschrey</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Dijn Hulp deedt ghy met <rhyme label="b" type="f" corresp="#s9:l2">sparen</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Quaemt haer te baet in sulcke <rhyme label="c" type="m">Noot</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">Die Vyanden sloecht ghy ter <rhyme label="c" type="m" corresp="#s9:l5">Doot</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Dijn Volck ginckt ghy <rhyme label="d" type="f">bevrijden</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Daerom wy dy oock Roepen <rhyme label="a" type="m">aen</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">In onsen grooten <rhyme label="b" type="f">Noode</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Want wy gheen ander hulpe <rhyme label="a" type="m" corresp="#s10:l1">haen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dan dy HEER Godt soo <rhyme label="b" type="f" corresp="#s10:l2">gaede</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Vergheeft ons onse Sonden <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">Verlost ons wt des Doodes <rhyme label="c" type="m" corresp="#s10:l5">Val</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Die Vyandt van ons <rhyme label="d" type="f">drijve</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Ach Jesu Christ Godt onse <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Wilt ons doch niet <rhyme label="b" type="f">vermijden</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Door dijn selffs goedicheyt end <rhyme label="a" type="m" corresp="#s11:l1">Eer</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Door dijn Menscheyt end <rhyme label="b" type="f" corresp="#s11:l2">Lijden</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Door dijnen swaren bitt'ren <rhyme label="c" type="m">Doot</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">End door dijn Opstandinghe <rhyme label="c" type="m" corresp="#s11:l5">vroet</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Help ons wt s'Vyants <rhyme label="d" type="f">handen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ach Heere Godt heylighe <rhyme label="a" type="m">Gheest</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Wy bidden dy al <rhyme label="b" type="f">tsame</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Sterckt all' die ghenen <rhyme label="a" type="m" corresp="#s12:l1">aldermeest</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Die gaen in dijnen <rhyme label="b" type="f" corresp="#s12:l2">Name</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Strijden teghen die Vyand' <rhyme label="c" type="m">dijn</rhyme>,</l>
        <l id="s12:l6" met="4" type="-+">Wilt ons ghetrouwe Veltheer <rhyme label="c" type="m" corresp="#s12:l5">zijn</rhyme>,</l>
        <l id="s12:l7" met="3" type="-+">Dat wy Victory <rhyme label="d" type="f">vechten</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">HEER onse Godt in hoochsten <rhyme label="a" type="m">Throon</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">Heylig' <rhyme label="b" type="f">Dryvuldicheyde</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Een Eewich Godt Heerlijck end <rhyme label="a" type="m" corresp="#s13:l1">schoon</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Houd' ons in dijn <rhyme label="b" type="f" corresp="#s13:l2">Gheleyde</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Behoed' ons aen ons laetsten <rhyme label="c" type="m">Eynd'</rhyme>,</l>
        <l id="s13:l6" met="4" type="-+">Als wy varen wt dees <rhyme label="c" type="m" corresp="#s13:l5">Elend'</rhyme>,</l>
        <l id="s13:l7" met="3" type="-+">Voor allen quaden, <rhyme label="d" type="f">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
