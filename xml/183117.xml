  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Lang leeft de Oranje <rhyme label="a" type="m">Vorst</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Lang mag Prins Willem <rhyme label="b" type="f">leeven</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Op dat zyn kloek <rhyme label="c" type="m">verstand</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Strekt tot voordeel van ons <rhyme label="c" type="m" corresp="#s1:l3">Land</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Met zeven pylen <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Kwamen zy hem <rhyme label="b" type="f">aanranden</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ja zelfs tot in der <rhyme label="c" type="m">dood</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy vreest geen klein of <rhyme label="c" type="m" corresp="#s2:l3">groot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">De Spruiten, jong en <rhyme label="a" type="m">teÃªr</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Gezegend van den <rhyme label="b" type="f">Heer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Die kwamen zy <rhyme label="c" type="m">aanranden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met haar bebloede <rhyme label="c" type="m" corresp="#s3:l3">handen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Onze oude Prinses <rhyme label="a" type="m">verheven</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wat rykdom zal men u <rhyme label="b" type="f">geeven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al voor uw Manlyke <rhyme label="c" type="m">Stam</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Toen gy Prins Willem <rhyme label="c" type="m" corresp="#s4:l3">vernam</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Geen zon zal hier meer <rhyme label="a" type="m">schynen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">De Maan zal hier <rhyme label="b" type="f">verdwynen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">De Sterren zullen stille <rhyme label="c" type="m">staan</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Eer Oranje zal <rhyme label="c" type="m" corresp="#s5:l3">vergaan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik wensch de Prins, ter hulp en <rhyme label="a" type="m">trouw</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van het vervallen <rhyme label="b" type="f">Staatsgebouw</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Nog lange leefd, bevryd van <rhyme label="c" type="m">kruis</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met zyn braaf en Vorstelyk <rhyme label="c" type="m" corresp="#s6:l3">Huis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dit Vers zal ik u <rhyme label="a" type="m">schenken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Laaten wy Prins Willem <rhyme label="b" type="f">gedenken</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Het is Willem van <rhyme label="c" type="m">Nassau</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Wy blyven hem steeds <rhyme label="c" type="m" corresp="#s7:l3">getrouw</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
