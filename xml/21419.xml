    <melody>Ontwaak van slaap wie dat gij zijt</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GHy die niet meer in tvleesch en <rhyme label="a" type="m">zijt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Maer soect des gheests <rhyme label="a" type="m" corresp="#s1:l1">profijt</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Inden bequamen <rhyme label="a" type="m" corresp="#s1:l1">tijt</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Seer hongherich zijt <rhyme label="b" type="f">beseuen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Werct spijse die nemmermeer en <rhyme label="a" type="m" corresp="#s1:l1">verslijt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Maer blijft <rhyme label="a" type="m" corresp="#s1:l1">ghebenedijt</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Int eewich leuen <rhyme label="a" type="m" corresp="#s1:l1">beurijt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Twelck smenschen sone can <rhyme label="b" type="f" corresp="#s1:l4">gheuen</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Dat is zijn edel <rhyme label="c" type="m">woort</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Uoor diet ghelooft en <rhyme label="c" type="m" corresp="#s1:l9">hoort</rhyme></l>
        <l id="s1:l11" met="2" type="+-">En daer in <rhyme label="d" type="f">blijuen</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Sullen by God <rhyme label="d" type="f" corresp="#s1:l11">beclijuen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Inden beghinne was twoort <rhyme label="a" type="m">eerbaer</rhyme></l>
        <l id="s2:l2" met="3" type="-+">En twoort was by God <rhyme label="a" type="m" corresp="#s2:l1">eenpaer</rhyme></l>
        <l id="s2:l3" met="3" type="-+">God was dat woort <rhyme label="a" type="m" corresp="#s2:l1">voorwaer</rhyme></l>
        <l id="s2:l4" met="3" type="-+">In hem was tleuen <rhyme label="b" type="f">wtuercoren</rhyme></l>
        <l id="s2:l5" met="4" type="-+">En tleuen was dlicht vanden menschen <rhyme label="a" type="m" corresp="#s2:l1">claer</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Maer die duysternisse <rhyme label="a" type="m" corresp="#s2:l1">swaer</rhyme></l>
        <l id="s2:l7" met="3" type="-+">En begrijpent hier noch <rhyme label="a" type="m" corresp="#s2:l1">daer</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Dan die wt God zijn <rhyme label="b" type="f" corresp="#s2:l4">gheboren</rhyme></l>
        <l id="s2:l9" met="3" type="-+">En dit woort in corter <rhyme label="c" type="m">stont</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Wort vleesch, dwelck by ons <rhyme label="c" type="m" corresp="#s2:l9">woont</rhyme></l>
        <l id="s2:l11" met="2" type="+-">Seer groot van <rhyme label="d" type="f">prijse</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Den gheloouighen tot eender <rhyme label="d" type="f" corresp="#s2:l11">spijse</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Ick ben (seyt Christus) dat hemels <rhyme label="a" type="m">broot</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghedaelt wt svaders <rhyme label="a" type="m" corresp="#s3:l1">schoot</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Uoor den hongherighen <rhyme label="a" type="m" corresp="#s3:l1">mijnioot</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Die tot my comen met <rhyme label="b" type="f">lusten</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Gheloouende mijn woort wt liefden <rhyme label="a" type="m" corresp="#s3:l1">groot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">En om hongher sweert, noch <rhyme label="a" type="m" corresp="#s3:l1">doot</rhyme></l>
        <l id="s3:l7" met="3" type="-+">En buyghen in gheender <rhyme label="a" type="m" corresp="#s3:l1">noot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Die en sullen hongheren noch <rhyme label="b" type="f" corresp="#s3:l4">dorsten</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Maer leuen met <rhyme label="c" type="m">voorspoet</rhyme></l>
        <l id="s3:l10" met="3" type="-+">In mijns vaders rijcke <rhyme label="c" type="m" corresp="#s3:l9">soet</rhyme></l>
        <l id="s3:l11" met="2" type="+-">Uol van <rhyme label="d" type="f">ghenaden</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Mijn woort can elcken <rhyme label="d" type="f" corresp="#s3:l11">versaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Om mijns woorts wille groot ende <rhyme label="a" type="m">cleyn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat ick v sprack <rhyme label="a" type="m" corresp="#s4:l1">ghemeyn</rhyme></l>
        <l id="s4:l3" met="3" type="-+">So zijdy alle <rhyme label="a" type="m" corresp="#s4:l1">reyn</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dus blijft daer in al te <rhyme label="b" type="f">samen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Want blijft ghy in my ick blijue in v <rhyme label="a" type="m" corresp="#s4:l1">certeyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Fondeert v op desen <rhyme label="a" type="m" corresp="#s4:l1">steyn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Om mijns naems wille <rhyme label="a" type="m" corresp="#s4:l1">alleyn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Uan mijn woort en wilt v niet <rhyme label="b" type="f" corresp="#s4:l4">schamen</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Maer dat seer stout <rhyme label="c" type="m">belijen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Het sal uwen gheest <rhyme label="c" type="m" corresp="#s4:l9">verblijen</rhyme></l>
        <l id="s4:l11" met="2" type="+-">En spijse <rhyme label="d" type="f">mede</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Al in der <rhyme label="d" type="f" corresp="#s4:l11">eewichede</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Cruyt noch plaestere aen gheenen <rhyme label="a" type="m">cant</rhyme></l>
        <l id="s5:l2" met="3" type="-+">En ghenas o Heere <rhyme label="a" type="m" corresp="#s5:l1">want</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Dijn soete woort <rhyme label="a" type="m" corresp="#s5:l1">playsant</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Heeft dat ghedaen <rhyme label="b" type="f">alleene</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Want het is die fonteyne vol wijsheyt <rhyme label="a" type="m" corresp="#s5:l1">geplant</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Een schilt een vierich <rhyme label="a" type="m" corresp="#s5:l1">brant</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Den gheloouighen een <rhyme label="a" type="m" corresp="#s5:l1">onderstant</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Om nemmermeer van hem te <rhyme label="b" type="f" corresp="#s5:l4">scheene</rhyme></l>
        <l id="s5:l9" met="3" type="-+">wt liefden hier <rhyme label="c" type="m">ghedaelt</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Die ons schult heeft <rhyme label="c" type="m" corresp="#s5:l9">betaelt</rhyme></l>
        <l id="s5:l11" met="2" type="+-">En de doot <rhyme label="d" type="f">versleghen</rhyme></l>
        <l id="s5:l12" met="3" type="-+">Die wy door Adam al <rhyme label="d" type="f" corresp="#s5:l11">vercreghen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Prince die Gods edel woort <rhyme label="a" type="m">ontfaen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En daer door vroom <rhyme label="a" type="m" corresp="#s6:l1">wederstaen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Al sweerelts valsch <rhyme label="a" type="m" corresp="#s6:l1">beraen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En haer cleederkens wasschen <rhyme label="b" type="f">blijde</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Int bloet des Lammekens dat edel <rhyme label="a" type="m" corresp="#s6:l1">graen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Het welcke sal af <rhyme label="a" type="m" corresp="#s6:l1">dwaen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Haer traenkens na <rhyme label="a" type="m" corresp="#s6:l1">tvermaen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Als zijn <rhyme label="b" type="f" corresp="#s6:l4">ghebenedijde</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Hongher ende dorst <rhyme label="c" type="m">gheschal</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Sal hy haer weeren <rhyme label="c" type="m" corresp="#s6:l9">al</rhyme></l>
        <l id="s6:l11" met="2" type="+-">Sonder <rhyme label="d" type="f">cesseren</rhyme></l>
        <l id="s6:l12" met="3" type="-+">Dat Lammeken sal <rhyme label="d" type="f" corresp="#s6:l11">domineren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
