  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Wer sin herze wil <rhyme label="a" type="f">instrickin</rhyme>.</l>
        <l id="s1:l2" met="4" type="+-">uz den sorgen de sal <rhyme label="a" type="f" corresp="#s1:l1">blicken</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">an die reyne guode <rhyme label="b" type="m">wip</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">vint het niet da siel und <rhyme label="c" type="f">ere</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">so gesucke <rhyme label="c" type="f" corresp="#s1:l4">nummermere</rhyme></l>
        <l id="s1:l6" met="4" type="+-">vreude an keyner stat sin <rhyme label="b" type="m" corresp="#s1:l3">lip</rhyme>.</l>
        <l id="s1:l7" met="3" type="+-">wip sint sunder <rhyme label="d" type="f">lougin</rhyme></l>
        <l id="s1:l8" met="4" type="-+">berinder winnin meyin <rhyme label="e" type="m">rys</rhyme>.</l>
        <l id="s1:l9" met="3" type="+-">is lijt under wibis <rhyme label="d" type="f" corresp="#s1:l7">ougin</rhyme></l>
        <l id="s1:l10" met="4" type="+-">aller vreudin <rhyme label="e" type="m" corresp="#s1:l8">paradys</rhyme>.</l>
      </lg>
    </lg>
