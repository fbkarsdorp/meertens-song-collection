        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="psalm" translation="psalm">psalm</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Mijn leefdaech noyt een <rhyme label="a" type="m">oochopslach</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Ic gaf dat u ooge niet en <rhyme label="a" type="m" corresp="#s1:l1">zach</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">O stercke God der <rhyme label="b" type="f">stercken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Zelfs nymer (?) rechte ic mijn <rhyme label="c" type="m">op</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Brengt mij t gedacht yet in den <rhyme label="c" type="m" corresp="#s1:l4">cop</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gij cont, al ben ic ver, <rhyme label="b" type="f" corresp="#s1:l3">bemercken</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Al mijne <rhyme label="b" type="f" corresp="#s1:l3">wercken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Al brengt mij d'onbevensde <rhyme label="a" type="m">nacht</rhyme></l>
        <l id="s2:l2" met="4" type="-+">De rust, al heeft mij t licht <rhyme label="a" type="m" corresp="#s2:l1">gebracht</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Verdwijnzorch mit <rhyme label="b" type="f">tontwaecken</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">U voet in trusten bij mij <rhyme label="c" type="m">staet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">U ooge in twaecken bij mij <rhyme label="c" type="m" corresp="#s2:l4">gaet</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Wat onbedachte <rhyme label="b" type="f" corresp="#s2:l3">spraecken</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Oyt uyt de <rhyme label="b" type="f" corresp="#s2:l3">kaecken</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Geschoten zijn, die gij, Heer, <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Lang hebt gehoort, noch <rhyme label="a" type="m" corresp="#s3:l1">ongeschiet</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Gij hout te saem <rhyme label="b" type="f">gebonden</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Met zulcke const het vleesch en <rhyme label="c" type="m">been</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat onder taertsch vernuft men <rhyme label="c" type="m" corresp="#s3:l4">geen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die zulc gebou recht const <rhyme label="b" type="f" corresp="#s3:l3">deurgronden</rhyme></l>
        <l id="s3:l7" met="2" type="-+">En heeft <rhyme label="b" type="f" corresp="#s3:l3">gevonden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hoe zal ic dan, ellendig <rhyme label="a" type="m">wicht</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Mij bergen, Heer, voor u <rhyme label="a" type="m" corresp="#s4:l1">gezicht</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nae wat verholen <rhyme label="b" type="f">holen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Hebbe ic om mij te bergen <rhyme label="c" type="m">hoop</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Zal naer den hemel zijn mijn <rhyme label="c" type="m" corresp="#s4:l4">loop</rhyme>,:</l>
        <l id="s4:l6" met="3" type="-+">Zal ic zijn opten <rhyme label="b" type="f" corresp="#s4:l3">polen</rhyme></l>
        <l id="s4:l7" met="2" type="-+">Voor u <rhyme label="b" type="f" corresp="#s4:l3">verscholen</rhyme>?</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Neen, kvinde' u daer, of ic dan <rhyme label="a" type="m">snel</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Mijn vlucht nam in de diepe <rhyme label="a" type="m" corresp="#s5:l1">hel</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Gij zijt daer ooc <rhyme label="b" type="f">voorhanden</rhyme>.</l>
        <l id="s5:l4" met="4" type="-+">Of zal ic opte vlercken <rhyme label="c" type="m">licht</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Des morgensters, die thooft eerst <rhyme label="c" type="m" corresp="#s5:l4">licht</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Aen dalderwijtste <rhyme label="b" type="f" corresp="#s5:l3">randen</rhyme></l>
        <l id="s5:l7" met="2" type="-+">Der aertsche <rhyme label="b" type="f" corresp="#s5:l3">landen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Mij voeren doen <rhyme label="a" type="m">gezwint</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">U snel gezicht mi; daer ooc <rhyme label="a" type="m" corresp="#s6:l1">vint</rhyme></l>
        <l id="s6:l3" met="3" type="-+">En zoude mij <rhyme label="b" type="f">achterhalen</rhyme>.</l>
        <l id="s6:l4" met="4" type="-+">Geen plaets, hoe diep, hooch, verde of <rhyme label="c" type="m">wijt</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Mij voor u armcracht maect <rhyme label="c" type="m" corresp="#s6:l4">bevri;t</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Gij en cont niet op mij u <rhyme label="b" type="f" corresp="#s6:l3">stralen</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Strac doen <rhyme label="b" type="f" corresp="#s6:l3">dalen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Indien ooc mijn gedacht <rhyme label="a" type="m">verwaent</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Duyct (?), als de zon ujn viers <rhyme label="a" type="m" corresp="#s7:l1">gedaent</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In twesten heeft <rhyme label="b" type="f">gedoocken</rhyme></l>
        <l id="s7:l4" met="4" type="-+">En dat den doncker <rhyme label="c" type="m">uytgestrect</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Zoo zal ic oec zijn <rhyme label="c" type="m" corresp="#s7:l4">bedect</rhyme>;</l>
        <l id="s7:l6" met="3" type="-+">Mer ben zulx niet <rhyme label="b" type="f" corresp="#s7:l3">ontogen</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Van uwe <rhyme label="b" type="f" corresp="#s7:l3">ogen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">De nacht, die ons tgesicht <rhyme label="a" type="m">verdwijnt</rhyme></l>
        <l id="s8:l2" met="4" type="-+">En in ons oogen duyster <rhyme label="a" type="m" corresp="#s8:l1">schijnt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Es u een licht der <rhyme label="b" type="f">lichten</rhyme>.</l>
        <l id="s8:l4" met="4" type="-+">Geen duysternisse heeft bij u <rhyme label="c" type="m">cracht</rhyme></l>
        <l id="s8:l5" met="4" type="-+">En u voerzien en heeft geen <rhyme label="c" type="m" corresp="#s8:l4">nacht</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gij cont tgeheym <rhyme label="b" type="f" corresp="#s8:l3">versieren</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Doorzien de <rhyme label="b" type="f" corresp="#s8:l3">nieren</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Naerdien, O Heer, u rechte <rhyme label="a" type="m">hand</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Oec, ja (?), de heymlicste <rhyme label="a" type="m" corresp="#s9:l1">ingewant</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Schiep, u danc ie in den <rhyme label="b" type="f">hogen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Dat mit een velleken hiel (?) <rhyme label="c" type="m">teer</rhyme></l>
        <l id="s9:l5" met="4" type="-+">Gij t vleysken en mijn nierkens, <rhyme label="c" type="m" corresp="#s9:l4">Heer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Smoers buyc om mij <rhyme label="b" type="f" corresp="#s9:l3">gebogen</rhyme></l>
        <l id="s9:l7" met="2" type="-+">Hebt <rhyme label="b" type="f" corresp="#s9:l3">overtogen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">De wonderlijcke en starcke <rhyme label="a" type="m">bant</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Daer mede ons lijve van de <rhyme label="a" type="m" corresp="#s10:l1">plant</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Der voeten totten <rhyme label="b" type="f">haren</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Vast inden nade zij (?) <rhyme label="c" type="m">vercnocht</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Hebt gij zeer wijsselic <rhyme label="c" type="m" corresp="#s10:l4">bedocht</rhyme></l>
        <l id="s10:l6" met="3" type="-+">En hoe gij zou <rhyme label="b" type="f" corresp="#s10:l3">ervaren</rhyme></l>
        <l id="s10:l7" met="2" type="-+">Ons wil <rhyme label="b" type="f" corresp="#s10:l3">verclaren</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Mer, Heer, u wonderlicke <rhyme label="a" type="m">daet</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Ver boven gaende eynde en <rhyme label="a" type="m" corresp="#s11:l1">maet</rhyme></l>
        <l id="s11:l3" met="3" type="-+">(En?) can geen mensch (?) <rhyme label="b" type="f">ontkinnen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Noch ooc ... t minste <rhyme label="c" type="m">let</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Dat aen ons leden es <rhyme label="c" type="m" corresp="#s11:l4">geset</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Can u wijsheyts <rhyme label="b" type="f" corresp="#s11:l3">versinnen</rhyme></l>
        <l id="s11:l7" met="2" type="-+">Wel <rhyme label="b" type="f" corresp="#s11:l3">onderwinnen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ons borst en heeft noch slot, noch <rhyme label="a" type="m">clinc</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Voor u, de schepper aller <rhyme label="a" type="m" corresp="#s12:l1">dinc</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Gij cont al vrij (?) <rhyme label="b" type="f">geraecken</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Het ru begin der grover <rhyme label="c" type="m">mas</rhyme>,</l>
        <l id="s12:l5" met="4" type="-+">Daer ic eerst of geschapen <rhyme label="c" type="m" corresp="#s12:l4">was</rhyme>;</l>
        <l id="s12:l6" met="3" type="-+">Jae d'ander erste <rhyme label="b" type="f" corresp="#s12:l3">oorzaecken</rhyme></l>
        <l id="s12:l7" met="2" type="-+">Om dit te <rhyme label="b" type="f" corresp="#s12:l3">maecken</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">U waren in een bouc <rhyme label="a" type="m">geprent</rhyme>.</l>
        <l id="s13:l2" met="4" type="-+">Beneffen dien was u <rhyme label="a" type="m" corresp="#s13:l1">bekent</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Mijn maecsel, doen en <rhyme label="b" type="f">zweven</rhyme> (?);</l>
        <l id="s13:l4" met="4" type="-+">Aleer mijn leden leden <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">Wist gij (?) wat tclaren(?) <rhyme label="c" type="m" corresp="#s13:l4">zonneschijn</rhyme>(?)</l>
        <l id="s13:l6" met="3" type="-+">Bij mij zou zijn <rhyme label="b" type="f" corresp="#s13:l3">bedreven</rhyme></l>
        <l id="s13:l7" met="2" type="-+">Van al mijn <rhyme label="b" type="f" corresp="#s13:l3">leven</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Als ic bij mij dit <rhyme label="a" type="m">overdenc</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">Ic van verbaestheyt schier <rhyme label="a" type="m" corresp="#s14:l1">verdrenc</rhyme>;</l>
        <l id="s14:l3" met="3" type="-+">Mijn hart dat wilt <rhyme label="b" type="f">bezwijcken</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Wanneer mij (?) comen voor t <rhyme label="c" type="m">ooch</rhyme></l>
        <l id="s14:l5" met="4" type="-+">De wonderlicke daden <rhyme label="c" type="m" corresp="#s14:l4">hooch</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Die u hant heeft doen <rhyme label="b" type="f" corresp="#s14:l3">blijcken</rhyme></l>
        <l id="s14:l7" met="2" type="-+">In allen <rhyme label="b" type="f" corresp="#s14:l3">wijcken</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Ic zou t onnommelic <rhyme label="a" type="m">getal</rhyme></l>
        <l id="s15:l2" met="4" type="-+">Der zanden dat doer (?) <rhyme label="a" type="m" corresp="#s15:l1">stormgeschal</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Aan d'ouvers wert <rhyme label="b" type="f">geslagen</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Getellen en uytspreecken <rhyme label="c" type="m">eer</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Al waert dat ooc mijn oogen, <rhyme label="c" type="m" corresp="#s15:l4">Heer</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Van al mijns levens <rhyme label="b" type="f" corresp="#s15:l3">dagen</rhyme></l>
        <l id="s15:l7" met="2" type="-+">Geen custen <rhyme label="b" type="f" corresp="#s15:l3">zagen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Och, wanneer wert der bozen <rhyme label="a" type="m">schaer</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Bij u verdelget ... <rhyme label="a" type="m" corresp="#s16:l1">altegaer</rhyme>,</l>
        <l id="s16:l3" met="3" type="-+">Wanneer werd' ic <rhyme label="b" type="f">ontbonden</rhyme></l>
        <l id="s16:l4" met="4" type="-+">Van tschelmsche volck dat een graech <rhyme label="c" type="m">gemoet</rhyme></l>
        <l id="s16:l5" met="4" type="-+">Om suypen heeft t ontschuldich <rhyme label="c" type="m" corresp="#s16:l4">bloet</rhyme>.</l>
        <l id="s16:l6" met="3" type="-+">Haer lasterende <rhyme label="b" type="f" corresp="#s16:l3">monden</rhyme></l>
        <l id="s16:l7" met="2" type="-+">Veel quaet <rhyme label="b" type="f" corresp="#s16:l3">verconden</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Van u ... es hem een <rhyme label="a" type="m">slot</rhyme></l>
        <l id="s17:l2" met="4" type="-+">Veel ... snorckt (?) gij, O <rhyme label="a" type="m" corresp="#s17:l1">God</rhyme></l>
        <l id="s17:l3" met="3" type="-+">... dus nijdige honden <rhyme label="b" type="f">bleffen</rhyme>.</l>
        <l id="s17:l4" met="4" type="-+">... die om mij <rhyme label="c" type="m">waer</rhyme></l>
        <l id="s17:l5" met="4" type="-+">In houdende doch zonder <rhyme label="c" type="m" corresp="#s17:l4">haer</rhyme></l>
        <l id="s17:l6" met="3" type="-+">Ic hebbe ... <rhyme label="b" type="f" corresp="#s17:l3">teffen</rhyme></l>
        <l id="s17:l7" met="2" type="-+">In cracht <rhyme label="b" type="f" corresp="#s17:l3">verheffen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">En ben ic niet een viant <rhyme label="a" type="m">quaet</rhyme>,</l>
        <l id="s18:l2" met="4" type="-+">Die u viandich es en <rhyme label="a" type="m" corresp="#s18:l1">haet</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Tuedraecht, zoude ic <rhyme label="b" type="f">beminnen</rhyme></l>
        <l id="s18:l4" met="4" type="-+">Die u afgunstich zijn <rhyme label="c" type="m">gesint</rhyme>?</l>
        <l id="s18:l5" met="4" type="-+">Geen van de zulcke zijn mijn <rhyme label="c" type="m" corresp="#s18:l4">vrint</rhyme>;</l>
        <l id="s18:l6" met="3" type="-+">Ic haet al die zoe <rhyme label="b" type="f" corresp="#s18:l3">binnen</rhyme></l>
        <l id="s18:l7" met="2" type="-+">Met hert en <rhyme label="b" type="f" corresp="#s18:l3">zinnen</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Doersouct hel binnenste <rhyme label="a" type="m">vertrec</rhyme></l>
        <l id="s19:l2" met="4" type="-+">Mijns borsten, ziet of ic recht <rhyme label="a" type="m" corresp="#s19:l1">zeg</rhyme> (?)</l>
        <l id="s19:l3" met="3" type="-+">Deurwandele, Heer, ten <rhyme label="b" type="f">paeden</rhyme>.</l>
        <l id="s19:l4" met="4" type="-+">Ten zij gij daer vint lieft en <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s19:l5" met="4" type="-+">Tot u werck thert <rhyme label="c" type="m" corresp="#s19:l4">gerust</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Zeynt mij dan metten <rhyme label="b" type="f" corresp="#s19:l3">quaden</rhyme></l>
        <l id="s19:l7" met="2" type="-+">Veel <rhyme label="b" type="f" corresp="#s19:l3">ongenaden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
