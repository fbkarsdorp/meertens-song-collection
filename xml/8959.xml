<song>
  <meta>
    <id>8959</id>
    <date>
      <year-start>1625</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>utrecht</orginal-name>
      <standard-name>Utrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>52.0949753</lon>
        <lat>5.109708</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="loflied" translation="praise song">loflied</tag>
    </tags>
    <melody>Jesus Christus unser Heiland</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IEsus CHristus onse <rhyme label="a" type="f">HEylandt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die den Doodt <rhyme label="a" type="f" corresp="#s1:l1">overwandt</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Is <rhyme label="b" type="f">Opghestanden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">De sond' heeft hy <rhyme label="b" type="f" corresp="#s1:l3">gevangen</rhyme>,</l>
        <l id="s1:l5" met="3" type="+-"><rhyme label="c" type="f">kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Die sonder Sond' was <rhyme label="a" type="f">gheboren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Droech voor ons Godes <rhyme label="a" type="f" corresp="#s2:l1">Thooren</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Heeft ons <rhyme label="b" type="f">versoenet</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Dat ons Godt syn Huld' <rhyme label="b" type="f" corresp="#s2:l3">gunnet</rhyme>,</l>
        <l id="s2:l5" met="3" type="+-"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doot, Sonde, Duyvel end <rhyme label="a" type="f">Genaed</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Alles in Handen <rhyme label="a" type="f" corresp="#s3:l1">haet</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Hy can <rhyme label="b" type="f">bevreden</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Alle die tot hem <rhyme label="b" type="f" corresp="#s3:l3">treden</rhyme>,</l>
        <l id="s3:l5" met="3" type="+-"><rhyme label="c" type="f">Kirieleyson</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
