<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21273</id>
    <date>
      <year-start>1591</year-start>
      <year-end>1592</year-end>
    </date>
    <tags>
      <tag standard-name="schriftuurlijk lied" translation="scripture song">schriftuurlijk lied</tag>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Ik heb gedragen wel zeven jaar</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Een Liet wil ick <rhyme label="a" type="f">beginnen</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Ter liefden de bedrute harten en <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Die daer nu zijn so <rhyme label="b" type="m">veel</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Het goet is te <rhyme label="a" type="f" corresp="#s1:l1">verwinnen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Al benouwet v dat <rhyme label="b" type="m" corresp="#s1:l3">heel</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Tgoet is te verwinnen hoort <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Maer na dees tijt sal geen genade <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">O lieue menschen <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Is v siel al <rhyme label="a" type="f" corresp="#s2:l1">ghenesen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Wilt dit ghedachtich <rhyme label="b" type="m" corresp="#s2:l3">zijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy segt meest <rhyme label="a" type="f">allegader</rhyme>,</l>
        <l id="s3:l2" met="5" type="-+">Christus heeft ons versoent met zijnen <rhyme label="a" type="f" corresp="#s3:l1">vader</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Maer acht toch op zijn <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Dat ghy moet comen <rhyme label="a" type="f" corresp="#s3:l1">nader</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Die rechte nieuwe <rhyme label="b" type="m" corresp="#s3:l3">herboort</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Johannes die wilt <rhyme label="a" type="f">hooren</rhyme></l>
        <l id="s4:l2" met="5" type="-+">Ten si dat yemant wort van nieus <rhyme label="a" type="f" corresp="#s4:l1">herboren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Hy en mach Gods rijck niet <rhyme label="b" type="m">sien</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En wilt v niet <rhyme label="a" type="f" corresp="#s4:l1">verstoren</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Dat ick dit seg v <rhyme label="b" type="m" corresp="#s4:l3">lien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy Mannen ende <rhyme label="a" type="f">vrouwen</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Verstaet waerom v, dit wort voor <rhyme label="a" type="f" corresp="#s5:l1">ghehouwen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Het schiet wt liefden <rhyme label="b" type="m">puer</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Wilt ghy op Godt <rhyme label="a" type="f" corresp="#s5:l1">betrouwen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">So leeft na die <rhyme label="b" type="m" corresp="#s5:l3">Schriftuer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Aenschout Gods wonder <rhyme label="a" type="f">daden</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Godt spijsde veertich Jaer met cleyne <rhyme label="a" type="f" corresp="#s6:l1">saden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Jacobs gheslachte <rhyme label="b" type="m">pleyn</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die Heer vol van <rhyme label="a" type="f" corresp="#s6:l1">ghenaden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Gaf dranck al wt den <rhyme label="b" type="m" corresp="#s6:l3">steyn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Doemen die seuenste <rhyme label="a" type="f">Jaren</rhyme></l>
        <l id="s7:l2" met="5" type="-+">Dat lant most vieren na schrifts <rhyme label="a" type="f" corresp="#s7:l1">verclaren</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Dat seste Jaer <rhyme label="b" type="m">verstaet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Liet Godt die vruchten <rhyme label="a" type="f" corresp="#s7:l1">vergaren</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Voor drie Jaren dit wel <rhyme label="b" type="m" corresp="#s7:l3">vaet</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Elias heeft veertich <rhyme label="a" type="f">daghen</rhyme></l>
        <l id="s8:l2" met="5" type="-+">Geleeft al met een brootken hoort <rhyme label="a" type="f" corresp="#s8:l1">gewagen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat den Engel hem wees <rhyme label="b" type="m">ras</rhyme></l>
        <l id="s8:l4" met="3" type="-+">So sober conde hy hem <rhyme label="a" type="f" corresp="#s8:l1">draghen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Die man gods op dat <rhyme label="b" type="m" corresp="#s8:l3">pas</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Hoe sent die heer zijn <rhyme label="a" type="f">gauen</rhyme></l>
        <l id="s9:l2" met="5" type="-+">Elias is gespijst al van een <rhyme label="a" type="f" corresp="#s9:l1">Rauen</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Al in den tijt der <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Die Heer can voeden en <rhyme label="a" type="f" corresp="#s9:l1">lauen</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Seer wonderlicken <rhyme label="b" type="m" corresp="#s9:l3">groot</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Christus heeft spijse <rhyme label="a" type="f">ghegeuen</rhyme></l>
        <l id="s10:l2" met="5" type="-+">Een groote menichte soo daer staet <rhyme label="a" type="f" corresp="#s10:l1">gheschreuen</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Van weynich broot en <rhyme label="b" type="m">visch</rhyme></l>
        <l id="s10:l4" met="3" type="-+">daer zijn noch ouer <rhyme label="a" type="f" corresp="#s10:l1">ghebleuen</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Also veel coruen <rhyme label="b" type="m" corresp="#s10:l3">wis</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">So wie na Christus <rhyme label="a" type="f">reden</rhyme></l>
        <l id="s11:l2" met="5" type="-+">dat rijcke gods soect, met zijn <rhyme label="a" type="f" corresp="#s11:l1">gerechticheden</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Weet dat haer worden <rhyme label="b" type="m">sal</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Gheworpen toe <rhyme label="a" type="f" corresp="#s11:l1">beneden</rhyme></l>
        <l id="s11:l5" met="3" type="-+">dat sy behoeuen <rhyme label="b" type="m" corresp="#s11:l3">sal</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Al die den heere <rhyme label="a" type="f">vresen</rhyme></l>
        <l id="s12:l2" met="5" type="-+">die en hebben geen gebreck, al so wy <rhyme label="a" type="f" corresp="#s12:l1">lesen</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Aen gheenderley <rhyme label="b" type="m">goet</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Bekeert v nu tot <rhyme label="a" type="f" corresp="#s12:l1">desen</rhyme></l>
        <l id="s12:l5" met="3" type="-+">Die Siel en lichaem <rhyme label="b" type="m" corresp="#s12:l3">voet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Waer al v schaed <rhyme label="a" type="f">verwonnen</rhyme></l>
        <l id="s13:l2" met="5" type="-+">En dat nieuwe leuen niet <rhyme label="a" type="f" corresp="#s13:l1">begonnen</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Als die heere com <rhyme label="b" type="m">subijt</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Wat salt dan baten <rhyme label="a" type="f" corresp="#s13:l1">connen</rhyme></l>
        <l id="s13:l5" met="3" type="-+">Als al die werelt <rhyme label="b" type="m" corresp="#s13:l3">crijt</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Den dach comt wilt het <rhyme label="a" type="f">gheloouen</rhyme></l>
        <l id="s14:l2" met="5" type="-+">die branden sal als een <rhyme label="a" type="f" corresp="#s14:l1">ouen</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Hoort Petrus oock <rhyme label="b" type="m">net</rhyme></l>
        <l id="s14:l4" met="3" type="-+">die elementen <rhyme label="a" type="f" corresp="#s14:l1">bouen</rhyme></l>
        <l id="s14:l5" met="3" type="-+">Sullen smelten vander <rhyme label="b" type="m" corresp="#s14:l3">het</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Na Malechias <rhyme label="a" type="f">schrijuen</rhyme></l>
        <l id="s15:l2" met="5" type="-+">daer sal noch tack, wortel ouer <rhyme label="a" type="f" corresp="#s15:l1">blijuen</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Ghy saecht hier al wat <rhyme label="b" type="m">vrij</rhyme></l>
        <l id="s15:l4" met="3" type="-+">O Mannen ende <rhyme label="a" type="f" corresp="#s15:l1">Wijuen</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Ten ghelijcket daer niet <rhyme label="b" type="m" corresp="#s15:l3">by</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Daer is knersinge der <rhyme label="a" type="f">tanden</rhyme></l>
        <l id="s16:l2" met="5" type="-+">Haer worm sal niet steruen of <rhyme label="a" type="f" corresp="#s16:l1">branden</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Noch haer vier niet wt <rhyme label="b" type="m">gheblust</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Die werelt sal haest tot <rhyme label="a" type="f" corresp="#s16:l1">schanden</rhyme></l>
        <l id="s16:l5" met="3" type="-+">dan zijn met al haer <rhyme label="b" type="m" corresp="#s16:l3">lust</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Gods volck sal <rhyme label="a" type="f">triumpheren</rhyme></l>
        <l id="s17:l2" met="5" type="-+">In groter vruecht, met witte syden <rhyme label="a" type="f" corresp="#s17:l1">cleren</rhyme></l>
        <l id="s17:l3" met="3" type="-+">O menschen wilt <rhyme label="b" type="m">opstaen</rhyme></l>
        <l id="s17:l4" met="3" type="-+">Keert tot den Heer der <rhyme label="a" type="f" corresp="#s17:l1">heeren</rhyme></l>
        <l id="s17:l5" met="3" type="-+">Hy en sal v niet <rhyme label="b" type="m" corresp="#s17:l3">versmaen</rhyme></l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wilt toch wt Babel <rhyme label="a" type="f">scheyden</rhyme></l>
        <l id="s18:l2" met="5" type="-+">Totten nieuwe herboort wilt v <rhyme label="a" type="f" corresp="#s18:l1">bereyden</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Waeyt niet met alk <rhyme label="b" type="m">wint</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Laet v niet langer <rhyme label="a" type="f" corresp="#s18:l1">leyden</rhyme></l>
        <l id="s18:l5" met="3" type="-+">Al van die Leytsluy <rhyme label="b" type="m" corresp="#s18:l3">blint</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Dit liet most ick v <rhyme label="a" type="f">schencken</rhyme></l>
        <l id="s19:l2" met="5" type="-+">Ofs yemant hier door creech een <rhyme label="a" type="f" corresp="#s19:l1">achterdencken</rhyme></l>
        <l id="s19:l3" met="3" type="-+">dat is mijn wenschen <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s19:l4" met="3" type="-+">dat ghy hier na mocht <rhyme label="a" type="f" corresp="#s19:l1">blencken</rhyme></l>
        <l id="s19:l5" met="3" type="-+">Want mijn iammert v <rhyme label="b" type="m" corresp="#s19:l3">verdriet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
