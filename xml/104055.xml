    <melody>Grande princesse</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">SOete Afgoddinne schoonder als <rhyme label="a" type="f">Aurore</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Rijcker als <rhyme label="a" type="f" corresp="#s1:l1">Pandore</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Uyt hun rijcke <rhyme label="b" type="m">woon</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Begift van alle <rhyme label="b" type="m" corresp="#s1:l3">Goon</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Schoone <rhyme label="c" type="f">leden</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Lieftal'ge <rhyme label="c" type="f" corresp="#s1:l5">zeden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Gesoute <rhyme label="c" type="f" corresp="#s1:l5">reden</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">En volmaecte <rhyme label="d" type="m">deught</rhyme>,</l>
        <l id="s1:l9" met="3" type="+-">Lockaes in het <rhyme label="e" type="f">minnen</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Doen <rhyme label="e" type="f" corresp="#s1:l9">besinnen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">My uwe frissche <rhyme label="d" type="m" corresp="#s1:l8">jeught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Gelyc de Persen, d'edle Sonne <rhyme label="a" type="f">pleghen</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Bid ic <rhyme label="a" type="f" corresp="#s2:l1">neergeseghen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">U mijn edele <rhyme label="b" type="m">Maan</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En eenich son-licht <rhyme label="b" type="m" corresp="#s2:l3">aen</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Ey, laet de <rhyme label="c" type="f">straelen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">[...] gonste <rhyme label="c" type="f" corresp="#s2:l5">daelen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">[...] alle <rhyme label="c" type="f" corresp="#s2:l5">quaelen</rhyme></l>
        <l id="s2:l8" met="3" type="+-">Lichten uyt mijn <rhyme label="d" type="m">hert</rhyme>.</l>
        <l id="s2:l9" met="3" type="+-">'Tis de Godes <rhyme label="e" type="f">eygen</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">'tOor te <rhyme label="e" type="f" corresp="#s2:l9">neygen</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Daar het gebeden <rhyme label="d" type="m" corresp="#s2:l8">wert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
