    <melody>Hei hoe helder schijnt het maantje</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">KEn ick jou noch niet <rhyme label="a" type="f">bepraten</rhyme>?</l>
        <l id="s1:l2" met="4" type="+-">Soete lieve hertje, <rhyme label="b" type="m">kom</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Gaat met mijn tot Neel van <rhyme label="a" type="f" corresp="#s1:l1">Straten</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Daar is sulcke lekkere <rhyme label="b" type="m" corresp="#s1:l2">Mom</rhyme>;</l>
        <l id="s1:l5" met="2" type="-+">Catrijn, de <rhyme label="c" type="m">pyn</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Die ick om jou <rhyme label="d" type="m">ly</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Maackt: ick naght en dagh schier <rhyme label="d" type="m" corresp="#s1:l6">schrey</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Heer Kees Piers hoe keunje <rhyme label="a" type="f">kallen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hebje dat in Stee <rhyme label="b" type="m">eliert</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Get je kent soo wonder <rhyme label="a" type="f" corresp="#s2:l1">mallen</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'k Sagh het lest by Jaep zyn <rhyme label="b" type="m" corresp="#s2:l2">Giert</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Kees Piet, hy <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s2:l6" met="3" type="+-">'t Is een moye <rhyme label="d" type="m">meyt</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">'k Heb het jou voor deus <rhyme label="d" type="m" corresp="#s2:l6">geseyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Se moet voor jou schoonheydt <rhyme label="a" type="f">wycken</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Want s' is by jou <rhyme label="b" type="m">niemendal</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">'k Willer by jou niet <rhyme label="a" type="f" corresp="#s3:l1">gelycken</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">'t Het met mijn en heur geen <rhyme label="b" type="m" corresp="#s3:l2">val</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Catrijn, selt <rhyme label="c" type="m">zyn</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Datje me heast <rhyme label="d" type="m">loont</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En mijn trouwe liefde <rhyme label="d" type="m" corresp="#s3:l6">toont</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">'k Sweer by errette en by <rhyme label="a" type="f">boone</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Potje beuling ryst en <rhyme label="b" type="m">bry</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Dat ickje sel weerliefd' <rhyme label="a" type="f" corresp="#s4:l1">toone</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Mien j' in deugt en eere <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>;</l>
        <l id="s4:l5" met="2" type="-+">Kees Piet, want <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Stonje na myn <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Ick en wouje <rhyme label="d" type="m" corresp="#s4:l6">nimmermeer</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'k Sou myn eerder droncken <rhyme label="a" type="f">drincken</rhyme></l>
        <l id="s5:l2" met="4" type="+-">An scharbier myn lieve <rhyme label="b" type="m">rock</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Ja myn hoet sou harder <rhyme label="a" type="f" corresp="#s5:l1">klincken</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Als de Wester kerricks <rhyme label="b" type="m" corresp="#s5:l2">klock</rhyme>;</l>
        <l id="s5:l5" met="2" type="-+">Catrijn, ick <rhyme label="c" type="m">myn</rhyme></l>
        <l id="s5:l6" met="3" type="+-">Jou in deugt en <rhyme label="d" type="m">eer</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Daer's op trouw die suyker <rhyme label="d" type="m" corresp="#s5:l6">peer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
