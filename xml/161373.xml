<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>161373</id>
    <date>
      <year-start>1767</year-start>
      <year-end>1767</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>La marine</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">NU zit ik in <rhyme label="a" type="f">benouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De Kermis is <rhyme label="b" type="m">voorby</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik mag myn Hoofd wel <rhyme label="a" type="f" corresp="#s1:l1">klouwen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Playzier is aen een <rhyme label="b" type="m" corresp="#s1:l2">zei</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Het Geld dat is <rhyme label="c" type="m">verteert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En 't Keel-gat is <rhyme label="c" type="m" corresp="#s1:l5">gesmeert</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Ik moet <rhyme label="d" type="f">zorgen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Ik kan niet <rhyme label="d" type="f" corresp="#s1:l7">borgen</rhyme>;</l>
        <l id="s1:l9" met="2" type="-+">By de <rhyme label="c" type="m" corresp="#s1:l5">Weerd</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik was vrolyk in myn <rhyme label="a" type="f">zaken</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En daer toe ryk van <rhyme label="b" type="m">schat</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ik ging een Pantje <rhyme label="a" type="f" corresp="#s2:l1">maken</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Ik nam myn Hembt van 't <rhyme label="b" type="m" corresp="#s2:l2">gat</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ik zag ieder zwieren <rhyme label="c" type="m">gaan</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat stont myn heel wel <rhyme label="c" type="m" corresp="#s2:l5">aen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Ik ging <rhyme label="d" type="f">zamen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Door de <rhyme label="d" type="f" corresp="#s2:l7">Kramen</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Met 'er <rhyme label="c" type="m" corresp="#s2:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Om vreugde te <rhyme label="a" type="f">aenschouwen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">'t Was lustig op de <rhyme label="b" type="m">loop</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Uan Kinders, Mans en <rhyme label="a" type="f" corresp="#s3:l1">Urouwen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dienst-meisjes by de <rhyme label="b" type="m" corresp="#s3:l2">hoop</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die zag ik zwieren <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Dat stond my heel wel <rhyme label="c" type="m" corresp="#s3:l5">aen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Ik ging <rhyme label="d" type="f">vragen</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Met <rhyme label="d" type="f" corresp="#s3:l7">behagen</rhyme></l>
        <l id="s3:l9" met="2" type="-+">Om mee te <rhyme label="c" type="m" corresp="#s3:l5">gaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sy zei wel lieve <rhyme label="a" type="f">Guytje</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Bent gy dat zoo <rhyme label="b" type="m">gezint</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of hebt gy dikke <rhyme label="a" type="f" corresp="#s4:l1">duptjes</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Ik roemde op myn <rhyme label="b" type="m" corresp="#s4:l2">splint</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ik dronk een Glaesje <rhyme label="c" type="m">Wyn</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Maer 't mogt geen Flesje <rhyme label="c" type="m" corresp="#s4:l5">zyn</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Ik kogt een <rhyme label="d" type="f">Koekje</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Al in het <rhyme label="d" type="f" corresp="#s4:l7">hoekje</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Uoor onse <rhyme label="c" type="m" corresp="#s4:l5">Tryn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'t Was Kermis nog naer <rhyme label="a" type="f">wenschen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ik zag de Spullen <rhyme label="b" type="m">staen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Uan Paerden en van <rhyme label="a" type="f" corresp="#s5:l1">Menschen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Ueel Klugten zynder <rhyme label="b" type="m" corresp="#s5:l2">gedaen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">'t Was alles met <rhyme label="c" type="m">fatzoen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Maer om dubbeltjes te <rhyme label="c" type="m" corresp="#s5:l5">doen</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">'t Was goede <rhyme label="d" type="f">Urientje</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Hebje <rhyme label="d" type="f" corresp="#s5:l7">Splintje</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">Dan krygje een <rhyme label="c" type="m" corresp="#s5:l5">zoen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
