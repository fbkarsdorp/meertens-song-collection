<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24186</id>
    <date>
      <year-start>1597</year-start>
      <year-end>1597</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Ik weet een vrouwtje amoureus</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Een Vrouken heb ic zoo zeer <rhyme label="a" type="m">bemint</rhyme>/</l>
        <l id="s1:l2" met="3" type="-+">Gheen liever en mach daer <rhyme label="b" type="f">leven</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Ic weet wel datse my nau en <rhyme label="a" type="m" corresp="#s1:l1">kent</rhyme>/</l>
        <l id="s1:l4" met="3" type="-+">In myn hert is sy <rhyme label="b" type="f" corresp="#s1:l2">gheschreven</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Mocht ic eens komen by haer <rhyme label="c" type="m">present</rhyme>/</l>
        <l id="s1:l6" met="3" type="-+">Dat icse mochte <rhyme label="d" type="f">spreken</rhyme>/</l>
        <l id="s1:l7" met="4" type="-+">Ic duchte neen/ sy is zoo gent/ zoo <rhyme label="c" type="m" corresp="#s1:l5">excellent</rhyme>/</l>
        <l id="s1:l8" met="5" type="-+">Het zal aen haer <rhyme label="d" type="f" corresp="#s1:l6">ghebreken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Van leden is sy zoo wel <rhyme label="a" type="m">ghemaect</rhyme>/</l>
        <l id="s2:l2" met="3" type="-+">Ic en machse by niemant <rhyme label="b" type="f">ghelijcken</rhyme>/</l>
        <l id="s2:l3" met="4" type="-+">Dan by Helena die wert <rhyme label="a" type="m" corresp="#s2:l1">ontschaect</rhyme>/</l>
        <l id="s2:l4" met="3" type="-+">Al wt dat lant van <rhyme label="b" type="f" corresp="#s2:l2">Grijcken</rhyme>/</l>
        <l id="s2:l5" met="4" type="-+">Van eenen jonghelinc die hiet <rhyme label="c" type="m">Parijs</rhyme>/</l>
        <l id="s2:l6" met="3" type="-+">Van Troyen was hy <rhyme label="d" type="f">gheboren</rhyme>:</l>
        <l id="s2:l7" met="4" type="-+">Sy is rein ende daer toe wijs/ sy voert den <rhyme label="c" type="m" corresp="#s2:l5">prijs</rhyme>/</l>
        <l id="s2:l8" met="5" type="-+">Boven alle vrouwen <rhyme label="d" type="f" corresp="#s2:l6">wtverkoren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Troylus liefde en was noyt zoo <rhyme label="a" type="m">groot</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">Dat mercke ic wel aen de <rhyme label="b" type="f">myne</rhyme>/</l>
        <l id="s3:l3" met="4" type="-+">Die omme Breseida sterf den <rhyme label="a" type="m" corresp="#s3:l1">doot</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">Met bitterlijcke <rhyme label="b" type="f" corresp="#s3:l2">pijne</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Sy heeft myn hert zoo zeer <rhyme label="c" type="m">doorwont</rhyme>/</l>
        <l id="s3:l6" met="3" type="-+">Doorschoten met vyerighe <rhyme label="d" type="f">stralen</rhyme>:</l>
        <l id="s3:l7" met="4" type="-+">Mocht icse eens kussen aen hare mont/ ic waer <rhyme label="c" type="m" corresp="#s3:l5">ghesont</rhyme>/</l>
        <l id="s3:l8" met="5" type="-+">Ghenesen al te <rhyme label="d" type="f" corresp="#s3:l6">male</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Van leden is sy zoo wel <rhyme label="a" type="m">ghestelt</rhyme>/</l>
        <l id="s4:l2" met="3" type="-+">Als Medea/ die wonder <rhyme label="b" type="f">wrochte</rhyme>/</l>
        <l id="s4:l3" met="4" type="-+">Sy hadde tgulde Vlies in haer <rhyme label="a" type="m" corresp="#s4:l1">ghewelt</rhyme>/</l>
        <l id="s4:l4" met="3" type="-+">Jason haer tot schanden <rhyme label="b" type="f" corresp="#s4:l2">brochte</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Hy lietse blijven in swaer <rhyme label="c" type="m">verdriet</rhyme>/</l>
        <l id="s4:l6" met="3" type="-+">De fleur boven alle <rhyme label="d" type="f">Vrouwen</rhyme>;</l>
        <l id="s4:l7" met="4" type="-+">Hy kreegh synen wil vaet dit bediet// end achtese <rhyme label="c" type="m" corresp="#s4:l5">niet</rhyme></l>
        <l id="s4:l8" met="5" type="-+">Sy bleef in grooten <rhyme label="d" type="f" corresp="#s4:l6">rouwe</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Eer ic schoon lieveken zoo zoude <rhyme label="a" type="m">zijn</rhyme>/</l>
        <l id="s5:l2" met="3" type="-+">Ic liete my liever <rhyme label="b" type="f">ontlijven</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Ende lijden in myn herte <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme>/</l>
        <l id="s5:l4" met="3" type="-+">Eer ic v zou laten <rhyme label="b" type="f" corresp="#s5:l2">blijven</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Maer als Pyramus de Jonghelingh <rhyme label="c" type="m">weert</rhyme>/</l>
        <l id="s5:l6" met="3" type="-+">Zoo zoude ic daer liever om <rhyme label="d" type="f">sterven</rhyme>:</l>
        <l id="s5:l7" met="4" type="-+">Die om Thisbe de bloeme expeert// al met syn <rhyme label="c" type="m" corresp="#s5:l5">sweert</rhyme>/</l>
        <l id="s5:l8" met="5" type="-+">Syn herte gingh <rhyme label="d" type="f" corresp="#s5:l6">deurkerven</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Balihasar noyt zulcken liefde en <rhyme label="a" type="m">droegh</rhyme>/</l>
        <l id="s6:l2" met="3" type="-+">Op Pallas/ Vrouwe vol <rhyme label="b" type="f">swaricheden</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Al wast dat hy ooc twee Reusen <rhyme label="a" type="m" corresp="#s6:l1">dootsloech</rhyme>/</l>
        <l id="s6:l4" met="3" type="-+">Ende twee Leeuwen daer <rhyme label="b" type="f" corresp="#s6:l2">mede</rhyme>/</l>
        <l id="s6:l5" met="4" type="-+">Hy wan syn Liefken met grooter <rhyme label="c" type="m">kracht</rhyme>/</l>
        <l id="s6:l6" met="3" type="-+">De Soudaen hadse hem <rhyme label="d" type="f">benomen</rhyme>.</l>
        <l id="s6:l7" met="4" type="-+">Al van twee Reusen werdt sy ghewacht/ by dagh en <rhyme label="c" type="m" corresp="#s6:l5">nacht</rhyme>/</l>
        <l id="s6:l8" met="5" type="-+">Datter niemant by en zoude <rhyme label="d" type="f" corresp="#s6:l6">komen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Omme v zoo wil ic waghen myn <rhyme label="a" type="m">lijf</rhyme>/</l>
        <l id="s7:l2" met="3" type="-+">Als Herculus de Ridder <rhyme label="b" type="f">boude</rhyme>/</l>
        <l id="s7:l3" met="4" type="-+">Die Euxonia met syn <rhyme label="a" type="m" corresp="#s7:l1">bedrijf</rhyme>/</l>
        <l id="s7:l4" met="3" type="-+">Haer leven heeft <rhyme label="b" type="f" corresp="#s7:l2">behouden</rhyme>:</l>
        <l id="s7:l5" met="4" type="-+">Hy verwan het Moster het felle <rhyme label="c" type="m">beest</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Dat haer zoude hebbe <rhyme label="d" type="f">gheten</rhyme>:</l>
        <l id="s7:l7" met="4" type="-+">Het quam wter Zee met groot tempeest/ alsoomen <rhyme label="c" type="m" corresp="#s7:l5">leest</rhyme>/</l>
        <l id="s7:l8" met="5" type="-+">Hy hevet doot <rhyme label="d" type="f" corresp="#s7:l6">ghesmeten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Zegghet my schoone hoet met v <rhyme label="a" type="m">staet</rhyme>/</l>
        <l id="s8:l2" met="3" type="-+">En wilt my niet <rhyme label="b" type="f">bedrieghen</rhyme>:</l>
        <l id="s8:l3" met="4" type="-+">Ic bid v dat ghy my niet en <rhyme label="a" type="m" corresp="#s8:l1">verraet</rhyme>/</l>
        <l id="s8:l4" met="3" type="-+">En wilt my doch niet <rhyme label="b" type="f" corresp="#s8:l2">lieghen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Als Dalida de vrouwe <rhyme label="c" type="m">wreet</rhyme>/</l>
        <l id="s8:l6" met="3" type="-+">Die Samson heeft <rhyme label="d" type="f">bedroghen</rhyme>:</l>
        <l id="s8:l7" met="4" type="-+">Al met een schaers syn hayr af-sneet/ groote pijn hy <rhyme label="c" type="m" corresp="#s8:l5">leet</rhyme>/</l>
        <l id="s8:l8" met="5" type="-+">Hy moeste den doot <rhyme label="d" type="f" corresp="#s8:l6">ghedooghen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Oorlof schoon lief/ doch om my <rhyme label="a" type="m">peyst</rhyme>/</l>
        <l id="s9:l2" met="3" type="-+">Wildy my niet staen ter <rhyme label="b" type="f">spraken</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">De liefde die ic draghe is <rhyme label="a" type="m" corresp="#s9:l1">ongheveyst</rhyme>/</l>
        <l id="s9:l4" met="3" type="-+">Dat zeggh' ic v by ware <rhyme label="b" type="f" corresp="#s9:l2">zaken</rhyme>:</l>
        <l id="s9:l5" met="4" type="-+">Laet my doch troost van v <rhyme label="c" type="m">ontfaen</rhyme>/</l>
        <l id="s9:l6" met="3" type="-+">Oft ic zal heel <rhyme label="d" type="f">verdwijnen</rhyme>:</l>
        <l id="s9:l7" met="4" type="-+">Ic ben om v met druc belaen/ dat heeft <rhyme label="c" type="m" corresp="#s9:l5">ghedaen</rhyme>/</l>
        <l id="s9:l8" met="5" type="-+">V vriendelijc <rhyme label="d" type="f" corresp="#s9:l6">aenschijne</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
