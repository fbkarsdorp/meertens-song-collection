<song>
  <meta>
    <id>165122</id>
    <date>
      <year-start>1663</year-start>
      <year-end>1663</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mijnheer Lalande</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">O Zoete <rhyme label="a" type="f">diertjes</rhyme>!</l>
        <l id="s1:l2" met="4" type="+-">Die den Heemel met den <rhyme label="b" type="m">egt</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Na veel moeit', en <rhyme label="a" type="f" corresp="#s1:l1">tiereliertjes</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Onverbreekelijken <rhyme label="b" type="m" corresp="#s1:l2">hegt</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Die na veele zugt, en <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Krijgd op 't eind tot <rhyme label="c" type="m" corresp="#s1:l5">Medicijn</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Blijschap, vreugd, en <rhyme label="a" type="f" corresp="#s1:l1">Minneviertjes</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">'t Welck u beid zal heilzaam <rhyme label="c" type="m" corresp="#s1:l5">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Zie <rhyme label="a" type="f">Kupidootje</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die uw' zieltjes had <rhyme label="b" type="m">verplet</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Door zijn pijl, en <rhyme label="a" type="f" corresp="#s2:l1">minneschootje</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hy wagt u vast op het <rhyme label="b" type="m" corresp="#s2:l2">bed</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Alwaar 't boefje zit, en <rhyme label="c" type="m">kijft</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En weet niet, waar datje <rhyme label="c" type="m" corresp="#s2:l5">blijft</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Want steeds wenscht dit Venus <rhyme label="a" type="f" corresp="#s2:l1">bootje</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Dat hy u deez' nagt <rhyme label="c" type="m" corresp="#s2:l5">gerijft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Hy heeft een <rhyme label="a" type="f">kooker</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Vol van pijltjes op zijn <rhyme label="b" type="m">lijf</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Waar meê hy is <rhyme label="a" type="f" corresp="#s3:l1">minnestooker</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">En tot yder eens <rhyme label="b" type="m" corresp="#s3:l2">gerijf</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Als hy 's nachts u in de <rhyme label="c" type="m">lust</rhyme></l>
        <l id="s3:l6" met="4" type="+-">'t Harte prent, en word <rhyme label="c" type="m" corresp="#s3:l5">gekust</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Want hy is een ziele <rhyme label="a" type="f" corresp="#s3:l1">strooker</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Als gy beide zijt in <rhyme label="c" type="m" corresp="#s3:l5">rust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">'r Komt op de <rhyme label="a" type="f">lippen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En het schijnt of 't u <rhyme label="b" type="m">verlaat</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Als 't in stilt' u wil <rhyme label="a" type="f" corresp="#s4:l1">ontslippen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">En dan ergens heenen <rhyme label="b" type="m" corresp="#s4:l2">gaat</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Want het is dan, of 't  <rhyme label="c" type="m">bezwijkt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">En zijn vaantje needer <rhyme label="c" type="m" corresp="#s4:l5">strijkt</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Als 't begeeft de minne <rhyme label="a" type="f" corresp="#s4:l1">klippen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En weêr in zijn holtjen <rhyme label="c" type="m" corresp="#s4:l5">wijkt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gy zoete <rhyme label="a" type="f">bekjes</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die uw' Speelgenood <rhyme label="b" type="m">onthoud</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En met zoo veel zoete <rhyme label="a" type="f" corresp="#s5:l1">trekjes</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Hier den Bruigom van haar <rhyme label="b" type="m" corresp="#s5:l2">houd</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Laatze glijen deeze <rhyme label="c" type="m">nagt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Zijd gy nijdig? hy <rhyme label="c" type="m" corresp="#s5:l5">verwagt</rhyme></l>
        <l id="s5:l7" met="4" type="+-">Haar in kuisheid zonder <rhyme label="a" type="f" corresp="#s5:l1">vlekjes</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Daar hy haar met wil <rhyme label="c" type="m" corresp="#s5:l5">verkragt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Zoo g' u niet <rhyme label="a" type="f">goedjes</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Draagd' en haar na 't bedde <rhyme label="b" type="m">leid</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Ende haastig spoeid uw' <rhyme label="a" type="f" corresp="#s6:l1">voetjes</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Hoor dan eens wat Venus <rhyme label="b" type="m" corresp="#s6:l2">zeid</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Zy zweerd by 't oog van haar <rhyme label="c" type="m">Pol</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">En zijn fijn gekruifde <rhyme label="c" type="m" corresp="#s6:l5">bol</rhyme>.</l>
        <l id="s6:l7" met="4" type="+-">Dat de vrijer u al <rhyme label="a" type="f" corresp="#s6:l1">zoetjes</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Zitten laaten voor een <rhyme label="c" type="m" corresp="#s6:l5">grol</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Wild g' uw' <rhyme label="a" type="f">Speelmeisje</rhyme></l>
        <l id="s7:l2" met="4" type="+-">In haar Maagdelijke <rhyme label="b" type="m">staad</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Kussen nog voor 't laatst een <rhyme label="a" type="f" corresp="#s7:l1">reisje</rhyme>?</l>
        <l id="s7:l4" met="4" type="+-">Eer dat gy haar vaare <rhyme label="b" type="m" corresp="#s7:l2">laat</rhyme>?</l>
        <l id="s7:l5" met="4" type="+-">Zoo begin deez' laaste <rhyme label="c" type="m">dag</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">t' Wijl het u gebeuren <rhyme label="c" type="m" corresp="#s7:l5">mag</rhyme>;</l>
        <l id="s7:l7" met="4" type="+-">God breng dan voors 't minne <rhyme label="a" type="f" corresp="#s7:l1">vleisje</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">En ons in zijn hoog <rhyme label="c" type="m" corresp="#s7:l5">gezag</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
