<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4346</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>C'est trop courir les eaux</melody>
  <author>Pieter Dubbels</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GEwenste dag! wiens <rhyme label="a" type="m">weeld'</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gewenste wellust <rhyme label="a" type="m" corresp="#s1:l1">teelt</rhyme>;</l>
        <l id="s1:l3" met="6" type="-+">Dewijl de zoete Min, door wedermin <rhyme label="b" type="m">gevoed</rhyme>,</l>
        <l id="s1:l4" met="6" type="-+">Als't roosje, door den daauw, zijn blompje rijzen <rhyme label="b" type="m" corresp="#s1:l3">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">VVijl't Zon-gelijke <rhyme label="a" type="m">licht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van Rozelijns <rhyme label="a" type="m" corresp="#s2:l1">gezigt</rhyme>,</l>
        <l id="s2:l3" met="6" type="-+">Met ongemene glans, den Bruidegom <rhyme label="b" type="m">begroe</rhyme>;</l>
        <l id="s2:l4" met="6" type="-+">VVaar me zijn lievende oog, zijn lievend harte <rhyme label="b" type="m" corresp="#s2:l3">voed</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ga, Rozidoor! <rhyme label="a" type="m">vermei</rhyme></l>
        <l id="s3:l2" met="3" type="-+">U, in de <rhyme label="a" type="m" corresp="#s3:l1">Roze-wei</rhyme></l>
        <l id="s3:l3" met="6" type="-+">Van Rozelijnaas schoot, tot demping van de <rhyme label="b" type="m">vlam</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Die, uit haar zoet gezigt, haar eersten oorsprongk <rhyme label="b" type="m" corresp="#s3:l3">nam</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hoe! schrik niet, <rhyme label="a" type="m">Rozelijn</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">U naakt geen wrede <rhyme label="a" type="m" corresp="#s4:l1">pijn</rhyme>,</l>
        <l id="s4:l3" met="6" type="-+">Van vijantlijk geweer; omhels uw'lot, en <rhyme label="b" type="m">kroon</rhyme>,</l>
        <l id="s4:l4" met="6" type="-+">Met Venus-kransje, het wapentuig van uw' <rhyme label="b" type="m" corresp="#s4:l3">Adoon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
