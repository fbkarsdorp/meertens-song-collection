  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Of schoon mijn ooghen inden <rhyme label="a" type="m">vloet</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Van traenen swemmen: als een swane <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Sal ick noch singhen nu ick scheyden <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Mijn hert (in plaets van <rhyme label="b" type="f">suchten</rhyme></l>
        <l id="s1:l5" met="5" type="-+">Door't groot verdriet van sware <rhyme label="c" type="m">minne-pijn</rhyme>)</l>
        <l id="s1:l6" met="2" type="-+">Schept nu <rhyme label="b" type="f" corresp="#s1:l4">ghenuchten</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Nu 'tkan <rhyme label="b" type="f" corresp="#s1:l4">ontvluchten</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Venus wreedt <rhyme label="c" type="m" corresp="#s1:l5">aenschijn</rhyme>.</l>
      </lg>
    </lg>
