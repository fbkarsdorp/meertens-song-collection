  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">NEt ghespannen <rhyme label="a" type="f">boochjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Net gespannen <rhyme label="a" type="f" corresp="#s1:l1">boochjes</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Op soo soete <rhyme label="a" type="f" corresp="#s1:l1">ooghjes</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Op soo soete <rhyme label="a" type="f" corresp="#s1:l1">ooghjes</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ghy schiet een veel scherper <rhyme label="b" type="m">schicht</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Als de pijl van 't <rhyme label="b" type="m" corresp="#s1:l5">Minne-wicht</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">En raeckt met een diepe <rhyme label="c" type="m">wond</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Onse hertjes tot de <rhyme label="c" type="m" corresp="#s1:l7">grond</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Poesel-sachte <rhyme label="a" type="f">hantjes</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Poesel-sachte <rhyme label="a" type="f" corresp="#s2:l1">hantjes</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Waerde <rhyme label="a" type="f" corresp="#s2:l1">Minne-pantjes</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Waerde <rhyme label="a" type="f" corresp="#s2:l1">Minne-pantjes</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ghy bestiert den <rhyme label="b" type="m">dissel-boom</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Van ons liefde met u <rhyme label="b" type="m" corresp="#s2:l5">toom</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">En hebt onder u <rhyme label="c" type="m">geweldt</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Onse sinnetjes <rhyme label="c" type="m" corresp="#s2:l7">gestelt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Roosich-roode <rhyme label="a" type="f">lipjes</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Roosich-roode <rhyme label="a" type="f" corresp="#s3:l1">lipjes</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">U coraele <rhyme label="a" type="f" corresp="#s3:l1">tipjes</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">U coraele <rhyme label="a" type="f" corresp="#s3:l1">tipjes</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Hebben 'k weet niet wat voor <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dat ons zieltjes smelten <rhyme label="b" type="m" corresp="#s3:l5">doet</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">En maer met een kus <rhyme label="c" type="m">geraeckt</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Ons geheel u slaven <rhyme label="c" type="m" corresp="#s3:l7">maeckt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Dus geboeyde <rhyme label="a" type="f">slaven</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Dus geboeyde <rhyme label="a" type="f" corresp="#s4:l1">slaven</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Aen u schoonheydts <rhyme label="a" type="f" corresp="#s4:l1">gaven</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Aen u schoonheydts <rhyme label="a" type="f" corresp="#s4:l1">gaven</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Vinden wy ons meeste <rhyme label="b" type="m">vreughdt</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In ons meeste <rhyme label="b" type="m" corresp="#s4:l5">ongeneught</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Noch en ruylden onse <rhyme label="c" type="m">pijn</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Met geen veyligh vry te <rhyme label="c" type="m" corresp="#s4:l7">zijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Want soo wy niet <rhyme label="a" type="f">missen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Want soo wy niet <rhyme label="a" type="f" corresp="#s5:l1">missen</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">In van u te <rhyme label="a" type="f" corresp="#s5:l1">gissen</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">In van u te <rhyme label="a" type="f" corresp="#s5:l1">gissen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Schuylter gheenen <rhyme label="b" type="m">wrevel-moedt</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Onder sulcken soeten <rhyme label="b" type="m" corresp="#s5:l5">soet</rhyme>.</l>
        <l id="s5:l7" met="4" type="+-">Noch u schoonheydt is geen <rhyme label="c" type="m">baeck</rhyme>.</l>
        <l id="s5:l8" met="4" type="+-">Of van wreetheyd, of van <rhyme label="c" type="m" corresp="#s5:l7">wraeck</rhyme>.</l>
      </lg>
    </lg>
  </text>
