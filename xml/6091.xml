<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6091</id>
    <date>
      <year-start>1632</year-start>
      <year-end>1632</year-end>
    </date>
    <location>
      <orginal-name>emmerik</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>La princesse</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EYlaes! ick mach <rhyme label="a" type="f">beclagen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Mijn druck, mijn droefheyt <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mijn cruys dat 't hert moet <rhyme label="a" type="f" corresp="#s1:l1">dragen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'T welck my mijn liefd' <rhyme label="b" type="m" corresp="#s1:l2">aendoet</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Ick heb gesocht mijn leven <rhyme label="c" type="m">lanck</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Haer met so menigen droeven <rhyme label="c" type="m" corresp="#s1:l5">ganck</rhyme></l>
        <l id="s1:l7" met="2" type="-+">En nu de <rhyme label="d" type="m">doot</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Sterf voor haer, siet liefde <rhyme label="d" type="m" corresp="#s1:l7">groot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Nochtans mijn siel gaet <rhyme label="a" type="f">vluchten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O wat <rhyme label="b" type="m">ondanckbaerheyt</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Van my, versmaet mijn <rhyme label="a" type="f" corresp="#s2:l1">suchten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En al mijn droevich <rhyme label="b" type="m" corresp="#s2:l2">leydt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Is dan vergeefs mijn lijden <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Mijn tranen seer veel int <rhyme label="c" type="m" corresp="#s2:l5">getal</rhyme>.</l>
        <l id="s2:l7" met="2" type="-+">O wat voor <rhyme label="d" type="m">pijn</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Komt my van u siele <rhyme label="d" type="m" corresp="#s2:l7">mijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Siet aen mijn hooft <rhyme label="a" type="f">doorgraven</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Met doornen wonder <rhyme label="b" type="m">groot</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Van onderen tot <rhyme label="a" type="f" corresp="#s3:l1">baven</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Mijn lijf van bloede <rhyme label="b" type="m" corresp="#s3:l2">root</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Dat ick wt liefd' voor u <rhyme label="c" type="m">vergoot</rhyme>.</l>
        <l id="s3:l6" met="4" type="-+">En my gegeven inden <rhyme label="c" type="m" corresp="#s3:l5">doot</rhyme></l>
        <l id="s3:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Mocht toch dit int herte <rhyme label="d" type="m" corresp="#s3:l7">sijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hebt ghy aen mijn <rhyme label="a" type="f">bevonden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Gedroch of <rhyme label="b" type="m">ontrouheyt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Nu of t'eenigen <rhyme label="a" type="f" corresp="#s4:l1">stonden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat ghy soo van my <rhyme label="b" type="m" corresp="#s4:l2">scheyt</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ick had dat noyt van u <rhyme label="c" type="m">verwacht</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Wie had dit oyt van u <rhyme label="c" type="m" corresp="#s4:l5">ghedacht</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Waer is u verstant en <rhyme label="d" type="m" corresp="#s4:l7">sin</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wilt ghy my nu <rhyme label="a" type="f">verlaten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Denckt eens aen uwen <rhyme label="b" type="m">eedt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Waer met ghy my <rhyme label="a" type="f" corresp="#s5:l1">beslaten</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Te dienen als ghy <rhyme label="b" type="m" corresp="#s5:l2">weet</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Int doopsel hebt ghy 's duyvels <rhyme label="c" type="m">prach</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Sijn raedt, en werrelt af <rhyme label="c" type="m" corresp="#s5:l5">ghesaght</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Sal dit dus vergeten <rhyme label="d" type="m" corresp="#s5:l7">sijn</rhyme> ?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Siet hoe ick hier <rhyme label="a" type="f">ghehanghen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn erm' heb wt <rhyme label="b" type="m">ghespreyt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Om u daer in <rhyme label="a" type="f" corresp="#s6:l1">t'ontfanghen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Och waer u hert <rhyme label="b" type="m" corresp="#s6:l2">bereyt</rhyme> !</l>
        <l id="s6:l5" met="4" type="-+">Segt my die werrelt wat <rhyme label="c" type="m">profijt</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Geeft s' u, dat ghy dus van my <rhyme label="c" type="m" corresp="#s6:l5">vliedt</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s6:l8" met="4" type="+-">V verlies aendoet my <rhyme label="d" type="m" corresp="#s6:l7">pijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Och mochte in u <rhyme label="a" type="f">dalen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Een bloedich dropken <rhyme label="b" type="m">root</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Dat ick stort te <rhyme label="a" type="f" corresp="#s7:l1">betalen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Vw'r sonden schulden <rhyme label="b" type="m" corresp="#s7:l2">groot</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Mocht toch u steenich hert een <rhyme label="c" type="m">traen</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Die ick wt mijn oogh' pars <rhyme label="c" type="m" corresp="#s7:l5">ontfaen</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Sal dit al verloren <rhyme label="d" type="m" corresp="#s7:l7">sijn</rhyme> ?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En wilt ghy in mijn <rhyme label="a" type="f">armen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met liefste vluchten <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Soo wilt u selfs <rhyme label="a" type="f" corresp="#s8:l1">erbarmen</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Om u eeuwich <rhyme label="b" type="m" corresp="#s8:l2">verdriet</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Dan ick dien ghy nu dus <rhyme label="c" type="m">versmaedt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Sal branden doen den wille <rhyme label="c" type="m" corresp="#s8:l5">quaet</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">O siele <rhyme label="d" type="m">mijn</rhyme></l>
        <l id="s8:l8" met="4" type="+-">Keert u wil en wilde <rhyme label="d" type="m" corresp="#s8:l7">sin</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
