    <melody/>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ghy die te bouen <rhyme label="a" type="m">gaet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Die schoonheyt der <rhyme label="b" type="f">naturen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Niet te gelycken <rhyme label="a" type="m" corresp="#s1:l1">staet</rhyme></l>
        <l id="s1:l4" met="3" type="-+">By aertsche <rhyme label="b" type="f" corresp="#s1:l2">creaturen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ghy schynt in mynen <rhyme label="c" type="m">sin</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Een hemelsche <rhyme label="c" type="m" corresp="#s1:l5">goddin</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als ick v schoonheyt <rhyme label="a" type="m">sach</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verblinden heel myn <rhyme label="b" type="f">oghen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Als een die niet en <rhyme label="a" type="m" corresp="#s2:l1">mach</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Den sonneschyn <rhyme label="b" type="f" corresp="#s2:l2">gedoghen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Want cranck is ons <rhyme label="c" type="m">gesicht</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Teghen dat hemels <rhyme label="c" type="m" corresp="#s2:l5">licht</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Noch hoop ick metter <rhyme label="a" type="m">spoet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Te comen in v <rhyme label="b" type="f">gracy</rhyme></l>
        <l id="s3:l3" met="3" type="-+">V wesen is so <rhyme label="a" type="m" corresp="#s3:l1">soet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Het dunckt my <rhyme label="b" type="f" corresp="#s3:l2">recoracy</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Swaer lyden valt my <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick neempt oock al int <rhyme label="c" type="m" corresp="#s3:l5">goet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dickwils heb ick myn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Door liefde willen <rhyme label="b" type="f">geuen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Maer ghy liet my in <rhyme label="a" type="m" corresp="#s4:l1">smert</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Troosteloos altyt <rhyme label="b" type="f" corresp="#s4:l2">leuen</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Daerom so sal ick <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Altyt veruult met <rhyme label="c" type="m" corresp="#s4:l5">pyn</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">En weest niet <rhyme label="a" type="m">opstinaet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In lieft wilt v <rhyme label="b" type="f">verneren</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Al spreecken die nyders <rhyme label="a" type="m" corresp="#s5:l1">quaet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Wilt sulcx by v <rhyme label="b" type="f" corresp="#s5:l2">verteren</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Ghy syt myn lief <rhyme label="c" type="m">volmaeckt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Daer myn herte na <rhyme label="c" type="m" corresp="#s5:l5">haeckt</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Princes al moet ick <rhyme label="a" type="m">veel</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Om vwent wille <rhyme label="b" type="f">lyden</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Nochtans sal ick in <rhyme label="a" type="m" corresp="#s6:l1">heel</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Myn tegenspoet <rhyme label="b" type="f" corresp="#s6:l2">verblyden</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Want soet is al <rhyme label="c" type="m">t'verdriet</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Tgheen my van v <rhyme label="c" type="m" corresp="#s6:l5">geschiet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
