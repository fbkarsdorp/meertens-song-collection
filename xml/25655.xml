      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Leenaert Clock</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ick bid u lieve <rhyme label="a" type="f">Heer</rhyme></l>
        <l id="s1:l2" met="3" type="-+">V gunst bewijst doch <rhyme label="b" type="m">my</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Nu in mijn jeucht noch <rhyme label="a" type="f" corresp="#s1:l1">teere</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ick met moede <rhyme label="b" type="m" corresp="#s1:l2">bly</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Mijn hert so mach bereyden <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Mijn hert so mach bereyden <rhyme label="b" type="m" corresp="#s1:l2">vry</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Door u ghenae // vroech ende <rhyme label="c" type="m">spae</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Staet my doch Heere <rhyme label="b" type="m" corresp="#s1:l2">by</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Alle mijn cleyn <rhyme label="a" type="f">vermoghen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is u seer wel <rhyme label="b" type="m">bekent</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Met ghenadighen <rhyme label="a" type="f" corresp="#s2:l1">ooghen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">V stadich tot my <rhyme label="b" type="m" corresp="#s2:l2">went</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Verlicht my door u claerheyt <rhyme label="b" type="m" corresp="#s2:l2">jent</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Verlicht my door u claerheyt <rhyme label="b" type="m" corresp="#s2:l2">jent</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Recht mijn ghemoedt // nae d’eeuwich <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Iae nae des Hemels <rhyme label="b" type="m" corresp="#s2:l2">tent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Neemt acht op al mijn <rhyme label="a" type="f">saken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Mijn kranckheyt doch <rhyme label="b" type="m">gheneest</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Wilt my recht ghesont <rhyme label="a" type="f" corresp="#s3:l1">maken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Door u licht <rhyme label="b" type="m" corresp="#s3:l2">onbevreest</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Leydt my Heer door s’Weerelts <rhyme label="b" type="m" corresp="#s3:l2">foreest</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Leydt my Heer door s’Weerelts <rhyme label="b" type="m" corresp="#s3:l2">foreest</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Door uwe cracht // seer hoogh <rhyme label="c" type="m">gheacht</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Den goeden heyl’ghen <rhyme label="b" type="m" corresp="#s3:l2">Gheest</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Niet en laet my doch <rhyme label="a" type="f">glijden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Maer my doch <rhyme label="b" type="m">onderwijst</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door u ghenae <rhyme label="a" type="f" corresp="#s4:l1">altijden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">My doch soo <rhyme label="b" type="m" corresp="#s4:l2">verjolijst</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">T’is t’ghene dat mijn ziele <rhyme label="b" type="m" corresp="#s4:l2">prijst</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">T’is t’ghene dat mijn ziele <rhyme label="b" type="m" corresp="#s4:l2">prijst</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">In hanghers noot // door t’hemels <rhyme label="c" type="m">broot</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Laetse worden <rhyme label="b" type="m" corresp="#s4:l2">ghespijst</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Een sober gherecht <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Hier te voeren <rhyme label="b" type="m">altijdt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Wilt my lief Heere <rhyme label="a" type="f" corresp="#s5:l1">gheven</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">My soo <rhyme label="b" type="m" corresp="#s5:l2">ghebenedijdt</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Door Christum my bystandich <rhyme label="b" type="m" corresp="#s5:l2">zijt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Door Christum my bystandich <rhyme label="b" type="m" corresp="#s5:l2">zijt</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">In my doch blust // des Weerelts <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Voert ghy voor my den <rhyme label="b" type="m" corresp="#s5:l2">strijt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Geeft my voor alle <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Sterckheyt ende <rhyme label="b" type="m">confoort</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Vwen wil te <rhyme label="a" type="f" corresp="#s6:l1">volbringhen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo my dat <rhyme label="b" type="m" corresp="#s6:l2">toebehoort</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Helpt ons crachtich int wercken <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Helpt ons crachtich int wercken <rhyme label="b" type="m" corresp="#s6:l2">voort</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Op desen padt // naer s’hemels <rhyme label="c" type="m">stadt</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Al voor die enghe <rhyme label="b" type="m" corresp="#s6:l2">poort</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Noch moet ick u Heer <rhyme label="a" type="f">beden</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Al uyt mijns herten <rhyme label="b" type="m">grondt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Siet op my hier <rhyme label="a" type="f" corresp="#s7:l1">beneden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Neemt my in u <rhyme label="b" type="m" corresp="#s7:l2">verbondt</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Verbonden vast tot aller <rhyme label="b" type="m" corresp="#s7:l2">stondt</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Verbonden vast tot aller <rhyme label="b" type="m" corresp="#s7:l2">stondt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Soo sal ick dan // soo veel ick <rhyme label="c" type="m">can</rhyme></l>
        <l id="s7:l8" met="3" type="-+">V goetdaedt maken <rhyme label="b" type="m" corresp="#s7:l2">condt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Tot aen een salich <rhyme label="a" type="f">eynde</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Recht, helpt en my doch <rhyme label="b" type="m">leydt</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Waer ick my keer oft <rhyme label="a" type="f" corresp="#s8:l1">weynde</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met uwen troost <rhyme label="b" type="m" corresp="#s8:l2">bereydt</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Draeght sorgh voor my, niet en <rhyme label="b" type="m" corresp="#s8:l2">verbeyt</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Draeght sorgh voor my, niet en <rhyme label="b" type="m" corresp="#s8:l2">verbeyt</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Wat my ontbreeckt // vervult <rhyme label="c" type="m">perfeckt</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Door u <rhyme label="b" type="m" corresp="#s8:l2">barmherticheyt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Niemandt werdt oyt <rhyme label="a" type="f">bevonden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Beschaemt, verlaten <rhyme label="b" type="m">siet</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Die hier t’eenighen <rhyme label="a" type="f" corresp="#s9:l1">stonden</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Hem vast op u <rhyme label="b" type="m" corresp="#s9:l2">verliet</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">V ghenade over ons <rhyme label="b" type="m" corresp="#s9:l2">giet</rhyme>,</l>
        <l id="s9:l6" met="4" type="-+">V ghenade over ons <rhyme label="b" type="m" corresp="#s9:l2">giet</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">T’is my ghenoech // spa ende <rhyme label="c" type="m">vroech</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Als ghy u handt my <rhyme label="b" type="m" corresp="#s9:l2">biet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Instort my u <rhyme label="a" type="f">ghenade</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Overvloeyende <rhyme label="b" type="m">seer</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Dat ick nae uwen <rhyme label="a" type="f" corresp="#s10:l1">rade</rhyme></l>
        <l id="s10:l4" met="3" type="-+">In u heylsame <rhyme label="b" type="m" corresp="#s10:l2">leer</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">My nu mach schicken altijdt <rhyme label="b" type="m" corresp="#s10:l2">meer</rhyme>,</l>
        <l id="s10:l6" met="4" type="-+">My nu mach schicken altijdt <rhyme label="b" type="m" corresp="#s10:l2">meer</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">V soo tot danck // mijn leven <rhyme label="c" type="m">lanck</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Dats mijne herten <rhyme label="b" type="m" corresp="#s10:l2">begeer</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Sluytlijck lief Heer int <rhyme label="a" type="f">leste</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Nu het ten eynde <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Keert al mijn saeck int <rhyme label="a" type="f" corresp="#s11:l1">beste</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En my voortaen <rhyme label="b" type="m" corresp="#s11:l2">beraet</rhyme>:</l>
        <l id="s11:l5" met="4" type="-+">Gheeft my den wil, volbrenght die <rhyme label="b" type="m" corresp="#s11:l2">daet</rhyme>,</l>
        <l id="s11:l6" met="4" type="-+">Gheeft my den wil, volbrenght die <rhyme label="b" type="m" corresp="#s11:l2">daet</rhyme>,</l>
        <l id="s11:l7" met="4" type="-+">Ghenadich weest // my <rhyme label="c" type="m">aldermeest</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Tot mijner zielen <rhyme label="b" type="m" corresp="#s11:l2">baet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
