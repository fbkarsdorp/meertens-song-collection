<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>159141</id>
    <date>
      <year-start>1770</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Les sept sauts</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Iuffertjes diehondjes <rhyme label="a" type="f">streelen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">En die kussen, ‘k wou dat <rhyme label="b" type="m">ik</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Was u hondje ‘k gaf met <rhyme label="b" type="m" corresp="#s1:l2">schik</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Voor elk een hondje u een <rhyme label="b" type="m" corresp="#s1:l2">lik</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Waf, waf, waf, waf, waf, <rhyme label="c" type="m">waf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Soetertjes die de Poesjes <rhyme label="a" type="f">aeijen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Langs haer zagt Fluweele <rhyme label="b" type="m">haer</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">‘k Wou dat ik u kaeter <rhyme label="b" type="m" corresp="#s2:l2">waer</rhyme>:</l>
        <l id="s2:l4" met="4" type="+-">‘k Sou nooit krabben! Neen ik <rhyme label="b" type="m" corresp="#s2:l2">riep</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Miauw, miauw, miauw, <rhyme label="c" type="m">miauw</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Streelstertjes van de <rhyme label="a" type="f">Papagaetje</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Set my ook eens in de <rhyme label="b" type="m">houw</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">‘k Roep gedurig mogt ik <rhyme label="b" type="m" corresp="#s3:l2">jouw</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Zoeke Papje! kus de <rhyme label="b" type="m" corresp="#s3:l2">Vrouw</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Kopje krouw, Kopje <rhyme label="c" type="m">krouw</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Liefjes die ‘t Parkietje <rhyme label="a" type="f">voeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Uit u mond met zopjes <rhyme label="b" type="m">wyn</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Als ik u Parkiet mag <rhyme label="b" type="m" corresp="#s4:l2">zyn</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ik verzopte al u <rhyme label="b" type="m" corresp="#s4:l2">Chagryn</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Parkie, parkie, parkie, <rhyme label="c" type="m">parkie</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Minnaers van <rhyme label="a" type="f">Vinkjes</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Neemt voor Vinkjes my slegts <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">‘k Zal dan fluyten singen <rhyme label="b" type="m" corresp="#s5:l2">slaen</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Trots de beste uit de <rhyme label="b" type="m" corresp="#s5:l2">baen</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Vink, vink, vink, vink, vink, <rhyme label="c" type="m">vink</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Ach! Lief hebstertjes van <rhyme label="a" type="f">Duyfjes</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">‘k Wou dat ik u Doffer <rhyme label="b" type="m">waer</rhyme>;</l>
        <l id="s6:l3" met="4" type="+-">‘k Tuymelde als een <rhyme label="b" type="m" corresp="#s6:l2">Tuymelaer</rhyme></l>
        <l id="s6:l4" met="4" type="+-">In u Zagten schoot, ‘k riep <rhyme label="b" type="m" corresp="#s6:l2">daer</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Ouwae! Ouwae! Ouwae! <rhyme label="c" type="m">Ouwae</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Meisjes die graeg Eentjes <rhyme label="a" type="f">houden</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Als gy my tot woon <rhyme label="b" type="m">verkoos</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Ik vermaekte me <rhyme label="b" type="m" corresp="#s7:l2">altoos</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">In uw kammetje in ‘t <rhyme label="b" type="m" corresp="#s7:l2">kroos</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Kwah, kwah, kwah, <rhyme label="c" type="m">kwah</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
