        <l id="s6:l4" met="3" type="-+">Om u hem dick <rhyme label="b" type="m" corresp="#s6:l2">verblijt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Want ghy zijt mijn lief <rhyme label="c" type="m">alleen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Want ghy zijt mijn lief <rhyme label="c" type="m" corresp="#s6:l5">alleen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Mijn troost tot aller <rhyme label="d" type="m">stont</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Mijn troost tot aller <rhyme label="d" type="m" corresp="#s6:l7">stont</rhyme>,</l>
