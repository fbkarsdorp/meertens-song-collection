        <l id="s2:l10" met="3" type="-+">Dus vertroost u <rhyme label="d" type="m" corresp="#s2:l7">dienaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ben ick schoon lief van <rhyme label="a" type="m">hier</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Soo vergeet u dienaer <rhyme label="b" type="m">niet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Of ghy brenght in <rhyme label="a" type="m" corresp="#s3:l1">dangier</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Mijn hert, in swaer <rhyme label="b" type="m" corresp="#s3:l2">verdriet</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Ghestadigh en <rhyme label="c" type="m">ghetrou</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ghestadigh en <rhyme label="c" type="m" corresp="#s3:l5">ghetrou</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want ghy zijt mijn lief <rhyme label="d" type="m">alleen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Want ghy zijt mijn lief <rhyme label="d" type="m" corresp="#s3:l7">alleen</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">O suyver <rhyme label="c" type="m" corresp="#s3:l5">karsou</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Daer alle mijn troost op <rhyme label="d" type="m" corresp="#s3:l7">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
