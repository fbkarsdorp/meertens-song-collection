        <l id="s5:l4" met="3" type="-+">Om u dick <rhyme label="b" type="m" corresp="#s5:l2">verheught</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ghedenckt schoon lief aen <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Ghedenckt schoon lief aen <rhyme label="c" type="m" corresp="#s5:l5">my</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Want mijn hert dat <rhyme label="d" type="m">blaeckt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Want mijn hert dat <rhyme label="d" type="m" corresp="#s5:l7">blaeckt</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Na u uyt liefden <rhyme label="c" type="m" corresp="#s5:l5">vry</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">'t Welck door u blijschap <rhyme label="d" type="m" corresp="#s5:l7">smaeckt</rhyme>.</l>
      </lg>
