        <l id="s4:l3" met="3" type="-+">Den tijt met smert <rhyme label="a" type="m" corresp="#s4:l1">verwacht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">In lijden en <rhyme label="b" type="m" corresp="#s4:l2">dangier</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat ick o Venus <rhyme label="c" type="m">dier</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat ick o Venus <rhyme label="c" type="m" corresp="#s4:l5">dier</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Met u lief in <rhyme label="d" type="m">eendracht</rhyme>,</l>
