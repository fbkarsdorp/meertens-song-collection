        <l id="s1:l8" met="3" type="-+">Om u schoon suyver <rhyme label="d" type="m" corresp="#s1:l7">greyn</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Tot u liefste <rhyme label="c" type="m" corresp="#s1:l5">jent</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Staet mijn toevlucht <rhyme label="d" type="m" corresp="#s1:l7">alleyn</rhyme>.</l>
      </lg>
