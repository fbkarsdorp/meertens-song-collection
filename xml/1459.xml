        <l id="s2:l3" met="3" type="-+">Reyn deughtsaem van <rhyme label="a" type="m" corresp="#s2:l1">aert</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sijt ghy vol alder <rhyme label="b" type="m" corresp="#s2:l2">eer</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Mijn bede doch <rhyme label="c" type="m">aenhoort</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mijn bede doch <rhyme label="c" type="m" corresp="#s2:l5">aenhoort</rhyme>,</l>
