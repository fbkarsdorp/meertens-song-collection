        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Van zeven dochters zaan</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Tot u kom ick <rhyme label="a" type="m">ghevlucht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Wt s'weereldts boos <rhyme label="a" type="m" corresp="#s1:l1">gherucht</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Mijn Bruydegom Godt <rhyme label="b" type="f">almachtich</rhyme></l>
        <l id="s1:l4" met="3" type="-+">U graci stort in <rhyme label="c" type="m">my</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Inwendich maeckt my <rhyme label="c" type="m" corresp="#s1:l4">bly</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">G'hebt my ghetoghen <rhyme label="b" type="f" corresp="#s1:l3">crachtich</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al s'weereldts <rhyme label="a" type="m">pompery</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Heb ik versmaeydt om <rhyme label="a" type="m" corresp="#s2:l1">dy</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Wilt my nu oock <rhyme label="b" type="f">beschencken</rhyme></l>
        <l id="s2:l4" met="3" type="-+">V heyligh dierbaer <rhyme label="c" type="m">bloet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Laet strecken tot een <rhyme label="c" type="m" corresp="#s2:l4">boet</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Mijn sonden wilt niet <rhyme label="b" type="f" corresp="#s2:l3">ghedencken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Laet my woonen in <rhyme label="a" type="m">u</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ende vry wesen <rhyme label="a" type="m" corresp="#s3:l1">nu</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Van alle aerdtsche <rhyme label="b" type="f">saecken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Een hert puer ende <rhyme label="c" type="m">reyn</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Dat schenck ick u <rhyme label="c" type="m" corresp="#s3:l4">alleyn</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">In liefde doet my <rhyme label="b" type="f" corresp="#s3:l3">blaecken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In u Wijnkelders <rhyme label="a" type="m">saen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Laet ons te samen <rhyme label="a" type="m" corresp="#s4:l1">gaen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En seght waer ghy wilt <rhyme label="b" type="f">rusten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Inden soeten <rhyme label="c" type="m">middagh</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Doet my hier van <rhyme label="c" type="m" corresp="#s4:l4">ghewagh</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ick haeck naer u <rhyme label="b" type="f" corresp="#s4:l3">wellusten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn ziele is <rhyme label="a" type="m">ontstelt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En haer van liefde <rhyme label="a" type="m" corresp="#s5:l1">quelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">O brandt! der <rhyme label="b" type="f">Seraphinnen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Mijn hert en mijn <rhyme label="c" type="m">ghemoet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Drijft door der minnen <rhyme label="c" type="m" corresp="#s5:l4">gloet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wilt tot my komen <rhyme label="b" type="f" corresp="#s5:l3">binnen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Comt schoon <rhyme label="a" type="m">Dryvuldicheydt</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Mijn hert u toch <rhyme label="a" type="m" corresp="#s6:l1">verbeydt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Een Godt en dry <rhyme label="b" type="f">persoonen</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Laet my strijden <rhyme label="c" type="m">valliandt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hier in dit aerdtsche <rhyme label="c" type="m" corresp="#s6:l4">landt</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Op dat ghy my moght <rhyme label="b" type="f" corresp="#s6:l3">croonen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
