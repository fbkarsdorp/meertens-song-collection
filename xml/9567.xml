    <tags>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody>Psalm 130 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VVAnneer ghy hebt <rhyme label="a" type="f">ghe-eeten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">En wel verzadight <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Soo moet ghy niet <rhyme label="a" type="f" corresp="#s1:l1">vergeeten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Des Heeren, maer <rhyme label="b" type="m" corresp="#s1:l2">verblijt</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hem dancken, om 't <rhyme label="c" type="f">verzaden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van hem aen u <rhyme label="d" type="m">ghedaen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En nimmermeer <rhyme label="c" type="f" corresp="#s1:l5">versmaden</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Sijn Wet te nemen <rhyme label="d" type="m" corresp="#s1:l6">aen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Heer, Hemelsche <rhyme label="a" type="f">Vader</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Nu seggen wy u <rhyme label="b" type="m">danck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dat ghy ons <rhyme label="a" type="f" corresp="#s2:l1">allegader</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verzaedt met spijs en <rhyme label="b" type="m" corresp="#s2:l2">dranck</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Wy dancken daer <rhyme label="c" type="f">beneden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dat ghy ons <rhyme label="d" type="m">weder-baert</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En tot een beter <rhyme label="c" type="f" corresp="#s2:l5">leven</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Genadelijck <rhyme label="d" type="m" corresp="#s2:l6">bewaert</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wy bidden u, o <rhyme label="a" type="f">Heere</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Dat ghy van 't aerdtsche <rhyme label="b" type="m">goedt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Wilt af, en hene <rhyme label="a" type="f" corresp="#s3:l1">keeren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ten Hemel ons <rhyme label="b" type="m" corresp="#s3:l2">gemoedt</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Verwachtend' uwen <rhyme label="c" type="f">Sone</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Die weder komen <rhyme label="d" type="m">sal</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En heer'lijck hem <rhyme label="c" type="f" corresp="#s3:l5">vertoonen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Verlosser van ons <rhyme label="d" type="m" corresp="#s3:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
