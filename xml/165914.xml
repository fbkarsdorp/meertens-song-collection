    <tags>
      <tag translation="New Year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Laat ons van de Heer gaan zingen</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Werelt wilt toch recht <rhyme label="a" type="f">aenmercken</rhyme></l>
        <l id="s1:l2" met="4" type="+-">godes wercken nu ter <rhyme label="b" type="m">tyt</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Bethlehem veracht bij <rhyme label="a" type="f" corresp="#s1:l1">stercken</rhyme></l>
        <l id="s1:l4" met="4" type="+-">t hooft der kercken trou <rhyme label="b" type="m" corresp="#s1:l2">bevrijt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">al t'welck veel vyanden <rhyme label="b" type="m" corresp="#s1:l2">spijt</rhyme></l>
        <l id="s1:l6" met="4" type="+-">uijt haet en nijt zynde te <rhyme label="c" type="m">velt</rhyme></l>
        <l id="s1:l7" met="4" type="+-">heer by nachten twachten <rhyme label="b" type="m" corresp="#s1:l2">slyt</rhyme></l>
        <l id="s1:l8" met="4" type="+-">voor oostende t'schenden <rhyme label="c" type="m" corresp="#s1:l6">spelt</rhyme></l>
        <l id="s1:l9" met="4" type="+-">dat voor Sonden wort <rhyme label="d" type="m">bevonden</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Beter straff dan <rhyme label="c" type="m" corresp="#s1:l6">gelt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Herodes Faem wort noch <rhyme label="a" type="f">gepresen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">t' godt loos wezen blijft Int <rhyme label="b" type="m">hooft</rhyme></l>
        <l id="s2:l3" met="4" type="+-">kinder moort zeer staet te <rhyme label="a" type="f" corresp="#s2:l1">vresen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">van die t'lesen acht t <rhyme label="b" type="m" corresp="#s2:l2">stoft</rhyme></l>
        <l id="s2:l5" met="4" type="-+">als gods woort en Eeuwich <rhyme label="b" type="m" corresp="#s2:l2">loft</rhyme></l>
        <l id="s2:l6" met="4" type="+-">hier van weet off den <rhyme label="c" type="m">antechrist</rhyme></l>
        <l id="s2:l7" met="4" type="+-">doet bedriegen ,, liegen <rhyme label="b" type="m" corresp="#s2:l2">groft</rhyme></l>
        <l id="s2:l8" met="4" type="+-">En berechten, slechten <rhyme label="c" type="m" corresp="#s2:l6">twist</rhyme></l>
        <l id="s2:l9" met="4" type="+-">Laetste dagen zullen <rhyme label="d" type="m">Dragen</rhyme></l>
        <l id="s2:l10" met="3" type="+-">wech des duijvels <rhyme label="c" type="m" corresp="#s2:l6">list</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Den Prince Int hoochst <rhyme label="a" type="f">geseeten</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Doet uijt meeten zyn <rhyme label="b" type="m">goetheijt</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Emanuel en vreucht om <rhyme label="a" type="f" corresp="#s3:l1">weeten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Byden Propheeten voor <rhyme label="b" type="m" corresp="#s3:l2">geseyt</rhyme></l>
        <l id="s3:l5" met="4" type="-+">en zyn vrede wyt <rhyme label="b" type="m" corresp="#s3:l2">verspreyt</rhyme></l>
        <l id="s3:l6" met="4" type="+-">waer In dat leyt den besten <rhyme label="c" type="m">raet</rhyme></l>
        <l id="s3:l7" met="4" type="+-">Om geloven t'boven <rhyme label="b" type="m" corresp="#s3:l2">scheijt</rhyme></l>
        <l id="s3:l8" met="4" type="+-">en deur t sterven erven <rhyme label="c" type="m" corresp="#s3:l6">baet</rhyme></l>
        <l id="s3:l9" met="4" type="+-">wilt te degen over <rhyme label="d" type="m">wegen</rhyme></l>
        <l id="s3:l10" met="3" type="+-">T broot des levens <rhyme label="c" type="m" corresp="#s3:l6">Daet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
