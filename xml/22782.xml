<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22782</id>
    <date>
      <year-start>1590</year-start>
      <year-end>1600</year-end>
    </date>
    <tags>
      <tag standard-name="geuzenlied" translation="Geuzen song">geuzenlied</tag>
      <tag standard-name="acrostichon" translation="acrostic song">acrostichon</tag>
    </tags>
    <melody>Wilhelmus</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Mauricius van <rhyme label="a" type="f">Nassouwen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">ben ick van Duytschen <rhyme label="b" type="m">bloet</rhyme>/</l>
        <l id="s1:l3" met="3" type="-+">Ons Vaderlandt <rhyme label="a" type="f" corresp="#s1:l1">getrouwe</rhyme>/</l>
        <l id="s1:l4" met="3" type="-+">blijf ich in Gods <rhyme label="b" type="m" corresp="#s1:l2">behoet</rhyme>/</l>
        <l id="s1:l5" met="3" type="-+">Mijnes Heer Vaders <rhyme label="c" type="f">gangen</rhyme>/</l>
        <l id="s1:l6" met="3" type="-+">volg' ic vry metter <rhyme label="d" type="m">daet</rhyme>/</l>
        <l id="s1:l7" met="3" type="-+">Den vyandt te <rhyme label="c" type="f" corresp="#s1:l5">verstrangen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">in zijnen loosen <rhyme label="d" type="m" corresp="#s1:l6">raet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al mijnen lust <rhyme label="a" type="f">gepresen</rhyme>/</l>
        <l id="s2:l2" met="3" type="-+">geweest is vast <rhyme label="b" type="m">doorgaens</rhyme>/</l>
        <l id="s2:l3" met="3" type="-+">Hoe ick doch mocht <rhyme label="a" type="f" corresp="#s2:l1">genesen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">die daer zijn soo goet <rhyme label="b" type="m" corresp="#s2:l2">Spaens</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">O lieve <rhyme label="c" type="f">Partriotten</rhyme>/</l>
        <l id="s2:l6" met="3" type="-+">hoe blijf dy dus <rhyme label="d" type="m">verblint</rhyme>?</l>
        <l id="s2:l7" met="3" type="-+">Dat ghy/ die met u <rhyme label="c" type="f" corresp="#s2:l5">spotten</rhyme>/</l>
        <l id="s2:l8" met="3" type="-+">noch alsoo seer <rhyme label="d" type="m" corresp="#s2:l6">ghesindt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">U mach doch wel <rhyme label="a" type="f">ghedencken</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">hoe sy tot aller <rhyme label="b" type="m">stondt</rhyme>/</l>
        <l id="s3:l3" met="3" type="-+">Ghesocht hebben te <rhyme label="a" type="f" corresp="#s3:l1">krencken</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">die schoon Landt inden <rhyme label="b" type="m" corresp="#s3:l2">grondt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">All ons trouwe <rhyme label="c" type="f">Voor-standers</rhyme></l>
        <l id="s3:l6" met="3" type="-+">zijn by haer gantz <rhyme label="d" type="m">verdacht</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hoe veel kloecke <rhyme label="c" type="f" corresp="#s3:l5">Neerlanders</rhyme></l>
        <l id="s3:l8" met="3" type="-+">zijnder ter doot <rhyme label="d" type="m" corresp="#s3:l6">ghebracht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Roem op mijn tonghe <rhyme label="a" type="f">reyne</rhyme>/</l>
        <l id="s4:l2" met="3" type="-+">hier van wilt doen <rhyme label="b" type="m">ghewach</rhyme>/</l>
        <l id="s4:l3" met="3" type="-+">Doe menich Edel <rhyme label="a" type="f" corresp="#s4:l1">greyne</rhyme>/</l>
        <l id="s4:l4" met="3" type="-+">weerdich van groot <rhyme label="b" type="m" corresp="#s4:l2">beclach</rhyme>/</l>
        <l id="s4:l5" met="3" type="-+">Zijn van dit wreet <rhyme label="c" type="f">ghebroetsel</rhyme>/</l>
        <l id="s4:l6" met="3" type="-+">dit listich <rhyme label="d" type="m">Basilisch</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Als oft het waer haer <rhyme label="c" type="f" corresp="#s4:l5">voetsel</rhyme>/</l>
        <l id="s4:l8" met="3" type="-+">verslonden <rhyme label="d" type="m" corresp="#s4:l6">venijnisch</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ick en weet waer <rhyme label="a" type="f">beghinnen</rhyme>/</l>
        <l id="s5:l2" met="3" type="-+">waer sal ick vanghen <rhyme label="b" type="m">aen</rhyme>?</l>
        <l id="s5:l3" met="3" type="-+">Al mijn verstant mijn <rhyme label="a" type="f" corresp="#s5:l1">sinnen</rhyme>/</l>
        <l id="s5:l4" met="3" type="-+">my nu gantzlick <rhyme label="b" type="m" corresp="#s5:l2">ontgaen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Wel op Marquis van <rhyme label="c" type="f">Berghen</rhyme>/</l>
        <l id="s5:l6" met="3" type="-+">Baroen van <rhyme label="d" type="m">Montigni</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Ken can niet langher <rhyme label="c" type="f" corresp="#s5:l5">berghen</rhyme>/</l>
        <l id="s5:l8" met="3" type="-+">wat ick nu gheschien <rhyme label="d" type="m" corresp="#s5:l6">sie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Comend' in <rhyme label="a" type="f">Spangnen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">om te vertooghen den <rhyme label="b" type="m">Coninck</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Hoet hier te <rhyme label="a" type="f" corresp="#s6:l1">land'</rhyme></l>
        <l id="s6:l4" met="3" type="-+">allomme gestelt was/ en <rhyme label="b" type="m" corresp="#s6:l2">toeginck</rhyme>/</l>
        <l id="s6:l5" met="3" type="-+">Beyde moesten sy <rhyme label="c" type="f">treuren</rhyme>/</l>
        <l id="s6:l6" met="3" type="-+">helaes! den bittren <rhyme label="d" type="m">doot</rhyme>/</l>
        <l id="s6:l7" met="3" type="-+">Ten mocht haer niet <rhyme label="c" type="f" corresp="#s6:l5">ghebeuren</rhyme>/</l>
        <l id="s6:l8" met="3" type="-+">weder te keeren <rhyme label="d" type="m" corresp="#s6:l6">oyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Is dit gheweest <rhyme label="a" type="f">d'beghin</rhyme>/</l>
        <l id="s7:l2" met="3" type="-+">van te storten d'Edel <rhyme label="b" type="m">bloet</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Sal oock sConincx <rhyme label="a" type="f" corresp="#s7:l1">ghesin</rhyme>/</l>
        <l id="s7:l4" met="3" type="-+">self hier van niet zijn <rhyme label="b" type="m" corresp="#s7:l2">behoet</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">O Karel Prins van <rhyme label="c" type="f">Spangnen</rhyme>/</l>
        <l id="s7:l6" met="3" type="-+">sConinc Phlips eenich <rhyme label="d" type="m">soon</rhyme>/</l>
        <l id="s7:l7" met="3" type="-+">Wat ist doch voor <rhyme label="c" type="f" corresp="#s7:l5">calangne</rhyme>/</l>
        <l id="s7:l8" met="3" type="-+">die u niet en <rhyme label="d" type="m" corresp="#s7:l6">verschoon</rhyme>?</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">U ghemoet wel <rhyme label="a" type="f">gheneghen</rhyme>/</l>
        <l id="s8:l2" met="3" type="-+">tot dit Nederlandts <rhyme label="b" type="m">Prieel</rhyme>/</l>
        <l id="s8:l3" met="3" type="-+">Ist dat daerom moest <rhyme label="a" type="f" corresp="#s8:l1">sneven</rhyme>/</l>
        <l id="s8:l4" met="3" type="-+">een soo Nobel <rhyme label="b" type="m" corresp="#s8:l2">Juweel</rhyme>?</l>
        <l id="s8:l5" met="3" type="-+">Denct nu ghy <rhyme label="c" type="f">Patriotten</rhyme>/</l>
        <l id="s8:l6" met="3" type="-+">wat U te wachten <rhyme label="d" type="m">staet</rhyme>/</l>
        <l id="s8:l7" met="3" type="-+">Nu men oock alsoo <rhyme label="c" type="f" corresp="#s8:l5">totten</rhyme>/</l>
        <l id="s8:l8" met="3" type="-+">Sone toe selfs om <rhyme label="d" type="m" corresp="#s8:l6">gaet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Sulcx is daer nae <rhyme label="a" type="f">bewesen</rhyme>/</l>
        <l id="s9:l2" met="3" type="-+">als menich <rhyme label="b" type="m">Edel-man</rhyme>/</l>
        <l id="s9:l3" met="3" type="-+">Te Brussel <rhyme label="a" type="f" corresp="#s9:l1">onverwesen</rhyme>/</l>
        <l id="s9:l4" met="3" type="-+">in Pincxter weeck om <rhyme label="b" type="m" corresp="#s9:l2">quam</rhyme>:</l>
        <l id="s9:l5" met="3" type="-+">Van Batenborch <rhyme label="c" type="f">ghepresen</rhyme>/</l>
        <l id="s9:l6" met="3" type="-+">twee broeders wel <rhyme label="d" type="m">befaemt</rhyme>/</l>
        <l id="s9:l7" met="3" type="-+">Moesten voor al haer <rhyme label="c" type="f" corresp="#s9:l5">leven</rhyme>/</l>
        <l id="s9:l8" met="3" type="-+">verliesen <rhyme label="d" type="m" corresp="#s9:l6">onbetaemt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Van Egmont en van <rhyme label="a" type="f">Hoorne</rhyme>/</l>
        <l id="s10:l2" met="3" type="-+">O Edel Graven <rhyme label="b" type="m">fier</rhyme>/</l>
        <l id="s10:l3" met="3" type="-+">Doodt u oock sConincx <rhyme label="a" type="f" corresp="#s10:l1">toorne</rhyme>/</l>
        <l id="s10:l4" met="3" type="-+">die hiet soo <rhyme label="b" type="m" corresp="#s10:l2">goedertier</rhyme>?</l>
        <l id="s10:l5" met="3" type="-+">Waer blijven u <rhyme label="c" type="f">Victorien</rhyme>/</l>
        <l id="s10:l6" met="3" type="-+">Egmont/ u daden <rhyme label="d" type="m">stout</rhyme>/</l>
        <l id="s10:l7" met="3" type="-+">Is dat tot een <rhyme label="c" type="f" corresp="#s10:l5">memori</rhyme></l>
        <l id="s10:l8" met="3" type="-+">uwer diensten <rhyme label="d" type="m" corresp="#s10:l6">menichfout</rhyme>?</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Alle ghy andre <rhyme label="a" type="f">Mannen</rhyme>/</l>
        <l id="s11:l2" met="3" type="-+">die cloeck en <rhyme label="b" type="m">onvertzaecht</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Van dese felle <rhyme label="a" type="f" corresp="#s11:l1">tanden</rhyme>/</l>
        <l id="s11:l4" met="3" type="-+">vernielt zijt, oft <rhyme label="b" type="m" corresp="#s11:l2">verjaecht</rhyme>:</l>
        <l id="s11:l5" met="3" type="-+">Om dat ghy 'tslandts <rhyme label="c" type="f">welvaren</rhyme>/</l>
        <l id="s11:l6" met="3" type="-+">soo ghetroulick voor <rhyme label="d" type="m">staet</rhyme>/</l>
        <l id="s11:l7" met="3" type="-+">Uwe Namen <rhyme label="c" type="f" corresp="#s11:l5">verclaren</rhyme>/</l>
        <l id="s11:l8" met="3" type="-+">uwen Loff vroech end <rhyme label="d" type="m" corresp="#s11:l6">'spaet</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Nemmermeer en sal <rhyme label="a" type="f">sterven</rhyme>/</l>
        <l id="s12:l2" met="3" type="-+">uwen roem noch u <rhyme label="b" type="m">eer</rhyme>/</l>
        <l id="s12:l3" met="3" type="-+">Maer doorgaens suldy <rhyme label="a" type="f" corresp="#s12:l1">erven</rhyme>/</l>
        <l id="s12:l4" met="3" type="-+">Loff en Prijs langs soo <rhyme label="b" type="m" corresp="#s12:l2">meer</rhyme>/</l>
        <l id="s12:l5" met="3" type="-+">Ghy vrome dappre <rhyme label="c" type="f">Helden</rhyme>,</l>
        <l id="s12:l6" met="3" type="-+">die cloeck en <rhyme label="d" type="m">onvervaert</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Door 't Vaderlandt u <rhyme label="c" type="f" corresp="#s12:l5">selven</rhyme>/</l>
        <l id="s12:l8" met="3" type="-+">lijf/ goet/ noch bloet en <rhyme label="d" type="m" corresp="#s12:l6">spaert</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Nassouw isch Huys <rhyme label="a" type="f">ghepresen</rhyme>/</l>
        <l id="s13:l2" met="3" type="-+">U moet ick boven <rhyme label="b" type="m">all</rhyme>/</l>
        <l id="s13:l3" met="3" type="-+">Loff, prijs end' Eere <rhyme label="a" type="f" corresp="#s13:l1">gheven</rhyme>/</l>
        <l id="s13:l4" met="3" type="-+">in dit gants aertsche <rhyme label="b" type="m" corresp="#s13:l2">dall</rhyme>.</l>
        <l id="s13:l5" met="3" type="-+">Van Keyserlicken <rhyme label="c" type="f">Stamme</rhyme>/</l>
        <l id="s13:l6" met="3" type="-+">doorluchtich gants soo <rhyme label="d" type="m">seer</rhyme>/</l>
        <l id="s13:l7" met="3" type="-+">Verschijnt nu als een <rhyme label="c" type="f" corresp="#s13:l5">vlamme</rhyme>/</l>
        <l id="s13:l8" met="3" type="-+">lofflick uwes Naems <rhyme label="d" type="m" corresp="#s13:l6">eer</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Als u Gheslacht <rhyme label="a" type="f">Hoochweerdich</rhyme>/</l>
        <l id="s14:l2" met="3" type="-+">sien wy cloeck ende <rhyme label="b" type="m">stout</rhyme>/</l>
        <l id="s14:l3" met="3" type="-+">Sich op t' offren <rhyme label="a" type="f" corresp="#s14:l1">volveerdich</rhyme>/</l>
        <l id="s14:l4" met="3" type="-+">tot 'tsVaderlandts <rhyme label="b" type="m" corresp="#s14:l2">behout</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Graeff Adolff die is <rhyme label="c" type="f">bleven</rhyme>/</l>
        <l id="s14:l6" met="3" type="-+">in Vrieslandt inden <rhyme label="d" type="m">slach</rhyme>:</l>
        <l id="s14:l7" met="3" type="-+">Graeff Lodwijck <rhyme label="c" type="f" corresp="#s14:l5">hoock-verheven</rhyme>/</l>
        <l id="s14:l8" met="3" type="-+">op Moker-heyde <rhyme label="d" type="m" corresp="#s14:l6">lach</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Sijn metghesellen <rhyme label="a" type="f">waren</rhyme>/</l>
        <l id="s15:l2" met="3" type="-+">Henrick de Jonghe <rhyme label="b" type="m">Graeff</rhyme>/</l>
        <l id="s15:l3" met="3" type="-+">Met hem tself <rhyme label="a" type="f" corresp="#s15:l1">wedervaren</rhyme></l>
        <l id="s15:l4" met="3" type="-+">is Christoffel <rhyme label="b" type="m" corresp="#s15:l2">Palsgraeff</rhyme>/</l>
        <l id="s15:l5" met="3" type="-+">Ghelijcmen oock Graeff <rhyme label="c" type="f">Phlips</rhyme>/</l>
        <l id="s15:l6" met="3" type="-+">als een strijtbaer <rhyme label="d" type="m">Campioen</rhyme>/</l>
        <l id="s15:l7" met="3" type="-+">Ghesien heeft byder <rhyme label="c" type="f" corresp="#s15:l5">lip</rhyme>/</l>
        <l id="s15:l8" met="3" type="-+">sodanighen offer <rhyme label="d" type="m" corresp="#s15:l6">doen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Sou men niet segghen <rhyme label="a" type="f">t'waer</rhyme>/</l>
        <l id="s16:l2" met="3" type="-+">soo ghenoch vuyt eenen <rhyme label="b" type="m">Stam</rhyme>/</l>
        <l id="s16:l3" met="3" type="-+">Om versaedt te zijn <rhyme label="a" type="f" corresp="#s16:l1">maer</rhyme>/</l>
        <l id="s16:l4" met="3" type="-+">noch een ander moester <rhyme label="b" type="m" corresp="#s16:l2">an</rhyme>/</l>
        <l id="s16:l5" met="3" type="-+">Guilelmus van <rhyme label="c" type="f">Nassouwen</rhyme>/</l>
        <l id="s16:l6" met="3" type="-+">sulck Edel Prince <rhyme label="d" type="m">wijs</rhyme></l>
        <l id="s16:l7" met="3" type="-+">Moest ons oock doen <rhyme label="c" type="f" corresp="#s16:l5">onthouwen</rhyme>/</l>
        <l id="s16:l8" met="3" type="-+">het moordadich <rhyme label="d" type="m" corresp="#s16:l6">advijs</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">O Vader hooch <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">des Vaderlandts soo <rhyme label="b" type="m">goet</rhyme>/</l>
        <l id="s17:l3" met="3" type="-+">Gheensins hebdy u <rhyme label="a" type="f" corresp="#s17:l1">leven</rhyme></l>
        <l id="s17:l4" met="3" type="-+">verschoont tot ons <rhyme label="b" type="m" corresp="#s17:l2">behoet</rhyme>.</l>
        <l id="s17:l5" met="3" type="-+">Laet nu O God <rhyme label="c" type="f">Almachtich</rhyme>/</l>
        <l id="s17:l6" met="3" type="-+">den Spaenschen hooghen <rhyme label="d" type="m">moet</rhyme>/</l>
        <l id="s17:l7" met="3" type="-+">Eens sincken/ die ghy <rhyme label="c" type="f" corresp="#s17:l5">crachtich</rhyme>/</l>
        <l id="s17:l8" met="3" type="-+">de grootse vallen <rhyme label="d" type="m" corresp="#s17:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Wilt ons voort-aen <rhyme label="a" type="f">behoeden</rhyme>/</l>
        <l id="s18:l2" met="3" type="-+">voor haer listen so <rhyme label="b" type="m">valsch</rhyme>/</l>
        <l id="s18:l3" met="3" type="-+">En met ons al de <rhyme label="a" type="f" corresp="#s18:l1">goede</rhyme></l>
        <l id="s18:l4" met="3" type="-+">voorstanders Duytsch en <rhyme label="b" type="m" corresp="#s18:l2">Walsch</rhyme>.</l>
        <l id="s18:l5" met="3" type="-+">Wy dancken u met <rhyme label="c" type="f">vreuchden</rhyme>/</l>
        <l id="s18:l6" met="3" type="-+">in dit Nederlandts <rhyme label="d" type="m">pleyn</rhyme>:</l>
        <l id="s18:l7" met="3" type="-+">Want ghy wilt ons in <rhyme label="c" type="f" corresp="#s18:l5">deuchden</rhyme></l>
        <l id="s18:l8" met="3" type="-+">beschermen <rhyme label="d" type="m" corresp="#s18:l6">alghemeyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
