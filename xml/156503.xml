    <melody>Si vous ne voulez me gu√©rir</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEen kusjens soeter als den <rhyme label="a" type="m">dauw</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Heb ick mijn leven oyt <rhyme label="b" type="f">genooten</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Als 'er zijn <rhyme label="b" type="f" corresp="#s1:l2">gesprooten</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Willich van mijn <rhyme label="a" type="m" corresp="#s1:l1">Vrouw</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Marij die alderschoonste <rhyme label="c" type="m">Maeght</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Die oyt natura baerd', of aerde <rhyme label="c" type="m" corresp="#s1:l5">draeght</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Doen ick mijn neder <rhyme label="d" type="m">booch</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">En uyt haer lipjes <rhyme label="d" type="m" corresp="#s1:l7">sooch</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Stracx maeckten wy daer een <rhyme label="a" type="m">verbondt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Om in ons liefd' getrouw te <rhyme label="b" type="f">blijven</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Kusjens deden 't <rhyme label="b" type="f" corresp="#s2:l2">stijven</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Voor haer corale <rhyme label="a" type="m" corresp="#s2:l1">mondt</rhyme>;</l>
        <l id="s2:l5" met="4" type="-+">Ick gaf de lipjes (nat van <rhyme label="c" type="m">douw</rhyme>)</l>
        <l id="s2:l6" met="5" type="-+">Veel kusjens, soo veel als ick selver <rhyme label="c" type="m" corresp="#s2:l5">wouw</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ia boven duysent <rhyme label="d" type="m">veer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Sy gafse daet'lijck <rhyme label="d" type="m" corresp="#s2:l7">we'er</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'K beken, sy heeft voldaen 't <rhyme label="a" type="m">getal</rhyme>:</l>
        <l id="s3:l2" met="4" type="-+">Maer nimmer sal men konnen <rhyme label="b" type="f">vinden</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Dat van twee <rhyme label="b" type="f" corresp="#s3:l2">beminden</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Venus 't prijsen <rhyme label="a" type="m" corresp="#s3:l1">sal</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Want min wil nimmer zijn <rhyme label="c" type="m">gedult</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Dat kusjes werden met getal <rhyme label="c" type="m" corresp="#s3:l5">vervult</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Want 't is te vaste <rhyme label="d" type="m">wet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van Venus <rhyme label="d" type="m" corresp="#s3:l7">ingeset</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Seght, waerom telt ghy juyst soo <rhyme label="a" type="m">nauw</rhyme></l>
        <l id="s4:l2" met="4" type="-+">De kusjes, en telt niet de <rhyme label="b" type="f">tranen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Die langhs 't aensicht <rhyme label="b" type="f" corresp="#s4:l2">banen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Als de <rhyme label="a" type="m" corresp="#s4:l1">Morgen-dauw</rhyme>?</l>
        <l id="s4:l5" met="4" type="-+">En sooje nu de traentjes <rhyme label="c" type="m">telt</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Telt kusjes me'e: maer sooje 't anders <rhyme label="c" type="m" corresp="#s4:l5">stelt</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Meet kusjes met geen <rhyme label="d" type="m">maet</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Van die in 't hertje <rhyme label="d" type="m" corresp="#s4:l7">staet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
