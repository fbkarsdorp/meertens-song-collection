  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAeckt uwen weghe <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Dit seyt ons Godt den <rhyme label="b" type="f">Heere</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Op dat ick my met v <rhyme label="b" type="f" corresp="#s1:l2">bekeere</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Al sonder teghen <rhyme label="a" type="m" corresp="#s1:l1">spoet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Al door die minne <rhyme label="a" type="m" corresp="#s1:l1">gloet</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Siet dat ghy <rhyme label="a" type="m" corresp="#s1:l1">doet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Uwen naesten duecht en <rhyme label="b" type="f" corresp="#s1:l2">eere</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Grijpt eenen <rhyme label="a" type="m" corresp="#s1:l1">moet</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Worpt v vlees onder de <rhyme label="a" type="m" corresp="#s1:l1">voet</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Ghy wort <rhyme label="a" type="m" corresp="#s1:l1">behoet</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Als vaders kinderen <rhyme label="b" type="f" corresp="#s1:l2">teere</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Het is Gods <rhyme label="b" type="f" corresp="#s1:l2">leere</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Want Christus <rhyme label="a" type="m" corresp="#s1:l1">bloet</rhyme></l>
        <l id="s1:l14" met="3" type="-+">Helpt ons wt allen <rhyme label="b" type="f" corresp="#s1:l2">seeren</rhyme></l>
        <l id="s1:l15" met="3" type="-+">Sijn lof//laet ons int <rhyme label="b" type="f" corresp="#s1:l2">grof//vermeeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Clopt met vruechden <rhyme label="a" type="m">stout</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Aen Christum die dore <rhyme label="b" type="f">ghepresen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Die duer zijn stem met hem wilt <rhyme label="b" type="f" corresp="#s2:l2">wesen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Sijt ghy <rhyme label="a" type="m" corresp="#s2:l1">benout</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Met sonden <rhyme label="a" type="m" corresp="#s2:l1">menichfout</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Godts liefde <rhyme label="a" type="m" corresp="#s2:l1">aenhoudt</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Hy sal ons wel <rhyme label="b" type="f" corresp="#s2:l2">ghenesen</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Den desem <rhyme label="a" type="m" corresp="#s2:l1">schout</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Waer duer Gods woort <rhyme label="a" type="m" corresp="#s2:l1">verkout</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Wert ghy <rhyme label="a" type="m" corresp="#s2:l1">verdout</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Al duer het swets <rhyme label="b" type="f" corresp="#s2:l2">wesen</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Wilt Gods woort <rhyme label="b" type="f" corresp="#s2:l2">lesen</rhyme></l>
        <l id="s2:l13" met="2" type="-+">Dus Christum <rhyme label="a" type="m" corresp="#s2:l1">betrout</rhyme></l>
        <l id="s2:l14" met="3" type="-+">Bemint zijn woort ten <rhyme label="b" type="f" corresp="#s2:l2">desen</rhyme></l>
        <l id="s2:l15" met="3" type="-+">Sijn licht is <rhyme label="b" type="f" corresp="#s2:l2">claer//voorwaer//gheresen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Prinselijck <rhyme label="a" type="m">medecijn</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die elcken can <rhyme label="b" type="f">cureren</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Al die ghy siet hem yet <rhyme label="b" type="f" corresp="#s3:l2">verueeren</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Als herder <rhyme label="a" type="m" corresp="#s3:l1">fijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Huet ghy dat schaepken <rhyme label="a" type="m" corresp="#s3:l1">dijn</rhyme></l>
        <l id="s3:l6" met="2" type="-+">Met groote <rhyme label="a" type="m" corresp="#s3:l1">pijn</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dwelck ghy saecht <rhyme label="b" type="f" corresp="#s3:l2">abuseren</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Op cort <rhyme label="a" type="m" corresp="#s3:l1">termijn</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Maeckt ghy van water <rhyme label="a" type="m" corresp="#s3:l1">wijn</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Sduyuels <rhyme label="a" type="m" corresp="#s3:l1">fenijn</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Cunt ghy wel <rhyme label="b" type="f" corresp="#s3:l2">inclineren</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Die doot wel <rhyme label="b" type="f" corresp="#s3:l2">refonderen</rhyme></l>
        <l id="s3:l13" met="2" type="-+">Want Godts <rhyme label="a" type="m" corresp="#s3:l1">aenschijn</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Moet eewelijck <rhyme label="b" type="f" corresp="#s3:l2">glorieeren</rhyme></l>
        <l id="s3:l15" met="3" type="-+">Dit slot//in Godt//moet eewich <rhyme label="b" type="f" corresp="#s3:l2">triumpheren</rhyme>.</l>
      </lg>
    </lg>
  </text>
