<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20403</id>
    <date>
      <year-start>1647</year-start>
      <year-end>1647</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Allemande amoureus</melody>
  <author>B. Targier, Anthony Jansen</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WIl yemandt hem <rhyme label="a" type="f">vermeyen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En eens sijn Lief uyt <rhyme label="a" type="f" corresp="#s1:l1">leyen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die hoeft niet langh te <rhyme label="a" type="f" corresp="#s1:l1">beyen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Hy kan wel sonder <rhyme label="b" type="f">gelden</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Sijn Oogh met lusten <rhyme label="a" type="f" corresp="#s1:l1">weyen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In d'Haerlemsche <rhyme label="a" type="f" corresp="#s1:l1">Contreyen</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Want daer vindtmen <rhyme label="a" type="f" corresp="#s1:l1">verscheyen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hoven Bosschen, en <rhyme label="b" type="f" corresp="#s1:l4">Velden</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Begeeft u maer nae 't <rhyme label="c" type="m">zuydt</rhyme></l>
        <l id="s1:l10" met="3" type="-+">De kleyne Hout-poort <rhyme label="c" type="m" corresp="#s1:l9">uyt</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Den Hovenier <rhyme label="c" type="m" corresp="#s1:l9">ontsluyt</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Sijn Hof vol Blom, en <rhyme label="c" type="m" corresp="#s1:l9">Kruyt</rhyme>:</l>
        <l id="s1:l13" met="3" type="-+">Spreeckt hem aen met goe <rhyme label="d" type="f">reden</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">So meught ghy vry in <rhyme label="d" type="f" corresp="#s1:l13">treden</rhyme>;</l>
        <l id="s1:l15" met="3" type="-+">Schoone <rhyme label="d" type="f" corresp="#s1:l13">vermaeck'lijckheden</rhyme></l>
        <l id="s1:l16" met="2" type="-+">Men daer <rhyme label="e" type="m">aensiet</rhyme>,</l>
        <l id="s1:l17" met="3" type="-+">Sonder te gelden <rhyme label="e" type="m" corresp="#s1:l16">yet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Fruyt-boomen hoogh <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Staen daer, die vruchten <rhyme label="a" type="f" corresp="#s2:l1">geven</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot voedtsel van ons <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">En ginder een <rhyme label="b" type="f">Fonteyne</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">De Tortel-duyfjes <rhyme label="a" type="f" corresp="#s2:l1">even</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Zietmen daer t'samen <rhyme label="a" type="f" corresp="#s2:l1">sweven</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Konijntjes daer <rhyme label="a" type="f" corresp="#s2:l1">beneven</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die springhen groot en <rhyme label="b" type="f" corresp="#s2:l4">kleyne</rhyme>.</l>
        <l id="s2:l9" met="3" type="-+">Pleysierigh staet, dunckt <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Dees' groene <rhyme label="c" type="m" corresp="#s2:l9">Gaeldery</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Met Haghen aen de <rhyme label="c" type="m" corresp="#s2:l9">zy</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Kruyden, Bloempjes daer <rhyme label="c" type="m" corresp="#s2:l9">by</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Anemia, <rhyme label="d" type="f">Lourieren</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Roo Roosen, <rhyme label="d" type="f" corresp="#s2:l13">Eg'lantieren</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Lelyen <rhyme label="d" type="f" corresp="#s2:l13">Anjelieren</rhyme>,</l>
        <l id="s2:l16" met="2" type="-+">Staen daer <rhyme label="e" type="m">gheplant</rhyme>,</l>
        <l id="s2:l17" met="3" type="-+">In dees' Hoven <rhyme label="e" type="m" corresp="#s2:l16">playsant</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doch wy willen dees <rhyme label="a" type="f">dinghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hier al op 't nauwst niet <rhyme label="a" type="f" corresp="#s3:l1">singhen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maer sullen verder <rhyme label="a" type="f" corresp="#s3:l1">springhen</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">De Hoven laet ick <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Op dat wy niet en <rhyme label="a" type="f" corresp="#s3:l1">dwinghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Den Wandelaer, maer <rhyme label="a" type="f" corresp="#s3:l1">bringhen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Hem, daer wy onlanghs <rhyme label="a" type="f" corresp="#s3:l1">ginghen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En seer ghenoegh'lijck <rhyme label="b" type="f" corresp="#s3:l4">waren</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">In 't groen bebladert <rhyme label="c" type="m">Wout</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Ghenaemt 't Haerlemmer <rhyme label="c" type="m" corresp="#s3:l9">Hout</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Het gras seer nat <rhyme label="c" type="m" corresp="#s3:l9">bedouwt</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Was wel een weynigh <rhyme label="c" type="m" corresp="#s3:l9">kout</rhyme>:</l>
        <l id="s3:l13" met="3" type="-+">Maer Phoebus heete <rhyme label="d" type="f">stralen</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Die quamen daer op <rhyme label="d" type="f" corresp="#s3:l13">dalen</rhyme>,</l>
        <l id="s3:l15" met="3" type="-+">En al den douw <rhyme label="d" type="f" corresp="#s3:l13">afhalen</rhyme>:</l>
        <l id="s3:l16" met="2" type="-+">Dies ick ne'er <rhyme label="e" type="m">sat</rhyme>,</l>
        <l id="s3:l17" met="3" type="-+">Om my te rusten <rhyme label="e" type="m" corresp="#s3:l16">wat</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ick socht met veel <rhyme label="a" type="f">pracktijcken</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hoe men best sou doen <rhyme label="a" type="f" corresp="#s4:l1">blijcken</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Door konst van <rhyme label="a" type="f" corresp="#s4:l1">Rethorijcken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Waer meest vermaeck mocht <rhyme label="b" type="f">wesen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Voor hem en sijns <rhyme label="a" type="f" corresp="#s4:l1">gelijcken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die gaern' wand'len en <rhyme label="a" type="f" corresp="#s4:l1">kijcken</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Door Bosschen, Berghen, <rhyme label="a" type="f" corresp="#s4:l1">Dijcken</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Sonder gelden in <rhyme label="b" type="f" corresp="#s4:l4">desen</rhyme>;</l>
        <l id="s4:l9" met="3" type="-+">Ick dacht, sy hebben <rhyme label="c" type="m">hier</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Wel aldermeest <rhyme label="c" type="m" corresp="#s4:l9">pleysier</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">In dit Suyder <rhyme label="c" type="m" corresp="#s4:l9">quartier</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Want daer vermeyden <rhyme label="c" type="m" corresp="#s4:l9">schier</rhyme></l>
        <l id="s4:l13" met="3" type="-+">Meest al de Burgers <rhyme label="d" type="f">goedigh</rhyme>,</l>
        <l id="s4:l14" met="3" type="-+">Jae 't Volck komt <rhyme label="d" type="f" corresp="#s4:l13">overvloedigh</rhyme>,</l>
        <l id="s4:l15" met="3" type="-+">Vyt ander Steden <rhyme label="d" type="f" corresp="#s4:l13">spoedigh</rhyme>,</l>
        <l id="s4:l16" met="2" type="-+">Om derwaerts <rhyme label="e" type="m">aen</rhyme></l>
        <l id="s4:l17" met="3" type="-+">Nae 't Haerlems Bosch te <rhyme label="e" type="m" corresp="#s4:l16">gaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wijd'-uytghebreyde <rhyme label="a" type="f">Linden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En Elst is daer te <rhyme label="a" type="f" corresp="#s5:l1">vinden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Om schuylen voor de <rhyme label="a" type="f" corresp="#s5:l1">Winden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">En voor de hitt' der <rhyme label="b" type="f">Sonnen</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ghy die u wilt <rhyme label="a" type="f" corresp="#s5:l1">verbinden</rhyme></l>
        <l id="s5:l6" met="3" type="-+">In Trouw, met u <rhyme label="a" type="f" corresp="#s5:l1">Beminde</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Komt hier met u <rhyme label="a" type="f" corresp="#s5:l1">Lerinde</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Den Leeuw'rick heeft <rhyme label="b" type="f" corresp="#s5:l4">begonnen</rhyme></l>
        <l id="s5:l9" met="3" type="-+">Sijn sangh, maer <rhyme label="c" type="m">principael</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Hoortmen den <rhyme label="c" type="m" corresp="#s5:l9">Nachtegael</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Die met sijn helder <rhyme label="c" type="m" corresp="#s5:l9">tael</rhyme></l>
        <l id="s5:l12" met="3" type="-+">De Vogels <rhyme label="c" type="m" corresp="#s5:l9">altemael</rhyme></l>
        <l id="s5:l13" met="3" type="-+">Seer verre gaet te <rhyme label="d" type="f">boven</rhyme>,</l>
        <l id="s5:l14" met="3" type="-+">In Bosschen, Velden, <rhyme label="d" type="f" corresp="#s5:l13">Hoven</rhyme>,</l>
        <l id="s5:l15" met="3" type="-+">Wil hy haer sangh <rhyme label="d" type="f" corresp="#s5:l13">verdoven</rhyme>:</l>
        <l id="s5:l16" met="2" type="-+">Komt hoort, siet <rhyme label="e" type="m">ghy</rhyme>,</l>
        <l id="s5:l17" met="3" type="-+">Blijft hier van gelden <rhyme label="e" type="m" corresp="#s5:l16">vry</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
