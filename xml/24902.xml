  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vor alre ghenouchte <rhyme label="a" type="m">vroylicheit</rhyme></l>
        <l id="s1:l2" met="2" type="-+">Up minen <rhyme label="a" type="m" corresp="#s1:l1">heit</rhyme></l>
        <l id="s1:l3" met="2" type="-+">So lieft mir <rhyme label="b" type="m">bas</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Eens minlics hertzen <rhyme label="a" type="m" corresp="#s1:l1">ommecleit</rhyme></l>
        <l id="s1:l5" met="2" type="-+">Daer trauwe aen <rhyme label="a" type="m" corresp="#s1:l1">steit</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Dies nie <rhyme label="b" type="m" corresp="#s1:l3">vergas</rhyme></l>
        <l id="s1:l7" met="4" type="+-">Reinlic zin daer stede in <rhyme label="b" type="m" corresp="#s1:l3">was</rhyme></l>
        <l id="s1:l8" met="4" type="-+">O stede dijn werde <rhyme label="a" type="m" corresp="#s1:l1">moghentheit</rhyme></l>
        <l id="s1:l9" met="2" type="-+">Die es so <rhyme label="a" type="m" corresp="#s1:l1">breit</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Dats nie <rhyme label="b" type="m" corresp="#s1:l3">ghenas</rhyme></l>
        <l id="s1:l11" met="4" type="+-">Sin die dijns lonens dienst <rhyme label="a" type="m" corresp="#s1:l1">ontseit</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Dats waer <rhyme label="a" type="m" corresp="#s1:l1">gheseit</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vrauwe vul alre <rhyme label="a" type="m">reinicheit</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Hets recht <rhyme label="a" type="m" corresp="#s2:l1">besceit</rhyme></l>
        <l id="s2:l3" met="2" type="-+">Ghelooft mir <rhyme label="b" type="m">das</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Dat steide in dinen zinne <rhyme label="a" type="m" corresp="#s2:l1">leit</rhyme></l>
        <l id="s2:l5" met="2" type="-+">Ich bems <rhyme label="a" type="m" corresp="#s2:l1">bereit</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Met willen <rhyme label="b" type="m" corresp="#s2:l3">ras</rhyme></l>
        <l id="s2:l7" met="4" type="+-">Onstede es broosscher vele dan <rhyme label="b" type="m" corresp="#s2:l3">glas</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Vor alre ghenouchte <rhyme label="a" type="m" corresp="#s2:l1">vroylicheit</rhyme></l>
        <l id="s2:l9" met="2" type="-+">Up minen <rhyme label="a" type="m" corresp="#s2:l1">heit</rhyme></l>
        <l id="s2:l10" met="2" type="-+">So lieft mir <rhyme label="b" type="m" corresp="#s2:l3">bas</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Eens minlics hertzen <rhyme label="a" type="m" corresp="#s2:l1">ommecleit</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Daer trauwe aen <rhyme label="a" type="m" corresp="#s2:l1">steit</rhyme></l>
      </lg>
    </lg>
  </text>
