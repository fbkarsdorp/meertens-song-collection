    <tags/>
    <melody>Psalm 081 Datheen</melody>
  <author>Jan van Eijk</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">Heer! wat zijt Gij <rhyme label="a" type="m">goed</rhyme>!</l>
        <l id="s1:l2" met="3" type="+-">'k Wil, ik zal U <rhyme label="b" type="f">prijzen</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Die mij draagt en <rhyme label="a" type="m" corresp="#s1:l1">hoedt</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Van het werken <rhyme label="c" type="m">moê</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Sloot ik de oogen <rhyme label="c" type="m" corresp="#s1:l4">toe</rhyme>.</l>
        <l id="s1:l6" met="3" type="+-">En zie 't licht weêr <rhyme label="b" type="f" corresp="#s1:l2">rijzen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Weerloos in den <rhyme label="a" type="m">nood</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Die mij kon <rhyme label="b" type="f">genaaken</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Was 'k als leevend <rhyme label="a" type="m" corresp="#s2:l1">dood</rhyme>.</l>
        <l id="s2:l4" met="3" type="+-">'k Lag en sliep <rhyme label="c" type="m">gerust</rhyme></l>
        <l id="s2:l5" met="3" type="+-">Van geen zorg <rhyme label="c" type="m" corresp="#s2:l4">bewust</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Gij woudt voor mij <rhyme label="b" type="f" corresp="#s2:l2">waaken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Ziekte dieven, <rhyme label="a" type="m">brand</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Duizend <rhyme label="b" type="f">ongevallen</rhyme></l>
        <l id="s3:l3" met="3" type="+-">Weerde Uw goede <rhyme label="a" type="m" corresp="#s3:l1">hand</rhyme>;</l>
        <l id="s3:l4" met="3" type="+-">Hoedde mensch en <rhyme label="c" type="m">vee</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">Huis en Schuuren <rhyme label="c" type="m" corresp="#s3:l4">meê</rhyme>;</l>
        <l id="s3:l6" met="3" type="+-">Zorgde voor ons <rhyme label="b" type="f" corresp="#s3:l2">allen</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">ô, Wat zijt Gij <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Vader van mijn <rhyme label="b" type="f">leeven</rhyme>!</l>
        <l id="s4:l3" met="3" type="+-">'k Wil, ik zal, ik <rhyme label="a" type="m" corresp="#s4:l1">moet</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">In den <rhyme label="c" type="m">morgenstond</rhyme>,</l>
        <l id="s4:l5" met="3" type="+-">U, met hart en <rhyme label="c" type="m" corresp="#s4:l4">mond</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Dauk en eere <rhyme label="b" type="f" corresp="#s4:l2">geeven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
