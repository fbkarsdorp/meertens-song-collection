<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>162882</id>
    <date>
      <year-start>1700</year-start>
      <year-end>1700</year-end>
    </date>
    <tags/>
    <melody>Truitje mijn zoete engelin</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Truytje mijn soete <rhyme label="a" type="m">Engelin</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gy zijt die geen die ik <rhyme label="a" type="m" corresp="#s1:l1">bemin</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Og laet mijn maer een Kus van u <rhyme label="b" type="f">genieten</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">En dat door al de Minne <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Laet Cupidootje in u <rhyme label="c" type="m" corresp="#s1:l4">hert</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sijn Minne pijlen <rhyme label="b" type="f" corresp="#s1:l3">schieten</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Truytje mijn <rhyme label="d" type="m">Lief</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Wie hoor ik klagen voor mijn <rhyme label="a" type="m">deur</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Mijn dunkt het is mijn <rhyme label="a" type="m" corresp="#s2:l1">Serviteur</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Hy soekt een lonkje van mijn min te <rhyme label="b" type="f">ontfange</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Jonkman ey segt mijn voor het <rhyme label="c" type="m">minst</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Al watter is tot uwen <rhyme label="c" type="m" corresp="#s2:l4">dienst</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Waer na is uw <rhyme label="b" type="f" corresp="#s2:l3">verlangen</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Frisse <rhyme label="d" type="m">gezel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Princesse van het aerts <rhyme label="a" type="m">Tonneel</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Ag had ik u maer tot mijn <rhyme label="a" type="m" corresp="#s3:l1">deel</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Voor u is al mijn sugten, klagen, <rhyme label="b" type="f">kermen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Helpt u Minnaer uyt de <rhyme label="c" type="m">nood</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hy zal u trouw zijn tot der <rhyme label="c" type="m" corresp="#s3:l4">dood</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Ey wilt u dog <rhyme label="b" type="f" corresp="#s3:l3">ontfermen</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Truytje mijn <rhyme label="d" type="m">lief</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Al de Jonkmans <rhyme label="a" type="m">getrouwigheyd</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En duurd niet langer als men <rhyme label="a" type="m" corresp="#s4:l1">vrijd</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Maer als getrout is en komt uyt de <rhyme label="b" type="f">Kerken</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Dan is 't jou helse <rhyme label="c" type="m">Duyvelin</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Jou magere Teef, en jou <rhyme label="c" type="m" corresp="#s4:l4">vuylin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Jou pekel hoer jou <rhyme label="b" type="f" corresp="#s4:l3">Verken</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">Stinkende <rhyme label="d" type="m">hond</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Eer sal verduystere de <rhyme label="a" type="m">Maen</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En ook de woeste Zee droog <rhyme label="a" type="m" corresp="#s5:l1">staen</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Eer ik op Trou mijn Lief u sal <rhyme label="b" type="f">veragten</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">O neen mijn alderliefste <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Helpt u Minnaer uyt de <rhyme label="c" type="m" corresp="#s5:l4">pijn</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Stelt al uyt u <rhyme label="b" type="f" corresp="#s5:l3">gedagten</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Truytje mijn <rhyme label="d" type="m">Lief</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Jonkman nu is het al <rhyme label="a" type="m">gedaen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Komt laet ons samen heenen <rhyme label="a" type="m" corresp="#s6:l1">gaen</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Daer is op trou mijn Lief en wel <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Door mijn Minnaers <rhyme label="c" type="m">getrouwigheyd</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Heb ik mijn selve nu <rhyme label="c" type="m" corresp="#s6:l4">bereyd</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Om mijn te gaen <rhyme label="b" type="f" corresp="#s6:l3">verbinden</rhyme></l>
        <l id="s6:l7" met="2" type="-+">Wellekom <rhyme label="d" type="m">vrind</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Ag Liefste Lief tot <rhyme label="a" type="m">onderpant</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Komt geeft mijn maer u Regter <rhyme label="a" type="m" corresp="#s7:l1">hand</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Op dat ik u een kus daer op mag <rhyme label="b" type="f">geven</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Jonkman als het is in <rhyme label="c" type="m">fatsoen</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Als Bruyd en Bruygom samen <rhyme label="c" type="m" corresp="#s7:l4">doen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Laet ons in Vreugden <rhyme label="b" type="f" corresp="#s7:l3">leven</rhyme>,</l>
        <l id="s7:l7" met="2" type="-+">Wellekom <rhyme label="d" type="m">vrind</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
