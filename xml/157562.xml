<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157562</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach schoonste nimf aanzie</melody>
  <author>Jacob Coenraetsz Mayvogel</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">HEer goedertier, in goetheyt <rhyme label="a" type="f">uytgelesen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe heerlijck is u <rhyme label="b" type="m">throon</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Gy zyt een reyn, een eeuwigh suyver <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In schoonheyt <rhyme label="b" type="m" corresp="#s1:l2">over-schoon</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Ghy schept, o Heer een lieflijck <rhyme label="c" type="f">wel-behagen</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">In reyn en suyver selschap <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Die haer altoos oprecht en <rhyme label="d" type="m" corresp="#s1:l6">goet</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Deughtsamigh <rhyme label="c" type="f" corresp="#s1:l5">dragen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Wy bidden u, leyt ons op uwe <rhyme label="a" type="f">wegen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Geeft dat geen vreemt <rhyme label="b" type="m">gedaent</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Ons doch bekoort, of 't herte maeckt <rhyme label="a" type="f" corresp="#s2:l1">genegen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Geen snoode wegh en <rhyme label="b" type="m" corresp="#s2:l2">baent</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Om in ontucht of quade <rhyme label="c" type="f">dartelheden</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Te leven, en met soete <rhyme label="d" type="m">vreught</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">In ydelheyt en <rhyme label="d" type="m" corresp="#s2:l6">ongeneught</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Ons tijt <rhyme label="c" type="f" corresp="#s2:l5">besteden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Geeft dat wy Heer in suyverheyt <rhyme label="a" type="f">verkeeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En op u wegen <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">d' Onkuysse lust gestadigh van ons <rhyme label="a" type="f" corresp="#s3:l1">weeren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met Ioseph <rhyme label="b" type="m" corresp="#s3:l2">wederstaen</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Het boose quaet, dat wy door u <rhyme label="c" type="f">genade</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Des werelts hooghmoedt, staet, en <rhyme label="d" type="m">pracht</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Met al het geen men sondigh <rhyme label="d" type="m" corresp="#s3:l6">acht</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Altoos <rhyme label="c" type="f" corresp="#s3:l5">versmaden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Soo ghy, o Vorst door u <rhyme label="a" type="f">voorsichtigheden</rhyme></l>
        <l id="s4:l2" met="3" type="-+">My tot den <rhyme label="b" type="m">Echten-Staet</rhyme></l>
        <l id="s4:l3" met="5" type="-+">Beroept, soo gunt u segen-rijcke <rhyme label="a" type="f" corresp="#s4:l1">vrede</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Tot vreught my vroegh en <rhyme label="b" type="m" corresp="#s4:l2">spaed</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">En dat ick Heer daer in mach deughtsaem <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Ghy zyt mijn troost, ick roep u <rhyme label="d" type="m">an</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Wilt my, o Heer een deughtsaem <rhyme label="d" type="m" corresp="#s4:l6">man</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Op aerden <rhyme label="c" type="f" corresp="#s4:l5">geven</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">De rechte trouw komt uyt den Hemel <rhyme label="a" type="f">dalen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ghy kent des menschen <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Bewaert ons Heer voor alle droeve <rhyme label="a" type="f" corresp="#s5:l1">qualen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Voor Huys-verdriet, voor <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Wilt ons altoos met uwen geest <rhyme label="c" type="f">regeeren</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Het zy in tegenspoet of <rhyme label="d" type="m">vreught</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Ons alles door de ware <rhyme label="d" type="m" corresp="#s5:l6">deught</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Ten besten <rhyme label="c" type="f" corresp="#s5:l5">keeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
