  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Sa Vrienden komt in 't <rhyme label="a" type="f">ronden</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Wat ik breng voor den <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Geschied en <rhyme label="a" type="f" corresp="#s1:l1">ondervonden</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">'t Was een <rhyme label="b" type="m" corresp="#s1:l2">Visiet-dag</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">'t Zal dit niet <rhyme label="c" type="f">verzwygen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ziet dat gy dit wel <rhyme label="d" type="m">vat</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Van zes-en-dertig <rhyme label="c" type="f" corresp="#s1:l5">Wyven</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Waren dronken en <rhyme label="d" type="m" corresp="#s1:l6">zat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Men zag die Vrouwtjes <rhyme label="a" type="f">zwieren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gy wilt die zaek <rhyme label="b" type="m">verstaen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Een Viesiet-dag te <rhyme label="a" type="f" corresp="#s2:l1">vieren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daer was een Vrouw in de <rhyme label="b" type="m" corresp="#s2:l2">kraem</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Het was maer om te <rhyme label="c" type="f">smullen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gesoden en <rhyme label="d" type="m">gebraed</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En alle viese <rhyme label="c" type="f" corresp="#s2:l5">grillen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Komen daer van op <rhyme label="d" type="m" corresp="#s2:l6">straet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Die Vrouwtjes waren <rhyme label="a" type="f">binnen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Schier van verstand <rhyme label="b" type="m">beroofd</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Een Falie die zy daer <rhyme label="a" type="f" corresp="#s3:l1">hingen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Een Vrouw daer op het <rhyme label="b" type="m" corresp="#s3:l2">hoofd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die Vrouw als eenen <rhyme label="c" type="f">vromen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zy daer de schone <rhyme label="d" type="m">was</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met eenen stoel <rhyme label="c" type="f" corresp="#s3:l5">geklommen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Sat boven op de <rhyme label="d" type="m" corresp="#s3:l6">kas</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een vot tusschen haar <rhyme label="a" type="f">tanden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Zo als men een kindje <rhyme label="b" type="m">geeft</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">'t Was als een <rhyme label="a" type="f" corresp="#s4:l1">Offerhande</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Nooit schoonder klugt <rhyme label="b" type="m" corresp="#s4:l2">geweest</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Dan gingen zy aan <rhyme label="c" type="f">dansen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Zy riepen op <rhyme label="d" type="m">Sinjoor</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Van viva zo de <rhyme label="c" type="f" corresp="#s4:l5">Fransen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ieder deed zyn <rhyme label="d" type="m" corresp="#s4:l6">devoir</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maar twee drie jonge <rhyme label="a" type="f">knapen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Die kwamen die al te <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Met vreugde en <rhyme label="a" type="f" corresp="#s5:l1">vermaken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Sprongen te kamer <rhyme label="b" type="m" corresp="#s5:l2">in</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Iacobus zonder <rhyme label="c" type="f">spreeken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Vist in de Koffi <rhyme label="d" type="m">moor</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En 't buur had <rhyme label="c" type="f" corresp="#s5:l5">uitgeseken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dompten als eenen <rhyme label="d" type="m" corresp="#s5:l6">smoor</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Vroutjes aan het <rhyme label="a" type="f">drinken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Zy riepen also <rhyme label="b" type="m">ras</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ik laat een ieder <rhyme label="a" type="f" corresp="#s6:l1">denken</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">Van die Verkens <rhyme label="b" type="m" corresp="#s6:l2">bras</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Zo en geeft hy my geen <rhyme label="c" type="f">wonder</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die Vrouwtjes zagen <rhyme label="d" type="m">vies</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Zy riepen wel wat <rhyme label="c" type="f" corresp="#s6:l5">drommel</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Maar 't was al Menschen <rhyme label="d" type="m" corresp="#s6:l6">pis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Die wyven aan <rhyme label="a" type="f">tempeesten</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Het wiert daar als een <rhyme label="b" type="m">hel</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zy zagen bleek als <rhyme label="a" type="f" corresp="#s7:l1">geesten</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Vielen daer op zyn <rhyme label="b" type="m" corresp="#s7:l2">vel</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zy hielpen daar <rhyme label="c" type="f">malkander</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Greepen Iacobus <rhyme label="d" type="m">aan</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">De een riep tegen de <rhyme label="c" type="f" corresp="#s7:l5">ander</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Zyn broek wierd <rhyme label="d" type="m" corresp="#s7:l6">afgedaan</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Maar Iacobus gy moet <rhyme label="a" type="f">weeten</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Gy wierd daar <rhyme label="b" type="m">overmand</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Ia nog zyt gy <rhyme label="a" type="f" corresp="#s8:l1">gesmeeten</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Tot op het <rhyme label="b" type="m" corresp="#s8:l2">Ledekant</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">En klesten op u <rhyme label="c" type="f">kaken</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">'t Was agter op u <rhyme label="d" type="m">bil</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Maer al die and're <rhyme label="c" type="f" corresp="#s8:l5">zaken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Hoord wat ik zeggen <rhyme label="d" type="m" corresp="#s8:l6">wil</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Wy moeten hem nog <rhyme label="a" type="f">uitligten</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Al met een brandende <rhyme label="b" type="m">kaers</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Zy riepen zonder <rhyme label="a" type="f" corresp="#s9:l1">zwigten</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Van agter aen zynen <rhyme label="b" type="m" corresp="#s9:l2">naers</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Met het hemslip in de <rhyme label="c" type="f">handen</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Zo kreeg hy daer zyn <rhyme label="d" type="m">straf</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Geen vlugger in ons <rhyme label="c" type="f" corresp="#s9:l5">landen</rhyme>,</l>
        <l id="s9:l8" met="3" type="-+">Snyd hem de kamer <rhyme label="d" type="m" corresp="#s9:l6">af</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Hy was zo vol <rhyme label="a" type="f">rouragie</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">En bidden om <rhyme label="b" type="m">pardon</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Hy riep dan niet als <rhyme label="a" type="f" corresp="#s10:l1">gratie</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Dan kwam hy uit <rhyme label="b" type="m" corresp="#s10:l2">prison</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Liep op de straet al <rhyme label="c" type="f">klagen</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Wel ag wat droevig <rhyme label="d" type="m">kruis</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Wel zes of zeven <rhyme label="c" type="f" corresp="#s10:l5">dagen</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Eer hy kwam uit zyn <rhyme label="d" type="m" corresp="#s10:l6">huis</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">De tweede viel aen <rhyme label="a" type="f">gruisen</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Dit liet daer menig <rhyme label="b" type="m">zugt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hy dagt ik moet <rhyme label="a" type="f" corresp="#s11:l1">verkuisen</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En nam aldaer de <rhyme label="b" type="m" corresp="#s11:l2">vlugt</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">De manke zonder <rhyme label="c" type="f">spreeken</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Het was nog een <rhyme label="d" type="m">geluk</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Van hals of been te <rhyme label="c" type="f" corresp="#s11:l5">breeken</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Hy viel over zyn <rhyme label="d" type="m" corresp="#s11:l6">kruk</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
