    <melody>Tircis au bord de la Seine</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">TIrsis au bord de la <rhyme label="a" type="f">Seine</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Disoit a Siluia vn <rhyme label="b" type="m">iour</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Au secours belle <rhyme label="a" type="f" corresp="#s1:l1">inhumaine</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Inhumaine, <rhyme label="a" type="f" corresp="#s1:l1">inhumaine</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ie brusle <rhyme label="b" type="m" corresp="#s1:l2">d'Amour</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Mais plus dure qu'une <rhyme label="a" type="f">Roche</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Siluia luy <rhyme label="b" type="m">repartit</rhyme></l>
        <l id="s2:l3" met="4" type="+-">Voyla leau, qui &amp; cy <rhyme label="a" type="f" corresp="#s2:l1">proche</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Cy proche, cy <rhyme label="a" type="f" corresp="#s2:l1">proche</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Pource <rhyme label="b" type="m" corresp="#s2:l2">guarantir</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Puis que tu le veux <rhyme label="a" type="f">Silvie</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Ie vay au fond de cest <rhyme label="b" type="m">eau</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Mettre mon Ame &amp; ma <rhyme label="a" type="f" corresp="#s3:l1">vie</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Et ma vie, &amp; ma <rhyme label="a" type="f" corresp="#s3:l1">vie</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Au dangier de <rhyme label="b" type="m" corresp="#s3:l2">l'Eau</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Cest ton obiect &amp; tes <rhyme label="a" type="f">peines</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Qu'il te foudra <rhyme label="b" type="m">supporter</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Les Rivieres sont <rhyme label="a" type="f" corresp="#s4:l1">certaines</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Certaines, <rhyme label="a" type="f" corresp="#s4:l1">certaines</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Pour te <rhyme label="b" type="m" corresp="#s4:l2">lamenter</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Quoy tu te ris de ma <rhyme label="a" type="f">peine</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Au beau millieu de cest <rhyme label="b" type="m">eau</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ie vay dedans l'eau de <rhyme label="a" type="f" corresp="#s5:l1">Seyne</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">De Seyne, de <rhyme label="a" type="f" corresp="#s5:l1">Seyne</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Faire mon <rhyme label="b" type="m" corresp="#s5:l2">tombeau</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Plure tans quettu <rhyme label="a" type="f">retourne</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Dans les abismes <rhyme label="b" type="m">iadis</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Et mon Amour ne se <rhyme label="a" type="f" corresp="#s6:l1">iourne</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Se iourne, se <rhyme label="a" type="f" corresp="#s6:l1">iourne</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Pour ton <rhyme label="b" type="m" corresp="#s6:l2">Paradis</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Iamais ais dans le Seyn de <rhyme label="a" type="f">londe</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Mon feu ne peut <rhyme label="b" type="m">s'appaiser</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Plutost aux meruellies du <rhyme label="a" type="f" corresp="#s7:l1">monde</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Du monde, du <rhyme label="a" type="f" corresp="#s7:l1">monde</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">N'aime qu'un <rhyme label="b" type="m" corresp="#s7:l2">baiser</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Baise donc ces pres <rhyme label="a" type="f">humides</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Quil aube embellist de <rhyme label="b" type="m">Fleurs</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dans les ruisseaux, <rhyme label="a" type="f" corresp="#s8:l1">liquides</rhyme></l>
        <l id="s8:l4" met="2" type="-+">Liquides, <rhyme label="a" type="f" corresp="#s8:l1">liquides</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Tous semes de <rhyme label="b" type="m" corresp="#s8:l2">Fleurs</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Quand son grand <rhyme label="a" type="f">cruellite</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Mon Ame donnast <rhyme label="b" type="m">necessite</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Tous mon tristes i'a <rhyme label="a" type="f" corresp="#s9:l1">bandonne</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">i'a bandonne, i'a <rhyme label="a" type="f" corresp="#s9:l1">bandonne</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Pourquoy ie encor <rhyme label="b" type="m" corresp="#s9:l2">lamente</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
