<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185075</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wilhelmus</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik wil ’t Oranje <rhyme label="a" type="f">draagen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot Vyfde willem’s <rhyme label="b" type="m">Eer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Nu hy, in deeze <rhyme label="a" type="f" corresp="#s1:l1">dagen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Verheugd mag leeven <rhyme label="b" type="m" corresp="#s1:l2">weer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Daar God hem tot Zyn <rhyme label="c" type="f">Wooning</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met zyn Doorlucht <rhyme label="d" type="m">Geslacht</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Gebragt heeft, door de <rhyme label="c" type="f" corresp="#s1:l5">Koning</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Van Pruissen’s <rhyme label="d" type="m" corresp="#s1:l6">Legermacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zal nu vrolyk <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En, met een bly <rhyme label="b" type="m">gemoed</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Daar Snoodäarts straffen <rhyme label="a" type="f" corresp="#s2:l1">vreezen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Steeds zingen, God is <rhyme label="b" type="m" corresp="#s2:l2">goed</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy hoorde de <rhyme label="c" type="f">gebeden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Der Vroomen, die, in ’t <rhyme label="d" type="m">Land</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Voor de Erfstadhouder <rhyme label="c" type="f" corresp="#s2:l5">streeden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">En red hem door Zyn <rhyme label="d" type="m" corresp="#s2:l6">Hand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Schoon zommigen dit <rhyme label="a" type="f">teken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Tot ’s Vorsten Eer, uit <rhyme label="b" type="m">dwang</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Maar droegen, voor zes <rhyme label="a" type="f" corresp="#s3:l1">weeken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik wil myn leeven <rhyme label="b" type="m" corresp="#s3:l2">lang</rhyme></l>
        <l id="s3:l5" met="3" type="-+">’t Oranje vrolyk <rhyme label="c" type="f">draagen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">En danken God den <rhyme label="d" type="m">Heer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Dat Hy, in deeze <rhyme label="c" type="f" corresp="#s3:l5">dagen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Ons schonk de Vryheid <rhyme label="d" type="m" corresp="#s3:l6">weer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Men zogt ons, <rhyme label="a" type="f">Prinsgezinden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">In dit vernederd <rhyme label="b" type="m">Land</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Om dat wy willem <rhyme label="a" type="f" corresp="#s4:l1">minden</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Te leggen aan een <rhyme label="b" type="m" corresp="#s4:l2">band</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Maar die ons wilde <rhyme label="c" type="f">drukken</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Heeft God, door Pruissen’s <rhyme label="d" type="m">Macht</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Vernederd, en doen <rhyme label="c" type="f" corresp="#s4:l5">bukken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Tot heil van ’t vroom <rhyme label="d" type="m" corresp="#s4:l6">Geslacht</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Laat ons, met blyde <rhyme label="a" type="f">klanken</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">God, die ons uit de <rhyme label="b" type="m">nood</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Verlost heeft, hartlyk <rhyme label="a" type="f" corresp="#s5:l1">danken</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Zyn liefd’ is eindloos <rhyme label="b" type="m" corresp="#s5:l2">groot</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die Hy ons wou <rhyme label="c" type="f">bewyzen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Daar Hy ons uit den <rhyme label="d" type="m">druk</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Deed met den Prins <rhyme label="c" type="f" corresp="#s5:l5">herryzen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En aan ons gaf <rhyme label="d" type="m" corresp="#s5:l6">geluk</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laat ons, verëend, steeds <rhyme label="a" type="f">smeeken</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">O Heer! wil in ons <rhyme label="b" type="m">Land</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Tog alle de <rhyme label="a" type="f" corresp="#s6:l1">gebreken</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Herstellen, door uw <rhyme label="b" type="m" corresp="#s6:l2">Hand</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">Laat lang Prins willem <rhyme label="c" type="f">leeven</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Tot heil van Land en <rhyme label="d" type="m">Kerk</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Wil hem uw bystand <rhyme label="c" type="f" corresp="#s6:l5">geeven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">In zyn zwaarwichtig <rhyme label="d" type="m" corresp="#s6:l6">werk</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wil door Uw Geest hem <rhyme label="a" type="f">leiden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Op dat hy, steeds <rhyme label="b" type="m">verheugd</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Zig mag in U <rhyme label="a" type="f" corresp="#s7:l1">verblyden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En doe altoos in <rhyme label="b" type="m" corresp="#s7:l2">vreugd</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Zyn Gemaalin en <rhyme label="c" type="f">Kindren</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Hier leeven, laat geen <rhyme label="d" type="m">kruis</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Nog ongeval, meer <rhyme label="c" type="f" corresp="#s7:l5">hindren</rhyme></l>
        <l id="s7:l8" met="3" type="-+">’t Doorluchte Oranje – <rhyme label="d" type="m" corresp="#s7:l6">Huis</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">En wil al de <rhyme label="a" type="f">Overheden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Die, met de Prins, ons <rhyme label="b" type="m">Land</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Wel stieren, steeds <rhyme label="a" type="f" corresp="#s8:l1">bekleeden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Met wysheid, wil <rhyme label="b" type="m" corresp="#s8:l2">verstand</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Hen geeven, dat uw <rhyme label="c" type="f">Zegen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Elk die ’s Lands heil <rhyme label="d" type="m">betracht</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Geniet op zyne <rhyme label="c" type="f" corresp="#s8:l5">wegen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Tot ’t laatste <rhyme label="d" type="m" corresp="#s8:l6">Nageslacht</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
