<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>27174</id>
    <date>
      <year-start>1598</year-start>
      <year-end>1600</year-end>
    </date>
    <location>
      <orginal-name>dordrecht?</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="meilied" translation="May song">meilied</tag>
    </tags>
    <melody>De lustelijke mei is nu in de tijd</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Den mey compt ons lieffelyck <rhyme label="a" type="m">aen</rhyme></l>
        <l id="s1:l2" met="5" type="+-">den baen / wert nu betreden / vter <rhyme label="b" type="f">steden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">van minnaers die daer buyten <rhyme label="a" type="m" corresp="#s1:l1">gaen</rhyme></l>
        <l id="s1:l4" met="5" type="+-">seer saen / volcht haer lieff mede / meest in <rhyme label="b" type="f" corresp="#s1:l2">vrede</rhyme></l>
        <l id="s1:l5" met="4" type="-+">met harpen luyten / soet <rhyme label="c" type="m">geclanck</rhyme></l>
        <l id="s1:l6" met="4" type="-+">vyolonoen fluyten / ende <rhyme label="c" type="m" corresp="#s1:l5">sanck</rhyme></l>
        <l id="s1:l7" met="3" type="+-">ouer die schoon groen <rhyme label="d" type="f">heye</rhyme></l>
        <l id="s1:l8" met="3" type="-+">met desen coelen <rhyme label="d" type="f" corresp="#s1:l7">meye</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer gaen geliefkens hant aen <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s2:l2" met="5" type="+-">opt lant / lanckx der reuieren / so <rhyme label="b" type="f">playsieren</rhyme></l>
        <l id="s2:l3" met="4" type="-+">beuanghen met der Liefden <rhyme label="a" type="m" corresp="#s2:l1">brandt</rhyme></l>
        <l id="s2:l4" met="5" type="+-">playsant / daer al de dieren / den mey <rhyme label="b" type="f" corresp="#s2:l2">cieren</rhyme></l>
        <l id="s2:l5" met="4" type="-+">de viskens in het waeter <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">spelen daer met een bly <rhyme label="c" type="m" corresp="#s2:l5">gemoet</rhyme></l>
        <l id="s2:l7" met="3" type="+-">in die selue <rhyme label="d" type="f">valeye</rhyme></l>
        <l id="s2:l8" met="3" type="-+">met desen coelen <rhyme label="d" type="f" corresp="#s2:l7">meye</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die veldekens staen <rhyme label="a" type="m">abondant</rhyme></l>
        <l id="s3:l2" met="5" type="+-">geplant / met bloemkens reyne / int <rhyme label="b" type="f">gemeyne</rhyme></l>
        <l id="s3:l3" met="4" type="-+">d'amoreusen seer <rhyme label="a" type="m" corresp="#s3:l1">tryumphant</rhyme></l>
        <l id="s3:l4" met="5" type="+-">valyant / na die fonteyne / in dat <rhyme label="b" type="f" corresp="#s3:l2">pleyne</rhyme></l>
        <l id="s3:l5" met="4" type="-+">treden sy dan / paer ende <rhyme label="c" type="m">paer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">menich Iongman / met syn lief <rhyme label="c" type="m" corresp="#s3:l5">daer</rhyme></l>
        <l id="s3:l7" met="3" type="+-">sy plucken d <rhyme label="d" type="f">acoleye</rhyme></l>
        <l id="s3:l8" met="3" type="-+">daer in den coelen <rhyme label="d" type="f" corresp="#s3:l7">meye</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Menich bloemken staeter <rhyme label="a" type="m">gebloeyt</rhyme></l>
        <l id="s4:l2" met="5" type="+-">daer groeyt / in die lantsdouwe / menich <rhyme label="b" type="f">kersouwe</rhyme></l>
        <l id="s4:l3" met="4" type="-+">zynde met shemels dou <rhyme label="a" type="m" corresp="#s4:l1">besproyt</rhyme></l>
        <l id="s4:l4" met="5" type="+-">oock vloyt / het waeter nouwe / schoon <rhyme label="b" type="f" corresp="#s4:l2">Ionckurouwe</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Phebus geeft alle cruyden <rhyme label="c" type="m">cracht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Aurora vochticht dach ende <rhyme label="c" type="m" corresp="#s4:l5">nacht</rhyme></l>
        <l id="s4:l7" met="3" type="+-">tgewas is <rhyme label="d" type="f">menigerleye</rhyme></l>
        <l id="s4:l8" met="3" type="-+">met desen coelen <rhyme label="d" type="f" corresp="#s4:l7">meye</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Ghy Princelycke minaers <rhyme label="a" type="m">hoort</rhyme></l>
        <l id="s5:l2" met="5" type="+-">treedt voort / condyse vinde / v <rhyme label="b" type="f">beminde</rhyme></l>
        <l id="s5:l3" met="4" type="-+">maeckt met v tween een <rhyme label="a" type="m" corresp="#s5:l1">accoort</rhyme></l>
        <l id="s5:l4" met="5" type="+-">oorboort / als wel gesinde / onder een <rhyme label="b" type="f" corresp="#s5:l2">linde</rhyme></l>
        <l id="s5:l5" met="4" type="-+">so amoreusen schuldich <rhyme label="c" type="m">syn</rhyme></l>
        <l id="s5:l6" met="4" type="-+">wilt vwen tyt geduldich <rhyme label="c" type="m" corresp="#s5:l5">zyn</rhyme></l>
        <l id="s5:l7" met="3" type="+-">wie dan dat hy <rhyme label="d" type="f">verbeye</rhyme></l>
        <l id="s5:l8" met="3" type="-+">tot in den coelen <rhyme label="d" type="f" corresp="#s5:l7">meye</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
