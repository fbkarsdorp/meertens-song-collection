<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11419</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="liefdeslied" translation="love song">liefdeslied</tag>
    </tags>
    <melody>Een ridder en een meisje jong</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Liefd' is <rhyme label="a" type="m">onverwinnelijck</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In vechten, strijden of in <rhyme label="b" type="f">kampen</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Daer by is hy <rhyme label="a" type="m" corresp="#s1:l1">een-sinnelijck</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En soeckt hem stadelijck te <rhyme label="b" type="f" corresp="#s1:l2">klampen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waer Overvloet, en Rijckdom <rhyme label="a" type="m">is</rhyme>.</l>
        <l id="s2:l2" met="4" type="-+">Dan noch soo vlieght hy oock te <rhyme label="b" type="f">woonen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">In 't suycker vande ooghjens <rhyme label="a" type="m" corresp="#s2:l1">wis</rhyme>.</l>
        <l id="s2:l4" met="4" type="-+">En op de schoone purp're <rhyme label="b" type="f" corresp="#s2:l2">koonen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Vande frissche Maeghdekens, <rhyme label="a" type="m">ach</rhyme>!</l>
        <l id="s3:l2" met="4" type="-+">Som-wijlen in het schuym der <rhyme label="b" type="f">baren</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Hanghd hy, en swemt op zijn <rhyme label="a" type="m" corresp="#s3:l1">bejach</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">En maeckt hem over al <rhyme label="b" type="f" corresp="#s3:l2">bevaren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Het Boeren-hut met riet <rhyme label="a" type="m">gedeckt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoe kleyn, en sal hy niet <rhyme label="b" type="f">versmaden</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Of Harder die zijn Liefjen <rhyme label="a" type="m" corresp="#s4:l1">weckt</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Het leefter al op sijn <rhyme label="b" type="f" corresp="#s4:l2">ghenade</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Noyt God, of Mensch, of wie hy <rhyme label="a" type="m">was</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">En kan sijn groote kracht <rhyme label="b" type="f">ontvluchten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">En wie hy raeckt die moet oock <rhyme label="a" type="m" corresp="#s5:l1">ras</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Verwoet, en sinneloosich <rhyme label="b" type="f" corresp="#s5:l2">suchten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Al waer hy schoon, te voren <rhyme label="a" type="m">vroom</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Hy wort dan valsch, en <rhyme label="b" type="f">ongerechtigh</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Het schynt ons als een wonder <rhyme label="a" type="m" corresp="#s6:l1">droom</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Dat die goed-aerigh was, wort <rhyme label="b" type="f" corresp="#s6:l2">nechtigh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">En tusschen vrienden zaeyt hy <rhyme label="a" type="m">haet</rhyme>:</l>
        <l id="s7:l2" met="4" type="-+">Want hy en kent, noch recht, noch <rhyme label="b" type="f">wetten</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Als die hy inde ooghjens <rhyme label="a" type="m" corresp="#s7:l1">gaet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Van een heel aerdigh Meysjen <rhyme label="b" type="f" corresp="#s7:l2">setten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
