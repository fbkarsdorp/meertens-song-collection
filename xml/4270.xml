<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4270</id>
    <date>
      <year-start>1602</year-start>
      <year-end>1602</year-end>
    </date>
    <tags>
      <tag>love song</tag>
      <tag>farewell song</tag>
      <tag>arcostic</tag>
    </tags>
    <melody>Paisibele bruid</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">PAisibel <rhyme label="a" type="m">Bruyt</rhyme> //</l>
        <l id="s1:l2" met="4" type="-+">Wonderbaer arcke <rhyme label="b" type="m">triumphael</rhyme> //</l>
        <l id="s1:l3" met="2" type="-+">Treet tot  my <rhyme label="a" type="m" corresp="#s1:l1">wt</rhyme> //</l>
        <l id="s1:l4" met="4" type="-+">Die zijt t'geneselicxste <rhyme label="a" type="m" corresp="#s1:l1">cruyt</rhyme> //</l>
        <l id="s1:l5" met="4" type="-+">In mijnen noot  <rhyme label="b" type="m" corresp="#s1:l2">medecinael</rhyme> //</l>
        <l id="s1:l6" met="4" type="-+">Dierbaer parceel die <rhyme label="b" type="m" corresp="#s1:l2">spetiael</rhyme> //</l>
        <l id="s1:l7" met="4" type="-+">Geestimeert moet zijn naer weer- <rhyme label="c" type="f">de</rhyme> //</l>
        <l id="s1:l8" met="4" type="-+">Boven de schatten <rhyme label="b" type="m" corresp="#s1:l2">principael</rhyme> //</l>
        <l id="s1:l9" met="4" type="-+">Des weerelts, dies ghy <rhyme label="b" type="m" corresp="#s1:l2">teenemael</rhyme> //</l>
        <l id="s1:l10" met="4" type="-+">Sult wesen mijnen schat op <rhyme label="c" type="f" corresp="#s1:l7">d'eerde</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ingelick <rhyme label="a" type="m">beelt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Gulden ciraet der minnen <rhyme label="b" type="m">choor</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Mijn herte <rhyme label="a" type="m" corresp="#s2:l1">speelt</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Dwelck duystderley genuchten <rhyme label="a" type="m" corresp="#s2:l1">steelt</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Wanneer my coemt u schoonheyt <rhyme label="b" type="m" corresp="#s2:l2">voor</rhyme>:</l>
        <l id="s2:l6" met="4" type="-+">Ghy overvloyt (O reyn <rhyme label="b" type="m" corresp="#s2:l2">tresoor</rhyme>)</l>
        <l id="s2:l7" met="4" type="-+">Van soetheyt, meer dan <rhyme label="c" type="f">Hippocrene</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Ghy port my als een gulden <rhyme label="b" type="m" corresp="#s2:l2">spoor</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Als ghy wtstort in mijn <rhyme label="b" type="m" corresp="#s2:l2">gehoor</rhyme></l>
        <l id="s2:l10" met="4" type="-+">Dijn soeten voys, als een <rhyme label="c" type="f" corresp="#s2:l7">Sirene</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Een herte <rhyme label="a" type="m">reyn</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">T'uwaert gebroken in <rhyme label="b" type="m">ootmoet</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Draegh ick <rhyme label="a" type="m" corresp="#s3:l1">certeyn</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">In spijte dorper, of <rhyme label="a" type="m" corresp="#s3:l1">vileyn</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Al soudt my costen vleesch, en <rhyme label="b" type="m" corresp="#s3:l2">bloet</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Wats aerdtsche weelde, schat, of <rhyme label="b" type="m" corresp="#s3:l2">goet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Der goden hemelsche <rhyme label="c" type="f">wellusten</rhyme>?</l>
        <l id="s3:l8" met="4" type="-+">T'is (sonder u) al <rhyme label="b" type="m" corresp="#s3:l2">tegenspoet</rhyme>:</l>
        <l id="s3:l9" met="4" type="-+">Zijt ghy absent, beeldeken <rhyme label="b" type="m" corresp="#s3:l2">soet</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Waer soud' mijn siele connen <rhyme label="c" type="f" corresp="#s3:l7">rusten</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Tis meer dan <rhyme label="a" type="m">recht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Schoon lief, dat ick u eygen <rhyme label="b" type="m">zy</rhyme>:</l>
        <l id="s4:l3" met="2" type="-+">Of ghy dijn <rhyme label="a" type="m" corresp="#s4:l1">knecht</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Thien duysendtmael t'leven <rhyme label="a" type="m" corresp="#s4:l1">ontsecht</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Ick moet u blijven dienstbaer <rhyme label="b" type="m" corresp="#s4:l2">by</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Liever te zijn my selfs <rhyme label="b" type="m" corresp="#s4:l2">onvry</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dan u mijn ooghen zouden <rhyme label="c" type="f">deruen</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Ghy meught u veynsen tegens <rhyme label="b" type="m" corresp="#s4:l2">my</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Waer ick in noot, ic weet dat <rhyme label="b" type="m" corresp="#s4:l2">ghy</rhyme></l>
        <l id="s4:l10" met="4" type="-+">De doodt voor my liever sout <rhyme label="c" type="f" corresp="#s4:l7">steruen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Eer salt doch <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Dat duyster nacht, sal zijn <rhyme label="b" type="m">verbaert</rhyme></l>
        <l id="s5:l3" met="2" type="-+">In sonne <rhyme label="a" type="m" corresp="#s5:l1">schijn</rhyme>:</l>
        <l id="s5:l4" met="4" type="-+">Dat dat ick zoud' verandren <rhyme label="a" type="m" corresp="#s5:l1">mijn</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Liefde, wt eenen valschen <rhyme label="b" type="m" corresp="#s5:l2">aert</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">En of Iason zijn lief <rhyme label="b" type="m" corresp="#s5:l2">ontvaert</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">En Hercules laet zijn <rhyme label="c" type="f">Amie</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Met haeren heb ick deel, noch <rhyme label="b" type="m" corresp="#s5:l2">paert</rhyme>,</l>
        <l id="s5:l9" met="4" type="-+">God houde my voor sulcx <rhyme label="b" type="m" corresp="#s5:l2">bewaert</rhyme>,</l>
        <l id="s5:l10" met="4" type="-+">Ick storve van <rhyme label="c" type="f" corresp="#s5:l7">Melancolie</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Raetsaem <rhyme label="a" type="m">Pallas</rhyme></l>
        <l id="s6:l2" met="4" type="-+">In wijsheyt hoogh <rhyme label="b" type="m">gebenedijt</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Noyt maeght en <rhyme label="a" type="m" corresp="#s6:l1">was</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Noch is, die meer verdriet <rhyme label="a" type="m" corresp="#s6:l1">genas</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Als ghy, die mijn Princesse <rhyme label="b" type="m" corresp="#s6:l2">zijt</rhyme>:</l>
        <l id="s6:l6" met="4" type="-+">De zoetste vreught, diet al <rhyme label="b" type="m" corresp="#s6:l2">verblijt</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Is, met u dus te <rhyme label="c" type="f">converseeren</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Maer siende nu soo cleyn <rhyme label="b" type="m" corresp="#s6:l2">respijt</rhyme>,</l>
        <l id="s6:l9" met="4" type="-+">Neem ick (met heusheyt, voor dees <rhyme label="b" type="m" corresp="#s6:l2">tijt</rhyme>)</l>
        <l id="s6:l10" met="4" type="-+">Oorlof, tot op een <rhyme label="c" type="f" corresp="#s6:l7">wederkeeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
