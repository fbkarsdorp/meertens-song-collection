<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>8200</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>hoorn</orginal-name>
      <standard-name>Hoorn, The Netherlands</standard-name>
      <coordinates>
        <lon>52.653272</lon>
        <lat>5.0735802</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="love song">liefdeslied</tag>
    </tags>
    <melody>Chambrière chambrière</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ICk vond' dees daeghs een meysje <rhyme label="a" type="f">slapen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">In dat groen bedouwde <rhyme label="b" type="m">gras</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">En alsoo ick bleef staen <rhyme label="a" type="f" corresp="#s1:l1">gaepen</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Dochtick dat het Venus <rhyme label="b" type="m" corresp="#s1:l2">was</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Want mijn ziel, mijn hart van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Door de liefde was <rhyme label="d" type="m">ontstelt</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Als ick op 't <rhyme label="d" type="m" corresp="#s1:l6">Velt</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">De lieve Moeder van het <rhyme label="c" type="f" corresp="#s1:l5">minnen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Schier half bloot sagh neer <rhyme label="d" type="m" corresp="#s1:l6">gevelt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mijn gesicht begon te <rhyme label="a" type="f">speelen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Op haer borsje, wit als <rhyme label="b" type="m">sneeu</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">En mijn zieltje begon te <rhyme label="a" type="f" corresp="#s2:l1">steelen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Van haer mont een kus of <rhyme label="b" type="m" corresp="#s2:l2">twee</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Maer sy bleef geduerigh <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Doch mijn ongebonden <rhyme label="d" type="m">wil</rhyme></l>
        <l id="s2:l7" met="2" type="-+">En bleef niet <rhyme label="d" type="m" corresp="#s2:l6">stil</rhyme>:</l>
        <l id="s2:l8" met="4" type="-+">Flucx ben ick vernieuwt van <rhyme label="c" type="f" corresp="#s2:l5">luste</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">En versocht het <rhyme label="d" type="m" corresp="#s2:l6">Minne-spel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Begonse wat te <rhyme label="a" type="f">woelen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">En liet dalen sucht op <rhyme label="b" type="m">sucht</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Ick docht soudt ghy het noch niet <rhyme label="a" type="f" corresp="#s3:l1">voelen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dat ick trachte naer 't <rhyme label="b" type="m" corresp="#s3:l2">gerucht</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Met soo deese haer ooghjes <rhyme label="c" type="f">oopen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Soo dat sy vriendelijck op mijn <rhyme label="d" type="m">sagh</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">En saghjes <rhyme label="d" type="m" corresp="#s3:l6">lagh</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Mijn jacht-hondt is in 't bos <rhyme label="c" type="f" corresp="#s3:l5">gekroopen</rhyme>,</l>
        <l id="s3:l9" met="4" type="+-">En dat al sonder groot <rhyme label="d" type="m" corresp="#s3:l6">gewagh</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Voor het lest kuste ik haer <rhyme label="a" type="f">wangen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Sy bleef hangen aen mijn <rhyme label="b" type="m">mont</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">En most noch een schootje <rhyme label="a" type="f" corresp="#s4:l1">ontfangen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dien ick heb gelost <rhyme label="b" type="m" corresp="#s4:l2">terstont</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Sy sprack, laet doch u Hont niet <rhyme label="c" type="f">rusten</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Want ick voel een soete <rhyme label="d" type="m">smaeck</rhyme></l>
        <l id="s4:l7" met="2" type="-+">In dit <rhyme label="d" type="m" corresp="#s4:l6">vermaeck</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Tot dat sy verkeert van <rhyme label="c" type="f" corresp="#s4:l5">lusten</rhyme>,</l>
        <l id="s4:l9" met="4" type="+-">Soo van malkander sijn <rhyme label="d" type="m" corresp="#s4:l6">afgeraeckt</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
