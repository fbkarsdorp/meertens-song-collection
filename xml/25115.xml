<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25115</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Dat scoenste kint es ons <rhyme label="a" type="f">geboren</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dat alre soetste <rhyme label="b" type="m">mannekijn</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Die sijn herte aen hem wilt <rhyme label="a" type="f" corresp="#s1:l1">keeren</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Hy sal hem getrouwe <rhyme label="b" type="m" corresp="#s1:l2">sijn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Die wilt goeden huwelic <rhyme label="a" type="f">doen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Ons vrouwe sal hem <rhyme label="b" type="m">beraden</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Sy heeft soe sconen iongen <rhyme label="a" type="f" corresp="#s2:l1">soen</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Die wilt sy al om <rhyme label="b" type="m" corresp="#s2:l2">bestaden</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Machmen goeden houwelic <rhyme label="a" type="f">doen</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Aen een arm <rhyme label="b" type="m">gesellekijn</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Het es van hauen also <rhyme label="a" type="f" corresp="#s3:l1">bloot</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Dat leet in een arme <rhyme label="b" type="m" corresp="#s3:l2">crebbekijn</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Al es dit soete <rhyme label="a" type="f">knapelkijn</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Alte arm van <rhyme label="b" type="m">goede</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Hy es doch edel ende <rhyme label="a" type="f" corresp="#s4:l1">fijn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Ende alte fier van <rhyme label="b" type="m" corresp="#s4:l2">moede</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sijn voetkens waren dicwile <rhyme label="a" type="f">cout</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Doen hy begonst te <rhyme label="b" type="m">gane</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Ende altoos was sijn helsken <rhyme label="a" type="f" corresp="#s5:l1">bloot</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Ende luttel hadde hy <rhyme label="b" type="m" corresp="#s5:l2">ane</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sijn groot armoede maect ons <rhyme label="a" type="f">rijcke</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Dat heeft ons sijn minne <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Van liefden en consti niet <rhyme label="a" type="f" corresp="#s6:l1">gerusten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Tot dat hy hadde die doot <rhyme label="b" type="m" corresp="#s6:l2">ontfaen</rhyme></l>
        <l id="s6:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die hemel vloeyde honech <rhyme label="a" type="f">vloet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">God vergaf den <rhyme label="b" type="m">toren</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Doen die ewige wijsheit <rhyme label="a" type="f" corresp="#s7:l1">weert</rhyme></l>
        <l id="s7:l4" met="4" type="-+">In eenen stal <rhyme label="b" type="m" corresp="#s7:l2">geboren</rhyme></l>
        <l id="s7:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">O ewich ende genadich <rhyme label="a" type="f">god</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Wat hebdi doer den mensce <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Als wy te male niet en <rhyme label="a" type="f" corresp="#s8:l1">sijn</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Wat suldij dan van ons <rhyme label="b" type="m" corresp="#s8:l2">ontfaen</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Deus eterne tu <rhyme label="c" type="m">pijssime</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
