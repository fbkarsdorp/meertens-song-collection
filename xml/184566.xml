<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184566</id>
    <date>
      <year-start>1803</year-start>
      <year-end>1803</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Het nachtegaaltje klein</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Het Nagtegaaltje <rhyme label="a" type="f">kleine</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Verheugt de zomer <rhyme label="b" type="m">schoon</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met zyne zoete <rhyme label="c" type="m">zang</rhyme>;</l>
        <l id="s1:l4" met="3" type="-+">In 't woud by een <rhyme label="a" type="f" corresp="#s1:l1">fontyne</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Daar heeft my een <rhyme label="b" type="m" corresp="#s1:l2">persoon</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Geluisterd na 't <rhyme label="c" type="m" corresp="#s1:l3">geklag</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">De tyd viel my niet <rhyme label="c" type="m" corresp="#s1:l3">lang</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">De wilde diertjes <rhyme label="d" type="f">sprongen</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Al in het groene <rhyme label="e" type="m">dal</rhyme>.</l>
        <l id="s1:l10" met="3" type="-+">De vogltjes daar <rhyme label="d" type="f" corresp="#s1:l8">zongen</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Lieffelyk <rhyme label="e" type="m" corresp="#s1:l9">overal</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">In 't woud stond een <rhyme label="a" type="f">valeye</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Vercierd met bloempjes <rhyme label="b" type="m">rein</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Schoon en <rhyme label="c" type="m">triumphant</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Daar ging ik my <rhyme label="a" type="f" corresp="#s2:l1">vermeyen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Al in dat lustig <rhyme label="b" type="m" corresp="#s2:l2">plein</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Daar ik de liefste <rhyme label="c" type="m" corresp="#s2:l3">vant</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Ik zag langs een <rhyme label="c" type="m" corresp="#s2:l3">kant</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Na my een maaget <rhyme label="d" type="f">komen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">In witte zy <rhyme label="e" type="m">gekleed</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">'t Was een zo schoone <rhyme label="d" type="f" corresp="#s2:l8">bloem</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Als ik ter waereld <rhyme label="e" type="m" corresp="#s2:l9">weet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Doen zy my wierd <rhyme label="a" type="f">gewaare</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Seer vriendelyk zy <rhyme label="b" type="m">loeg</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Met haare roode <rhyme label="c" type="m">mond</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Daar kwam in 't <rhyme label="a" type="f" corresp="#s3:l1">openbaare</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Wat liefde zy my <rhyme label="b" type="m" corresp="#s3:l2">droeg</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Toen ik dees Vrouwe <rhyme label="c" type="m" corresp="#s3:l3">vond</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Myn herte wierd <rhyme label="c" type="m" corresp="#s3:l3">gezond</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Door de rein maagd <rhyme label="d" type="f">gepreezen</rhyme>:</l>
        <l id="s3:l9" met="3" type="-+">Want als Helena <rhyme label="e" type="m">schoon</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Zo dogt zy my te <rhyme label="d" type="f" corresp="#s3:l8">weezen</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Onder des Hemels <rhyme label="e" type="m" corresp="#s3:l9">troon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wy gingen <rhyme label="a" type="f">diverteeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Houdende hand aan <rhyme label="b" type="m">hand</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Als Amoreusen <rhyme label="c" type="m">doen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Zeer vriendelyk <rhyme label="a" type="f" corresp="#s4:l1">spanceeren</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">'t Scheen het beloosde <rhyme label="b" type="m" corresp="#s4:l2">land</rhyme>.</l>
        <l id="s4:l6" met="3" type="-+">Zo lustig was in 't <rhyme label="c" type="m" corresp="#s4:l3">groen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">De Mey stond in 't <rhyme label="c" type="m" corresp="#s4:l3">saizoen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">'t Was 'er duizend <rhyme label="d" type="f">vreugde</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Dat door de <rhyme label="e" type="m">minnegloed</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Ous harte zeer <rhyme label="d" type="f" corresp="#s4:l8">vetheugde</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Denk wat de Jeugd al <rhyme label="e" type="m" corresp="#s4:l9">doet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy zongen daar <rhyme label="a" type="f">muzyke</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Al op de <rhyme label="b" type="m">snaarenspel</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Melodieus en <rhyme label="c" type="m">zoet</rhyme>.</l>
        <l id="s5:l4" met="3" type="-+">Wy spraken <rhyme label="a" type="f" corresp="#s5:l1">rethoryke</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Lustig zonder <rhyme label="b" type="m" corresp="#s5:l2">gekwel</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met eenen blyden <rhyme label="c" type="m" corresp="#s5:l3">moed</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Het was 'er al <rhyme label="c" type="m" corresp="#s5:l3">voorspoed</rhyme></l>
        <l id="s5:l8" met="3" type="-+">In deeze schoon <rhyme label="d" type="f">waranden</rhyme>.</l>
        <l id="s5:l9" met="3" type="-+">Wy maakten daar een <rhyme label="e" type="m">dans</rhyme>.</l>
        <l id="s5:l10" met="3" type="-+">Myn lief vlogt met haar <rhyme label="d" type="f" corresp="#s5:l8">handen</rhyme></l>
        <l id="s5:l11" met="3" type="+-">Van roosen eenen <rhyme label="e" type="m" corresp="#s5:l9">krans</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Daaraa gingen wy <rhyme label="a" type="f">rusten</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Onder het groene <rhyme label="b" type="m">rys</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Al op dezelfde <rhyme label="c" type="m">pas</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">In dit priÃ«el van <rhyme label="a" type="f" corresp="#s6:l1">lusten</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Het scheen een <rhyme label="b" type="m" corresp="#s6:l2">paradys</rhyme></l>
        <l id="s6:l6" met="3" type="-+">'t Welk voor plaizantheid <rhyme label="c" type="m" corresp="#s6:l3">was</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Verborgen in het <rhyme label="c" type="m" corresp="#s6:l3">gras</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Daar heeft een slang <rhyme label="d" type="f">gezeten</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">Die myn lief in den <rhyme label="e" type="m">voet</rhyme></l>
        <l id="s6:l10" met="3" type="-+">Doodelyk heeft <rhyme label="d" type="f" corresp="#s6:l8">gebeten</rhyme>,</l>
        <l id="s6:l11" met="3" type="+-">'t Welk ik betreuren <rhyme label="e" type="m" corresp="#s6:l9">moet</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">'t Fenyn heeft haar <rhyme label="a" type="f">bevangen</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Zy stierf de bittere <rhyme label="b" type="m">dood</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In alzo korten <rhyme label="c" type="m">stond</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die daar met spel en <rhyme label="a" type="f" corresp="#s7:l1">zangen</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En bloemken in haar <rhyme label="b" type="m" corresp="#s7:l2">schoot</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Fraai was ende <rhyme label="c" type="m" corresp="#s7:l3">gezond</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Och haar <rhyme label="c" type="m" corresp="#s7:l3">roodermond</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Die werd zo bleek van <rhyme label="d" type="f">verwen</rhyme>.</l>
        <l id="s7:l9" met="3" type="-+">Al dees <rhyme label="e" type="m">wellustigheid</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Heeft dees maagd moeten <rhyme label="d" type="f" corresp="#s7:l8">derven</rhyme>,</l>
        <l id="s7:l11" met="3" type="+-">Haar tot sterven <rhyme label="e" type="m" corresp="#s7:l9">bereid</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu mag ik wel <rhyme label="a" type="f">beklaagen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Myne <rhyme label="b" type="m">geboortedag</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Met traanen <rhyme label="c" type="m">menigfout</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Nu ik alleen moet <rhyme label="a" type="f" corresp="#s8:l1">dragen</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Lyden zonder <rhyme label="b" type="m" corresp="#s8:l2">verdrag</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Hebbende dit <rhyme label="c" type="m" corresp="#s8:l3">aanschouwt</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Hoe zeer ben ik <rhyme label="c" type="m" corresp="#s8:l3">benouwt</rhyme>.</l>
        <l id="s8:l8" met="3" type="-+">Nog moet ik in myn <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s8:l9" met="3" type="-+">Met eerbaar herte <rhyme label="e" type="m">koen</rhyme></l>
        <l id="s8:l10" met="3" type="-+">Haar doode wangen <rhyme label="d" type="f" corresp="#s8:l8">geeven</rhyme></l>
        <l id="s8:l11" met="3" type="+-">Uit liefde eenen <rhyme label="e" type="m" corresp="#s8:l9">zoen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O scheiden, bitter <rhyme label="a" type="f">scheiden</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Hoe hart is uw <rhyme label="b" type="m">geklag</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Zo ik gevoel <rhyme label="c" type="m">present</rhyme>.</l>
        <l id="s9:l4" met="3" type="-+">Wy gingen ons <rhyme label="a" type="f" corresp="#s9:l1">vermeiden</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Nog heden deezen <rhyme label="b" type="m" corresp="#s9:l2">dag</rhyme>.</l>
        <l id="s9:l6" met="3" type="-+">Nu leid my lief <rhyme label="c" type="m" corresp="#s9:l3">geschend</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Och wat is 's menschen <rhyme label="c" type="m" corresp="#s9:l3">end</rhyme>!</l>
        <l id="s9:l8" met="3" type="-+">Men ziet hem snel <rhyme label="d" type="f">verdwynen</rhyme>,</l>
        <l id="s9:l9" met="3" type="-+">Gelyk een rook <rhyme label="e" type="m">vergaan</rhyme>,</l>
        <l id="s9:l10" met="3" type="-+">Met droefheid en vol <rhyme label="d" type="f" corresp="#s9:l8">pynen</rhyme>,</l>
        <l id="s9:l11" met="3" type="+-">Het is zeer haast <rhyme label="e" type="m" corresp="#s9:l9">gedaan</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Wat baat het <rhyme label="a" type="f">triumpheeren</rhyme>?</l>
        <l id="s10:l2" met="3" type="-+">Wat baat soulaas en <rhyme label="b" type="m">vreugd</rhyme>?</l>
        <l id="s10:l3" met="3" type="-+">Die wy een kleine <rhyme label="c" type="m">tyd</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Wellustig hier <rhyme label="a" type="f" corresp="#s10:l1">hanteeren</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">En neemen ons <rhyme label="b" type="m" corresp="#s10:l2">genugt</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Vleeschlyk breed en <rhyme label="c" type="m" corresp="#s10:l3">wyd</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">De dood die volgt <rhyme label="c" type="m" corresp="#s10:l3">subyt</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Men kan ze niet <rhyme label="d" type="f">ontkomen</rhyme>,</l>
        <l id="s10:l9" met="3" type="-+">Met haar straalen <rhyme label="e" type="m">steekt</rhyme>.</l>
        <l id="s10:l10" met="3" type="-+">Wy worden <rhyme label="d" type="f" corresp="#s10:l8">weggenomen</rhyme></l>
        <l id="s10:l11" met="3" type="+-">Gelyk een glas dat <rhyme label="e" type="m" corresp="#s10:l9">breekt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Gy Amoreuse <rhyme label="a" type="f">dieren</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Spiegelt u hier aan <rhyme label="b" type="m">wel</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Hoe dat dees maaget <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">In een woud ging <rhyme label="a" type="f" corresp="#s11:l1">plyzieren</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Met zang en <rhyme label="b" type="m" corresp="#s11:l2">snaarenspel</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Onder des Hemels <rhyme label="c" type="m" corresp="#s11:l3">troon</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Met haar dienaar <rhyme label="c" type="m" corresp="#s11:l3">Ydoon</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Hy hield haar wel in <rhyme label="d" type="f">waarde</rhyme>,</l>
        <l id="s11:l9" met="3" type="-+">Met dees waerelds <rhyme label="e" type="m">geraas</rhyme>,</l>
        <l id="s11:l10" met="3" type="-+">Nu leid ze in der <rhyme label="d" type="f" corresp="#s11:l8">narde</rhyme>,</l>
        <l id="s11:l11" met="3" type="+-">En is der wormen <rhyme label="e" type="m" corresp="#s11:l9">aas</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
