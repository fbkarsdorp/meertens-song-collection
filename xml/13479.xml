    <melody>Zo God de Heer het huis niet en maakt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">SO God de Heer 't huis niet en <rhyme label="a" type="m">maekt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vergeefs de bouwers werken <rhyme label="b" type="m">daer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">So God niet self neemt in <rhyme label="b" type="m" corresp="#s1:l2">bewaer</rhyme></l>
        <l id="s1:l4" met="4" type="-+">De Stadt, vergeefs de <rhyme label="a" type="m" corresp="#s1:l1">wachterwaekt</rhyme>.</l>
      </lg>
    </lg>
  </text>
