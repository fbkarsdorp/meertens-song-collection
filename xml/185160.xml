<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>185160</id>
    <date>
      <year-start>1790</year-start>
      <year-end>1792</year-end>
    </date>
    <location>
      <orginal-name>dordrecht</orginal-name>
      <standard-name>Dordrecht, The Netherlands</standard-name>
      <coordinates>
        <lon>51.7958812</lon>
        <lat>4.6779351</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Je le compare avec Louis</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">'k Geniet nu al de vreugd der <rhyme label="a" type="m">aard</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Heb dank o Liefde! die mijn <rhyme label="b" type="f">zinnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Het zalig huwlijk deet <rhyme label="b" type="f" corresp="#s1:l2">beminnen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat mij een bron van wellust <rhyme label="a" type="m" corresp="#s1:l1">baart</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">'k Mag nu, na mijn ziels <rhyme label="c" type="f">verlangen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Doris in mijn arm <rhyme label="c" type="f" corresp="#s1:l5">ontvangen</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Op het dons, op het <rhyme label="d" type="m">dons</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">'t Zoet der liefde <rhyme label="e" type="f">smaaken</rhyme>, -</l>
        <l id="s1:l9" met="3" type="+-">'t Zoet der liefde <rhyme label="e" type="f" corresp="#s1:l8">smaaken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ach! hoe lief, als ik <rhyme label="a" type="m">ontwaak</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Drukt mij Doris aan zijn <rhyme label="b" type="f">lippen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Waar van duizend kusjes <rhyme label="b" type="f" corresp="#s2:l2">glippen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Duizend kusjes, zoet van <rhyme label="a" type="m" corresp="#s2:l1">smaak</rhyme>!</l>
        <l id="s2:l5" met="4" type="+-">Meisjes, kies in mijne <rhyme label="c" type="f">jaaren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Boven al het echt <rhyme label="c" type="f" corresp="#s2:l5">vergaaren</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Ach het zoet! ach het <rhyme label="d" type="m">zoet</rhyme>!</l>
        <l id="s2:l8" met="3" type="+-">Laat zich niet <rhyme label="e" type="f">beschrijven</rhyme>, -</l>
        <l id="s2:l9" met="3" type="+-">Laat zich niet <rhyme label="e" type="f" corresp="#s2:l8">beschrijven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Gaat mijn Doris van mijn <rhyme label="a" type="m">zij</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">De uurtjes schijnen voor mij <rhyme label="b" type="f">dagen</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Had de klok al tien <rhyme label="b" type="f" corresp="#s3:l2">geslagen</rhyme>! -</l>
        <l id="s3:l4" met="4" type="-+">Ach hoe vergenoegt en <rhyme label="a" type="m" corresp="#s3:l1">blij</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Zou ik hem weer in mijn <rhyme label="c" type="f">armen</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Streelen, kussen en <rhyme label="c" type="f" corresp="#s3:l5">verwarmen</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Daal o zon! daal o <rhyme label="d" type="m">zon</rhyme>!</l>
        <l id="s3:l8" met="3" type="+-">In de wester <rhyme label="e" type="f">kimmen</rhyme>, -</l>
        <l id="s3:l9" met="3" type="+-">In de wester <rhyme label="e" type="f" corresp="#s3:l8">kimmen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
