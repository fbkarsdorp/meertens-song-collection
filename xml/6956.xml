  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">SChoone <rhyme label="a" type="f">Vyandinne</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Sult ghy noyt <rhyme label="b" type="f">af-laten</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">My, die u <rhyme label="a" type="f" corresp="#s1:l1">beminne</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Eeuwighlijck te <rhyme label="b" type="f" corresp="#s1:l2">haten</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Helaes! wat kan 't dan <rhyme label="b" type="f" corresp="#s1:l2">baten</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Of ick u val te <rhyme label="c" type="m">voet</rhyme>?</l>
        <l id="s1:l7" met="2" type="-+">Mijn steenen, noch <rhyme label="d" type="f">weenen</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Noch klachten, <rhyme label="e" type="f">versachten</rhyme></l>
        <l id="s1:l9" met="2" type="+-">U wreet <rhyme label="c" type="m" corresp="#s1:l6">gemoedt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Ick, die eens voor <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">Leefde wel te <rhyme label="b" type="f">vreden</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">En mijn gantsche <rhyme label="a" type="f" corresp="#s2:l1">wesen</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Stierde nae de <rhyme label="b" type="f" corresp="#s2:l2">reden</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Die sie my nu <rhyme label="b" type="f" corresp="#s2:l2">af-treden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Van 't geen ick eerst <rhyme label="c" type="m">verkoos</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">soo malen, en <rhyme label="d" type="f">dwaelen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Mijn sinnen, door 't <rhyme label="e" type="f">minnen</rhyme>,</l>
        <l id="s2:l9" met="2" type="+-">Gantsch <rhyme label="c" type="m" corresp="#s2:l6">vruchteloos</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Alde waerde <rhyme label="a" type="f">boecken</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Die ick plagh te <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Legghen achter <rhyme label="a" type="f" corresp="#s3:l1">hoecken</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Dick en dicht <rhyme label="b" type="f" corresp="#s3:l2">bestoven</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Vermits dat nu drijft <rhyme label="b" type="f" corresp="#s3:l2">boven</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Een nieuw-gheboren <rhyme label="c" type="m">lust</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Die 't herte, met <rhyme label="d" type="f">smerte</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Door 't pogen nae 't <rhyme label="e" type="f">hooghe</rhyme>,</l>
        <l id="s3:l9" met="2" type="+-">Geheel <rhyme label="c" type="m" corresp="#s3:l6">ontrust</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Sinnetjes keert <rhyme label="a" type="f">weder</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Van 't verdrietigh <rhyme label="b" type="f">mallen</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Ghy zijt veel te <rhyme label="a" type="f" corresp="#s4:l1">teder</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Om soo hard te <rhyme label="b" type="f" corresp="#s4:l2">vallen</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">U kosen, en u <rhyme label="b" type="f" corresp="#s4:l2">kallen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Is <rhyme label="c" type="m">liffelaffery</rhyme>;</l>
        <l id="s4:l7" met="2" type="-+">V woorden <rhyme label="d" type="f">bekoorden</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Noch dichten <rhyme label="e" type="f">verrichten</rhyme>,</l>
        <l id="s4:l9" met="2" type="+-">Daer noyt yet <rhyme label="c" type="m" corresp="#s4:l6">by</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
