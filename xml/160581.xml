<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>160581</id>
    <date>
      <year-start>1770</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Moet ik zo langer <rhyme label="a" type="f">leeven</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Cupido in deeze <rhyme label="b" type="m">stryd</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wilt myn verligtenis <rhyme label="a" type="f" corresp="#s1:l1">geven</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In myne jonge <rhyme label="b" type="m" corresp="#s1:l2">tyd</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Ik zal â€˜t den Hemel <rhyme label="c" type="f">klagen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ook het <rhyme label="d" type="m">Firmament</rhyme>;</l>
        <l id="s1:l7" met="3" type="-+">Dat ik myn jonge <rhyme label="c" type="f" corresp="#s1:l5">dage</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Verslyten in <rhyme label="d" type="m" corresp="#s1:l6">elend</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Herder dien ik <rhyme label="a" type="f">minde</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hy heeft myn hert <rhyme label="b" type="m">doorwond</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hy noemde myn zyn <rhyme label="a" type="f" corresp="#s2:l1">hinde</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hy kuste myne <rhyme label="b" type="m" corresp="#s2:l2">mond</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En zyne zoete <rhyme label="c" type="f">woorden</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Behoorde myne <rhyme label="d" type="m">zin</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En zyne <rhyme label="c" type="f" corresp="#s2:l5">Minnelonkjes</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dwongen my tot de <rhyme label="d" type="m" corresp="#s2:l6">Min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Na veele zoete <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">So bleef hy van my <rhyme label="b" type="m">af</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Dat zal myn nog doen <rhyme label="a" type="f" corresp="#s3:l1">doolen</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Tot in het duyster <rhyme label="b" type="m" corresp="#s3:l2">graf</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">O boome zoete <rhyme label="c" type="f">vrugten</rhyme>:</l>
        <l id="s3:l6" met="3" type="-+">O Waterstroomen <rhyme label="d" type="m">zoet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Voldoet myn <rhyme label="c" type="f" corresp="#s3:l5">Minne-nugten</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Eer dat ik sterven <rhyme label="d" type="m" corresp="#s3:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een Herder die ik <rhyme label="a" type="f">minde</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En die myn zwoer zyn <rhyme label="b" type="m">trouw</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">Onder de Groene <rhyme label="a" type="f" corresp="#s4:l1">Linde</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Helaes verlaet myn <rhyme label="b" type="m" corresp="#s4:l2">nou</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hy die myn Schaepjes <rhyme label="c" type="f">hoeden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Met vreugde en zoete <rhyme label="d" type="m">lust</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Hy die zyn Vee trouw <rhyme label="c" type="f" corresp="#s4:l5">hoede</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Tot hy ging in de <rhyme label="d" type="m" corresp="#s4:l6">rust</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">De Zon verligt zyn <rhyme label="a" type="f">stralen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hy laet myn in zyn <rhyme label="b" type="m">rust</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">Onder de Groene <rhyme label="a" type="f" corresp="#s5:l1">haegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Daer in zoo schep ik <rhyme label="b" type="m" corresp="#s5:l2">lust</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Maer als hy myn <rhyme label="c" type="f">ontwaekte</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Wat vreugd is dat voor <rhyme label="d" type="m">myn</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En hy myn wakker <rhyme label="c" type="f" corresp="#s5:l5">maekte</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Gedaen was al myn <rhyme label="d" type="m" corresp="#s5:l6">pyn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ryst op dan <rhyme label="a" type="f">Engelinne</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Ik heb u al lang <rhyme label="b" type="m">gezogt</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gy wist dat ik u <rhyme label="a" type="f" corresp="#s6:l1">beminde</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Wat hebt gy wel <rhyme label="b" type="m" corresp="#s6:l2">gedogd</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Dat ik niet ben <rhyme label="c" type="f">gekoomen</rhyme>?</l>
        <l id="s6:l6" met="3" type="-+">Op den bestelde <rhyme label="d" type="m">tyd</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ik zeg ik heb u <rhyme label="c" type="f" corresp="#s6:l5">vernomen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Scheld mt myn schuld dog <rhyme label="d" type="m" corresp="#s6:l6">kwyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ik heb u al <rhyme label="a" type="f">verkooren</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Lief tot myn Egte <rhyme label="b" type="m">Vrouw</rhyme>?</l>
        <l id="s7:l3" met="3" type="-+">Wy zyn voor een <rhyme label="a" type="f" corresp="#s7:l1">gebooren</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">Ik zal uw zyn <rhyme label="b" type="m" corresp="#s7:l2">getrouw</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Laet droefheud ende <rhyme label="c" type="f">zugten</rhyme>!</l>
        <l id="s7:l6" met="3" type="-+">Maer varen het is nu <rhyme label="d" type="m">tyd</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wy zullen met <rhyme label="c" type="f" corresp="#s7:l5">genugte</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Leven in <rhyme label="d" type="m" corresp="#s7:l6">eenzaemheid</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Komt herders by <rhyme label="a" type="f">malkander</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ter vreugde nu ter <rhyme label="b" type="m">Feest</rhyme>!</l>
        <l id="s8:l3" met="3" type="-+">Van daeg zoo trouwd <rhyme label="a" type="f" corresp="#s8:l1">Leander</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Weest nu vrolyk om het <rhyme label="b" type="m" corresp="#s8:l2">meest</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Wild nu spelen op de <rhyme label="c" type="f">Fluyten</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Brengt Bas en Fiool daer <rhyme label="d" type="m">by</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Coridon zal tot <rhyme label="c" type="f" corresp="#s8:l5">besluyten</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Maken een groot <rhyme label="d" type="m" corresp="#s8:l6">Melody</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
