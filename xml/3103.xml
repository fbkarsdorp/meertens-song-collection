  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">RUst u schoone Nimph <rhyme label="a" type="f">Kloorinde</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Onder dese hooge <rhyme label="a" type="f" corresp="#s1:l1">Linde</rhyme></l>
        <l id="s1:l3" met="4" type="+-">In het Jong'-gewassen <rhyme label="b" type="m">groen</rhyme>?</l>
        <l id="s1:l4" met="4" type="+-">En verkoelt mijn brandt, mijn <rhyme label="c" type="f">lusjes</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Met u Hemel-soete <rhyme label="c" type="f" corresp="#s1:l4">kusjes</rhyme>;</l>
        <l id="s1:l6" met="4" type="+-">Komt en laet ons lusjes <rhyme label="b" type="m" corresp="#s1:l3">voen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Neen, o vleyende <rhyme label="a" type="f">Leander</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Want ghy mint voor my een <rhyme label="a" type="f" corresp="#s2:l1">ander</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Een veel soeter <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">'k Meen u Philida u <rhyme label="c" type="f">Liefje</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Die ghy soete <rhyme label="c" type="f" corresp="#s2:l4">Minne-diefje</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Hebt gezworen uwe <rhyme label="b" type="m" corresp="#s2:l3">min</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">'k Sweer u by het <rhyme label="a" type="f">Minne-guytje</rhyme></l>
        <l id="s3:l2" met="4" type="+-">En oock by Leanders <rhyme label="a" type="f" corresp="#s3:l1">Fluytje</rhyme></l>
        <l id="s3:l3" met="4" type="+-">En oock by mijn Herders <rhyme label="b" type="m">trouw</rhyme>:</l>
        <l id="s3:l4" met="4" type="+-">Dat ick geene Nimph sal <rhyme label="c" type="f">minnen</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Als u (soete <rhyme label="c" type="f" corresp="#s3:l4">Herderinne</rhyme>,)</l>
        <l id="s3:l6" met="4" type="+-">Siet dit schenck ick u mijn <rhyme label="b" type="m" corresp="#s3:l3">Vrou</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Daer op geef ick u mijn <rhyme label="a" type="f">Vlockje</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Met mijn Else Lamm're <rhyme label="a" type="f" corresp="#s4:l1">stockje</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Hoed en wytse neffens d' <rhyme label="b" type="m">uw</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Want ick set mijn Leetjes <rhyme label="c" type="f">teeder</rhyme></l>
        <l id="s4:l5" met="4" type="+-">By u soete liefje <rhyme label="c" type="f" corresp="#s4:l4">neder</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Aen het ruysend' Beeckje <rhyme label="b" type="m" corresp="#s4:l3">leuw</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">U zy danck en eer <rhyme label="a" type="f">Goddinne</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Stoocksters van onse <rhyme label="a" type="f" corresp="#s5:l1">minne</rhyme></l>
        <l id="s5:l3" met="4" type="+-">En u lof o Venus <rhyme label="b" type="m">Soon</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Laet ick u ter eer mijn <rhyme label="c" type="f">Bruytje</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Quelen, spelen op mijn <rhyme label="c" type="f" corresp="#s5:l4">Fluytje</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Na der Pindarissen <rhyme label="b" type="m" corresp="#s5:l3">thoon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Juygt nu Beeckjes met u <rhyme label="a" type="f">Stroomen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat de blaetjes van de <rhyme label="a" type="f" corresp="#s6:l1">Boomen</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Drillen met een groote <rhyme label="b" type="m">vreught</rhyme>:</l>
        <l id="s6:l4" met="4" type="+-">Want ick sie een groot <rhyme label="c" type="f">gewemel</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">In den blaeuw' Lasuren <rhyme label="c" type="f" corresp="#s6:l4">Hemel</rhyme>;</l>
        <l id="s6:l6" met="4" type="+-">Om te kroonen Kloraes <rhyme label="b" type="m" corresp="#s6:l3">deught</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ach mijn lief u gluer'ge <rhyme label="a" type="f">lonckjes</rhyme></l>
        <l id="s7:l2" met="4" type="+-">Die doen blaecken sulcke <rhyme label="a" type="f" corresp="#s7:l1">vonckjes</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Alsoo dat mijn hertje <rhyme label="b" type="m">bernt</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Ende woelt in <rhyme label="c" type="f">Minne-strickjes</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Want u tintelende <rhyme label="c" type="f" corresp="#s7:l4">blickjes</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Die verdooven 't guld' <rhyme label="b" type="m" corresp="#s7:l3">Gesternt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Tsa dan Nimphjes, Bos <rhyme label="a" type="f">goddinnen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Herders, en ghy <rhyme label="a" type="f" corresp="#s8:l1">Herderinnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Vlecht nu voor mijn Lief een <rhyme label="b" type="m">Crans</rhyme>;</l>
        <l id="s8:l4" met="4" type="+-">Bloem en Kruytjes soet <rhyme label="c" type="f">gemengelt</rhyme></l>
        <l id="s8:l5" met="4" type="+-">En met Roosjes door <rhyme label="c" type="f" corresp="#s8:l4">gestrengelt</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">Om te singen aen den <rhyme label="b" type="m" corresp="#s8:l3">Dans</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
