<song>
  <meta>
    <id>35740</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Een kindje is ons geboren</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">IOannes, uwe <rhyme label="a" type="f">name</rhyme></l>
        <l id="s1:l2" met="3" type="-+">GabriÃ«l voor-seyt <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Aen u Vader, en <rhyme label="a" type="f" corresp="#s1:l1">fame</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Voor 't leven in u <rhyme label="b" type="m" corresp="#s1:l2">leeft</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Soo dra Goods Moeder <rhyme label="c" type="f">groete</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">U Moeder <rhyme label="d" type="m">maer</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">U hart verheught, en <rhyme label="c" type="f" corresp="#s1:l5">voete</rhyme>;</l>
        <l id="s1:l8" met="2" type="-+">Ghy springht in <rhyme label="d" type="m" corresp="#s1:l6">haer</rhyme>.</l>
        <l id="s1:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Sint Jan, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s1:l9">sterven</rhyme></l>
        <l id="s1:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s1:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy zijt sonder <rhyme label="a" type="f">erf-sonde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Heyligh, volmaeckt <rhyme label="b" type="m">gebaert</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Open u Vaders <rhyme label="a" type="f" corresp="#s2:l1">monde</rhyme>;</l>
        <l id="s2:l4" met="3" type="-+">Verblijt Hemel en <rhyme label="b" type="m" corresp="#s2:l2">Aerdt</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Ghy zijt een kint van <rhyme label="c" type="f">Iaren</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Sonder <rhyme label="d" type="m">kintsheydt</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">'s Werelts klip, wint noch <rhyme label="c" type="f" corresp="#s2:l5">baren</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Van Godt u <rhyme label="d" type="m" corresp="#s2:l6">leyt</rhyme>.</l>
        <l id="s2:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Sint Jan, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s2:l9">sterven</rhyme></l>
        <l id="s2:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s2:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy die zijt <rhyme label="a" type="f">d'aldermeesten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Leeft als een <rhyme label="b" type="m">Seraphijn</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Met 't vel bekleedt der <rhyme label="a" type="f" corresp="#s3:l1">Beesten</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">d'Eerste in de <rhyme label="b" type="m" corresp="#s3:l2">woestijn</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Drinckt Water; eet <rhyme label="c" type="f">Sprinckhanen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Niet is u <rhyme label="d" type="m">kost</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Sondaers door u <rhyme label="c" type="f" corresp="#s3:l5">vermanen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Van d'hell' <rhyme label="d" type="m" corresp="#s3:l6">verlost</rhyme>.</l>
        <l id="s3:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Sint Jan, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s3:l9">sterven</rhyme></l>
        <l id="s3:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s3:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy zijt Christ <rhyme label="a" type="f">Voorlooper</rhyme>;</l>
        <l id="s4:l2" met="3" type="-+">Ghy zijt zijn waertste <rhyme label="b" type="m">vrint</rhyme>;</l>
        <l id="s4:l3" met="3" type="-+">Ghy zijt Messias <rhyme label="a" type="f" corresp="#s4:l1">Dooper</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">In u men alles <rhyme label="b" type="m" corresp="#s4:l2">vint</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">De heylighste <rhyme label="c" type="f">geheeten</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Groot voor de <rhyme label="d" type="m">Heer</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Lof, en endt der <rhyme label="c" type="f" corresp="#s4:l5">Propheten</rhyme>.</l>
        <l id="s4:l8" met="2" type="-+">Haet lof en <rhyme label="d" type="m" corresp="#s4:l6">eer</rhyme>.</l>
        <l id="s4:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Sint Jan, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s4:l9">sterven</rhyme></l>
        <l id="s4:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s4:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy zijt een zee vol <rhyme label="a" type="f">zegen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Patroon der <rhyme label="b" type="m">heyligheyt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Bereyder van Goods <rhyme label="a" type="f" corresp="#s5:l1">wegen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Spiegel van <rhyme label="b" type="m" corresp="#s5:l2">reynigheydt</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ootmoedigh sonder <rhyme label="c" type="f">gronde</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Toonder van 't <rhyme label="d" type="m">Lam</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Trompet tegens de <rhyme label="c" type="f" corresp="#s5:l5">zonde</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Vol liefde <rhyme label="d" type="m" corresp="#s5:l6">vlam</rhyme>.</l>
        <l id="s5:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s5:l10" met="2" type="-+">Sint Ian, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s5:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s5:l9">sterven</rhyme></l>
        <l id="s5:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s5:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ghy zijt een school der <rhyme label="a" type="f">deughden</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En Sondaeren <rhyme label="b" type="m">zeyl-steen</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">Niet als Godt u <rhyme label="a" type="f" corresp="#s6:l1">verheughden</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Met Godt zijt ghy <rhyme label="b" type="m" corresp="#s6:l2">gemeen</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Olijf-tack van de <rhyme label="c" type="f">vrede</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Goods Kercx <rhyme label="d" type="m">Pylaer</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Licht sonder <rhyme label="c" type="f" corresp="#s6:l5">duysterhede</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Brandende <rhyme label="d" type="m" corresp="#s6:l6">klaer</rhyme>.</l>
        <l id="s6:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s6:l10" met="2" type="-+">Sint Jan, Sint <rhyme label="f" type="m">Jan</rhyme>,</l>
        <l id="s6:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s6:l9">sterven</rhyme></l>
        <l id="s6:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s6:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Ghy zijt meer als <rhyme label="a" type="f">Elias</rhyme>.</l>
        <l id="s7:l2" met="3" type="-+">Tempel van d'heyligh <rhyme label="b" type="m">Geest</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gerekent als <rhyme label="a" type="f" corresp="#s7:l1">Messias</rhyme>;</l>
        <l id="s7:l4" met="3" type="-+">Als Sampson <rhyme label="b" type="m" corresp="#s7:l2">onbevreest</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">U hart dat raeckt geen <rhyme label="c" type="f">aerde</rhyme>.</l>
        <l id="s7:l6" met="2" type="-+">Sint Ian! Sint <rhyme label="d" type="m">Ian</rhyme>!</l>
        <l id="s7:l7" met="3" type="-+">Niemant yet van u <rhyme label="c" type="f" corresp="#s7:l5">waerde</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Verhale <rhyme label="d" type="m" corresp="#s7:l6">kan</rhyme>.</l>
        <l id="s7:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s7:l10" met="2" type="-+">Sint Ian, Sint <rhyme label="f" type="m">Ian</rhyme>,</l>
        <l id="s7:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s7:l9">sterven</rhyme></l>
        <l id="s7:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s7:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy zijt, die 't quaedt <rhyme label="a" type="f">afschafte</rhyme>;</l>
        <l id="s8:l2" met="3" type="-+">Niet door de vingers <rhyme label="b" type="m">siet</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Herodes feyt <rhyme label="a" type="f" corresp="#s8:l1">bestrafte</rhyme>.</l>
        <l id="s8:l4" met="3" type="-+">Ghy zijt geen buygent <rhyme label="b" type="m" corresp="#s8:l2">riet</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">U ziel als sneeuw blijft <rhyme label="c" type="f">witter</rhyme>.</l>
        <l id="s8:l6" met="2" type="-+">Ghy zijt <rhyme label="d" type="m">Patroon</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Lucifers plaets <rhyme label="c" type="f" corresp="#s8:l5">besitter</rhyme></l>
        <l id="s8:l8" met="2" type="-+">In 's Hemels <rhyme label="d" type="m" corresp="#s8:l6">throon</rhyme>.</l>
        <l id="s8:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s8:l10" met="2" type="-+">Waerde <rhyme label="f" type="m">Patroon</rhyme>!</l>
        <l id="s8:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s8:l9">sterven</rhyme></l>
        <l id="s8:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s8:l10">loon</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Als dorstigh hart sonder <rhyme label="a" type="f">bedaeren</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Hebt ghy <rhyme label="b" type="m">gejaeght</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Na de kroone der <rhyme label="a" type="f" corresp="#s9:l1">Martelaeren</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">Doctoor en <rhyme label="b" type="m" corresp="#s9:l2">Maeght</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Ghy zijt gekroont met <rhyme label="c" type="f">kroone</rhyme></l>
        <l id="s9:l6" met="2" type="-+">Van Lely en <rhyme label="d" type="m">Laurier</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">En Palm van Godes <rhyme label="c" type="f" corresp="#s9:l5">Zoone</rhyme></l>
        <l id="s9:l8" met="2" type="-+">Met Hemelsche <rhyme label="d" type="m" corresp="#s9:l6">playsier</rhyme>.</l>
        <l id="s9:l9" met="4" type="-+">Gelieft ons te <rhyme label="e" type="f">verwerven</rhyme></l>
        <l id="s9:l10" met="2" type="-+">Waerde <rhyme label="f" type="m">Patroon</rhyme>!</l>
        <l id="s9:l11" met="4" type="+-">Dat wy krijgen in 't <rhyme label="e" type="f" corresp="#s9:l9">sterven</rhyme></l>
        <l id="s9:l12" met="2" type="-+">Godt voor een <rhyme label="f" type="m" corresp="#s9:l10">loon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
