    <melody>Gelobt seist du Jesu Christ</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IN Godes name varen <rhyme label="a" type="m">wy</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Sijne ghenade die sy ons <rhyme label="a" type="m" corresp="#s1:l1">by</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Den Vader goet in zijn <rhyme label="b" type="m">bedrijf</rhyme>,</l>
        <l id="s1:l4" met="6" type="-+">Beware siele ende lijf. <rhyme label="c" type="m">Kyrieleys</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Christus sy onsen Leytsman <rhyme label="a" type="m">hier</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">En blijue by onsen weghe <rhyme label="a" type="m" corresp="#s2:l1">fier</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Keerende van ons Sathans <rhyme label="b" type="m">list</rhyme>,</l>
        <l id="s2:l4" met="6" type="-+">En wat zijn woort van ons verquist. <rhyme label="c" type="m">Kyrie</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den heyligen Gheest die leyde ons <rhyme label="a" type="m">voort</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">In zijnen gauen vol <rhyme label="a" type="m" corresp="#s3:l1">confoort</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Troostende ons in allen <rhyme label="b" type="m">noot</rhyme>,</l>
        <l id="s3:l4" met="6" type="-+">Opdat wy leuen, sonder doot. <rhyme label="c" type="m">Kyerieleys</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
