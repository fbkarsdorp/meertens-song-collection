        <l id="s1:l4" met="3" type="-+">En, Ã´ <rhyme label="c" type="m">Bruydje-maet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ick wensch dat die <rhyme label="c" type="m" corresp="#s1:l4">staet</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Daer ghy nu in treedt, 't Min-Goodtje <rhyme label="a" type="m" corresp="#s1:l1">bestier</rhyme>.</l>
