<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3877</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="saint song">heiligenlied</tag>
    </tags>
    <melody>Ik weet een vrouwtje wel bereid</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WI willen heden vrolic <rhyme label="a" type="f">sijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op desen hooghen <rhyme label="b" type="m">dach</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Met onsen vader <rhyme label="c" type="f">Franciscus</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Diemen niet volprisen en <rhyme label="b" type="m" corresp="#s1:l2">mach</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s1:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s1:l5">noot</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hi dede wt sijn <rhyme label="a" type="f">cleederen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die hi te draghen <rhyme label="b" type="m">plach</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Hi werpse voer sijns vaders <rhyme label="c" type="f">voeten</rhyme></l>
        <l id="s2:l4" met="3" type="-+">De werelt was hem een <rhyme label="b" type="m" corresp="#s2:l2">caf</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s2:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s2:l5">noot</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Franciscus sach op sinen <rhyme label="a" type="f">voet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ghelijc dat doet die <rhyme label="b" type="m">pauwe</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hi liet sincken sinen hogen <rhyme label="c" type="f">moet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ende dede aen die cleeden <rhyme label="b" type="m" corresp="#s3:l2">grauwe</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s3:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s3:l5">noot</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hi is seer <rhyme label="a" type="f">goedertieren</rhyme></l>
        <l id="s4:l2" met="3" type="-+">In sinen <rhyme label="b" type="m">ommeganck</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Sijn hertken bloeyt van <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ghelijc een wijngaert <rhyme label="b" type="m" corresp="#s4:l2">ranck</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s4:l5">noot</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Doen dese heylighe <rhyme label="a" type="f">vader</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dus vast in duechden <rhyme label="b" type="m">ghinck</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Werthi so vierich in Christus <rhyme label="c" type="f">minne</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dat hi die vijf wonden <rhyme label="b" type="m" corresp="#s5:l2">ontfinc</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s5:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s5:l5">noot</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hij is seer <rhyme label="a" type="f">goedertieren</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van duechden op <rhyme label="b" type="m">aertrijck</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Van binnen ende ooc van <rhyme label="c" type="f">buten</rhyme></l>
        <l id="s6:l4" met="3" type="-+">So en is niemant sijns <rhyme label="b" type="m" corresp="#s6:l2">ghelijc</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s6:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s6:l5">noot</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu is hy <rhyme label="a" type="f">gheuaren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hier bouen int hemelsce <rhyme label="b" type="m">lant</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Al in dat choor der <rhyme label="c" type="f">seraphinnen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">By Christus rechter <rhyme label="b" type="m" corresp="#s7:l2">hant</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s7:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s7:l5">noot</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu laet ons hem <rhyme label="a" type="f">bidden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Met groter <rhyme label="b" type="m">innicheyt</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat hi ons wil <rhyme label="c" type="f">verweruen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Die duecht der <rhyme label="b" type="m" corresp="#s8:l2">ootmoedicheyt</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Want hi is waerlijck <rhyme label="d" type="m">groot</rhyme></l>
        <l id="s8:l6" met="3" type="-+">O heylige vader <rhyme label="e" type="f">Franciscus</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Staet ons bi in onser <rhyme label="d" type="m" corresp="#s8:l5">noot</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
