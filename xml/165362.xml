<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>165362</id>
    <date>
      <year-start>1620</year-start>
      <year-end>1625</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags>
      <tag>new year song</tag>
    </tags>
    <melody>Es ist das Heil uns kommen her</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Verblijt u al weest <rhyme label="a" type="m">onversaecht</rhyme></l>
        <l id="s1:l2" met="3" type="-+">nae des heren woort wilt <rhyme label="b" type="f">leven</rhyme></l>
        <l id="s1:l3" met="4" type="-+">daer is geboren van een <rhyme label="a" type="m" corresp="#s1:l1">maecht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">een zoon is ons <rhyme label="b" type="f" corresp="#s1:l2">gegeven</rhyme></l>
        <l id="s1:l5" met="4" type="-+">die daer is t'belooffde vrouwen <rhyme label="c" type="m">zaet</rhyme></l>
        <l id="s1:l6" met="4" type="-+">voor onsen meesten <rhyme label="c" type="m" corresp="#s1:l5">toeverlaet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">alzo daer staet <rhyme label="b" type="f" corresp="#s1:l2">geschreven</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Synen Naem is <rhyme label="a" type="m">Emanuel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">heerlick en oock <rhyme label="b" type="f">bewesen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">zo wie daer doet nae zyn <rhyme label="a" type="m" corresp="#s2:l1">bevel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">die wert van hem <rhyme label="b" type="f" corresp="#s2:l2">gepresen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">hy is ons uijt lieffden <rhyme label="c" type="m">groot</rhyme></l>
        <l id="s2:l6" met="4" type="-+">gesonden uijt des vaders <rhyme label="c" type="m" corresp="#s2:l5">schoot</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Om ons hier te <rhyme label="b" type="f" corresp="#s2:l2">genesen</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">het hantschrift dat heeft hij <rhyme label="a" type="m">gescheurt</rhyme></l>
        <l id="s3:l2" met="3" type="-+">voor ons ginck hy <rhyme label="b" type="f">betalen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">de Mensch hadde de doot <rhyme label="a" type="m" corresp="#s3:l1">verbeurt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">hij vrijt ons al te <rhyme label="b" type="f" corresp="#s3:l2">malen</rhyme></l>
        <l id="s3:l5" met="4" type="-+">door hem wy tot den vader <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s3:l6" met="4" type="-+">de tweede Doot mach ons nyet <rhyme label="c" type="m" corresp="#s3:l5">schaen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">al na des schrifts <rhyme label="b" type="f" corresp="#s3:l2">verhalen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Hij heeft vercregen door zijn <rhyme label="a" type="m">Cracht</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Over de Doot <rhyme label="b" type="f">victorij</rhyme></l>
        <l id="s4:l3" met="4" type="-+">en ons de Salicheyt <rhyme label="a" type="m" corresp="#s4:l1">gewracht</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Is verresen In <rhyme label="b" type="f" corresp="#s4:l2">glory</rhyme></l>
        <l id="s4:l5" met="4" type="-+">zidt nu aen svaders rechten <rhyme label="c" type="m">hant</rhyme></l>
        <l id="s4:l6" met="4" type="-+">voor ons bereyt het besten <rhyme label="c" type="m" corresp="#s4:l5">pant</rhyme></l>
        <l id="s4:l7" met="3" type="-+">hout dit doch In <rhyme label="b" type="f" corresp="#s4:l2">Memorij</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Souckt den Coninck met <rhyme label="a" type="m">ootmoet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">also de wijsen <rhyme label="b" type="f">deden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">u Schatten voor hem <rhyme label="a" type="m" corresp="#s5:l1">opendoet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Offert met <rhyme label="b" type="f" corresp="#s5:l2">naersticheden</rhyme></l>
        <l id="s5:l5" met="4" type="-+">daer hij noch In armoede <rhyme label="c" type="m">leijt</rhyme></l>
        <l id="s5:l6" met="4" type="-+">u loon wert hemels <rhyme label="c" type="m" corresp="#s5:l5">toegesent</rhyme></l>
        <l id="s5:l7" met="3" type="-+">zo sal hij zijn te <rhyme label="b" type="f" corresp="#s5:l2">vreden</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Looft desen Prins <rhyme label="a" type="m">gebenedijt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">die ons verlost een <rhyme label="b" type="f">paer</rhyme></l>
        <l id="s6:l3" met="4" type="-+">en maeckt voor hem u sonden <rhyme label="a" type="m" corresp="#s6:l1">quijt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">hy is een <rhyme label="b" type="f" corresp="#s6:l2">Middelaer</rhyme></l>
        <l id="s6:l5" met="4" type="-+">doe nae zyn woort tot Inden <rhyme label="c" type="m">ent</rhyme></l>
        <l id="s6:l6" met="4" type="-+">En toont dat lieffd'es <rhyme label="c" type="m" corresp="#s6:l5">t'Fondament</rhyme></l>
        <l id="s6:l7" met="3" type="-+">In desen Nieuwe <rhyme label="b" type="f" corresp="#s6:l2">Jaer</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
