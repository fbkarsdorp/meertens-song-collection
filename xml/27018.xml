  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Die le the die machen sich <rhyme label="a" type="f">spitzich</rhyme> /</l>
        <l id="s1:l2" met="3" type="-+">a ff mich gar vn ersch <rhyme label="b" type="m">ldtt</rhyme> /</l>
        <l id="s1:l3" met="3" type="-+">ich sollt das pillich <rhyme label="a" type="f" corresp="#s1:l1">rechenn</rhyme> /</l>
        <l id="s1:l4" met="3" type="-+">noch leidtt ichs mitt ged <rhyme label="b" type="m" corresp="#s1:l2">ldtt</rhyme> /</l>
        <l id="s1:l5" met="3" type="-+">mochtt ichs recht <rhyme label="c" type="m">erfarrenn</rhyme> /</l>
        <l id="s1:l6" met="3" type="-+">ich wollt sei wieder nitt <rhyme label="d" type="m">sparrenn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Deß glaub d  mir f <rhyme label="c" type="m" corresp="#s1:l5">rware</rhyme></l>
        <l id="s1:l8" met="3" type="-+">eß m s gerechentt <rhyme label="e" type="m">sein</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sei k nnenn sich k rtzweilig <rhyme label="a" type="f">machenn</rhyme> /</l>
        <l id="s2:l2" met="3" type="-+">wann sei gedenckenn an <rhyme label="b" type="m">mich</rhyme> /</l>
        <l id="s2:l3" met="3" type="-+">Mitt schimpfenn vnnd mitt <rhyme label="a" type="f" corresp="#s2:l1">schertzenn</rhyme> /</l>
        <l id="s2:l4" met="3" type="-+">So moeß ich bei ir <rhyme label="b" type="m" corresp="#s2:l2">sein</rhyme> /</l>
        <l id="s2:l5" met="3" type="-+">Vnnd was mir brengett fre <rhyme label="c" type="m">de</rhyme> /</l>
        <l id="s2:l6" met="3" type="-+">Daß wirtt mir nemantz <rhyme label="d" type="m">leidenn</rhyme> /</l>
        <l id="s2:l7" met="3" type="-+">Jch reddenn das bei meinem <rhyme label="c" type="m" corresp="#s2:l5">eidte</rhyme> /</l>
        <l id="s2:l8" met="3" type="-+">Jch wils jm pleiben <rhyme label="e" type="m">holtt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sie th ntt sich bei mich <rhyme label="a" type="f">setzenn</rhyme> /</l>
        <l id="s3:l2" met="3" type="-+">vnnd gebenn mir g ette <rhyme label="b" type="m">wortt</rhyme> /</l>
        <l id="s3:l3" met="3" type="-+">A ß jrem falschenn <rhyme label="a" type="f" corresp="#s3:l1">hertzenn</rhyme> /</l>
        <l id="s3:l4" met="3" type="-+">Jch hab es offt <rhyme label="b" type="m" corresp="#s3:l2">gehortt</rhyme> /</l>
        <l id="s3:l5" met="3" type="-+">Sei th ntt mich fre ndtlich <rhyme label="c" type="m">fragenn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">mein leidt sei hertzlich <rhyme label="d" type="m">beclagenn</rhyme> /</l>
        <l id="s3:l7" met="3" type="-+">Die mich so falschlich <rhyme label="c" type="m" corresp="#s3:l5">tragenn</rhyme> /</l>
        <l id="s3:l8" met="3" type="-+">ich th e als mirck ichs <rhyme label="e" type="m">nitt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die mich regierenn mitt <rhyme label="a" type="f">krefftenn</rhyme></l>
        <l id="s4:l2" met="3" type="-+">woll offt a ff manchem <rhyme label="b" type="m">artt</rhyme> /</l>
        <l id="s4:l3" met="3" type="-+">Sei dreiben vill der <rhyme label="a" type="f" corresp="#s4:l1">geschefftenn</rhyme> /</l>
        <l id="s4:l4" met="3" type="-+">Jch thrag es gar klein sch <rhyme label="b" type="m" corresp="#s4:l2">ldtt</rhyme> /</l>
        <l id="s4:l5" met="3" type="-+">So bin ichs nitt <rhyme label="c" type="m">alleine</rhyme> /</l>
        <l id="s4:l6" met="3" type="-+">Man findett sei fast <rhyme label="d" type="m">gemein</rhyme> /</l>
        <l id="s4:l7" met="3" type="-+">Die grossenn vnnd a ch die <rhyme label="c" type="m" corresp="#s4:l5">kleinenn</rhyme> /</l>
        <l id="s4:l8" met="3" type="-+">Dasselbig erfrewett <rhyme label="e" type="m">mich</rhyme>.</l>
      </lg>
    </lg>
