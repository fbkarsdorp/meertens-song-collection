  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">ALs een gheslaghen <rhyme label="a" type="m">ros</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Is mijn hert seer <rhyme label="b" type="f">banghe</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ick loop in’t Veldt en <rhyme label="a" type="m" corresp="#s1:l1">Bos</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Al om een Wildt te <rhyme label="b" type="f" corresp="#s1:l2">vanghen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Het Wildt ick seer wel <rhyme label="c" type="f">kenne</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Daer ick na jaghe <rhyme label="d" type="m">siet</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Waer dat ick loop of <rhyme label="c" type="f" corresp="#s1:l5">renne</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Ghevanghen kan ick <rhyme label="d" type="m" corresp="#s1:l6">niet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dat wildt daer ick na <rhyme label="a" type="m">jaegh</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Is een uyt Venus <rhyme label="b" type="f">zale</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Reyn liefde ick haer <rhyme label="a" type="m" corresp="#s2:l1">draegh</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Want Cupido sijn <rhyme label="b" type="f" corresp="#s2:l2">stralen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">In’t hert my heeft <rhyme label="c" type="f">gheschoten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En tot der doot <rhyme label="d" type="m">ghewondt</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Doch ‘theeft my noyt <rhyme label="c" type="f" corresp="#s2:l5">verdroten</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Als ick troost by haer <rhyme label="d" type="m" corresp="#s2:l6">vondt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Retorijcke <rhyme label="a" type="m">playsant</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Spreeckt sy met held’re <rhyme label="b" type="f">tale</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Haer keeltjen <rhyme label="a" type="m" corresp="#s3:l1">tryumphant</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Klinckt als de <rhyme label="b" type="f" corresp="#s3:l2">Nachtegale</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Net is sy op haer <rhyme label="c" type="f">ganghen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Als een Paeu moedich <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Haer schoon blosende <rhyme label="c" type="f" corresp="#s3:l5">Wanghen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Schijnen als een Roos <rhyme label="d" type="m" corresp="#s3:l6">root</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In eer en deucht <rhyme label="a" type="m">loyael</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Sal ick mijn lief <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Haer Lipkens als <rhyme label="a" type="m" corresp="#s4:l1">korael</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Twee Ooghjens als <rhyme label="b" type="f" corresp="#s4:l2">robinnen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Haer Hals wit als een <rhyme label="c" type="f">Swane</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Daer toe een rooder <rhyme label="d" type="m">Mont</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Noyt schoonder op Venus <rhyme label="c" type="f" corresp="#s4:l5">bane</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Vondt ick in ghener <rhyme label="d" type="m" corresp="#s4:l6">stont</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Clara mijn liefste <rhyme label="a" type="m">lief</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Reyn Bloemken <rhyme label="b" type="f">uytghelesen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Ghy sijt mijn’s sins <rhyme label="a" type="m" corresp="#s5:l1">gherief</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Wilt hier mijn troost doch <rhyme label="b" type="f" corresp="#s5:l2">wesen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Wilt mijn treuren <rhyme label="c" type="f">bemercken</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Godt sal hier langh <rhyme label="d" type="m">voorwaer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">De liefde wel <rhyme label="c" type="f" corresp="#s5:l5">verstercken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Geeft troost uwen <rhyme label="d" type="m" corresp="#s5:l6">dienaer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Niemant in ‘swerelts <rhyme label="a" type="m">ront</rhyme></l>
        <l id="s6:l2" met="3" type="-+">En weet ick Maghet <rhyme label="b" type="f">reyne</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Die my nu maeckt <rhyme label="a" type="m" corresp="#s6:l1">ghesont</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Dan ghy schoon lief <rhyme label="b" type="f" corresp="#s6:l2">alleyne</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Schoon maeghdelijcke <rhyme label="c" type="f">kroone</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Doet my doch <rhyme label="d" type="m">onderstant</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Gheeft my hier tot een <rhyme label="c" type="f" corresp="#s6:l5">loone</rhyme></l>
        <l id="s6:l8" met="3" type="-+">U lieve <rhyme label="d" type="m" corresp="#s6:l6">rechterhandt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
