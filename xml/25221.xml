    <tags>
      <tag standard-name="morgenlied" translation="morning song">morgenlied</tag>
      <tag standard-name="gebedslied" translation="prayer">gebedslied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Nae dat tlichaem doer rust is <rhyme label="a" type="m">kloeck</rhyme></l>
        <l id="s1:l2" met="4" type="-+">latende tbet in sijnen <rhyme label="a" type="m" corresp="#s1:l1">hoeck</rhyme></l>
        <l id="s1:l3" met="4" type="-+">wij bidden vader zijt ons <rhyme label="b" type="m">bij</rhyme></l>
        <l id="s1:l4" met="4" type="-+">die sijn singende <rhyme label="b" type="m" corresp="#s1:l3">melodij</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Gedoecht ons tong v singe <rhyme label="a" type="m">eerst</rhyme></l>
        <l id="s2:l2" met="4" type="-+">oock sherten brandt, v minne <rhyme label="a" type="m" corresp="#s2:l1">meest</rhyme></l>
        <l id="s2:l3" met="4" type="-+">op dat van ons toecomende <rhyme label="b" type="m">werck</rhyme></l>
        <l id="s2:l4" met="4" type="-+">tbegin ghij sijt en vortganck <rhyme label="b" type="m" corresp="#s2:l3">sterck</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Laet den duijster plaets geuen <rhyme label="a" type="m">tlicht</rhyme></l>
        <l id="s3:l2" met="4" type="-+">oock nacht den son van v <rhyme label="a" type="m" corresp="#s3:l1">gesticht</rhyme></l>
        <l id="s3:l3" met="4" type="-+">op dat schult welck nacht heft <rhyme label="b" type="m">gemaeckt</rhyme></l>
        <l id="s3:l4" met="4" type="-+">doer tlichts verscijn wert <rhyme label="b" type="m" corresp="#s3:l3">affgemaeckt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wij bidden oock met diep <rhyme label="a" type="m">ootmoet</rhyme></l>
        <l id="s4:l2" met="4" type="-+">wilt drucken tquaet onder de <rhyme label="a" type="m" corresp="#s4:l1">voet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">dat wij singende louen <rhyme label="b" type="m">v</rhyme></l>
        <l id="s4:l4" met="4" type="-+">In ewicheijt altijt als <rhyme label="b" type="m" corresp="#s4:l3">nu</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
