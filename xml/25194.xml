  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Huot der eren zallin <rhyme label="a" type="f">stundin</rhyme>.</l>
        <l id="s1:l2" met="3" type="+-">armuote wirt wol <rhyme label="b" type="m">rait</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">ere dye wirt nemer <rhyme label="a" type="f" corresp="#s1:l1">vundin</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">dem der si verloren <rhyme label="b" type="m" corresp="#s1:l2">hait</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">der werbe ummerme uom <rhyme label="c" type="f">ere</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">si wirt ym gar nummer <rhyme label="c" type="f" corresp="#s1:l5">mere</rhyme>.</l>
        <l id="s1:l7" met="4" type="+-">matum und ere sint wol in <rhyme label="d" type="m">eyn</rhyme>.</l>
        <l id="s1:l8" met="7" type="-+">wer dye zeyme male verlurit. si kuomt yme gar nemer <rhyme label="d" type="m" corresp="#s1:l7">heym</rhyme>.</l>
      </lg>
