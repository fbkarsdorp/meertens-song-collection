  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">U zoete gonst, o <rhyme label="a" type="m">Rosemont</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Die is 't alleen daer ick tot aller <rhyme label="a" type="m" corresp="#s1:l1">stont</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">En steets nae <rhyme label="b" type="m">haeck</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dies geeft my het <rhyme label="b" type="m" corresp="#s1:l3">vermaeck</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Dat ghy my geven <rhyme label="a" type="m" corresp="#s1:l1">kont</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ey went u ook tot <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l7" met="5" type="-+">En siet eens aen mijn trouwe dienst <rhyme label="c" type="m" corresp="#s1:l6">waerdy</rhyme>:</l>
        <l id="s1:l8" met="2" type="-+">Ghy kont my 't <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s1:l9" met="3" type="+-">Dat ghy naemt weer <rhyme label="d" type="f" corresp="#s1:l8">geven</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">En my maecken <rhyme label="c" type="m" corresp="#s1:l6">bly</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Nadien natuur u heeft <rhyme label="a" type="m">gegunt</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">Dat ghy my vreucht en droefheyt geven <rhyme label="a" type="m" corresp="#s2:l1">kunt</rhyme>;</l>
        <l id="s2:l3" met="2" type="-+">Ey geeft my 't <rhyme label="b" type="m">soet</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">Dat my u lieven <rhyme label="b" type="m" corresp="#s2:l3">doet</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En dat in u <rhyme label="a" type="m" corresp="#s2:l1">uytmunt</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Verlicht mijn sware <rhyme label="c" type="m">smert</rhyme>,</l>
        <l id="s2:l7" met="5" type="-+">Die staech door hoop en vrees gevoedet <rhyme label="c" type="m" corresp="#s2:l6">wert</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Of wilt ghy 't <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s2:l9" met="3" type="+-">My niet weder <rhyme label="d" type="f" corresp="#s2:l8">geven</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Soo neemt oock mijn <rhyme label="c" type="m" corresp="#s2:l6">hert</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
