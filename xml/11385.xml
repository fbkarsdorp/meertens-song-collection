  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt Beeld van Venus <rhyme label="a" type="f">aanzicht</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Met mellik, en blosjes <rhyme label="b" type="m">geverft</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verdooft Dianâs Maan - <rhyme label="a" type="f" corresp="#s1:l1">licht</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En ellik zich losjes <rhyme label="b" type="m" corresp="#s1:l2">doorkerft</rhyme>,</l>
        <l id="s1:l5" met="1" type="-+">Als <rhyme label="c" type="f">klachjes</rhyme></l>
        <l id="s1:l6" met="1" type="-+">En <rhyme label="c" type="f" corresp="#s1:l5">lachjes</rhyme>,</l>
        <l id="s1:l7" met="1" type="-+">En <rhyme label="d" type="f">kluchjes</rhyme></l>
        <l id="s1:l8" met="1" type="-+">En <rhyme label="d" type="f" corresp="#s1:l7">zuchjes</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Getempert op hun <rhyme label="e" type="m">pas</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Haar Goddelijkheid <rhyme label="f" type="f">ontvallen</rhyme>;</l>
        <l id="s1:l11" met="3" type="+-">Het hart, dat stenig <rhyme label="e" type="m" corresp="#s1:l9">was</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Verbreizelt zy als <rhyme label="e" type="m" corresp="#s1:l9">glas</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Lucretia, mijn <rhyme label="a" type="f">waarde</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Giet luchjes in mijn <rhyme label="b" type="m">long</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">O Nimf! zoo gy <rhyme label="a" type="f" corresp="#s2:l1">bedaarde</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Zou mijn verstokte <rhyme label="b" type="m" corresp="#s2:l2">tong</rhyme></l>
        <l id="s2:l5" met="1" type="-+">Met <rhyme label="c" type="f">d'Eng'len</rhyme></l>
        <l id="s2:l6" met="1" type="-+">Zich <rhyme label="c" type="f" corresp="#s2:l5">meng'len</rhyme>,</l>
        <l id="s2:l7" met="1" type="-+">En <rhyme label="d" type="f">treffen</rhyme>,</l>
        <l id="s2:l8" met="1" type="-+">Door 't <rhyme label="d" type="f" corresp="#s2:l7">heffen</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Het pit van Iunôs <rhyme label="e" type="m">hert</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Zoo immer, door <rhyme label="f" type="f">gebeden</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Een Maegd bewogen <rhyme label="e" type="m" corresp="#s2:l9">werdt</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Geef lichting aan mijn <rhyme label="e" type="m" corresp="#s2:l9">smert</rhyme>.</l>
      </lg>
    </lg>
  </text>
