<song>
  <meta>
    <id>36225</id>
    <date>
      <year-start>1675</year-start>
      <year-end>1675</year-end>
    </date>
    <location>
      <orginal-name>gent</orginal-name>
      <standard-name>Gent, Belgium</standard-name>
      <coordinates>
        <lon>51.0538286</lon>
        <lat>3.7250121</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="meilied" translation="May song">meilied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Met seer lieffelijcke <rhyme label="a" type="f">sanghen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Heel verblijdt al naer den <rhyme label="b" type="m">gheest</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Hebben wy ghedaen ons <rhyme label="a" type="f" corresp="#s1:l1">ganghen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Uyt dat gheestelijck <rhyme label="b" type="m" corresp="#s1:l2">foreest</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Wy commen tot u <rhyme label="c" type="f">ghetreden</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Met den Mey seer <rhyme label="d" type="m">excellent</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Die daer is ghebonden <rhyme label="c" type="f" corresp="#s1:l5">heden</rhyme></l>
        <l id="s1:l8" met="4" type="+-">In een pleyn seer <rhyme label="d" type="m" corresp="#s1:l6">pertinent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Al de boomen en de <rhyme label="a" type="f">meyen</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Al uytspruytsel, en <rhyme label="b" type="m">plantsoen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Syn in schoonicheyt <rhyme label="a" type="f" corresp="#s2:l1">verscheyen</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Elck gheeft daer diversch syn <rhyme label="b" type="m" corresp="#s2:l2">groen</rhyme>;</l>
        <l id="s2:l5" met="4" type="+-">Maer waer me wy u <rhyme label="c" type="f">begroeten</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Is den Mey van Christus <rhyme label="d" type="m">soet</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Uyt wiens handen ende <rhyme label="c" type="f" corresp="#s2:l5">voeten</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Hy noch draeght dat weerdich <rhyme label="d" type="m" corresp="#s2:l6">Bloet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Compt by ons wilt u <rhyme label="a" type="f">vertooghen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Compt siet desen Mey:boom <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Compt en wilt u selven <rhyme label="a" type="f" corresp="#s3:l1">pooghen</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Wilt niet op u selven <rhyme label="b" type="m" corresp="#s3:l2">staen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Een ziele die heeft dat <rhyme label="c" type="f">inne</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat sy lief en ghern <rhyme label="d" type="m">heeft</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Daer is sy meer door Godts <rhyme label="c" type="f" corresp="#s3:l5">minne</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Dan daer sy het leven <rhyme label="d" type="m" corresp="#s3:l6">gheeft</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">T'Cruys dat hier nu is <rhyme label="a" type="f">ghevonden</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Dat moet wesen <rhyme label="b" type="m">inghevoert</rhyme>;</l>
        <l id="s4:l3" met="4" type="+-">Doet dat toch in alle <rhyme label="a" type="f" corresp="#s4:l1">stonden</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Ghy sult lyden <rhyme label="b" type="m" corresp="#s4:l2">onberoert</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Van alle u <rhyme label="c" type="f">onghevallen</rhyme></l>
        <l id="s4:l6" met="4" type="+-">In wat menichte <rhyme label="d" type="m">vervult</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Sult ghy op Godts Cruyce <rhyme label="c" type="f" corresp="#s4:l5">vallen</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Dat ghy t'uw' vergheten <rhyme label="d" type="m" corresp="#s4:l6">sult</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Maeckt hier af een <rhyme label="a" type="f">oeffeninghe</rhyme></l>
        <l id="s5:l2" met="4" type="+-">Stelt Godts Cruyce in u <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">U sal dincken <rhyme label="a" type="f" corresp="#s5:l1">sonderlinghe</rhyme></l>
        <l id="s5:l4" met="4" type="+-">Dat ghy leeft hier sonder <rhyme label="b" type="m" corresp="#s5:l2">smert</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Want t'lyden dat wordt <rhyme label="c" type="f">verdwenen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">En het compt op niet seer <rhyme label="d" type="m">ras</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Alsmen dinckt op Godt den <rhyme label="c" type="f" corresp="#s5:l5">ghenen</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Die het Cruys onschuldich <rhyme label="d" type="m" corresp="#s5:l6">was</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Lof laet ons te saemen <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Godt tot inder <rhyme label="b" type="m">eeuwicheyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Dat hy ons heeft willen <rhyme label="a" type="f" corresp="#s6:l1">wijsen</rhyme></l>
        <l id="s6:l4" met="4" type="+-">Waer dien Mey:boom was <rhyme label="b" type="m" corresp="#s6:l2">gheleyt</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Laet ons oock, hier ons <rhyme label="c" type="f">verbinden</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Hem te wesen over <rhyme label="d" type="m">al</rhyme></l>
        <l id="s6:l7" met="4" type="+-">Onsen aldermeest <rhyme label="c" type="f" corresp="#s6:l5">beminden</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Alsoo langh' hy wesen <rhyme label="d" type="m" corresp="#s6:l6">sal</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
