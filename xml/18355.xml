<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>18355</id>
    <date>
      <year-start>1903</year-start>
      <year-end>1908</year-end>
    </date>
    <tags/>
    <melody>Het vinnige stralen van de zon</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1">Schoon lief, u wesen excellent</l>
        <l id="s1:l2">doorstraelt mijn hert van binnen;</l>
        <l id="s1:l3">om u soo lyd' ick swaer torment,</l>
        <l id="s1:l4">Cupido doet my minnen.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1">U ooghskens bruyn die blincken klaer,</l>
        <l id="s2:l2">dies ben ick heel ontsteecken;</l>
        <l id="s2:l3">u liefde, lief, valt my te swaer</l>
        <l id="s2:l4">door Venus loose treecken.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1">Mocht ic met u spaceren gaen</l>
        <l id="s3:l2">hier buyten in het groene,</l>
        <l id="s3:l3">ick soud' u clachtig doen vermaen</l>
        <l id="s3:l4">van mijn reyn liefde coene.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1">Princesse lief, neemt dit in danck,</l>
        <l id="s4:l2">vergeet mijn stout vermeten,</l>
        <l id="s4:l3">vertroost my, lief, eer 'tvalt te lanck,</l>
        <l id="s4:l4">al zijt ghy hoogh gheseten.</l>
      </lg>
    </lg>
  </text>
</song>
