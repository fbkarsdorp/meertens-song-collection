    <melody>Het daagt in de oosten</melody>
    <categories>
      <category translation="religion">godsdienst</category>
      <category translation="emotions">emoties</category>
      <category translation="formal genres">formele genres</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HOe gherne soude ic <rhyme label="a" type="f">dichten</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Een gheestelijc nieuwe <rhyme label="b" type="m">liet</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">Ende mijn siele <rhyme label="a" type="f" corresp="#s1:l1">verlichten</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Si lijdt so swaren <rhyme label="b" type="m" corresp="#s1:l2">verdriet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Het is haer van minnen <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Soe si mi doet <rhyme label="b" type="m">verstaen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Ende si en mach niet <rhyme label="a" type="f" corresp="#s2:l1">vromen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Si en heuet troost <rhyme label="b" type="m" corresp="#s2:l2">ontfaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dyen si heeft <rhyme label="a" type="f">wtuercoren</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die heeft hem om <rhyme label="b" type="m">ghekeert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Si ducht hi is <rhyme label="a" type="f" corresp="#s3:l1">verloren</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Si blijft al <rhyme label="b" type="m" corresp="#s3:l2">ongheleert</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Si derft der soeter <rhyme label="a" type="f">inspraken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die si ghewone <rhyme label="b" type="m">was</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Mocht si daer toe <rhyme label="a" type="f" corresp="#s4:l1">gheraken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">So waer si wel te <rhyme label="b" type="m" corresp="#s4:l2">pas</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Haer suchten ende haer <rhyme label="a" type="f">kermen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat is so <rhyme label="b" type="m">menichfout</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Woude hi doch haers <rhyme label="a" type="f" corresp="#s5:l1">ontfermen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Si en koer daer voer geen <rhyme label="b" type="m" corresp="#s5:l2">gout</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Si moet den tijt <rhyme label="a" type="f">verbeyden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Al vallet haer veel te <rhyme label="b" type="m">lanck</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Si en can daer niet af <rhyme label="a" type="f" corresp="#s6:l1">ghesceyden</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat doet der minnen <rhyme label="b" type="m" corresp="#s6:l2">bant</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Och dorste sijt <rhyme label="a" type="f">ghewaghen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Si haddes gaern <rhyme label="b" type="m">respijt</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Mer wat batet veel te <rhyme label="a" type="f" corresp="#s7:l1">claghen</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Si moet verbeyuden <rhyme label="b" type="m" corresp="#s7:l2">tijt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Si beducht haer also <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Dat si yet heeft <rhyme label="b" type="m">misdaen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Teghen haren <rhyme label="a" type="f" corresp="#s8:l1">heere</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Dies hi hem draghet <rhyme label="b" type="m" corresp="#s8:l2">aen</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Waer omme moyt si <rhyme label="a" type="f">vrienden</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Al om te spreken haer <rhyme label="b" type="m">woert</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Si duchtet dat haer <rhyme label="a" type="f" corresp="#s9:l1">bedinghe</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Niet en sal sijn <rhyme label="b" type="m" corresp="#s9:l2">verhoort</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Si en voelt gheen <rhyme label="a" type="f">antwoorde</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Dat is haerder herten te <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Recht oft hijt niet en <rhyme label="a" type="f" corresp="#s10:l1">hoorde</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Noch en vraechde ooc daer <rhyme label="b" type="m" corresp="#s10:l2">na</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Si en weet niet wat <rhyme label="a" type="f">beginnen</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Den druck is haer so <rhyme label="b" type="m">groot</rhyme></l>
        <l id="s11:l3" met="3" type="-+">Si haddes gaerne een <rhyme label="a" type="f" corresp="#s11:l1">inde</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Al waert oock metter <rhyme label="b" type="m" corresp="#s11:l2">doot</rhyme></l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Si is wel also <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Al met haer seluen <rhyme label="b" type="m">ghelast</rhyme></l>
        <l id="s12:l3" met="3" type="-+">Dat si op genoechte oft <rhyme label="a" type="f" corresp="#s12:l1">eere</rhyme></l>
        <l id="s12:l4" met="3" type="-+">In deser tijt niet en <rhyme label="b" type="m" corresp="#s12:l2">past</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Si leyt in eenen <rhyme label="a" type="f">haken</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ende verlanghen <rhyme label="b" type="m">swaer</rhyme></l>
        <l id="s13:l3" met="3" type="-+">Mocht si Gods <rhyme label="a" type="f" corresp="#s13:l1">ghesmaken</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Si en vraechde nerghens <rhyme label="b" type="m" corresp="#s13:l2">na</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">Dat si draecht inder <rhyme label="a" type="f">herten</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Dat moet verborghen <rhyme label="b" type="m">sijn</rhyme></l>
        <l id="s14:l3" met="3" type="-+">Niemant en kent haer <rhyme label="a" type="f" corresp="#s14:l1">smerte</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Dan haer lief <rhyme label="b" type="m" corresp="#s14:l2">minnekijn</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Al mocht sijt yemant <rhyme label="a" type="f">clagen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Si en hads niet veel te <rhyme label="b" type="m">bat</rhyme></l>
        <l id="s15:l3" met="3" type="-+">Haer cruys moet si selue <rhyme label="a" type="f" corresp="#s15:l1">dragen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Ende volgen der minnen <rhyme label="b" type="m" corresp="#s15:l2">pat</rhyme></l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Hoe seer is si <rhyme label="a" type="f">verladen</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Hoer deerlijck si omme <rhyme label="b" type="m">siet</rhyme></l>
        <l id="s16:l3" met="3" type="-+">Si roet aen God <rhyme label="a" type="f" corresp="#s16:l1">ghenade</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Het schijnt hi en hoortse <rhyme label="b" type="m" corresp="#s16:l2">niet</rhyme></l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Si witet haren <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Met menigen heeten <rhyme label="b" type="m">traen</rhyme></l>
        <l id="s17:l3" met="3" type="-+">Dat liden moet si <rhyme label="a" type="f" corresp="#s17:l1">dooghen</rhyme>.</l>
        <l id="s17:l4" met="3" type="-+">Si en cans doch niet <rhyme label="b" type="m" corresp="#s17:l2">ontgaen</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Si heeft voer haer <rhyme label="a" type="f">genomen</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Dat si een claghen sal <rhyme label="b" type="m">bestaen</rhyme></l>
        <l id="s18:l3" met="3" type="-+">Aen sijnder lieuer <rhyme label="a" type="f" corresp="#s18:l1">moeder</rhyme></l>
        <l id="s18:l4" met="3" type="-+">Hi is haer <rhyme label="b" type="m" corresp="#s18:l2">onderdaen</rhyme></l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Maria maghet <rhyme label="a" type="f">reyne</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Hoort mi doch nu een <rhyme label="b" type="m">woort</rhyme></l>
        <l id="s19:l3" met="3" type="-+">V sone laet mi <rhyme label="a" type="f" corresp="#s19:l1">alleyne</rhyme>.</l>
        <l id="s19:l4" met="3" type="-+">Ende ick en can niet <rhyme label="b" type="m" corresp="#s19:l2">voort</rhyme></l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">Het waer noch yet te <rhyme label="a" type="f">liden</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Dat hi mi liete <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s20:l3" met="3" type="-+">Maer dat ick dan moet <rhyme label="a" type="f" corresp="#s20:l1">striden</rhyme></l>
        <l id="s20:l4" met="3" type="-+">Dat doet mi vreese <rhyme label="b" type="m" corresp="#s20:l2">ontfaen</rhyme></l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Dit doen daer toe sijn <rhyme label="a" type="f">knechten</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Daer ouer hi heeft <rhyme label="b" type="m">ghebot</rhyme></l>
        <l id="s21:l3" met="3" type="-+">Sij en souden mi niet <rhyme label="a" type="f" corresp="#s21:l1">aenuechten</rhyme></l>
        <l id="s21:l4" met="3" type="-+">Hi en gauet hen allen <rhyme label="b" type="m" corresp="#s21:l2">oerlof</rhyme></l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Hi cost mi soetelijc <rhyme label="a" type="f">temmen</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Doen hi my haelde <rhyme label="b" type="m">an</rhyme></l>
        <l id="s22:l3" met="3" type="-+">Hi wil mi nu gaen <rhyme label="a" type="f" corresp="#s22:l1">speenen</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Eer icx ghederuen <rhyme label="b" type="m" corresp="#s22:l2">can</rhyme></l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Hi heuet mi <rhyme label="a" type="f">gheschoncken</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Wel also goeden <rhyme label="b" type="m">wijn</rhyme></l>
        <l id="s23:l3" met="3" type="-+">Dat ic ben worden <rhyme label="a" type="f" corresp="#s23:l1">droncken</rhyme></l>
        <l id="s23:l4" met="3" type="-+">Ic en cans niet ledich <rhyme label="b" type="m" corresp="#s23:l2">ghesijn</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">O sone dit hoordy <rhyme label="a" type="f">wale</rhyme></l>
        <l id="s24:l2" met="3" type="-+">Dat mi dit wort <rhyme label="b" type="m">gheclaecht</rhyme></l>
        <l id="s24:l3" met="3" type="-+">Het is dijns liefkens <rhyme label="a" type="f" corresp="#s24:l1">tale</rhyme></l>
        <l id="s24:l4" met="3" type="-+">Daer ghi minne toe <rhyme label="b" type="m" corresp="#s24:l2">draecht</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">O eerweerdighe <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Sout ghijs v draghen <rhyme label="b" type="m">an</rhyme></l>
        <l id="s25:l3" met="3" type="-+">Ick ben doch haer <rhyme label="a" type="f" corresp="#s25:l1">behoeder</rhyme>.</l>
        <l id="s25:l4" met="3" type="-+">Hoe wel dat ic mi berghen <rhyme label="b" type="m" corresp="#s25:l2">can</rhyme></l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">O mi wel lieue <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s26:l2" met="3" type="-+">Haer tranen sijn minen <rhyme label="b" type="m">wijn</rhyme></l>
        <l id="s26:l3" met="3" type="-+">Haer suchten doet mi <rhyme label="a" type="f" corresp="#s26:l1">lachten</rhyme></l>
        <l id="s26:l4" met="3" type="-+">Ick wel wat vrolijck <rhyme label="b" type="m" corresp="#s26:l2">sijn</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="3" type="-+">O mi wel lieue <rhyme label="a" type="f">sone</rhyme></l>
        <l id="s27:l2" met="3" type="-+">Dat is haer <rhyme label="b" type="m">onbekent</rhyme></l>
        <l id="s27:l3" met="3" type="-+">Waer si dit doch <rhyme label="a" type="f" corresp="#s27:l1">ghewone</rhyme></l>
        <l id="s27:l4" met="3" type="-+">Dat waer een ander <rhyme label="b" type="m" corresp="#s27:l2">dinck</rhyme></l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="3" type="-+">O moeder sijt te <rhyme label="a" type="f">vreden</rhyme></l>
        <l id="s28:l2" met="3" type="-+">Ic en ben op haer niet <rhyme label="b" type="m">gram</rhyme></l>
        <l id="s28:l3" met="3" type="-+">Ic heb meer om haer <rhyme label="a" type="f" corresp="#s28:l1">gheleden</rhyme></l>
        <l id="s28:l4" met="3" type="-+">Dan si om mi gheliden <rhyme label="b" type="m" corresp="#s28:l2">can</rhyme></l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="3" type="-+">Ick wil dat si <rhyme label="a" type="f">bekenne</rhyme></l>
        <l id="s29:l2" met="3" type="-+">Hoedanich dat si <rhyme label="b" type="m">sy</rhyme></l>
        <l id="s29:l3" met="3" type="-+">Dan sal si eerst <rhyme label="a" type="f" corresp="#s29:l1">versinnen</rhyme></l>
        <l id="s29:l4" met="3" type="-+">Wat si ontfangt van <rhyme label="b" type="m" corresp="#s29:l2">mi</rhyme></l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="3" type="-+">O mi wel lieue <rhyme label="a" type="f">sone</rhyme></l>
        <l id="s30:l2" met="3" type="-+">Ghi cont daer mede om <rhyme label="b" type="m">gaen</rhyme></l>
        <l id="s30:l3" met="3" type="-+">Het is al tot haerder <rhyme label="a" type="f" corresp="#s30:l1">vromen</rhyme>.</l>
        <l id="s30:l4" met="3" type="-+">Soe ick aen v heb <rhyme label="b" type="m" corresp="#s30:l2">verstaen</rhyme>.</l>
      </lg>
      <lg id="s31" type="stanza">
        <l id="s31:l1" met="3" type="-+">O moeder ic hebse <rhyme label="a" type="f">ghescreuen</rhyme></l>
        <l id="s31:l2" met="3" type="-+">In minen handen <rhyme label="b" type="m">diep</rhyme></l>
        <l id="s31:l3" met="3" type="-+">Ende in mijn minnelijck <rhyme label="a" type="f" corresp="#s31:l1">herte</rhyme></l>
        <l id="s31:l4" met="3" type="-+">Ick en cander vergheten <rhyme label="b" type="m" corresp="#s31:l2">niet</rhyme>.</l>
      </lg>
      <lg id="s32" type="stanza">
        <l id="s32:l1" met="3" type="-+">Mer soude ic bi haer <rhyme label="a" type="f">wesen</rhyme></l>
        <l id="s32:l2" met="3" type="-+">Al na haers herten <rhyme label="b" type="m">lost</rhyme></l>
        <l id="s32:l3" met="3" type="-+">Si en mochts niet <rhyme label="a" type="f" corresp="#s32:l1">ghenesen</rhyme></l>
        <l id="s32:l4" met="3" type="-+">Si doet te grooten <rhyme label="b" type="m" corresp="#s32:l2">cost</rhyme></l>
      </lg>
      <lg id="s33" type="stanza">
        <l id="s33:l1" met="3" type="-+">Si en heeft niet dat <rhyme label="a" type="f">versinnen</rhyme></l>
        <l id="s33:l2" met="3" type="-+">Dat si yet ghesparen <rhyme label="b" type="m">can</rhyme></l>
        <l id="s33:l3" met="3" type="-+">Si stort haer wt in <rhyme label="a" type="f" corresp="#s33:l1">minnen</rhyme></l>
        <l id="s33:l4" met="3" type="-+">Haer en leyt nerghens <rhyme label="b" type="m" corresp="#s33:l2">an</rhyme></l>
      </lg>
      <lg id="s34" type="stanza">
        <l id="s34:l1" met="3" type="-+">Haer clagen mach ic niet <rhyme label="a" type="f">stillen</rhyme></l>
        <l id="s34:l2" met="3" type="-+">Dat weet ick herde <rhyme label="b" type="m">wel</rhyme></l>
        <l id="s34:l3" met="3" type="-+">Si en heeft mi tot haren <rhyme label="a" type="f" corresp="#s34:l1">wille</rhyme></l>
        <l id="s34:l4" met="3" type="-+">Al schine ick haer dus <rhyme label="b" type="m" corresp="#s34:l2">fel</rhyme></l>
      </lg>
      <lg id="s35" type="stanza">
        <l id="s35:l1" met="3" type="-+">Al hadde si ghebruyc van <rhyme label="a" type="f">alle</rhyme></l>
        <l id="s35:l2" met="3" type="-+">Dat ick oyt wesen <rhyme label="b" type="m">gaf</rhyme></l>
        <l id="s35:l3" met="3" type="-+">Ten soude haer niet <rhyme label="a" type="f" corresp="#s35:l1">beuallen</rhyme></l>
        <l id="s35:l4" met="3" type="-+">Noch versaden niet een <rhyme label="b" type="m" corresp="#s35:l2">caf</rhyme></l>
      </lg>
      <lg id="s36" type="stanza">
        <l id="s36:l1" met="3" type="-+">Hier op so ben ick <rhyme label="a" type="f">koene</rhyme></l>
        <l id="s36:l2" met="3" type="-+">Want ic haer nature wel <rhyme label="b" type="m">weet</rhyme></l>
        <l id="s36:l3" met="3" type="-+">Si en siet niet bat te <rhyme label="a" type="f" corresp="#s36:l1">ghedoene</rhyme></l>
        <l id="s36:l4" met="3" type="-+">Waer dat si haer toe <rhyme label="b" type="m" corresp="#s36:l2">keert</rhyme></l>
      </lg>
      <lg id="s37" type="stanza">
        <l id="s37:l1" met="3" type="-+">Si soude mi dicwils <rhyme label="a" type="f">hoonen</rhyme>.</l>
        <l id="s37:l2" met="3" type="-+">En saghe ick niet voer <rhyme label="b" type="m">mi</rhyme></l>
        <l id="s37:l3" met="3" type="-+">Si can so deerlijck <rhyme label="a" type="f" corresp="#s37:l1">croonen</rhyme></l>
        <l id="s37:l4" met="3" type="-+">Ick en mochts niet <rhyme label="b" type="m" corresp="#s37:l2">voerbi</rhyme></l>
      </lg>
      <lg id="s38" type="stanza">
        <l id="s38:l1" met="3" type="-+">Haer verwondert te <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s38:l2" met="3" type="-+">Ende ooc is si daer om <rhyme label="b" type="m">gram</rhyme></l>
        <l id="s38:l3" met="3" type="-+">Dat icker mi niet toe en <rhyme label="a" type="f" corresp="#s38:l1">keere</rhyme></l>
        <l id="s38:l4" met="3" type="-+">Hoe ict gheherden <rhyme label="b" type="m" corresp="#s38:l2">can</rhyme></l>
      </lg>
      <lg id="s39" type="stanza">
        <l id="s39:l1" met="3" type="-+">Si leyt mi dick voer <rhyme label="a" type="f">ooghen</rhyme></l>
        <l id="s39:l2" met="3" type="-+">Dat si mi heeft <rhyme label="b" type="m">ghecost</rhyme></l>
        <l id="s39:l3" met="3" type="-+">Om dat ic mi soude <rhyme label="a" type="f" corresp="#s39:l1">booghen</rhyme></l>
        <l id="s39:l4" met="3" type="-+">Te versaden haren <rhyme label="b" type="m" corresp="#s39:l2">lost</rhyme></l>
      </lg>
      <lg id="s40" type="stanza">
        <l id="s40:l1" met="3" type="-+">Mi verwondert also <rhyme label="a" type="f">seere</rhyme></l>
        <l id="s40:l2" met="3" type="-+">Dat si gheen scaemte en <rhyme label="b" type="m">heeft</rhyme></l>
        <l id="s40:l3" met="3" type="-+">Voer also grooten <rhyme label="a" type="f" corresp="#s40:l1">heere</rhyme></l>
        <l id="s40:l4" met="3" type="-+">Daer si minne toe <rhyme label="b" type="m" corresp="#s40:l2">dreecht</rhyme></l>
      </lg>
      <lg id="s41" type="stanza">
        <l id="s41:l1" met="3" type="-+">O heere al sidy <rhyme label="a" type="f">hooghe</rhyme></l>
        <l id="s41:l2" met="3" type="-+">Ick ben v <rhyme label="b" type="m">susterkijn</rhyme></l>
        <l id="s41:l3" met="3" type="-+">Ghi draecht toch mijn <rhyme label="a" type="f" corresp="#s41:l1">nature</rhyme></l>
        <l id="s41:l4" met="3" type="-+">Al voer een <rhyme label="b" type="m" corresp="#s41:l2">mantelkijn</rhyme></l>
      </lg>
      <lg id="s42" type="stanza">
        <l id="s42:l1" met="3" type="-+">Al ben ick nu <rhyme label="a" type="f">gheuanghen</rhyme></l>
        <l id="s42:l2" met="3" type="-+">In eenen kercker <rhyme label="b" type="m">diep</rhyme></l>
        <l id="s42:l3" met="3" type="-+">Ick hope mi nocht te <rhyme label="a" type="f" corresp="#s42:l1">vergangen</rhyme></l>
        <l id="s42:l4" met="3" type="-+">Ende spreken mijn schoon <rhyme label="b" type="m" corresp="#s42:l2">lief</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
