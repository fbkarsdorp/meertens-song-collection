  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Cupido hoort myn <rhyme label="a" type="f">klaegen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Op dit <rhyme label="b" type="m">termyn</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">By nachten en by <rhyme label="a" type="f" corresp="#s1:l1">dagen</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Lyd' ik <rhyme label="b" type="m" corresp="#s1:l2">gepyn</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Myn lief heeft my <rhyme label="c" type="f">begeven</rhyme> //</l>
        <l id="s1:l6" met="2" type="-+">Om 't groote <rhyme label="d" type="m">goed</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Och Cupido <rhyme label="c" type="f" corresp="#s1:l5">verheven</rhyme> //</l>
        <l id="s1:l8" met="2" type="-+">My bystand <rhyme label="d" type="m" corresp="#s1:l6">doet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wilt my uw bystand <rhyme label="a" type="f">toonen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Vrouw Venuw <rhyme label="b" type="m">kind</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Myn lief wilt my niet <rhyme label="a" type="f" corresp="#s2:l1">loonen</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Zoo nu 't wel <rhyme label="b" type="m" corresp="#s2:l2">schynt</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Door die groote <rhyme label="c" type="f">rykdommen</rhyme> //</l>
        <l id="s2:l6" met="2" type="-+">Word zy <rhyme label="d" type="m">verblend</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Hierom is 't dat ik <rhyme label="c" type="f" corresp="#s2:l5">kome</rhyme> //</l>
        <l id="s2:l8" met="2" type="-+">In dit <rhyme label="d" type="m" corresp="#s2:l6">torment</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">'t Goed is de meeste <rhyme label="a" type="f">cause</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Van myn <rhyme label="b" type="m">getreur</rhyme> //</l>
        <l id="s3:l3" met="3" type="-+">Dus beginn' ik myn' <rhyme label="a" type="f" corresp="#s3:l1">clause</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Met groot <rhyme label="b" type="m" corresp="#s3:l2">douleur</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Liefde plagt te <rhyme label="c" type="f">floreren</rhyme> //</l>
        <l id="s3:l6" met="2" type="-+">Aan elken <rhyme label="d" type="m">kant</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Maer 't goed heeft nu te <rhyme label="c" type="f" corresp="#s3:l5">zeere</rhyme> //</l>
        <l id="s3:l8" met="2" type="-+">De <rhyme label="d" type="m" corresp="#s3:l6">overhand</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waer ik van goede <rhyme label="a" type="f">ryke</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">Ik waer <rhyme label="b" type="m">verheugd</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Zy en zoud' van my niet <rhyme label="a" type="f" corresp="#s4:l1">wyken</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Die zuyv're <rhyme label="b" type="m" corresp="#s4:l2">jeugd</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Ik zoud' wel troost <rhyme label="c" type="f">gewinnen</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Nu treuren ik <rhyme label="d" type="m">moet</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Want zy stelt all' haer' <rhyme label="c" type="f" corresp="#s4:l5">zinnen</rhyme> //</l>
        <l id="s4:l8" met="2" type="-+">Op't groote <rhyme label="d" type="m" corresp="#s4:l6">goed</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">'t Goed neemt (laecen) zyn' <rhyme label="a" type="f">gangen</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Als sneeuw op 't <rhyme label="b" type="m">veld</rhyme> //</l>
        <l id="s5:l3" met="3" type="-+">Daer zy al met <rhyme label="a" type="f" corresp="#s5:l1">verlangen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Haer zin op <rhyme label="b" type="m" corresp="#s5:l2">stelt</rhyme> //</l>
        <l id="s5:l5" met="3" type="-+">Maer daer is liefde <rhyme label="c" type="f">puere</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Die zy kleyn <rhyme label="d" type="m">acht</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Groeyt van ure tot <rhyme label="c" type="f" corresp="#s5:l5">ure</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Dag ende <rhyme label="d" type="m" corresp="#s5:l6">nacht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat mag ter weireld <rhyme label="a" type="f">baeten</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Goed oft <rhyme label="b" type="m">schat</rhyme> //</l>
        <l id="s6:l3" met="3" type="-+">Zy moeten 't hier al <rhyme label="a" type="f" corresp="#s6:l1">laeten</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">'t Zy dorp ofte <rhyme label="b" type="m" corresp="#s6:l2">stad</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Wat baet haer grond oft <rhyme label="c" type="f">erven</rhyme></l>
        <l id="s6:l6" met="2" type="-+">In 't aerdsche <rhyme label="d" type="m">dal</rhyme>?</l>
        <l id="s6:l7" met="3" type="-+">Zoo wel als zy moet <rhyme label="c" type="f" corresp="#s6:l5">sterven</rhyme> //</l>
        <l id="s6:l8" met="2" type="-+">Het blyft hier <rhyme label="d" type="m" corresp="#s6:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
