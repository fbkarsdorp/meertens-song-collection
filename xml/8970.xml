    <melody>Gott der vater wohn uns bei</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">GOdt de Vader woon ons <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En laet ons niet <rhyme label="b" type="f">verderven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Maeck ons aller Sonden <rhyme label="a" type="m" corresp="#s1:l1">vry</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">End help ons Salich <rhyme label="b" type="f" corresp="#s1:l2">sterven</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Voor den Duyvel ons <rhyme label="c" type="m">bewaert</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Houdt ons by vasten <rhyme label="d" type="f">Gheloove</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">End laet ons op u <rhyme label="e" type="f">bouwen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Wt Herten grondt <rhyme label="e" type="f" corresp="#s1:l7">betrouwen</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Dy ons laten gants end <rhyme label="f" type="m">gaer</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Met allen rechten <rhyme label="g" type="f">Christen</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Ontvlieden s'Duyvels <rhyme label="g" type="f" corresp="#s1:l10">Listen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Met Wapen Gods ons <rhyme label="g" type="f" corresp="#s1:l10">fristen</rhyme>,</l>
        <l id="s1:l13" met="4" type="+-">Amen, Amen, dat sy <rhyme label="f" type="m" corresp="#s1:l9">waer</rhyme>,</l>
        <l id="s1:l14" met="4" type="-+">So singhen wy <rhyme label="h" type="m">Alleluia</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">JEsus Christus woon ons <rhyme label="a" type="m">by</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En laet ons niet <rhyme label="b" type="f">verderven</rhyme>,</l>
