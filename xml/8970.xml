        <l id="s3:l13" met="4" type="+-">Amen, Amen, dat sy <rhyme label="f" type="m" corresp="#s3:l9">waer</rhyme>,</l>
        <l id="s3:l14" met="4" type="-+">So singhen wy <rhyme label="h" type="m">Alleluia</rhyme>.</l>
