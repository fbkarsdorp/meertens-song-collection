    <melody>Psalm 103 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">Laet ons al t’samen den Heere heerlijck <rhyme label="a" type="f">prijsen</rhyme></l>
        <l id="s1:l2" met="5" type="-+">Loven, dancken, en oock eere <rhyme label="a" type="f" corresp="#s1:l1">bewijsen</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Voor al sijn weldaden groot en <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Die hy ons bewijst uyt louter <rhyme label="c" type="f">ghenade</rhyme>,</l>
        <l id="s1:l5" met="5" type="-+">Laet ons t’samen, vierighlijck vroegh ende <rhyme label="c" type="f" corresp="#s1:l4">spade</rhyme>,</l>
        <l id="s1:l6" met="5" type="-+">Verheffen seer den Naem des Heeren <rhyme label="b" type="m" corresp="#s1:l3">reyn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Doen wy alhier doot lagen in veel <rhyme label="a" type="f">zonden</rhyme></l>
        <l id="s2:l2" met="5" type="-+">Als verdoolde Schapen waren <rhyme label="a" type="f" corresp="#s2:l1">bevonden</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Soo heeft hy hem goedigh tot ons <rhyme label="b" type="m">ghewent</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Hy socht en riep ons door sijn liefde <rhyme label="c" type="f">vierigh</rhyme>,</l>
        <l id="s2:l5" met="5" type="-+">Seer vriendelijk, als een Vader <rhyme label="c" type="f" corresp="#s2:l4">goedertierig</rhyme></l>
        <l id="s2:l6" met="5" type="-+">En wil ons sijn liefde maken <rhyme label="b" type="m" corresp="#s2:l3">bekent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Uyt genaden seer trostelijck ende <rhyme label="a" type="f">goedig</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Soo wiesch hy aff al onse zonden <rhyme label="a" type="f" corresp="#s3:l1">bloedigh</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En verdelgde onse groote <rhyme label="b" type="m">misdaet</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Hy is lieflijck en vol <rhyme label="c" type="f">bartmhertigheden</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Daeghlijcks reynight hy ons sondige <rhyme label="c" type="f" corresp="#s3:l4">leden</rhyme>,</l>
        <l id="s3:l6" met="5" type="-+">Want sijn ghenade altijt open <rhyme label="b" type="m" corresp="#s3:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Hy is de Heere seer sterk ende <rhyme label="a" type="f">machtig</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Die ons verlost van ons vyanden <rhyme label="a" type="f" corresp="#s4:l1">krachtigh</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Van Duyvel, Doot, en der zonden <rhyme label="b" type="m">gewelt</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Hy voert ons uyt dat rijck der <rhyme label="c" type="f">duysterheden</rhyme></l>
        <l id="s4:l5" met="5" type="-+">In sijnen licht vol blijdtschap ende <rhyme label="c" type="f" corresp="#s4:l4">vreden</rhyme>,</l>
        <l id="s4:l6" met="5" type="-+">Al waer hy ons helpet behouwen ‘t <rhyme label="b" type="m" corresp="#s4:l3">Velt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Door sijnen Geest so maekt hy ons <rhyme label="a" type="f">ghenegen</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Om te wand’len in sijn Wetten en <rhyme label="a" type="f" corresp="#s5:l1">weghen</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Hy is ‘t alleen die ons hier <rhyme label="b" type="m">wederbaert</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">En vernieut al ons inwendighe <rhyme label="c" type="f">krachten</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Die ons vervult met Hemelsche <rhyme label="c" type="f" corresp="#s5:l4">ghedachten</rhyme></l>
        <l id="s5:l6" met="5" type="-+">En maeckt ons geestelijck van sin ende <rhyme label="b" type="m" corresp="#s5:l3">aert</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Godt door sijn liefd’ als een Vader <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s6:l2" met="5" type="-+">Belooft ons sijn Rijck ende eeuwich <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">En heeft ons tot Kind’ren ghenomen <rhyme label="b" type="m">aen</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">On-uyt-spreeckelijck zijn alle de <rhyme label="c" type="f">weldaden</rhyme>,</l>
        <l id="s6:l5" met="5" type="-+">Die hy ons bewijst uyt louter <rhyme label="c" type="f" corresp="#s6:l4">ghenaden</rhyme>,</l>
        <l id="s6:l6" met="5" type="-+">Waer door wy al voor hem in vrede <rhyme label="b" type="m" corresp="#s6:l3">staen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Des laet ons hem loveb, prijsen, en <rhyme label="a" type="f">eeren</rhyme>,</l>
        <l id="s7:l2" met="5" type="-+">Godt de Koningh ende Heere der <rhyme label="a" type="f" corresp="#s7:l1">heeren</rhyme>,</l>
        <l id="s7:l3" met="5" type="-+">Laetons uyt-roepen sijn macht en <rhyme label="b" type="m">goetheyt</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Soo langhe als wy hier zijn ende <rhyme label="c" type="f">leven</rhyme>,</l>
        <l id="s7:l5" met="5" type="-+">Laet ons hem loff, prijs ende eere <rhyme label="c" type="f" corresp="#s7:l4">gheven</rhyme>,</l>
        <l id="s7:l6" met="5" type="-+">Verkondighen sijn deuchden wijt en <rhyme label="b" type="m" corresp="#s7:l3">breyt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">U loven wy Godt, Heere der <rhyme label="a" type="f">heyrscharen</rhyme></l>
        <l id="s8:l2" met="5" type="-+">U dancken wy, u goetheyt wy <rhyme label="a" type="f" corresp="#s8:l1">verklaren</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">U prijsen wy, o Godt <rhyme label="b" type="m">ghebenedijt</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Uyt herten gront spreken wy la te <rhyme label="c" type="f">same</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">Eer lof, en prijs zy uwen heyl’ghen <rhyme label="c" type="f" corresp="#s8:l4">Name</rhyme>,</l>
        <l id="s8:l6" met="5" type="-+">Want ghy O Godt, alleen des weerdig <rhyme label="b" type="m" corresp="#s8:l3">zijt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="5" type="-+">De Enghelen in den Hemel daer <rhyme label="a" type="f">boven</rhyme></l>
        <l id="s9:l2" met="5" type="-+">Moeten u stedich prijsen ende <rhyme label="a" type="f" corresp="#s9:l1">loven</rhyme>,</l>
        <l id="s9:l3" met="5" type="-+">De gheheele Christenheyt hier met <rhyme label="b" type="m">een</rhyme></l>
        <l id="s9:l4" met="5" type="-+">Moet u op-off’ren lof, prijs ende <rhyme label="c" type="f">eere</rhyme>,</l>
        <l id="s9:l5" met="5" type="-+">U onse Godt jae Koningh ende <rhyme label="c" type="f" corresp="#s9:l4">Heere</rhyme>,</l>
        <l id="s9:l6" met="5" type="-+">In den gheest en waerheyt van herten <rhyme label="b" type="m" corresp="#s9:l3">reen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
