<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182565</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody>Mijn lieve Saartje</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wel vrienden wat hoort men niet <rhyme label="a" type="m">al</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Het zyn steeds raare <rhyme label="b" type="f">zaaken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">De meisjes die zyn byna <rhyme label="a" type="m" corresp="#s1:l1">mal</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Zy gaan een kansje <rhyme label="b" type="f" corresp="#s1:l2">waagen</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zy zyn jandome <rhyme label="c" type="m">dol</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Zondags gaan zy op <rhyme label="c" type="m" corresp="#s1:l5">hol</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Het zyn gemaakte <rhyme label="d" type="f">streeken</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Omra de Rotter damse kermis te <rhyme label="e" type="m">gaan</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Gelyk het is <rhyme label="d" type="f" corresp="#s1:l7">gebleeken</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Dat Jan de Grutter heeft <rhyme label="e" type="m" corresp="#s1:l8">gedaan</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Mietje met haar beste <rhyme label="a" type="m">goed</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Die ging haar eens <rhyme label="b" type="f">vermaken</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Met Jan de Grutter die vol <rhyme label="a" type="m" corresp="#s2:l1">moed</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Het zo ver wist te <rhyme label="b" type="f" corresp="#s2:l2">maaken</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat Mie je kreeg in der <rhyme label="c" type="m">lyf</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">tot Jan de Pikker zyn <rhyme label="c" type="m" corresp="#s2:l5">geryf</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Ik zal nog meer <rhyme label="d" type="f">verhaalen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Hoe dat het gaat, des 's avonds <rhyme label="e" type="m">laat</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Ik zal 'er niet om <rhyme label="d" type="f" corresp="#s2:l7">draalen</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Van al die Pikkers <rhyme label="e" type="m" corresp="#s2:l8">praat</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">In Portugaal is een vroome <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wie zou 'er niet vo√≥r <rhyme label="b" type="f">zwigten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Hy moest al na Gouda <rhyme label="a" type="m" corresp="#s3:l1">gaan</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Om boodschappen te <rhyme label="b" type="f" corresp="#s3:l2">verrigten</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Maar 't staat niet <rhyme label="c" type="m">popergaal</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zeggen de Menschen <rhyme label="c" type="m" corresp="#s3:l5">altemaal</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Wat dunkt u van die <rhyme label="d" type="f">streeken</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Wat zegje van die doove <rhyme label="e" type="m">Mie</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Gelyk het is <rhyme label="d" type="f" corresp="#s3:l7">gebleeken</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Daar het veel Menschen <rhyme label="e" type="m" corresp="#s3:l8">ziet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Overvaaren met 'er <rhyme label="a" type="m">spoed</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En dan nog ras gaan <rhyme label="b" type="f">loopen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Jan de Grutter die slegt <rhyme label="a" type="m" corresp="#s4:l1">doed</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Met kwam haar eens <rhyme label="b" type="f" corresp="#s4:l2">oploopen</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die zei: hoe loopje zo <rhyme label="c" type="m">zagt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Jan had dat niet <rhyme label="c" type="m" corresp="#s4:l5">gewagt</rhyme>;</l>
        <l id="s4:l7" met="3" type="-+">Hy zei: hy kon niet <rhyme label="d" type="f">loopen</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Hy had een Bloedzweer aan 't <rhyme label="e" type="m">Lyf</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Die moest zyn <rhyme label="d" type="f" corresp="#s4:l7">doorgestooken</rhyme>.</l>
        <l id="s4:l10" met="3" type="-+">Tot lieve Mietje haar <rhyme label="e" type="m" corresp="#s4:l8">geryf</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Zy maakte toen een goed <rhyme label="a" type="m">accoord</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En gingen zo na Gouda <rhyme label="b" type="f">voort</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Tot dat zy kwaamen aan de <rhyme label="a" type="m" corresp="#s5:l1">Stad</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Een Vriend die daar de wagt op <rhyme label="b" type="f" corresp="#s5:l2">had</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Die liep ze agter <rhyme label="c" type="m">na</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Waar dat zy heenen <rhyme label="c" type="m" corresp="#s5:l5">ga</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Vrienden ik zal <rhyme label="d" type="f">verhaalen</rhyme>,</l>
        <l id="s5:l8" met="4" type="-+">Hoe dat dit slegte dolle <rhyme label="e" type="m">paar</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">De poort uit gingen <rhyme label="d" type="f" corresp="#s5:l7">dwaalen</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Hy zag ze niet verder <rhyme label="e" type="m" corresp="#s5:l8">naar</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Wat hoort men niet een groot <rhyme label="a" type="m">gerugt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat zy zyn thuis <rhyme label="b" type="f">gekoomen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Wie heeft gehoord ooit grooter <rhyme label="a" type="m" corresp="#s6:l1">klugt</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Van zo een Man der <rhyme label="b" type="f" corresp="#s6:l2">vroomen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Die zo drommels Kerks <rhyme label="c" type="m">was</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Kom lieve Mietje wel te <rhyme label="c" type="m" corresp="#s6:l5">pas</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Om in Gouda te gaan <rhyme label="d" type="f">woonen</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Veeltyds krygt zy wat warms in t' <rhyme label="e" type="m">lyf</rhyme></l>
        <l id="s6:l9" met="3" type="-+">Van jonge Goudse <rhyme label="d" type="f" corresp="#s6:l7">knaapen</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Dat is tot <rhyme label="e" type="m" corresp="#s6:l8">tydverdryf</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
