  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gaet wt wilt vreucht <rhyme label="a" type="f">aenuaerden</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Aurora compt op <rhyme label="b" type="m">hant</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Phebus spant syne <rhyme label="a" type="f" corresp="#s1:l1">paerden</rhyme></l>
        <l id="s1:l4" met="3" type="-+">In syn waegen <rhyme label="b" type="m" corresp="#s1:l2">playsant</rhyme></l>
        <l id="s1:l5" met="3" type="-+">De blomkens te <rhyme label="c" type="f">bestieren</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Met vreuchden <rhyme label="d" type="m">menichfoult</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Ceres toont haer <rhyme label="c" type="f" corresp="#s1:l5">manieren</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Des meys vyrtuyt <rhyme label="d" type="m" corresp="#s1:l6">anschout</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Eolus is v <rhyme label="a" type="f">gonstich</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Hy aquilo <rhyme label="b" type="m">bedwingt</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Want die is seer <rhyme label="a" type="f" corresp="#s2:l1">afgonstich</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Flora die nu <rhyme label="b" type="m" corresp="#s2:l2">voortbringt</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Blomkens van al <rhyme label="c" type="f">coleuren</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Men vintse schoon <rhyme label="d" type="m">bedout</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Elck gebruyckt sy <rhyme label="c" type="f" corresp="#s2:l5">nateuren</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Des meys vyrtuyt <rhyme label="d" type="m" corresp="#s2:l6">anschout</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy amoreuse <rhyme label="a" type="f">geesten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Behaelt nu prys en <rhyme label="b" type="m">loff</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Compt al tot deser <rhyme label="a" type="f" corresp="#s3:l1">feesten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Buyten in Cupydos <rhyme label="b" type="m" corresp="#s3:l2">hoff</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Want men aldaer sal <rhyme label="c" type="f">schieten</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Pylen van loot en <rhyme label="d" type="m">goudt</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dus laet v niet <rhyme label="c" type="f" corresp="#s3:l5">verdrieten</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Des meys virtuyt <rhyme label="d" type="m" corresp="#s3:l6">aenschout</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Haest v om daer te <rhyme label="a" type="f">comen</rhyme></l>
        <l id="s4:l2" met="3" type="-+">V lieff aldaer <rhyme label="b" type="m">verwacht</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Wilt geen perykel <rhyme label="a" type="f" corresp="#s4:l1">schromen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Diana is op die <rhyme label="b" type="m" corresp="#s4:l2">wacht</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Gaet met v lieff <rhyme label="c" type="f">spanceren</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Buyten in het groene <rhyme label="d" type="m">wout</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Wilt met haer <rhyme label="c" type="f" corresp="#s4:l5">accorderen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Des meys virtuyt <rhyme label="d" type="m" corresp="#s4:l6">aenschout</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Men sal vreucht <rhyme label="a" type="f">orboren</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Prince merckt smeys <rhyme label="b" type="m">virtuyt</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Nimphen sult ghy daer <rhyme label="a" type="f" corresp="#s5:l1">horen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Accoort van soet <rhyme label="b" type="m" corresp="#s5:l2">geluyt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Wonder is daer <rhyme label="c" type="f">geschepen</rhyme></l>
        <l id="s5:l6" met="3" type="-+">T gauts blomken  daer <rhyme label="d" type="m">aenschout</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Al wt jonste <rhyme label="c" type="f" corresp="#s5:l5">begrepen</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Des meys virtuyt <rhyme label="d" type="m" corresp="#s5:l6">aenschout</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
