<song>
  <meta>
    <id>11068</id>
    <date>
      <year-start>1608</year-start>
      <year-end>1608</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="nieuwjaarslied" translation="new year's song">nieuwjaarslied</tag>
    </tags>
    <melody>Tant que vivrai</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">HEt Oude is <rhyme label="a" type="f">verganghen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Het Iaer vernieuwt <rhyme label="b" type="m">weer</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Een blijtschap wy <rhyme label="a" type="f" corresp="#s1:l1">ontfangen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Deur d'Englen vanden <rhyme label="b" type="m" corresp="#s1:l2">Heer</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Die gheven deur haer <rhyme label="a" type="f" corresp="#s1:l1">sanghen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Gode Loff prijs en <rhyme label="b" type="m" corresp="#s1:l2">eer</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Sy wenschen, die daer <rhyme label="c" type="f">laghen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Als Harderkeus opt <rhyme label="d" type="m">Velt</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">Vrede in haren <rhyme label="c" type="f" corresp="#s1:l7">daghen</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Met een goe wil <rhyme label="d" type="m" corresp="#s1:l8">verselt</rhyme></l>
        <l id="s1:l11" met="3" type="+-">En oock een wel <rhyme label="c" type="f" corresp="#s1:l7">behaghen</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Die dese vreed <rhyme label="d" type="m" corresp="#s1:l8">vertelt</rhyme>:</l>
        <l id="s1:l13" met="3" type="-+">Heer wilt deur u <rhyme label="e" type="f">ghenaden</rhyme>,</l>
        <l id="s1:l14" met="3" type="-+">Met u gheest ons <rhyme label="e" type="f" corresp="#s1:l13">versaden</rhyme>,</l>
        <l id="s1:l15" met="3" type="-+">T'gheloof maackt in ons <rhyme label="f" type="m">waar</rhyme>,</l>
        <l id="s1:l16" met="3" type="-+">Deur Liefd en kennis <rhyme label="f" type="m" corresp="#s1:l15">claar</rhyme>.</l>
        <l id="s1:l17" met="3" type="-+">En gunt ons dees <rhyme label="e" type="f" corresp="#s1:l13">weldaden</rhyme>,</l>
        <l id="s1:l18" met="3" type="-+">Om ons van druck <rhyme label="e" type="f" corresp="#s1:l13">tontladen</rhyme></l>
        <l id="s1:l19" met="3" type="-+">Op dat wy <rhyme label="f" type="m" corresp="#s1:l15">allegaer</rhyme></l>
        <l id="s1:l20" met="3" type="-+">Vercringhen, tnieuwe <rhyme label="f" type="m" corresp="#s1:l15">Iaer</rhyme>,</l>
        <l id="s1:l21" met="3" type="-+">Tot salicheyt hier <rhyme label="f" type="m" corresp="#s1:l15">naer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dees vreed goet boven <rhyme label="a" type="f">maten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Brengt ons veel <rhyme label="b" type="m">overvloets</rhyme></l>
        <l id="s2:l3" met="3" type="-+">De Liefd' wil niemant <rhyme label="a" type="f" corresp="#s2:l1">haten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Maer wenscht haer veele <rhyme label="b" type="m" corresp="#s2:l2">goets</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">En plaut in alle <rhyme label="a" type="f" corresp="#s2:l1">straten</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Gherustheyt des <rhyme label="b" type="m" corresp="#s2:l2">ghemoets</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Sy wil oock niet <rhyme label="c" type="f">ghedencken</rhyme>.</l>
        <l id="s2:l8" met="3" type="-+">Het quat dat haer <rhyme label="d" type="m">gheschiet</rhyme></l>
        <l id="s2:l9" met="3" type="-+">In plaets van weer te <rhyme label="c" type="f" corresp="#s2:l7">crencken</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Sy haere hulp <rhyme label="d" type="m" corresp="#s2:l8">anbiet</rhyme></l>
        <l id="s2:l11" met="3" type="+-">En wil seer minlyck <rhyme label="c" type="f" corresp="#s2:l7">schencken</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">Haer rust en vreed om <rhyme label="d" type="m" corresp="#s2:l8">niet</rhyme>.</l>
        <l id="s2:l13" met="3" type="-+">Heer wilt deur v <rhyme label="e" type="f">ghenaden</rhyme>,</l>
        <l id="s2:l14" met="3" type="-+">Met v gheeft ons <rhyme label="e" type="f" corresp="#s2:l13">versaden</rhyme>,</l>
        <l id="s2:l15" met="3" type="-+">Tgheloof maeckt in ons <rhyme label="f" type="m">waer</rhyme>.</l>
        <l id="s2:l16" met="3" type="-+">Deur Liefd en kennis <rhyme label="f" type="m" corresp="#s2:l15">claer</rhyme>.</l>
        <l id="s2:l17" met="3" type="-+">En gunt ons dees <rhyme label="e" type="f" corresp="#s2:l13">vveldaden</rhyme></l>
        <l id="s2:l18" met="3" type="-+">Om ons van druck <rhyme label="e" type="f" corresp="#s2:l13">t'ontladen</rhyme></l>
        <l id="s2:l19" met="3" type="-+">Op dat vvy <rhyme label="f" type="m" corresp="#s2:l15">allegaer</rhyme></l>
        <l id="s2:l20" met="3" type="-+">Vercrijghen, tnieuvve <rhyme label="f" type="m" corresp="#s2:l15">Iaer</rhyme>,</l>
        <l id="s2:l21" met="3" type="-+">Tot salicheyt hier <rhyme label="f" type="m" corresp="#s2:l15">naer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Twaer goet, dat nu den <rhyme label="a" type="f">tijde</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De haet, nijt en <rhyme label="b" type="m">tweedracht</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Gherackten aen d'een <rhyme label="a" type="f" corresp="#s3:l1">zyde</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Nu liefde is <rhyme label="b" type="m" corresp="#s3:l2">volbracht</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Van die <rhyme label="a" type="f" corresp="#s3:l1">ghebenedijde</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Over t'menschlijck <rhyme label="b" type="m" corresp="#s3:l2">gheslacht</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Dit hoort ons an te <rhyme label="c" type="f">wijsen</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Dat d'een d'ander <rhyme label="d" type="m">bemint</rhyme>.</l>
        <l id="s3:l9" met="3" type="-+">En ons vyanden <rhyme label="c" type="f" corresp="#s3:l7">spijsen</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Diemen met deucht <rhyme label="d" type="m" corresp="#s3:l8">verwint</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">De daad sal ons dan <rhyme label="c" type="f" corresp="#s3:l7">prijsen</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Als wy zijn eens <rhyme label="d" type="m" corresp="#s3:l8">ghesint</rhyme>.</l>
        <l id="s3:l13" met="3" type="-+">Heer vvilt deur u <rhyme label="e" type="f">ghenaden</rhyme></l>
        <l id="s3:l14" met="3" type="-+">Met u gheeft ons <rhyme label="e" type="f" corresp="#s3:l13">versaden</rhyme></l>
        <l id="s3:l15" met="3" type="-+">Tgheloof maeckt in ons <rhyme label="f" type="m">vvaer</rhyme>,</l>
        <l id="s3:l16" met="3" type="-+">Deur Liefd en kennis <rhyme label="f" type="m" corresp="#s3:l15">claer</rhyme>.</l>
        <l id="s3:l17" met="3" type="-+">En gunt ons dees <rhyme label="e" type="f" corresp="#s3:l13">vveldaden</rhyme>,</l>
        <l id="s3:l18" met="3" type="-+">Om ons van druck <rhyme label="e" type="f" corresp="#s3:l13">t'ontladen</rhyme>,</l>
        <l id="s3:l19" met="3" type="-+">Op dat vvy <rhyme label="f" type="m" corresp="#s3:l15">allegaer</rhyme></l>
        <l id="s3:l20" met="3" type="-+">Vercrijghen, tnieuvve <rhyme label="f" type="m" corresp="#s3:l15">Iaer</rhyme>,</l>
        <l id="s3:l21" met="3" type="-+">Tot salicheyt hier <rhyme label="f" type="m" corresp="#s3:l15">naer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ghy aertsche Princen <rhyme label="a" type="f">moedich</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dietschepsel Gods <rhyme label="b" type="m">regeert</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Verlaet het Oorloch <rhyme label="a" type="f" corresp="#s4:l1">vloedich</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En u tot vreed <rhyme label="b" type="m" corresp="#s4:l2">verneert</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">So suldy sien <rhyme label="a" type="f" corresp="#s4:l1">voorspoedich</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Doe u ghemeent <rhyme label="b" type="m" corresp="#s4:l2">floreert</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">U Lant sal <rhyme label="c" type="f">overvloeyen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Met Honich, Melck, en <rhyme label="d" type="m">Wijn</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Een nieuwe vrientschap <rhyme label="c" type="f" corresp="#s4:l7">groeyen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Int trecken van een <rhyme label="d" type="m" corresp="#s4:l8">lijn</rhyme>.</l>
        <l id="s4:l11" met="3" type="+-">So liefd in u mach <rhyme label="c" type="f" corresp="#s4:l7">bloeyen</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Salt haest goe vreede <rhyme label="d" type="m" corresp="#s4:l8">zijn</rhyme>.</l>
        <l id="s4:l13" met="3" type="-+">Heer vvilt deur v <rhyme label="e" type="f">ghenaden</rhyme></l>
        <l id="s4:l14" met="3" type="-+">Met v gheeft ons <rhyme label="e" type="f" corresp="#s4:l13">versaden</rhyme>,</l>
        <l id="s4:l15" met="3" type="-+">Tgheloof maeckt in ons <rhyme label="f" type="m">vvaer</rhyme>,</l>
        <l id="s4:l16" met="3" type="-+">Deur Lieft en kennis <rhyme label="f" type="m" corresp="#s4:l15">claer</rhyme>,</l>
        <l id="s4:l17" met="3" type="-+">En gunt ons dees <rhyme label="e" type="f" corresp="#s4:l13">vveldaden</rhyme></l>
        <l id="s4:l18" met="3" type="-+">Om ons van druck <rhyme label="e" type="f" corresp="#s4:l13">t'ont;aden</rhyme></l>
        <l id="s4:l19" met="3" type="-+">Op dat vvy <rhyme label="f" type="m" corresp="#s4:l15">allegaer</rhyme>,</l>
        <l id="s4:l20" met="3" type="-+">Vercrijghen, tnieuvve <rhyme label="f" type="m" corresp="#s4:l15">Iaer</rhyme>,</l>
        <l id="s4:l21" met="3" type="-+">Tot salicheyt hier <rhyme label="f" type="m" corresp="#s4:l15">naer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
