<song>
  <meta>
    <id>13360</id>
    <date>
      <year-start>1678</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>dialog song</tag>
      <tag>narrative song</tag>
    </tags>
    <melody>Rein maagdje met eer</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WEl Schipper hoogh <rhyme label="a" type="f">gepresen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Seght ons metter <rhyme label="b" type="m">spoet</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Wanneer so sal het <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Ghy vertrecken <rhyme label="b" type="m" corresp="#s1:l2">moet</rhyme>?</l>
        <l id="s1:l5" met="3" type="-+">Wilt ons dat seggen <rhyme label="c" type="m">hoort</rhyme>;</l>
        <l id="s1:l6" met="3" type="-+">Op dat wy met den <rhyme label="d" type="f">ander</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">U order soo 't <rhyme label="c" type="m" corresp="#s1:l5">behoort</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Beramen met <rhyme label="d" type="f" corresp="#s1:l6">malkander</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Mijn Heeren hoogh van <rhyme label="a" type="f">waerden</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Luystert met <rhyme label="b" type="m">verdragh</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">De Reys hoop ick <rhyme label="a" type="f" corresp="#s2:l1">t'aenvaerden</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Nu van desen <rhyme label="b" type="m" corresp="#s2:l2">dagh</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Want 't Schip in 't Texel <rhyme label="c" type="m">leyt</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Dus wilt de tijt niet <rhyme label="d" type="f">recken</rhyme>;</l>
        <l id="s2:l7" met="3" type="-+">Maer geeft mijn voort <rhyme label="c" type="m" corresp="#s2:l5">afscheyt</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Op dat ick mach <rhyme label="d" type="f" corresp="#s2:l6">vertrecken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Wel luystert sonder <rhyme label="a" type="f">sneven</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Ist'er soo te <rhyme label="b" type="m">doen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">U order sal ick <rhyme label="a" type="f" corresp="#s3:l1">geven</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">Komt eens nae den <rhyme label="b" type="m" corresp="#s3:l2">Noen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Tot mijnent, op dat <rhyme label="c" type="m">pas</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wilt dan niet langer <rhyme label="d" type="f">beyden</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Spoet u nae boort toe <rhyme label="c" type="m" corresp="#s3:l5">ras</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">De Heer wil u <rhyme label="d" type="f" corresp="#s3:l6">geleyden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel ick gae dan <rhyme label="a" type="f">heenen</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">En ick sal met <rhyme label="b" type="m">vlijt</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Oock passen dan met <rhyme label="a" type="f" corresp="#s4:l1">eenen</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Sneedigh op den <rhyme label="b" type="m" corresp="#s4:l2">tijdt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Nu mach ick oock <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Den Wagenaer <rhyme label="d" type="f">besprecken</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat hy sijn wagen <rhyme label="c" type="m" corresp="#s4:l5">klaer</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Maeckt, sonder te <rhyme label="d" type="f" corresp="#s4:l6">gebreken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Nu ick mach my mee <rhyme label="a" type="f">spoeden</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">t'Huys al op dat <rhyme label="b" type="m">pas</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En wesen op mijn <rhyme label="a" type="f" corresp="#s5:l1">hoeden</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Want de Schipper <rhyme label="b" type="m" corresp="#s5:l2">ras</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Tot mijnent komen <rhyme label="c" type="m">sal</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En sijn Pampieren <rhyme label="d" type="f">halen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Die ick te samen <rhyme label="c" type="m" corresp="#s5:l5">al</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Klaer hebbe sonder <rhyme label="d" type="f" corresp="#s5:l6">falen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn tijt die is <rhyme label="a" type="f">verloopen</rhyme>.</l>
        <l id="s6:l2" met="3" type="+-">Goeden dagh in <rhyme label="b" type="m">Huys</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Komt doet de deur eens <rhyme label="a" type="f" corresp="#s6:l1">open</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Is mijn Heer niet <rhyme label="b" type="m" corresp="#s6:l2">t'huys</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Seght ick hem spreecken <rhyme label="c" type="m">moet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ick sal hier soo langh <rhyme label="d" type="f">wachten</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Gaet heenen voort met <rhyme label="c" type="m" corresp="#s6:l5">spoet</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">En wilt u dienst <rhyme label="d" type="f" corresp="#s6:l6">betrachten</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Komt Schipper hoogh <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s7:l2" met="3" type="+-">Gaet met mijn met <rhyme label="b" type="m">spoet</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Ick sal jou Paspoort <rhyme label="a" type="f" corresp="#s7:l1">geven</rhyme>,</l>
        <l id="s7:l4" met="3" type="+-">voort op staende <rhyme label="b" type="m" corresp="#s7:l2">voet</rhyme>;</l>
        <l id="s7:l5" met="3" type="-+">Want alle dinck is <rhyme label="c" type="m">klaer</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Maer wilt een luttel <rhyme label="d" type="f">beyden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wy moeten met <rhyme label="c" type="m" corresp="#s7:l5">malkaer</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Eens drincken, eer wy <rhyme label="d" type="f" corresp="#s7:l6">scheyden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Wel ick sal u <rhyme label="a" type="f">gerieven</rhyme>;</l>
        <l id="s8:l2" met="3" type="+-">Is het u <rhyme label="b" type="m">begeer</rhyme>,</l>
        <l id="s8:l3" met="3" type="-+">Soe doet na u <rhyme label="a" type="f" corresp="#s8:l1">believen</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Ick verwacht het <rhyme label="b" type="m" corresp="#s8:l2">Heer</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Want ick u dienaer <rhyme label="c" type="m">ben</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">En blijven sal <rhyme label="d" type="f">verheeven</rhyme></l>
        <l id="s8:l7" met="3" type="-+">So langh maer als ick <rhyme label="c" type="m" corresp="#s8:l5">ken</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Ten eynde van mijn <rhyme label="d" type="f" corresp="#s8:l6">Leeven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Nu komt ontfanght u <rhyme label="a" type="f">order</rhyme>,</l>
        <l id="s9:l2" met="3" type="+-">Neemt het van mijn <rhyme label="b" type="m">handt</rhyme>;</l>
        <l id="s9:l3" met="3" type="-+">De Heer die wil u <rhyme label="a" type="f" corresp="#s9:l1">vorder</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Brengen wel te <rhyme label="b" type="m" corresp="#s9:l2">landt</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Ick brengh u nae den <rhyme label="c" type="m">eys</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Dit glaesjen met <rhyme label="d" type="f">verblyen</rhyme></l>
        <l id="s9:l7" met="3" type="-+">Eens op behouden <rhyme label="c" type="m" corresp="#s9:l5">reys</rhyme>;</l>
        <l id="s9:l8" met="3" type="-+">Godt laet het wel <rhyme label="d" type="f" corresp="#s9:l6">gedyen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Ick danck jou sonder <rhyme label="a" type="f">falen</rhyme>,</l>
        <l id="s10:l2" met="3" type="+-">En sal doen <rhyme label="b" type="m">bescheyt</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Oock sonder langh te <rhyme label="a" type="f" corresp="#s10:l1">dralen</rhyme>,</l>
        <l id="s10:l4" met="3" type="+-">'t Zy mijn lief of <rhyme label="b" type="m" corresp="#s10:l2">leyt</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">U order wel <rhyme label="c" type="m">verstaet</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Die sal ick sonder <rhyme label="d" type="f">schroomen</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">In alles met <rhyme label="c" type="m" corresp="#s10:l5">beraet</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Ten vollen nae gaen <rhyme label="d" type="f" corresp="#s10:l6">komen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Wel laet het daer by <rhyme label="a" type="f">blijven</rhyme>;</l>
        <l id="s11:l2" met="3" type="+-">Wilt met alle <rhyme label="b" type="m">vlijt</rhyme>,</l>
        <l id="s11:l3" met="3" type="-+">Niet laten doch <rhyme label="a" type="f" corresp="#s11:l1">beschrijven</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Ter gelegen <rhyme label="b" type="m" corresp="#s11:l2">tijt</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">En so voor 't laest vaer <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Godts Engelen u <rhyme label="d" type="f">bewaren</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Voor storm en winden <rhyme label="c" type="m" corresp="#s11:l5">fel</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Voor Klippen en <rhyme label="d" type="f" corresp="#s11:l6">Barbaren</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Ick wens jou sonder <rhyme label="a" type="f">sneven</rhyme>,</l>
        <l id="s12:l2" met="3" type="+-">Voor het laest <rhyme label="b" type="m">Hadie</rhyme>:</l>
        <l id="s12:l3" met="3" type="-+">Godt geeft u een langh <rhyme label="a" type="f" corresp="#s12:l1">leven</rhyme>,</l>
        <l id="s12:l4" met="3" type="+-">Tot ick u weer <rhyme label="b" type="m" corresp="#s12:l2">sie</rhyme>;</l>
        <l id="s12:l5" met="3" type="-+">Ick sal met alle <rhyme label="c" type="m">vlijt</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Jou woorden wel <rhyme label="d" type="f">betrachten</rhyme>:</l>
        <l id="s12:l7" met="3" type="-+">Vaer wel, het is hoogh <rhyme label="c" type="m" corresp="#s12:l5">tijdt</rhyme>,</l>
        <l id="s12:l8" met="3" type="-+">De Wagen staet te <rhyme label="d" type="f" corresp="#s12:l6">wachten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
