<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6222</id>
    <date>
      <year-start>1636</year-start>
      <year-end>1636</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Vrouwtje is uw man niet thuis</melody>
  <author>C.P. van Wesbusch</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">MOrghen met den <rhyme label="a" type="m">dagheraet</rhyme></l>
        <l id="s1:l2" met="3" type="+-">Wil ick my <rhyme label="b" type="f">begheven</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Daer mijn Nymphjen jagen <rhyme label="a" type="m" corresp="#s1:l1">gaet</rhyme>;</l>
        <l id="s1:l4" met="3" type="+-">(Van de lust <rhyme label="b" type="f" corresp="#s1:l2">ghedreven</rhyme>)</l>
        <l id="s1:l5" met="4" type="+-">Dat is: door het groene <rhyme label="c" type="m">Wout</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Daer de Vog'len <rhyme label="d" type="f">singhen</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Vorder: nae den <rhyme label="c" type="m" corresp="#s1:l5">Aerden-hout</rhyme>,</l>
        <l id="s1:l8" met="3" type="+-">Daer de Dieren <rhyme label="d" type="f" corresp="#s1:l6">springhen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hou Diana! niet soo <rhyme label="a" type="m">snel</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Niemandt wil u <rhyme label="b" type="f">jaghen</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Ach Diana! al te <rhyme label="a" type="m" corresp="#s2:l1">fel</rhyme>:</l>
        <l id="s2:l4" met="3" type="+-">Lust u my te <rhyme label="b" type="f" corresp="#s2:l2">plaghen</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Neen Diana! u <rhyme label="c" type="m">bedaert</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Set u wat ter <rhyme label="d" type="f">neder</rhyme>;</l>
        <l id="s2:l7" met="4" type="+-">Schout u eyghen <rhyme label="c" type="m" corresp="#s2:l5">qualijckvaert</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Ghy zijt jongh en <rhyme label="d" type="f" corresp="#s2:l6">teder</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Waer om vlucht gy voor my <rhyme label="a" type="m">heen</rhyme>?</l>
        <l id="s3:l2" met="3" type="+-">Wat heb ick <rhyme label="b" type="f">bedreven</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Waerom, Lief. niet met ons <rhyme label="a" type="m" corresp="#s3:l1">tween</rhyme>?</l>
        <l id="s3:l4" met="3" type="+-">Wilt my antwoordt <rhyme label="b" type="f" corresp="#s3:l2">gheven</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Ach Diana! soete <rhyme label="c" type="m">Sus</rhyme>:</l>
        <l id="s3:l6" met="3" type="+-">Laet my u <rhyme label="d" type="f">in-halen</rhyme>;</l>
        <l id="s3:l7" met="4" type="+-">Soo sal ick u met een <rhyme label="c" type="m" corresp="#s3:l5">Kus</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Al de Schult <rhyme label="d" type="f" corresp="#s3:l6">betalen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Siet Diana: dits een <rhyme label="a" type="m">Lijn</rhyme>;</l>
        <l id="s4:l2" met="3" type="+-">Laet ons daer mee <rhyme label="b" type="f">binden</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Dijne Brackjes aen de <rhyme label="a" type="m" corresp="#s4:l1">mijn</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Van ghelijck, de <rhyme label="b" type="f" corresp="#s4:l2">Winden</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Tot der tijdt, dat wy <rhyme label="c" type="m">bequaem</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Vinden plaets om <rhyme label="d" type="f">Iaghen</rhyme>;</l>
        <l id="s4:l7" met="4" type="+-">Wat! nu gaen wy, soet, te <rhyme label="c" type="m" corresp="#s4:l5">saem</rhyme>;</l>
        <l id="s4:l8" met="3" type="+-">Dit is mijn <rhyme label="d" type="f" corresp="#s4:l6">behaghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Hier me zijn wy aen de <rhyme label="a" type="m">kant</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Van de hooghe <rhyme label="b" type="f">Sanden</rhyme>;</l>
        <l id="s5:l3" met="4" type="+-">Setten wy 't soo nae de <rhyme label="a" type="m" corresp="#s5:l1">Strandt</rhyme>;</l>
        <l id="s5:l4" met="3" type="+-">d' Honden van de <rhyme label="b" type="f" corresp="#s5:l2">Banden</rhyme>;</l>
        <l id="s5:l5" met="4" type="+-">Hier me krijghen wy een <rhyme label="c" type="m">veldt</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Achter dese <rhyme label="d" type="f">Tuynen</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">Daer Conijnen <rhyme label="c" type="m" corresp="#s5:l5">onghetelt</rhyme>,</l>
        <l id="s5:l8" met="3" type="+-">Swieren door de <rhyme label="d" type="f" corresp="#s5:l6">Duynen</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Nu Diana! soo 't u <rhyme label="a" type="m">lust</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Wilt ghediert te <rhyme label="b" type="f">vanghen</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">Rockjes op, en u <rhyme label="a" type="m" corresp="#s6:l1">toe-rust</rhyme>;</l>
        <l id="s6:l4" met="3" type="+-">Neemt tot 't Wilt u <rhyme label="b" type="f" corresp="#s6:l2">ganghen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Wacker, wacker, nae de <rhyme label="c" type="m">Zee</rhyme></l>
        <l id="s6:l6" met="3" type="+-">(In de windt ist <rhyme label="d" type="f">lustigh</rhyme>)</l>
        <l id="s6:l7" met="4" type="+-">Daer u Suster <rhyme label="c" type="m" corresp="#s6:l5">Galathe</rhyme></l>
        <l id="s6:l8" met="3" type="+-">Drijft haer Schaepjes <rhyme label="d" type="f" corresp="#s6:l6">rustigh</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
