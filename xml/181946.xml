<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>181946</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ach schoonste nimf aanzie</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">AY schoone Nimf aansiet een magtig <rhyme label="a" type="f">Koning</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Knielt voor u zoete <rhyme label="b" type="m">Beeld</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">En bied u aan zyn staat, zyn ryk zyn <rhyme label="a" type="f" corresp="#s1:l1">kroning</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Uit Monarchy <rhyme label="b" type="m" corresp="#s1:l2">geteelt</rhyme>?</l>
        <l id="s1:l5" met="5" type="-+">Slegts voor een Kus van uwe lieve <rhyme label="c" type="f">Lipjes</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Vergun dat ik myn brand <rhyme label="d" type="m">verkoel</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Vergun dat ik u borsjes <rhyme label="d" type="m" corresp="#s1:l6">voel</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Haar rooder <rhyme label="c" type="f" corresp="#s1:l5">tipjes</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Ik ben een Maagt een slegt <rhyme label="a" type="f">Schaap-Herderesse</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Myn Rykdom is myn <rhyme label="b" type="m">Vee</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Myn Ouders oud die leerde myn de <rhyme label="a" type="f" corresp="#s2:l1">lessen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En ’t was haar laatste <rhyme label="b" type="m" corresp="#s2:l2">bee</rhyme>!</l>
        <l id="s2:l5" met="5" type="-+">Gedenkt och kind van wie gy zyt <rhyme label="c" type="f">gesprooten</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ziet dat gy wel u Schaapjes <rhyme label="d" type="m">hoed</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Gy syt voor Herders <rhyme label="d" type="m" corresp="#s2:l6">opgevoed</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Niet voor de <rhyme label="c" type="f" corresp="#s2:l5">grooten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Ik sal u Hoofd een gouden Kroon <rhyme label="a" type="f">opsetten</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En voeren doen een <rhyme label="b" type="m">Staf</rhyme></l>
        <l id="s3:l3" met="5" type="-+">ô Neen u Kroon die sou myn eer <rhyme label="a" type="f" corresp="#s3:l1">verpletten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En zinken doen in ’t <rhyme label="b" type="m" corresp="#s3:l2">Graf</rhyme>,</l>
        <l id="s3:l5" met="5" type="-+">Myn groote Prael die sal u schand <rhyme label="c" type="f">bedekken</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Och dat verhoede al de <rhyme label="d" type="m">Goon</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">Dat ik sou om een Konings <rhyme label="d" type="m" corresp="#s3:l6">Kroon</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Myn eer <rhyme label="c" type="f" corresp="#s3:l5">bevlekken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">In mynen Throon daer sult gy syn <rhyme label="a" type="f">verheeven</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En heerlyk munten <rhyme label="b" type="m">uyt</rhyme>:</l>
        <l id="s4:l3" met="5" type="-+">ô Neen myn lust in ’t groene Velt te <rhyme label="a" type="f" corresp="#s4:l1">leeven</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En worden Herders <rhyme label="b" type="m" corresp="#s4:l2">Bruid</rhyme>,</l>
        <l id="s4:l5" met="5" type="-+">En sy een Koninginne van veel <rhyme label="c" type="f">Schaepjes</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In digte Linde is ons <rhyme label="d" type="m">Throon</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Een Roosekrans met Bloempjes <rhyme label="d" type="m" corresp="#s4:l6">schoon</rhyme>;</l>
        <l id="s4:l8" met="2" type="-+">Pronk voor de <rhyme label="c" type="f" corresp="#s4:l5">knaepjes</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Wat geeft u ’t Veld en deese boom <rhyme label="a" type="f">Gewassen</rhyme>:</l>
        <l id="s5:l2" met="3" type="-+">Wat gaet gy <rhyme label="b" type="m">Herderin</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">Des ’s morgens vroeg al door dauw en <rhyme label="a" type="f" corresp="#s5:l1">plassen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Strekt liever niet u <rhyme label="b" type="m" corresp="#s5:l2">sin</rhyme>,</l>
        <l id="s5:l5" met="5" type="-+">Te zyn omringt met Goud en blank <rhyme label="c" type="f">Bepeereld</rhyme>:</l>
        <l id="s5:l6" met="4" type="-+">Voor ’t veld een Marmer steene <rhyme label="d" type="m">Vloer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Een Koning voor een lompen <rhyme label="d" type="m" corresp="#s5:l6">Boer</rhyme>;</l>
        <l id="s5:l8" met="2" type="-+">Een pronk der <rhyme label="c" type="f" corresp="#s5:l5">weereld</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">Natuur die heeft ô Vorst doen ’t myn <rhyme label="a" type="f">teelde</rhyme>;</l>
        <l id="s6:l2" met="3" type="-+">Geen Kroone <rhyme label="b" type="m">toegevoegt</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">En nu ’t Fortuin, myn geen gebied mee <rhyme label="a" type="f" corresp="#s6:l1">deelde</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Soo ben ik wel <rhyme label="b" type="m" corresp="#s6:l2">vergenoegt</rhyme>:</l>
        <l id="s6:l5" met="5" type="-+">En sal op ’t Velt of in Bosschagie <rhyme label="c" type="f">woonen</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">In stille rust en <rhyme label="d" type="m">nederigheid</rhyme>:</l>
        <l id="s6:l7" met="4" type="-+">Dus bid ik dan syn <rhyme label="d" type="m" corresp="#s6:l6">Majesteid</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Myn wil <rhyme label="c" type="f" corresp="#s6:l5">verschoonen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="5" type="-+">Onnoosele Maegt hoe syt gy soo <rhyme label="a" type="f">versteeken</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Van weeldens zoet <rhyme label="b" type="m">aenschijn</rhyme>:</l>
        <l id="s7:l3" met="5" type="-+">Sijn Hoogheid weet eer ik myn trouw sou <rhyme label="a" type="f" corresp="#s7:l1">breeken</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Soo straft myn Godt <rhyme label="b" type="m" corresp="#s7:l2">Jupyn</rhyme>:</l>
        <l id="s7:l5" met="5" type="-+">Want in myn hert heb ik een Lief <rhyme label="c" type="f">verkooren</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Aen die daer uit het <rhyme label="d" type="m">geboomt</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Met myn gewolde Schaepjes <rhyme label="d" type="m" corresp="#s7:l6">koomt</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Myn Trouw <rhyme label="c" type="f" corresp="#s7:l5">gezwooren</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="5" type="-+">Wel gaat en blyft gy Boerse <rhyme label="a" type="f">Herderinne</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Met uwen Boer in ’t <rhyme label="b" type="m">Wout</rhyme>,</l>
        <l id="s8:l3" met="5" type="-+">Saeyt, maayt en plant en maekt de Kost te <rhyme label="a" type="f" corresp="#s8:l1">winne</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Wanneer gy syt <rhyme label="b" type="m" corresp="#s8:l2">getrouwt</rhyme>,</l>
        <l id="s8:l5" met="5" type="-+">En gaet gy Koning ik blyf in myn <rhyme label="c" type="f">Bosschagie</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Gaet by u Juffers van het <rhyme label="d" type="m">Hof</rhyme>;</l>
        <l id="s8:l7" met="4" type="-+">Versoek die door u Konings <rhyme label="d" type="m" corresp="#s8:l6">Lof</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">En s[...] <rhyme label="c" type="f" corresp="#s8:l5">Boelasy</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
