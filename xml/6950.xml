      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="bruiloftslied" translation="wedding song">bruiloftslied</tag>
    </tags>
    <melody>Iets moet ik u Laura vragen</melody>
    <categories>
      <category translation="cycle of life">levenscyclus</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ALs de Maen begint te <rhyme label="a" type="f">blincken</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Nae het spade <rhyme label="a" type="f" corresp="#s1:l1">ondersincken</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van de groote <rhyme label="b" type="m">Sonne-pracht</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Als den Hemel komt <rhyme label="c" type="f">vertooghen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Soo veel duysend gulden <rhyme label="c" type="f" corresp="#s1:l4">ooghen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Tot verlichtingh van de <rhyme label="b" type="m" corresp="#s1:l3">nacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Gaeter menigh hertjen <rhyme label="a" type="f">open</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die op t'avond-uyrtjen <rhyme label="a" type="f" corresp="#s2:l1">hopen</rhyme></l>
        <l id="s2:l3" met="4" type="+-">En verlangen nae de <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Menigh sietmen sich <rhyme label="c" type="f">verblijen</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Om des avonds heen te <rhyme label="c" type="f" corresp="#s2:l4">tijen</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Op de soete <rhyme label="b" type="m" corresp="#s2:l3">vrijster-jacht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Avond geeftse tongh en <rhyme label="a" type="f">leven</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Die sich tot het vrijen <rhyme label="a" type="f" corresp="#s3:l1">geven</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Avond voed de <rhyme label="b" type="m">vry-gheneugd</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Avond, vriend der <rhyme label="c" type="f">minne-knapen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Avond doet meer vreugde <rhyme label="c" type="f" corresp="#s3:l4">rapen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">In de soete <rhyme label="b" type="m" corresp="#s3:l3">vrije-vreugd</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer de soetst van dese <rhyme label="a" type="f">stonden</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Is den avond, die <rhyme label="a" type="f" corresp="#s4:l1">ghesonden</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Werd op trou en <rhyme label="b" type="m">bruylofs-dagh</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dien werd alle vreugd <rhyme label="c" type="f">ontslooten</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En met al het soet <rhyme label="c" type="f" corresp="#s4:l4">begooten</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Datmen in het vrijen <rhyme label="b" type="m" corresp="#s4:l3">sach</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gaet geluckigh Paer, gaet <rhyme label="a" type="f">henen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Desen tijd is u <rhyme label="a" type="f" corresp="#s5:l1">verschenen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Gaet daer u den Echt <rhyme label="b" type="m">verplicht</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Nae den lust-hof van u <rhyme label="c" type="f">zielen</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Daer de kittel-lustjes <rhyme label="c" type="f" corresp="#s5:l4">krielen</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Om het kleyne <rhyme label="b" type="m" corresp="#s5:l3">Minne-wicht</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Spaert u monden noch u <rhyme label="a" type="f">handen</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Doet uwe lusten <rhyme label="a" type="f" corresp="#s6:l1">branden</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Dat de vlam nae buyten <rhyme label="b" type="m">slae</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">En, om t'vyer dan weer te <rhyme label="c" type="f">blussen</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Smoort de vonckjes in het <rhyme label="c" type="f" corresp="#s6:l4">kussen</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Tot dat u brand wat <rhyme label="b" type="m" corresp="#s6:l3">over-gae</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
