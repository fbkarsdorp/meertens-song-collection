  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ik staa steeds <rhyme label="a" type="m">pal</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Voor aangebooren <rhyme label="b" type="f">rechten</rhyme>.</l>
        <l id="s1:l3" met="4" type="-+">Ik ben gereed elk te <rhyme label="b" type="f" corresp="#s1:l2">bevechten</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Wie ook myn vryheid rooven <rhyme label="c" type="m">wil</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">'k Min anders rust en hou my <rhyme label="c" type="m" corresp="#s1:l4">stil</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Maar liever <rhyme label="a" type="m">dood</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dan dwaas my te <rhyme label="b" type="f">verneÃªren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">'k Heb achting voor die 't Land <rhyme label="b" type="f" corresp="#s2:l2">regeeren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Als zy in waarheid, niet in <rhyme label="c" type="m">schyn</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">De Vaders van de Burgers <rhyme label="c" type="m" corresp="#s2:l4">zyn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">'k Roep nooit <rhyme label="a" type="m">hoezee</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Die boven noch die <rhyme label="b" type="f">onder</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Ik wensch ook niemand na den <rhyme label="b" type="f" corresp="#s3:l2">donder</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die woorden zyn maar enkel <rhyme label="c" type="m">wind</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Maar 'k ben een waare vryheids <rhyme label="c" type="m" corresp="#s3:l4">vrind</rhyme>.</l>
      </lg>
    </lg>
  </text>
