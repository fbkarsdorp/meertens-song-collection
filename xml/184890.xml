<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184890</id>
    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Allons tous à la crèche</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GOdt wacht om u te <rhyme label="a" type="f">schencken</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Het geen gy, mensch, hem <rhyme label="b" type="m">vraeght</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Al dat men kan <rhyme label="a" type="f" corresp="#s1:l1">bedencken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Aen u den Heer <rhyme label="b" type="m" corresp="#s1:l2">op-draeght</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Maer soo gy u <rhyme label="b" type="m" corresp="#s1:l2">behaeght</rhyme></l>
        <l id="s1:l6" met="3" type="-+">in d'aerdtsche <rhyme label="c" type="f">lustigheden</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Gy mist, dit <rhyme label="d" type="m">al</rhyme></l>
        <l id="s1:l8" met="2" type="-+">Is <rhyme label="d" type="m" corresp="#s1:l7">niet-met-al</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">Ons ooge <rhyme label="e" type="m">moet</rhyme></l>
        <l id="s1:l10" met="2" type="-+">Naer t'hooghste <rhyme label="e" type="m" corresp="#s1:l9">goedt</rhyme>:</l>
        <l id="s1:l11" met="3" type="+-">Vraeght dit door uw' <rhyme label="c" type="f" corresp="#s1:l6">gebeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sien in drift <rhyme label="a" type="f">ontsteken</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Voor eerst den <rhyme label="b" type="m">Gierighaert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Hem alles schynt <rhyme label="a" type="f" corresp="#s2:l1">t'ontbreken</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Hoe seer dat hy noch <rhyme label="b" type="m" corresp="#s2:l2">spaert</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Terwyl hy t'al <rhyme label="b" type="m" corresp="#s2:l2">bewaert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wenscht hy meer <rhyme label="c" type="f">lustigheden</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Hy mist, dit <rhyme label="d" type="m">al</rhyme></l>
        <l id="s2:l8" met="2" type="-+">Is <rhyme label="d" type="m" corresp="#s2:l7">niet-met-al</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">Ons ooge <rhyme label="e" type="m">moet</rhyme></l>
        <l id="s2:l10" met="2" type="-+">Naer t'hooghste <rhyme label="e" type="m" corresp="#s2:l9">goedt</rhyme>:</l>
        <l id="s2:l11" met="3" type="+-">Vraeght dit door uw' <rhyme label="c" type="f" corresp="#s2:l6">gebeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dees' wilt tot eer <rhyme label="a" type="f">geraeken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Groot door <rhyme label="b" type="m">hooveerdigheydt</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En syn gemoedt doen <rhyme label="a" type="f" corresp="#s3:l1">blaeken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In een <rhyme label="b" type="m" corresp="#s3:l2">verheventheydt</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Hy berst door <rhyme label="b" type="m" corresp="#s3:l2">nydigheydt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Soo hy niet wordt <rhyme label="c" type="f">aenbeden</rhyme>.</l>
        <l id="s3:l7" met="2" type="-+">Hy mist, mist, dit <rhyme label="d" type="m">al</rhyme></l>
        <l id="s3:l8" met="2" type="-+">Is <rhyme label="d" type="m" corresp="#s3:l7">niet-met-al</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">Ons ooge <rhyme label="e" type="m">moet</rhyme></l>
        <l id="s3:l10" met="2" type="-+">Naer t'hooghste <rhyme label="e" type="m" corresp="#s3:l9">goedt</rhyme>:</l>
        <l id="s3:l11" met="3" type="+-">Vraeght dit door uw' <rhyme label="c" type="f" corresp="#s3:l6">gebeden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Een kleyn saeck heeft <rhyme label="a" type="f">ontsteken</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Een anders gram <rhyme label="b" type="m">gelaet</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy wenscht te mogen <rhyme label="a" type="f" corresp="#s4:l1">vreken</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Het geen hem <rhyme label="b" type="m" corresp="#s4:l2">tegen-gaet</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Soo wie hem <rhyme label="b" type="m" corresp="#s4:l2">weder-staet</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Wordt self van hem <rhyme label="c" type="f">bestreden</rhyme>.</l>
        <l id="s4:l7" met="2" type="-+">Hy mist, mist, dit <rhyme label="d" type="m">al</rhyme></l>
        <l id="s4:l8" met="2" type="-+">Is <rhyme label="d" type="m" corresp="#s4:l7">niet-met-al</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">Ons ooge <rhyme label="e" type="m">moet</rhyme></l>
        <l id="s4:l10" met="2" type="-+">Naer t'hooghste <rhyme label="e" type="m" corresp="#s4:l9">goedt</rhyme>:</l>
        <l id="s4:l11" met="3" type="+-">Vraeght dit door uw' <rhyme label="c" type="f" corresp="#s4:l6">gebeden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy siet Godt ons <rhyme label="a" type="f">beminnen</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Syn groote liefde <rhyme label="b" type="m">blyckt</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Stiert naer dit goedt uw' <rhyme label="a" type="f" corresp="#s5:l1">sinnen</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dit is't, dat u <rhyme label="b" type="m" corresp="#s5:l2">gelyckt</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Of dat uw' vraege <rhyme label="b" type="m" corresp="#s5:l2">wyckt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Of vraeght des Hemels <rhyme label="c" type="f">panden</rhyme>:</l>
        <l id="s5:l7" met="2" type="-+">O mensch, hoe <rhyme label="d" type="m">soet</rhyme></l>
        <l id="s5:l8" met="2" type="-+">Is ons dit <rhyme label="d" type="m" corresp="#s5:l7">goedt</rhyme>!</l>
        <l id="s5:l9" met="2" type="-+">Blyft gy <rhyme label="e" type="m">verflouwt</rhyme>,</l>
        <l id="s5:l10" met="2" type="-+">En noch soo <rhyme label="e" type="m" corresp="#s5:l9">koudt</rhyme>,</l>
        <l id="s5:l11" met="3" type="+-">Die kont in liefde <rhyme label="c" type="f" corresp="#s5:l6">branden</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
