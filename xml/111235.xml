  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">WAt moet een Minnaar <rhyme label="a" type="f">stryden</rhyme>;</l>
        <l id="s1:l2" met="3" type="-+">Wat leet heeft hy te <rhyme label="a" type="f" corresp="#s1:l1">lyden</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">Om 't Meisje dat hy <rhyme label="b" type="m">mint</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Om 't Meisje dat hy <rhyme label="b" type="m" corresp="#s1:l3">mint</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zyn zinnen en <rhyme label="c" type="f">gedagten</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Vermoeid door Minne <rhyme label="c" type="f" corresp="#s1:l5">klagten</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Zyn als een <rhyme label="b" type="m" corresp="#s1:l3">wervel-wind</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy dwaalt gelyk de <rhyme label="a" type="f">blinden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hy kan geen duurplaats <rhyme label="a" type="f" corresp="#s2:l1">vinden</rhyme>;</l>
        <l id="s2:l3" met="3" type="-+">Waar hy zich keert of <rhyme label="b" type="m">went</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Waar hy zich keert of <rhyme label="b" type="m" corresp="#s2:l3">went</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy kent geen blyde <rhyme label="c" type="f">daagen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wyl strenge <rhyme label="c" type="f" corresp="#s2:l5">nootlots-plaagen</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">Hem dompelen in <rhyme label="b" type="m" corresp="#s2:l3">elent</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nu schynt zyn hoop <rhyme label="a" type="f">verlooren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dan weer zyn heil <rhyme label="a" type="f" corresp="#s3:l1">herbooren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Door vreugd en <rhyme label="b" type="m">tegenspoet</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">Door vreugd en <rhyme label="b" type="m" corresp="#s3:l3">tegenspoet</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Wat zal zyn uitkomst <rhyme label="c" type="f">weezen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hy dryft door hoop en <rhyme label="c" type="f" corresp="#s3:l5">vreezen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Als in een eb en <rhyme label="b" type="m" corresp="#s3:l3">vloed</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hy worsteld gantsche <rhyme label="a" type="f">nagten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En stryd met zyn <rhyme label="a" type="f" corresp="#s4:l1">gedagten</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gelyk een <rhyme label="b" type="m">martelaar</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Gelyk een <rhyme label="b" type="m" corresp="#s4:l3">martelaar</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dan hoopt hy duyzend <rhyme label="c" type="f">werven</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Ã´ Hemel doet my <rhyme label="c" type="f" corresp="#s4:l5">sterven</rhyme>!</l>
        <l id="s4:l7" met="3" type="-+">Het leven valt my <rhyme label="b" type="m" corresp="#s4:l3">swaar</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Maar word den dag <rhyme label="a" type="f">gebooren</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Dat hy zyn <rhyme label="a" type="f" corresp="#s5:l1">uitverkooren</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Eens met een kusje <rhyme label="b" type="m">groet</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Eens met een kusje <rhyme label="b" type="m" corresp="#s5:l3">groet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Dan wenst hy na geen <rhyme label="c" type="f">sterven</rhyme>?</l>
        <l id="s5:l6" met="3" type="-+">Maar denkt veel duizent <rhyme label="c" type="f" corresp="#s5:l5">werven</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wat smaakt een kusje <rhyme label="b" type="m" corresp="#s5:l3">zoet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wat blydschap moet het <rhyme label="a" type="f">weezen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Als hy zyn <rhyme label="a" type="f" corresp="#s6:l1">uitgeleezen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Eens in zyn armen <rhyme label="b" type="m">drukt</rhyme>?</l>
        <l id="s6:l4" met="3" type="-+">Eens in zyn armen <rhyme label="b" type="m" corresp="#s6:l3">drukt</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Hoe moet hy zig <rhyme label="c" type="f">verkwikken</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">In weinig <rhyme label="c" type="f" corresp="#s6:l5">ogenblikken</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Als hy haar roosje <rhyme label="b" type="m" corresp="#s6:l3">plukt</rhyme>.</l>
      </lg>
    </lg>
  </text>
