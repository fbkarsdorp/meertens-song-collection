    <tags>
      <tag translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Op een kerstnacht werd God geboren</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Op eenen Kerstnacht werdt Godt <rhyme label="a" type="m">geboren</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">In eener kribben werdt Iesus <rhyme label="a" type="m" corresp="#s1:l1">erkoren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vnd daer en was nymandt na noch <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Van Ioseph en die reyne Moeder <rhyme label="b" type="m" corresp="#s1:l3">Mary</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maria die tooch na Ioodsche <rhyme label="a" type="m">landt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Met Ioseph tot dat sy Betlehem <rhyme label="a" type="m" corresp="#s2:l1">vandt</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Daer hadde geen plaetse die Ionckfrou <rhyme label="b" type="m">reyn</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Noch eenich huys, daer sy mocht gaen <rhyme label="b" type="m" corresp="#s2:l3">hinein</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Maria die soud gebaren een <rhyme label="a" type="m">Soon</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Een Koninck wt der hemelen <rhyme label="a" type="m" corresp="#s3:l1">throon</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sy soecket die kribbe der Beesten <rhyme label="b" type="m">stal</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Daer baert sy dat kindeken sonder <rhyme label="b" type="m" corresp="#s3:l3">misfael</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Weest willekom heer Iongeling, heer <rhyme label="a" type="m">Iongeling</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Die ons boven is neder <rhyme label="a" type="m" corresp="#s4:l1">gesint</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Hy is der Maget Marie haer <rhyme label="b" type="m">kint</rhyme></l>
        <l id="s4:l4" met="5" type="-+">Hy is der Ioden, en ons <rhyme label="b" type="m" corresp="#s4:l3">Koning</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Maria die was niet wel <rhyme label="a" type="m">bereyt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Met cierlijcke doecken of windsel <rhyme label="a" type="m" corresp="#s5:l1">kleydt</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">In doexkens reyn dat kindt <rhyme label="b" type="m">gewant</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Op hoy der kribben werdt Christus <rhyme label="b" type="m" corresp="#s5:l3">bekant</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Den Engel Godes die haest <rhyme label="a" type="m">verbreyt</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Den Herders int velt met desen <rhyme label="a" type="m" corresp="#s6:l1">bescheyt</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">V is geboren een Koninck <rhyme label="b" type="m">groot</rhyme>,</l>
        <l id="s6:l4" met="5" type="-+">Die alle volck helpen sal wt der <rhyme label="b" type="m" corresp="#s6:l3">noot</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Nae Bethlehem gaet en soeckt den <rhyme label="a" type="m">Heer</rhyme>.</l>
        <l id="s7:l2" met="4" type="-+">Int hoy der kribben, hoort dese nye <rhyme label="a" type="m" corresp="#s7:l1">meer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Mits den so singet die Engelsche <rhyme label="b" type="m">schaer</rhyme>,</l>
        <l id="s7:l4" met="5" type="-+">Loff Godt en den menscyhen vreed <rhyme label="b" type="m" corresp="#s7:l3">allegaer</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die Herders terstont na Betlehem <rhyme label="a" type="m">gaen</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Bevinden gelijck sy vanden Engel <rhyme label="a" type="m" corresp="#s8:l1">verstaen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Wat sy spreecken Maria <rhyme label="b" type="m">behelt</rhyme>,</l>
        <l id="s8:l4" met="5" type="-+">Die heeft dit al onsen Koninck <rhyme label="b" type="m" corresp="#s8:l3">getelt</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Hoe wel was haer doe, <rhyme label="c" type="m">Susanina</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Den lieven Heeren Iesus, den heyligen <rhyme label="d" type="m">Kerst</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Marie, Marie <rhyme label="e" type="m">Soon</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
