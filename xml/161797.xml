  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">SChoone <rhyme label="a" type="f">Engelinne</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Celestyn, zie <rhyme label="b" type="m">daar</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Zijn twee Roozen; <rhyme label="a" type="f" corresp="#s1:l1">binne'</rhyme></l>
        <l id="s1:l4" met="4" type="-+">En ook buyten, na het <rhyme label="b" type="m" corresp="#s1:l2">jaar</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Zult gy d'Eene Rood <rhyme label="a" type="f" corresp="#s1:l1">bevinne'</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Maar de Tweede Wit en <rhyme label="b" type="m" corresp="#s1:l2">klaar</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Als gy ziet na <rhyme label="a" type="f">waarde</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Eerst de Witte, <rhyme label="b" type="m">keur</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Of ik dan niet <rhyme label="a" type="f" corresp="#s2:l1">aarde</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Met mijn weezen na die <rhyme label="b" type="m" corresp="#s2:l2">kleur</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Sint dat ik de min <rhyme label="a" type="f" corresp="#s2:l1">vergaarde</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En om U die bragt te <rhyme label="b" type="m" corresp="#s2:l2">veur</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Als gy ziet de <rhyme label="a" type="f">Roode</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Denk dan eens met <rhyme label="b" type="m">smart</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Dit 's de verw, ô <rhyme label="a" type="f" corresp="#s3:l1">Goode</rhyme>!</l>
        <l id="s3:l4" met="4" type="-+">Van mijn slaaf en minnaars <rhyme label="b" type="m" corresp="#s3:l2">hart</rhyme>;</l>
        <l id="s3:l5" met="4" type="-+">Nu hy, volgens mijn <rhyme label="a" type="f" corresp="#s3:l1">geboode</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Is in 't minnevuur <rhyme label="b" type="m" corresp="#s3:l2">verwart</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
