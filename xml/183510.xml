<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183510</id>
    <date>
      <year-start>1789</year-start>
      <year-end>1789</year-end>
    </date>
    <tags/>
    <melody>Vrijheid blijheid lust van het leven</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Vryheid, blyheid, lust van 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Vryheid, Neerlands <rhyme label="b" type="m">Pronksieraad</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Bato's Zonen, - wilt steeds <rhyme label="c" type="f">tonen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Dat gy snode Dwangzucht <rhyme label="b" type="m" corresp="#s1:l2">haat</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vryheid blyheid lust van 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Vryheid, 't allerdierbaarst <rhyme label="b" type="m">pand</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">Gy zyt heilig, - schoon niet <rhyme label="c" type="f">veilig</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">In 't verdrukte <rhyme label="b" type="m" corresp="#s2:l2">Vaderland</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Vryheid blyheid, lust van 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Vryheid, door elk Braav' <rhyme label="b" type="m">bemind</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Thans gebaunen - door <rhyme label="c" type="f">Tirannen</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Nergens gy hier Schuilplaats <rhyme label="b" type="m" corresp="#s3:l2">vind</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Vryheid, blyheid, lust van 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Vryheid, 't grootst geschenk op d' <rhyme label="b" type="m">Aard'</rhyme></l>
        <l id="s4:l3" met="4" type="-+">'t Is wel zeker, - dat een <rhyme label="c" type="f">Wreker</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Voor U, heeft het zwaard <rhyme label="b" type="m" corresp="#s4:l2">aanvaard</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Vryheid, blyheid, lust van 't <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Vryheid, Neerlands <rhyme label="b" type="m">Hartsvrindin</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Dat elk U pryze, - eer <rhyme label="c" type="f">bewyze</rhyme>;</l>
        <l id="s5:l4" met="4" type="-+">Keer haast weer ons Neerland <rhyme label="b" type="m" corresp="#s5:l2">in</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
