    <tags/>
    <melody>Houd aan ik zal u zingen voor</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">EEn Liet // wil ick gaen <rhyme label="a" type="f">beginnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">'t Bediet // sult ghy oock wel <rhyme label="a" type="f" corresp="#s1:l1">beminnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Want al mijn hart en <rhyme label="a" type="f" corresp="#s1:l1">sinnen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die winnen // nu niet dan <rhyme label="b" type="m">vreught</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Dus schenckt en <rhyme label="c" type="m">brenght</rhyme></l>
        <l id="s1:l6" met="4" type="-+">'tHert denckt // en wenckt // op u reyn <rhyme label="b" type="m" corresp="#s1:l4">jeught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick moet // dit nu schoontjens uyt <rhyme label="a" type="f">vegen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Het voet // de mensch in alle <rhyme label="a" type="f" corresp="#s2:l1">wegen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Dus moet // ick dit te <rhyme label="a" type="f" corresp="#s2:l1">degen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Gaen legen // en klaren <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Volght na // seght <rhyme label="c" type="m">jae</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Het gae seer dra al in mijn <rhyme label="b" type="m" corresp="#s2:l4">huyt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Dit glas is nu ledigh <rhyme label="a" type="f">ghedroncken</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Seer ras sat 't worden <rhyme label="a" type="f" corresp="#s3:l1">gheschoncken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">En niet langh laten <rhyme label="a" type="f" corresp="#s3:l1">proncken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Maer loncken // hier aen mijn <rhyme label="b" type="m">zy</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Op dat // het <rhyme label="c" type="m">nat</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Int vat // ons mach maken <rhyme label="b" type="m" corresp="#s3:l4">bly</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
