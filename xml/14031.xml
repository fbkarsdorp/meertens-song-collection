<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14031</id>
    <date>
      <year-start>1621</year-start>
      <year-end>1621</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Come shepherds deck your heads</melody>
  <author>Jan Jansz. Starter</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Lieflocksters van de <rhyme label="a" type="m">min</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die voor syn soete <rhyme label="b" type="f">krachten</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Bereydt huysvesting <rhyme label="a" type="m" corresp="#s1:l1">in</rhyme></l>
        <l id="s1:l4" met="3" type="-+">het pit van u <rhyme label="b" type="f" corresp="#s1:l2">ghedachten</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Ach! treurd, klaeghd, weend, ick <rhyme label="c" type="m">sie</rhyme></l>
        <l id="s1:l6" met="3" type="-+">elendigh hier <rhyme label="d" type="f">verslagen</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">De trouste Minnaer <rhyme label="c" type="m" corresp="#s1:l5">die</rhyme></l>
        <l id="s1:l8" met="3" type="-+">d'Aerdbodem heeft <rhyme label="d" type="f" corresp="#s1:l6">gedragen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Dienaer van syn <rhyme label="a" type="m">Vrou</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Van buyten en van <rhyme label="b" type="f">binnen</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Standvastigh in syn <rhyme label="a" type="m" corresp="#s2:l1">trou</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En eerbaer in het <rhyme label="b" type="f" corresp="#s2:l2">minnen</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Soet-aerdigh, braef van <rhyme label="c" type="m">geest</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Wtmuntend' schoon van <rhyme label="d" type="f">leden</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Is dese Vorst <rhyme label="c" type="m" corresp="#s2:l5">geweest</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vol Konincklycke <rhyme label="d" type="f" corresp="#s2:l6">zeden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Des' ongemeene <rhyme label="a" type="m">man</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dit Beeld vol alle <rhyme label="b" type="f">deughden</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">De ware spiegel <rhyme label="a" type="m" corresp="#s3:l1">van</rhyme></l>
        <l id="s3:l4" met="3" type="-+">De minnelycke <rhyme label="b" type="f" corresp="#s3:l2">vreughden</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Die Venus tot een <rhyme label="c" type="m">proef</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Had van haer konst <rhyme label="d" type="f">gekoren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Die heeft (ach! 't is te <rhyme label="c" type="m" corresp="#s3:l5">droef</rhyme>)</l>
        <l id="s3:l8" met="3" type="-+">Syn leven hier <rhyme label="d" type="f" corresp="#s3:l6">verloren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Alleenigh, om dat <rhyme label="a" type="m">hy</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Geweest is wat te <rhyme label="b" type="f">voorlick</rhyme></l>
        <l id="s4:l3" met="3" type="-+">In syne <rhyme label="a" type="m" corresp="#s4:l1">minnery</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Meer als hem was <rhyme label="b" type="f" corresp="#s4:l2">behoorlick</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Doch 't ging in deughd en <rhyme label="c" type="m">trou</rhyme>;</l>
        <l id="s4:l6" met="3" type="-+">Maer sonder raed van <rhyme label="d" type="f">Vrienden</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dies hem den Hemel <rhyme label="c" type="m" corresp="#s4:l5">rou</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In plaets van vreughd <rhyme label="d" type="f" corresp="#s4:l6">verlienden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O, spiegeld u aen 't <rhyme label="a" type="m">leedt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Ghy ionge jeughd van <rhyme label="b" type="f">desen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">In 't minnen niet te <rhyme label="a" type="m" corresp="#s5:l1">heet</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Maer wel bedacht wild <rhyme label="b" type="f" corresp="#s5:l2">wesen</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Volghd doch u vrienden <rhyme label="c" type="m">raedt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En wild de gaylheyd <rhyme label="d" type="f">myden</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Soo hebt ghy (hoe 't <rhyme label="c" type="m" corresp="#s5:l5">beslaet</rhyme>)</l>
        <l id="s5:l8" met="3" type="-+">Een toevlucht in u <rhyme label="d" type="f" corresp="#s5:l6">lyden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
