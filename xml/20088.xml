    <tags>
      <tag translation="love song">liefdeslied</tag>
    </tags>
    <melody>Ik drink de nieuwe most</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Nooit-volprese <rhyme label="a" type="m">leen</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">In wien de <rhyme label="a" type="m" corresp="#s1:l1">vrolijckheen</rhyme></l>
        <l id="s1:l3" met="5" type="-+">Van Junos praal-ziraazjen staan ten <rhyme label="b" type="m">toon</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Die dwingen kon de dapperste aller <rhyme label="b" type="m" corresp="#s1:l3">Goon</rhyme>:</l>
        <l id="s1:l5" met="1" type="-+">
          <rhyme label="c" type="m">Wanneer</rhyme>
        </l>
        <l id="s1:l6" met="2" type="-+">Ick d’oogen <rhyme label="c" type="m" corresp="#s1:l5">keer</rhyme></l>
        <l id="s1:l7" met="2" type="-+">Na ’t lief <rhyme label="d" type="m">gestalt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Daer gy steeds mede <rhyme label="d" type="m" corresp="#s1:l7">bralt</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Dan roep ick: fiere <rhyme label="e" type="f">Katarine</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">In de wereld <rhyme label="f" type="m">zijn</rhyme></l>
        <l id="s1:l11" met="3" type="+-">Oock Susters van <rhyme label="f" type="m" corresp="#s1:l10">Jupijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Sach Paris <rhyme label="a" type="m">ruggewaaart</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Nimf! gy zoudt op <rhyme label="a" type="m" corresp="#s2:l1">aard</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Na sijn gevoel, des werelds parel <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">Gy kreegt de vrugt alwaerdige <rhyme label="b" type="m" corresp="#s2:l3">Katrijn</rhyme>,</l>
        <l id="s2:l5" met="1" type="-+">Want <rhyme label="c" type="m">sy</rhyme></l>
        <l id="s2:l6" met="2" type="-+">Zijn, met haer <rhyme label="c" type="m" corresp="#s2:l5">dry</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">Vergode <rhyme label="d" type="m">Maagd</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Die d’ Hemel self <rhyme label="d" type="m" corresp="#s2:l7">behaagt</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">En Pallas in uw’ brein <rhyme label="e" type="f">besloten</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Juno in den <rhyme label="f" type="m">pronck</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">En Venus in ’t <rhyme label="f" type="m" corresp="#s2:l10">gelonck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Puyck-pronckje van de <rhyme label="a" type="m">Stad</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die d’ heele wereld <rhyme label="a" type="m" corresp="#s3:l1">vat</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Waer door het jagt-rijck Amstels-Stroompje <rhyme label="b" type="m">stroomt</rhyme>,</l>
        <l id="s3:l4" met="5" type="-+">Mijn veder, Lief, uw’ jeugd <rhyme label="b" type="m" corresp="#s3:l3">verwellekoomt</rhyme>,</l>
        <l id="s3:l5" met="1" type="-+">O <rhyme label="c" type="m">Vrou</rhyme>!</l>
        <l id="s3:l6" met="2" type="-+">Voor wien ick <rhyme label="c" type="m" corresp="#s3:l5">zou</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Met ziel en <rhyme label="d" type="m">sin</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">My hechten aen de <rhyme label="d" type="m" corresp="#s3:l7">min</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Om wien ick roep: o <rhyme label="e" type="f">Katarine</rhyme>!</l>
        <l id="s3:l10" met="3" type="+-">In de wereld <rhyme label="f" type="m">zijn</rhyme></l>
        <l id="s3:l11" met="3" type="+-">Oock Susters van <rhyme label="f" type="m" corresp="#s3:l10">Iupijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
