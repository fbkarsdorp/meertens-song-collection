  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Hier rust Anna. Laat haar <rhyme label="a" type="f">slapen</rhyme>.</l>
        <l id="s1:l2" met="4" type="+-">'T wigje moede, en <rhyme label="b" type="m">afgestreen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Wouw zig aan geen pragt <rhyme label="a" type="f" corresp="#s1:l1">vergapen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Van de werrelt hier <rhyme label="b" type="m" corresp="#s1:l2">beneen</rhyme>.</l>
        <l id="s1:l5" met="4" type="+-">Moeder wilt haar rust niet <rhyme label="c" type="f">storen</rhyme>.</l>
        <l id="s1:l6" met="4" type="-+">Ze is ter eewigheyt <rhyme label="c" type="f" corresp="#s1:l5">verkoren</rhyme>.</l>
      </lg>
    </lg>
  </text>
