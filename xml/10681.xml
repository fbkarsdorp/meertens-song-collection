    <melody>Schoon lief gij zijt prijs waard alleen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">PEynst om my schoon Lief <rhyme label="a" type="f">ghepresen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In mijn hart staet ghy <rhyme label="b" type="m">gheprent</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In mijn hart staet ghy <rhyme label="b" type="m" corresp="#s1:l2">gheprent</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Eerbaer en schoon zijt ghy van <rhyme label="a" type="f" corresp="#s1:l1">wesen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Ter Weerelt gheen soo <rhyme label="b" type="m" corresp="#s1:l2">jent</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Eerlijck en <rhyme label="b" type="m" corresp="#s1:l2">pertinent</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Zijt ghy schoon bloem <rhyme label="c" type="f">verheven</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">Wilt my schoon lief doch eens troost <rhyme label="c" type="f" corresp="#s1:l7">gheven</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Wilt my schoon lief doch eens troost <rhyme label="c" type="f" corresp="#s1:l7">gheven</rhyme>,</l>
        <l id="s1:l10" met="4" type="-+">Haest liefken comt my <rhyme label="c" type="f" corresp="#s1:l7">beneven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">En dan so mach ick vreucht <rhyme label="a" type="f">vermeeren</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Reyn wtvercoren <rhyme label="b" type="m">greyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Reyn wtvercoren <rhyme label="b" type="m" corresp="#s2:l2">greyn</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Recht als een die met aller <rhyme label="a" type="f" corresp="#s2:l1">eeren</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Eens is vertroost <rhyme label="b" type="m" corresp="#s2:l2">certeyn</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Om v schoon bloem <rhyme label="b" type="m" corresp="#s2:l2">alleyn</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Staet alle mijn <rhyme label="c" type="f">betrouwen</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Soete lief aen my, wilt v doch <rhyme label="c" type="f" corresp="#s2:l7">houwen</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Soete lief aen my, wilt v doch <rhyme label="c" type="f" corresp="#s2:l7">houwen</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">Ontgaen moet dan alle mijn <rhyme label="c" type="f" corresp="#s2:l7">rouwen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Om v schoon lief is alle mijn <rhyme label="a" type="f">jaghen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Niemant dan ghy <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Niemant dan ghy <rhyme label="b" type="m" corresp="#s3:l2">voorwaer</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Van schoonheyt my soo sal <rhyme label="a" type="f" corresp="#s3:l1">behaghen</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Als ghy sult lief <rhyme label="b" type="m" corresp="#s3:l2">openbaer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Lief laten wy te <rhyme label="b" type="m" corresp="#s3:l2">gaer</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">T'samen eens vreucht <rhyme label="c" type="f">oorboren</rhyme>,</l>
        <l id="s3:l8" met="4" type="-+">Keert v tot my liefken <rhyme label="c" type="f" corresp="#s3:l7">wtvercoren</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Keert v tot my liefken <rhyme label="c" type="f" corresp="#s3:l7">wtvercoren</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Dan soo moet alle mijn druck <rhyme label="c" type="f" corresp="#s3:l7">versmoren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Noyt gheen Princesse soo fier en <rhyme label="a" type="f">schoone</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Bemint stont in mijn <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Bemint stont in mijn <rhyme label="b" type="m" corresp="#s4:l2">sin</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Van Venus spant sy ver die <rhyme label="a" type="f" corresp="#s4:l1">Croone</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Reyn lief ick v <rhyme label="b" type="m" corresp="#s4:l2">bemin</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Crijgh' ick v tot <rhyme label="b" type="m" corresp="#s4:l2">ghewin</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Dat my v mach <rhyme label="c" type="f">ghebueren</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">Hey dan heb ick gheen <rhyme label="c" type="f" corresp="#s4:l7">treuren</rhyme>,</l>
        <l id="s4:l9" met="4" type="-+">Hey dan heb ick gheen <rhyme label="c" type="f" corresp="#s4:l7">treuren</rhyme>,</l>
        <l id="s4:l10" met="4" type="-+">Bequaem en lieflijck zijt ghy t'aller <rhyme label="c" type="f" corresp="#s4:l7">uren</rhyme>.</l>
      </lg>
    </lg>
