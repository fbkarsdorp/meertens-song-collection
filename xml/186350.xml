<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>186350</id>
    <date>
      <year-start>1672</year-start>
      <year-end>1672</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Lille Lille gardes-tu bien</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Sa Nederlanders komt ten <rhyme label="a" type="m">strijt</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Eer dat ghy word u Steden <rhyme label="a" type="m" corresp="#s1:l1">quijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">En hebt ghy niet de Macht <rhyme label="b" type="f">vernomen</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Die men van Vranckrijck af siet <rhyme label="b" type="f" corresp="#s1:l3">komen</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hoort de Fransse Trommel <rhyme label="a" type="m">slaen</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Sa Coopman maeckt u van de <rhyme label="a" type="m" corresp="#s2:l1">baen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Want die Trompet die gaet ter <rhyme label="b" type="f">eere</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Men hoortse roepen a la <rhyme label="b" type="f" corresp="#s2:l3">guere</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Vijftigh-duysent kloeck te <rhyme label="a" type="m">Paert</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Hebben alree den wegh <rhyme label="a" type="m" corresp="#s3:l1">aenvaert</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Te Voet oock vijftigh-duysent <rhyme label="b" type="f">mede</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Die men siet trecken na <rhyme label="b" type="f" corresp="#s3:l3">beneden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Duysent Moren zijn <rhyme label="a" type="m">getelt</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Op witte Paerden in het <rhyme label="a" type="m" corresp="#s4:l1">Velt</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Het schijnen Duyvels van der <rhyme label="b" type="f">Hellen</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Maer 'k hoop God salse neder <rhyme label="b" type="f" corresp="#s4:l3">vellen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Fransse en Switsers met <rhyme label="a" type="m">Levry</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Heeft men sien ryden op de <rhyme label="a" type="m" corresp="#s5:l1">Hey</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Langs Piemont en langs <rhyme label="b" type="f">Picardyen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Om ons te quellen aen twee <rhyme label="b" type="f" corresp="#s5:l3">zyen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Sa Hollanders kloeck van <rhyme label="a" type="m">aert</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Laet ons toch wesen <rhyme label="a" type="m" corresp="#s6:l1">onvervaert</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Met een Pistool en Spaense <rhyme label="b" type="f">Degen</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Laet ons den Fransman t' saem gaen <rhyme label="b" type="f" corresp="#s6:l3">tegen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Al ons Steden zijn <rhyme label="a" type="m">versterckt</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Noch dagelijckx men daer aen <rhyme label="a" type="m" corresp="#s7:l1">werckt</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Ia met meer als twee duysent <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Om te bevryden so ons <rhyme label="b" type="f" corresp="#s7:l3">Landen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Binnen Brussel met <rhyme label="a" type="m">trouweel</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Daer wert gemaeckt een sterck <rhyme label="a" type="m" corresp="#s8:l1">Casteel</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Met schoon Bolwercken hoogh <rhyme label="b" type="f">verheven</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Om op den Vyant vuur te <rhyme label="b" type="f" corresp="#s8:l3">geven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Binnen Mechelen men <rhyme label="a" type="m">bevon</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Daer wert gegoten grof <rhyme label="a" type="m" corresp="#s9:l1">Canon</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Granaden, Bomben en <rhyme label="b" type="f">Mortieren</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Om op den Vyant te doen <rhyme label="b" type="f" corresp="#s9:l3">zwieren</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Gent, Dermonde en <rhyme label="a" type="m">Condee</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Zijn al versterckt, met Sint <rhyme label="a" type="m" corresp="#s10:l1">Omee</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Oock Bergen, Valencijn en <rhyme label="b" type="f">Namen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Of daer de Fransse eens aen <rhyme label="b" type="f" corresp="#s10:l3">quamen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Sa Hollanders hand aen <rhyme label="a" type="m">hand</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Slaen wy de Fransman uyt ons <rhyme label="a" type="m" corresp="#s11:l1">Land</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En roept dan viva viva <rhyme label="b" type="f">Spanje</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">En viva Prince van <rhyme label="b" type="f" corresp="#s11:l3">Oranje</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Prince huysluy wilt u <rhyme label="a" type="m">Graen</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Nu in de Steden brengen <rhyme label="a" type="m" corresp="#s12:l1">gaen</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Eer het de Fransse komen <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Sy souden het u niet <rhyme label="b" type="f" corresp="#s12:l3">betalen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
