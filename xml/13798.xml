<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13798</id>
    <date>
      <year-start>1643</year-start>
      <year-end>1643</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Vrouwtje is uw man niet thuis</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Schoon ick dickmael heb’ <rhyme label="a" type="m">ghevraeght</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Of ghy wout <rhyme label="b" type="f">beminnen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Een die u sijn lijden <rhyme label="a" type="m" corresp="#s1:l1">klaeght</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Met bedruckte <rhyme label="b" type="f" corresp="#s1:l2">sinnen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Ick en heb o fiere <rhyme label="a" type="m" corresp="#s1:l1">Maeght</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Noyt yets konnen <rhyme label="b" type="f" corresp="#s1:l2">vinnen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Vrysters wat ick bidden <rhyme label="a" type="m">mach</rhyme>,</l>
        <l id="s2:l2" met="3" type="+-">Laet u trotsheydt <rhyme label="b" type="f">varen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">‘t Gaet als noch ghelijck het <rhyme label="a" type="m" corresp="#s2:l1">plach</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Met ons groene <rhyme label="b" type="f" corresp="#s2:l2">jaren</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Want daer komt een ouden <rhyme label="a" type="m" corresp="#s2:l1">dagh</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Niet bequaem te <rhyme label="b" type="f" corresp="#s2:l2">paren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ghy daerom o soete <rhyme label="a" type="m">kint</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Gunt u teere <rhyme label="b" type="f">wanghen</rhyme></l>
        <l id="s3:l3" met="4" type="+-">Aen een vrient die u <rhyme label="a" type="m" corresp="#s3:l1">bemint</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Met een diep <rhyme label="b" type="f" corresp="#s3:l2">verlanghen</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">En voor al doch haest <rhyme label="a" type="m" corresp="#s3:l1">begint</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Wilje danck <rhyme label="b" type="f" corresp="#s3:l2">ontfanghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Een die u van herten <rhyme label="a" type="m">vrijt</rhyme>,</l>
        <l id="s4:l2" met="3" type="+-">Klopt voor uwe <rhyme label="b" type="f">deuren</rhyme>:</l>
        <l id="s4:l3" met="4" type="+-">Soo u eens de kans <rhyme label="a" type="m" corresp="#s4:l1">ontglijt</rhyme>,</l>
        <l id="s4:l4" met="3" type="+-">Soo en baet geen <rhyme label="b" type="f" corresp="#s4:l2">treuren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Tast dan toe met alle <rhyme label="a" type="m" corresp="#s4:l1">vlijt</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">Terwijl ‘t u mach <rhyme label="b" type="f" corresp="#s4:l2">ghebeuren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">’t Roosje dat soo lustigh <rhyme label="a" type="m">bloeyt</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Kan niet langhe <rhyme label="b" type="f">dueren</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Iae al stont het <rhyme label="a" type="m" corresp="#s5:l1">onghemoeyt</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Tusschen hooghe <rhyme label="b" type="f" corresp="#s5:l2">mueren</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">Ieught, en wat in Hoven <rhyme label="a" type="m" corresp="#s5:l1">groeyt</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Is van weynigh <rhyme label="b" type="f" corresp="#s5:l2">uren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
