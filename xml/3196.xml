<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3196</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>O Angenietje</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ONrustighe <rhyme label="a" type="f">Menschen</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Die stadelijck <rhyme label="a" type="f" corresp="#s1:l1">wenschen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Waer elders vrede <rhyme label="b" type="m">woont</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat ghy 't deur twist <rhyme label="b" type="m" corresp="#s1:l3">ontkroont</rhyme>:</l>
        <l id="s1:l5" met="2" type="-+">Dat ghy de <rhyme label="c" type="f">ghedachten</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Die lieffelijck <rhyme label="c" type="f" corresp="#s1:l5">trachten</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Te leven in de <rhyme label="d" type="m">Deught</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Door twist beletten <rhyme label="d" type="m" corresp="#s1:l7">meught</rhyme>:</l>
        <l id="s1:l9" met="2" type="-+">Om voort te <rhyme label="e" type="m">gaen</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Op Vredes soete <rhyme label="e" type="m" corresp="#s1:l9">baen</rhyme>:</l>
        <l id="s1:l11" met="2" type="+-">Ghy tracht met <rhyme label="f" type="m">macht</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+">Tot ghy <rhyme label="f" type="m" corresp="#s1:l11">verkracht</rhyme></l>
        <l id="s1:l13" met="3" type="-+">haer vredigh soet <rhyme label="f" type="m" corresp="#s1:l11">ghedacht</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">V sinnen die <rhyme label="a" type="f">woelen</rhyme></l>
        <l id="s2:l2" met="2" type="-+">In slijckrighe <rhyme label="a" type="f" corresp="#s2:l1">poelen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van twisten-drift, en <rhyme label="b" type="m">lust</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Verbastert van de <rhyme label="b" type="m" corresp="#s2:l3">rust</rhyme>;</l>
        <l id="s2:l5" met="2" type="-+">V narrighe <rhyme label="c" type="f">kueren</rhyme></l>
        <l id="s2:l6" met="2" type="-+">En konnen niet <rhyme label="c" type="f" corresp="#s2:l5">dueren</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Want door te veel <rhyme label="d" type="m">ghekijf</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Soo raeckje vaeck om 't <rhyme label="d" type="m" corresp="#s2:l7">lijf</rhyme>:</l>
        <l id="s2:l9" met="2" type="-+">Die vuylt en <rhyme label="e" type="m">kladt</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Elck een met dit of <rhyme label="e" type="m" corresp="#s2:l9">dat</rhyme>,</l>
        <l id="s2:l11" met="2" type="+-">Raeckt vaeck in't <rhyme label="f" type="m">net</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+">En wordt <rhyme label="f" type="m" corresp="#s2:l11">gheset</rhyme></l>
        <l id="s2:l13" met="3" type="-+">Daer 't klappen wort <rhyme label="f" type="m" corresp="#s2:l11">belet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dan moeje wel <rhyme label="a" type="f">buyghen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Dan moetje <rhyme label="a" type="f" corresp="#s3:l1">betuyghen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Het vals en vuyl <rhyme label="b" type="m">ghesnap</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">De snoode <rhyme label="b" type="m" corresp="#s3:l3">achterklap</rhyme>;</l>
        <l id="s3:l5" met="2" type="-+">Dan moetje <rhyme label="c" type="f">betalen</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Iou schandigh <rhyme label="c" type="f" corresp="#s3:l5">af-malen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Den armen krijght 't <rhyme label="d" type="m">ghenot</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Van uwen <rhyme label="d" type="m" corresp="#s3:l7">lachter-spot</rhyme>:</l>
        <l id="s3:l9" met="2" type="-+">Ay! laet dit <rhyme label="e" type="m">staen</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">'t Is beter niet <rhyme label="e" type="m" corresp="#s3:l9">ghedaen</rhyme>,</l>
        <l id="s3:l11" met="2" type="+-">Dan <rhyme label="f" type="m">naderderhandt</rhyme></l>
        <l id="s3:l12" met="2" type="-+">In strick of <rhyme label="f" type="m" corresp="#s3:l11">bandt</rhyme></l>
        <l id="s3:l13" met="3" type="-+">Te raecken <rhyme label="f" type="m" corresp="#s3:l11">overmant</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
