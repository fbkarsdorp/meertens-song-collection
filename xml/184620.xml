<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184620</id>
    <date>
      <year-start>1755</year-start>
      <year-end>1755</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon liefje jent zeer excellent</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Aanhoord dog eens na dit <rhyme label="a" type="m">Nieuw-Lied</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Gy Menschen <rhyme label="b" type="f">allegare</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hoe dat het den Heer hier al <rhyme label="a" type="m" corresp="#s1:l1">voorziet</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Dat hy ons dit <rhyme label="a" type="m" corresp="#s1:l1">verdriet</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Van onzen Halze <rhyme label="a" type="m" corresp="#s1:l1">hiet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ons Vreede zend <rhyme label="b" type="f" corresp="#s1:l2">eenpaare</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Godt die komt ons nu troosten <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">En ons blijdschap <rhyme label="b" type="f">vereeren</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Dat hy ons wend al uyt <rhyme label="a" type="m" corresp="#s2:l1">gekwel</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Van dien Engels-man <rhyme label="a" type="m" corresp="#s2:l1">fel</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">En dien Oorlog <rhyme label="a" type="m" corresp="#s2:l1">snel</rhyme>:</l>
        <l id="s2:l6" met="3" type="-+">Die gaat hy van ons <rhyme label="b" type="f" corresp="#s2:l2">weeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Eertijds het Parlement <rhyme label="a" type="m">voortaan</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In Cromwels tijd <rhyme label="b" type="f">beneven</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Tegen den Koning plag op te <rhyme label="a" type="m" corresp="#s3:l1">staan</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zo dat het is <rhyme label="a" type="m" corresp="#s3:l1">gegaan</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">En gingen hem dood <rhyme label="a" type="m" corresp="#s3:l1">slaan</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Al met een Bijl <rhyme label="b" type="f" corresp="#s3:l2">verheven</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dien Iongen Koning most <rhyme label="a" type="m">aldaar</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">Al uyt zijn Land gaan <rhyme label="b" type="f">vlugten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Het viel den Iongeling zo <rhyme label="a" type="m" corresp="#s4:l1">zwaar</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Al door dat groot <rhyme label="a" type="m" corresp="#s4:l1">misbaar</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Dat zy maakten <rhyme label="a" type="m" corresp="#s4:l1">voorwaar</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Agter Land ging hy <rhyme label="b" type="f" corresp="#s4:l2">zugten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hy ging zo menig Iaar <rhyme label="a" type="m">minjoot</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Al in het wilde <rhyme label="b" type="f">doolen</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Zijn Ballingschap was voor hem <rhyme label="a" type="m" corresp="#s5:l1">groot</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Met smerten alzo <rhyme label="a" type="m" corresp="#s5:l1">bloot</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Al om zijn Vaders <rhyme label="a" type="m" corresp="#s5:l1">dood</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Was ’t Rijk hem daar <rhyme label="b" type="f" corresp="#s5:l2">ontstoolen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ten leste sprak zy <rhyme label="a" type="m">aldaar</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat hy nog was in ’t <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Zy spraken doen ook mee <rhyme label="a" type="m" corresp="#s6:l1">malkaar</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">En dat zy hem hier <rhyme label="a" type="m" corresp="#s6:l1">naar</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">In stee al van zijn <rhyme label="a" type="m" corresp="#s6:l1">Vaar</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Zou tot zijn Rijk <rhyme label="b" type="f" corresp="#s6:l2">begeven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Zy maakten een Vloot Scheepen <rhyme label="a" type="m">klaar</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Men sloeg daar aan de <rhyme label="b" type="f">Zeyle</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En zy zeylden ook mee <rhyme label="a" type="m" corresp="#s7:l1">voorwaar</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Na Holland hoord <rhyme label="a" type="m" corresp="#s7:l1">aldaar</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">En kwam ook hier <rhyme label="a" type="m" corresp="#s7:l1">naar</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">En dat al zonder <rhyme label="b" type="f" corresp="#s7:l2">feyle</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Voor Scheveling aldaar te <rhyme label="a" type="m">Land</rhyme>:</l>
        <l id="s8:l2" met="3" type="-+">Kwamen zy ook <rhyme label="b" type="f">gevaare</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En dat al aan de Zyze <rhyme label="a" type="m" corresp="#s8:l1">kant</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En ook al voor het <rhyme label="a" type="m" corresp="#s8:l1">Strant</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Al in den Haag <rhyme label="a" type="m" corresp="#s8:l1">faljant</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Zy schoote lustig <rhyme label="b" type="f" corresp="#s8:l2">daare</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Zy kwamen om haar Koning <rhyme label="a" type="m">weer</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Om in zijn Rijk te <rhyme label="b" type="f">halen</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">De Staate dienden hem al <rhyme label="a" type="m" corresp="#s9:l1">meer</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Hulpen hem na ’t <rhyme label="a" type="m" corresp="#s9:l1">begeer</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Al in zijn Rijk met <rhyme label="a" type="m" corresp="#s9:l1">eer</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Hy sprak al zonder <rhyme label="b" type="f" corresp="#s9:l2">falen</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Dat zy met vreede zoude <rhyme label="a" type="m">daar</rhyme>:</l>
        <l id="s10:l2" met="3" type="-+">En vriendschap zamen <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Dat hy geen vyandschap <rhyme label="a" type="m" corresp="#s10:l1">voorwaar</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Zou voeren ook hier <rhyme label="a" type="m" corresp="#s10:l1">naar</rhyme>,</l>
        <l id="s10:l5" met="3" type="-+">Hy loofden ’t wel zo, <rhyme label="a" type="m" corresp="#s10:l1">maar</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Ten is zo niet <rhyme label="b" type="f" corresp="#s10:l2">gebleven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">In Engeland was men <rhyme label="a" type="m">gerust</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Om dien Koning, wild <rhyme label="b" type="f">weeten</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Te Zonden heeft men <rhyme label="a" type="m" corresp="#s11:l1">gekust</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">En ook ’t Geschut <rhyme label="a" type="m" corresp="#s11:l1">geblust</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Het was voor haar een <rhyme label="a" type="m" corresp="#s11:l1">lust</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Van blijdschap hoog <rhyme label="b" type="f" corresp="#s11:l2">gemeten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Het was geluk Heer Koning <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Godt geeft u een lang <rhyme label="b" type="f">leven</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">En dat gy dog met goed <rhyme label="a" type="m" corresp="#s12:l1">playzier</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">Mag leven ook nu <rhyme label="a" type="m" corresp="#s12:l1">hier</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">Om dat zy hem ook <rhyme label="a" type="m" corresp="#s12:l1">dier</rhyme></l>
        <l id="s12:l6" met="3" type="-+">Aldaar kregen <rhyme label="b" type="f" corresp="#s12:l2">beneven</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Doen men schreef Zestien-honderd <rhyme label="a" type="m">Iaar</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Wild dit weeten hier <rhyme label="b" type="f">neven</rhyme>,</l>
        <l id="s13:l3" met="4" type="-+">Kwam ‘er in Engeland <rhyme label="a" type="m" corresp="#s13:l1">aldaar</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Zulken twist <rhyme label="a" type="m" corresp="#s13:l1">voorwaar</rhyme>,</l>
        <l id="s13:l5" met="3" type="-+">Met zulken groot <rhyme label="a" type="m" corresp="#s13:l1">misbaar</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Van Vyandschap <rhyme label="b" type="f" corresp="#s13:l2">verheven</rhyme></l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Zo kwam hy wroeten te met <rhyme label="a" type="m">aan</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Hy ging ons Schepen <rhyme label="b" type="f">houwen</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">En hy ging dat alzo <rhyme label="a" type="m" corresp="#s14:l1">verstaan</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">En ’t mogt alzo wel <rhyme label="a" type="m" corresp="#s14:l1">gaan</rhyme>,</l>
        <l id="s14:l5" met="3" type="-+">Zo dogt hy al <rhyme label="a" type="m" corresp="#s14:l1">voortaan</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Dat was de vriendschap <rhyme label="b" type="f" corresp="#s14:l2">trouwen</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Al in ’t Vijf-en-zestigste <rhyme label="a" type="m">Iaar</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Indien Oorlog <rhyme label="b" type="f">zwaare</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Doen maakt’ men de Schepe <rhyme label="a" type="m" corresp="#s15:l1">klaar</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">Aan weder-zyde <rhyme label="a" type="m" corresp="#s15:l1">daar</rhyme>,</l>
        <l id="s15:l5" met="3" type="-+">Al om te strijde <rhyme label="a" type="m" corresp="#s15:l1">zwaar</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Zy zijn doen t’Zeyl <rhyme label="b" type="f" corresp="#s15:l2">gevare</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Zy kwamen doen voor <rhyme label="a" type="m">Engeland</rhyme>,</l>
        <l id="s16:l2" met="3" type="-+">Voor den Vyand haar <rhyme label="b" type="f">Haven</rhyme>,</l>
        <l id="s16:l3" met="4" type="-+">Den Engels-man die schrikte, <rhyme label="a" type="m" corresp="#s16:l1">want</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Van onze Vloot <rhyme label="a" type="m" corresp="#s16:l1">faljant</rhyme>,</l>
        <l id="s16:l5" met="3" type="-+">Doen zy al van haar <rhyme label="a" type="m" corresp="#s16:l1">kant</rhyme></l>
        <l id="s16:l6" met="3" type="-+">Haar t’Zeewaard in <rhyme label="b" type="f" corresp="#s16:l2">begaven</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Zy kwamen daar malkander <rhyme label="a" type="m">by</rhyme></l>
        <l id="s17:l2" met="3" type="-+">In Zee, hey wil het <rhyme label="b" type="f">weeten</rhyme>,</l>
        <l id="s17:l3" met="4" type="-+">De Hollanders hadden de <rhyme label="a" type="m" corresp="#s17:l1">ly</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Het was ook stille <rhyme label="a" type="m" corresp="#s17:l1">vry</rhyme>,</l>
        <l id="s17:l5" met="3" type="-+">Zy schooten ons ook <rhyme label="a" type="m" corresp="#s17:l1">zy</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">’t Was droevig <rhyme label="b" type="f" corresp="#s17:l2">ongemeeten</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Godt den Heere die ons zeer <rhyme label="a" type="m">zwaar</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Den neerlag heeft <rhyme label="b" type="f">gegeven</rhyme>,</l>
        <l id="s18:l3" met="4" type="-+">Wy en konnen ’t niet helpen <rhyme label="a" type="m" corresp="#s18:l1">daar</rhyme>:</l>
        <l id="s18:l4" met="3" type="-+">Maar Godt den Heer <rhyme label="a" type="m" corresp="#s18:l1">voorwaar</rhyme>,</l>
        <l id="s18:l5" met="3" type="-+">Als wy die bidden <rhyme label="a" type="m" corresp="#s18:l1">maar</rhyme>,</l>
        <l id="s18:l6" met="3" type="-+">Die zal ons niet <rhyme label="b" type="f" corresp="#s18:l2">begeven</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">Doe men schreef Zes-en-zestig <rhyme label="a" type="m">hoord</rhyme></l>
        <l id="s19:l2" met="3" type="-+">Doen ging men weer <rhyme label="b" type="f">vergaren</rhyme></l>
        <l id="s19:l3" met="4" type="-+">De Schepen om te helpen <rhyme label="a" type="m" corresp="#s19:l1">voort</rhyme>,</l>
        <l id="s19:l4" met="3" type="-+">En met een goed <rhyme label="a" type="m" corresp="#s19:l1">accoord</rhyme>,</l>
        <l id="s19:l5" met="3" type="-+">Het Volk moet aan <rhyme label="a" type="m" corresp="#s19:l1">Boord</rhyme>,</l>
        <l id="s19:l6" met="3" type="-+">Om na den Krijg te <rhyme label="b" type="f" corresp="#s19:l2">vaaren</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="4" type="-+">Onze Vloot die voer doen al <rhyme label="a" type="m">weer</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Na Zee al om te <rhyme label="b" type="f">stryden</rhyme>,</l>
        <l id="s20:l3" met="4" type="-+">Zy kwamen doen na haar <rhyme label="a" type="m" corresp="#s20:l1">begeer</rhyme></l>
        <l id="s20:l4" met="3" type="-+">In Zee ook mee niet <rhyme label="a" type="m" corresp="#s20:l1">veer</rhyme>,</l>
        <l id="s20:l5" met="3" type="-+">Zy b’vogten grooten <rhyme label="a" type="m" corresp="#s20:l1">eer</rhyme>,</l>
        <l id="s20:l6" met="3" type="-+">En dat al met <rhyme label="b" type="f" corresp="#s20:l2">verblyden</rhyme>.</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="4" type="-+">Den Engels-man, hoord eens na <rhyme label="a" type="m">my</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Die kwam lustig <rhyme label="b" type="f">brageren</rhyme>,</l>
        <l id="s21:l3" met="4" type="-+">Zy kwamen vaaren ook mee <rhyme label="a" type="m" corresp="#s21:l1">bly</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">Gelijk eens of en <rhyme label="a" type="m" corresp="#s21:l1">zy</rhyme></l>
        <l id="s21:l5" met="3" type="-+">Ons in de vlugt weer <rhyme label="a" type="m" corresp="#s21:l1">vrym</rhyme></l>
        <l id="s21:l6" met="3" type="-+">Maar hy moest haastig <rhyme label="b" type="f" corresp="#s21:l2">keeren</rhyme>.</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="4" type="-+">Maar onze Helden ook niet <rhyme label="a" type="m">slegt</rhyme>,</l>
        <l id="s22:l2" met="3" type="-+">Die voor haar niet en <rhyme label="b" type="f">beefden</rhyme>,</l>
        <l id="s22:l3" met="4" type="-+">Hebben zijn staart ook wel <rhyme label="a" type="m" corresp="#s22:l1">getreft</rhyme>,</l>
        <l id="s22:l4" met="3" type="-+">En de Krans wel <rhyme label="a" type="m" corresp="#s22:l1">gehegt</rhyme>,</l>
        <l id="s22:l5" met="3" type="-+">Ik zeg het jou te <rhyme label="a" type="m" corresp="#s22:l1">regt</rhyme>,</l>
        <l id="s22:l6" met="3" type="-+">Zy vogten dat het <rhyme label="b" type="f" corresp="#s22:l2">kleefden</rhyme>.</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="4" type="-+">Zo dat dien Engels-man zijn <rhyme label="a" type="m">Vloot</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Meestendeel ging <rhyme label="b" type="f">verstroye</rhyme>,</l>
        <l id="s23:l3" met="4" type="-+">‘k Meen dat men wakker Bloed <rhyme label="a" type="m" corresp="#s23:l1">vergoot</rhyme>,</l>
        <l id="s23:l4" met="3" type="-+">Veel Menschen bleven <rhyme label="a" type="m" corresp="#s23:l1">dood</rhyme>,</l>
        <l id="s23:l5" met="3" type="-+">Met Schepen alzo <rhyme label="a" type="m" corresp="#s23:l1">groot</rhyme>,</l>
        <l id="s23:l6" met="3" type="-+">Zag men daar in <rhyme label="b" type="f" corresp="#s23:l2">convoye</rhyme>.</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="4" type="-+">Godt zy gelooft over die <rhyme label="a" type="m">Slag</rhyme>,</l>
        <l id="s24:l2" met="3" type="-+">Die men daar zag <rhyme label="b" type="f">gebeuren</rhyme>,</l>
        <l id="s24:l3" met="4" type="-+">Laat ons loven dog Nagt en <rhyme label="a" type="m" corresp="#s24:l1">Dag</rhyme>,</l>
        <l id="s24:l4" met="3" type="-+">En met een goed <rhyme label="a" type="m" corresp="#s24:l1">verdrag</rhyme>,</l>
        <l id="s24:l5" met="3" type="-+">Den Engels-man die <rhyme label="a" type="m" corresp="#s24:l1">zag</rhyme></l>
        <l id="s24:l6" met="3" type="-+">Men hier na ook wel <rhyme label="b" type="f" corresp="#s24:l2">treuren</rhyme>.</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="4" type="-+">Godt heeft zijn hoogmoed daar <rhyme label="a" type="m">geveld</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Daar voor moet ik Godt <rhyme label="b" type="f">loven</rhyme>,</l>
        <l id="s25:l3" met="4" type="-+">En heeft ons nu de Vree <rhyme label="a" type="m" corresp="#s25:l1">besteld</rhyme>,</l>
        <l id="s25:l4" met="3" type="-+">Dien Koning zijn haet <rhyme label="a" type="m" corresp="#s25:l1">smelt</rhyme>,</l>
        <l id="s25:l5" met="3" type="-+">Die ons zo heeft <rhyme label="a" type="m" corresp="#s25:l1">gekweld</rhyme>,</l>
        <l id="s25:l6" met="3" type="-+">Ey prijst dien Heer van <rhyme label="b" type="f" corresp="#s25:l2">boven</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="4" type="-+">Hoe dikwils hebben wy <rhyme label="a" type="m">belaan</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">O Heere Godt <rhyme label="b" type="f">gezeten</rhyme>!</l>
        <l id="s26:l3" met="4" type="-+">Gy hebt ons Vyand gaan <rhyme label="a" type="m" corresp="#s26:l1">verstaan</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">Al uyt de Zee <rhyme label="a" type="m" corresp="#s26:l1">voortaan</rhyme>,</l>
        <l id="s26:l5" met="3" type="-+">Gy hebt voor ons <rhyme label="a" type="m" corresp="#s26:l1">gegaan</rhyme>,</l>
        <l id="s26:l6" met="3" type="-+">O Vader hoog wy <rhyme label="b" type="f" corresp="#s26:l2">weeten</rhyme>.</l>
      </lg>
      <lg id="s27" type="stanza">
        <l id="s27:l1" met="4" type="-+">Daarom moet ik verbreyden <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s27:l2" met="3" type="-+">En uwen lof <rhyme label="b" type="f">verkonden</rhyme>,</l>
        <l id="s27:l3" met="4" type="-+">Ik moet u loven ook met <rhyme label="a" type="m" corresp="#s27:l1">vliet</rhyme>,</l>
        <l id="s27:l4" met="3" type="-+">Want doe ik dat ook <rhyme label="a" type="m" corresp="#s27:l1">niet</rhyme>,</l>
        <l id="s27:l5" met="3" type="-+">Gy zoud my meer <rhyme label="a" type="m" corresp="#s27:l1">verdriet</rhyme>,</l>
        <l id="s27:l6" met="3" type="-+">Leggen voor mijnen <rhyme label="b" type="f" corresp="#s27:l2">monden</rhyme>.</l>
      </lg>
      <lg id="s28" type="stanza">
        <l id="s28:l1" met="4" type="-+">Nu leven wy ook wel te <rhyme label="a" type="m">vreen</rhyme>,</l>
        <l id="s28:l2" met="3" type="-+">Wy Menschen in ons <rhyme label="b" type="f">Landen</rhyme>,</l>
        <l id="s28:l3" met="4" type="-+">Ons Gebed dat heeft Godt <rhyme label="a" type="m" corresp="#s28:l1">certeen</rhyme>,</l>
        <l id="s28:l4" met="3" type="-+">Verhoord ook daar <rhyme label="a" type="m" corresp="#s28:l1">alleen</rhyme>,</l>
        <l id="s28:l5" met="3" type="-+">Hy geeft ons daar met <rhyme label="a" type="m" corresp="#s28:l1">een</rhyme>,</l>
        <l id="s28:l6" met="3" type="-+">Aldaar een vast <rhyme label="b" type="f" corresp="#s28:l2">bystanden</rhyme>.</l>
      </lg>
      <lg id="s29" type="stanza">
        <l id="s29:l1" met="4" type="-+">Looft Visschers hier nu <rhyme label="a" type="m">allegaar</rhyme>,</l>
        <l id="s29:l2" met="3" type="-+">Den Heer ook vroeg en <rhyme label="b" type="f">spaden</rhyme>,</l>
        <l id="s29:l3" met="4" type="-+">En dankt hem nog van Iaar tot <rhyme label="a" type="m" corresp="#s29:l1">Iaar</rhyme>,</l>
        <l id="s29:l4" met="3" type="-+">Die ons verblijden <rhyme label="a" type="m" corresp="#s29:l1">daar</rhyme>,</l>
        <l id="s29:l5" met="3" type="-+">Die ons verhoord heeft <rhyme label="a" type="m" corresp="#s29:l1">klaar</rhyme>,</l>
        <l id="s29:l6" met="3" type="-+">Hy geeft ons nu <rhyme label="b" type="f" corresp="#s29:l2">genaden</rhyme>.</l>
      </lg>
      <lg id="s30" type="stanza">
        <l id="s30:l1" met="4" type="-+">Princesse neemt in dank dit <rhyme label="a" type="m">Lied</rhyme>,</l>
        <l id="s30:l2" met="3" type="-+">Ik ken ’t niet al <rhyme label="b" type="f">verhalen</rhyme>,</l>
        <l id="s30:l3" met="4" type="-+">Gy Menscyhen dog wel voor u <rhyme label="a" type="m" corresp="#s30:l1">ziet</rhyme>,</l>
        <l id="s30:l4" met="3" type="-+">En dankt den Heer met <rhyme label="a" type="m" corresp="#s30:l1">vliet</rhyme>,</l>
        <l id="s30:l5" met="3" type="-+">En zondig ook zo <rhyme label="a" type="m" corresp="#s30:l1">niet</rhyme>,</l>
        <l id="s30:l6" met="3" type="-+">Godt zal ons wel <rhyme label="b" type="f" corresp="#s30:l2">betalen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
