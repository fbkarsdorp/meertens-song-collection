<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>162747</id>
    <date>
      <year-start>1666</year-start>
      <year-end>1678</year-end>
    </date>
    <location>
      <orginal-name>enkhuizen</orginal-name>
      <standard-name>Enkhuizen, The Netherlands</standard-name>
      <coordinates>
        <lon>52.7036282</lon>
        <lat>5.2901222</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Si c'est pour mon pucelage</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">TReusel soud' soo garen <rhyme label="a" type="f">Trouwen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Maer sy weet seer weynigh <rhyme label="b" type="m">raet</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Want geen Vryer nae haer <rhyme label="b" type="m" corresp="#s1:l2">staet</rhyme>;</l>
        <l id="s1:l4" met="4" type="+-">Daerom zit sy steets te <rhyme label="a" type="f" corresp="#s1:l1">rouwen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hoe sy daer aen-raecken <rhyme label="c" type="m">sal</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Want een Vryer dat is 't <rhyme label="c" type="m" corresp="#s1:l5">al</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Waer ick (seyt sy) slim van <rhyme label="a" type="f">leden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Waer ick Kreupel ofte <rhyme label="b" type="m">Lam</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Waer ick oudt en dor van <rhyme label="b" type="m" corresp="#s2:l2">stam</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Waer ick loom en traegh in't <rhyme label="a" type="f" corresp="#s2:l1">treden</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Waer ick doof, of dof van <rhyme label="c" type="m">spraeck</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Dan was 't heel een ander <rhyme label="c" type="m" corresp="#s2:l5">saeck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Maer nu ben ick jongh en <rhyme label="a" type="f">aerdigh</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Door-gaens vrolick van <rhyme label="b" type="m">gemoet</rhyme>;</l>
        <l id="s3:l3" met="4" type="+-">Rad van tongh, en licht te <rhyme label="b" type="m" corresp="#s3:l2">voet</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Seecker wel een Ionghman <rhyme label="a" type="f" corresp="#s3:l1">waerdigh</rhyme>;</l>
        <l id="s3:l5" met="4" type="+-">Maer wat is't, hoe vris en <rhyme label="c" type="m">kloeck</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">'k heb van Vryers geen <rhyme label="c" type="m" corresp="#s3:l5">versoeck</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ick moet my wat moy <rhyme label="a" type="f">op-maecken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En gaen proncken by de <rhyme label="b" type="m">straet</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Want de schaed' moet uyt om <rhyme label="b" type="m" corresp="#s4:l2">baet</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Soo een Rockjen van <rhyme label="a" type="f" corresp="#s4:l1">Schar-laecken</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">En mijn Bouwen helpen <rhyme label="c" type="m">kan</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Moet dit alle dagen <rhyme label="c" type="m" corresp="#s4:l5">an</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Daer gaet Treusel henen <rhyme label="a" type="f">strijcken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Niet een steeghje looptse <rhyme label="b" type="m">deur</rhyme>:</l>
        <l id="s5:l3" met="4" type="+-">Of sy hoopt men siet nae <rhyme label="b" type="m" corresp="#s5:l2">heur</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Komt'er dan een Vryer <rhyme label="a" type="f" corresp="#s5:l1">kijcken</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Om het hoeckjen van de <rhyme label="c" type="m">straet</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Treusels hertjen open <rhyme label="c" type="m" corresp="#s5:l5">gaet</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Kan sy aen geselschap <rhyme label="a" type="f">raecken</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Stracks soo voeghtse haer daer <rhyme label="b" type="m">by</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Om de soete <rhyme label="b" type="m" corresp="#s6:l2">Vryery</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Dat alleen is haer <rhyme label="a" type="f" corresp="#s6:l1">vermaecken</rhyme>;</l>
        <l id="s6:l5" met="4" type="+-">Maer het beste noch van <rhyme label="c" type="m">al</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Datmen haer wat Vryen <rhyme label="c" type="m" corresp="#s6:l5">zal</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Doch kan Treusel het niet <rhyme label="a" type="f">ramen</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Dat sy jongh geselschap <rhyme label="b" type="m">vint</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Daer haer hert is toe <rhyme label="b" type="m" corresp="#s7:l2">gesint</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Dan noot syse <rhyme label="a" type="f" corresp="#s7:l1">al-te-samen</rhyme></l>
        <l id="s7:l5" met="4" type="+-">In haer huys, op datse <rhyme label="c" type="m">mach</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Yemandt krijgen onder't <rhyme label="c" type="m" corresp="#s7:l5">slach</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Treusel krijght gelijck een <rhyme label="a" type="f">ander</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Mee een klopjen aen de <rhyme label="b" type="m">deur</rhyme>:</l>
        <l id="s8:l3" met="4" type="+-">Wel, seyt sy, wie is daer <rhyme label="b" type="m" corresp="#s8:l2">veur</rhyme>?</l>
        <l id="s8:l4" met="4" type="+-">Lieve Treusel, ick ben <rhyme label="a" type="f" corresp="#s8:l1">Sander</rhyme>.</l>
        <l id="s8:l5" met="4" type="+-">Kom als Vriendt dan by my <rhyme label="c" type="m">in</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Seytse, want dat is mijn <rhyme label="c" type="m" corresp="#s8:l5">sin</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Sander, wat is u <rhyme label="a" type="f">begeeren</rhyme>?</l>
        <l id="s9:l2" met="4" type="+-">Treusel, daerom koom ick <rhyme label="b" type="m">an</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Wilje my wel tot jou <rhyme label="b" type="m" corresp="#s9:l2">Man</rhyme>?</l>
        <l id="s9:l4" met="4" type="+-">Maer ick heb seer weynigh <rhyme label="a" type="f" corresp="#s9:l1">Kleeren</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">En mijn Geltjen is niet <rhyme label="c" type="m">groot</rhyme>.</l>
        <l id="s9:l6" met="4" type="+-">'k Ben te vreed', seyd' sy, geen <rhyme label="c" type="m" corresp="#s9:l5">noot</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Sy gaet stracks sijn Beurs <rhyme label="a" type="f">versorgen</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Steeckt hem wacker in de <rhyme label="b" type="m">Wol</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Schaft hem ook het buyckjen <rhyme label="b" type="m" corresp="#s10:l2">vol</rhyme>:</l>
        <l id="s10:l4" met="4" type="+-">Want de dagh van <rhyme label="a" type="f" corresp="#s10:l1">over-morgen</rhyme></l>
        <l id="s10:l5" met="4" type="+-">Is van haer al vast <rhyme label="c" type="m">beraemt</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Om in d'Echt te sijn <rhyme label="c" type="m" corresp="#s10:l5">versaemt</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Stracks hoortmen de Kind'ren <rhyme label="a" type="f">singen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Houdtje weelde in de <rhyme label="b" type="m">schuyt</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Onse Treusel is de <rhyme label="b" type="m" corresp="#s11:l2">Bruyt</rhyme>;</l>
        <l id="s11:l4" met="4" type="+-">Laet ons nu van vreughde <rhyme label="a" type="f" corresp="#s11:l1">springen</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Gaetse recht of gaetse <rhyme label="c" type="m">krom</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Sander is de <rhyme label="c" type="m" corresp="#s11:l5">Bruydegom</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Maer wat was't? Nae weynigh <rhyme label="a" type="f">Iaren</rhyme></l>
        <l id="s12:l2" met="4" type="+-">Was dees vreughde gantsch <rhyme label="b" type="m">gedaen</rhyme>;</l>
        <l id="s12:l3" met="4" type="+-">Treusel schreyt soo menigh <rhyme label="b" type="m" corresp="#s12:l2">traen</rhyme>:</l>
        <l id="s12:l4" met="4" type="+-">Wat haer Ouders suynigh <rhyme label="a" type="f" corresp="#s12:l1">sparen</rhyme>,</l>
        <l id="s12:l5" met="4" type="+-">In 't vergaren van het <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Sander 't al-te-mael <rhyme label="c" type="m" corresp="#s12:l5">verdoet</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Sander, seyt sy, lieve <rhyme label="a" type="f">Sander</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Denck doch aen den ouden <rhyme label="b" type="m">Man</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Als men niet veel winnen <rhyme label="b" type="m" corresp="#s13:l2">kan</rhyme>.</l>
        <l id="s13:l4" met="4" type="+-">Swijgh (seyt hy) ick weet een <rhyme label="a" type="f" corresp="#s13:l1">ander</rhyme>,</l>
        <l id="s13:l5" met="4" type="+-">Al jou goetjen moet <rhyme label="c" type="m">verteert</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Want ghy zijt niet beter <rhyme label="c" type="m" corresp="#s13:l5">weert</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Wegh sijn Treusels blijde <rhyme label="a" type="f">dagen</rhyme>,</l>
        <l id="s14:l2" met="4" type="+-">Soo een Schobjack maeckt haer <rhyme label="b" type="m">kael</rhyme>.</l>
        <l id="s14:l3" met="4" type="+-">Dit gebeurt wel <rhyme label="b" type="m" corresp="#s14:l2">meenighmael</rhyme>,</l>
        <l id="s14:l4" met="4" type="+-">Als een Vryster het <rhyme label="a" type="f" corresp="#s14:l1">behagen</rhyme></l>
        <l id="s14:l5" met="4" type="+-">Van de Vryer selver <rhyme label="c" type="m">koopt</rhyme>,</l>
        <l id="s14:l6" met="4" type="+-">Dat de Vriendtschap van haer <rhyme label="c" type="m" corresp="#s14:l5">loopt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
