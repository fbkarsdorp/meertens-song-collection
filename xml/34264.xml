  <author>Jan Mommaert</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ONlans hoord’ ick een <rhyme label="a" type="m">gheklagh</rhyme></l>
        <l id="s1:l2" met="4" type="+-">ONlans hoord’ ick een <rhyme label="a" type="m" corresp="#s1:l1">gheklagh</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Van Rosette, die ick <rhyme label="a" type="m" corresp="#s1:l1">sagh</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Van Rosette, die ick <rhyme label="a" type="m" corresp="#s1:l1">sagh</rhyme></l>
        <l id="s1:l5" met="4" type="+-">Door een spleetken van de <rhyme label="b" type="f">deure</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Met haer Moeder sitten <rhyme label="b" type="f" corresp="#s1:l5">treuren</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Ay my, ay <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Lieve Moeder staet my <rhyme label="c" type="m" corresp="#s1:l7">by</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">So het meysken liet een <rhyme label="a" type="m">sucht</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">So het meysken liet een <rhyme label="a" type="m" corresp="#s2:l1">sucht</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Seyd’ de moeder, maeckt <rhyme label="a" type="m" corresp="#s2:l1">genucht</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Seyd’ de moeder, maeckt <rhyme label="a" type="m" corresp="#s2:l1">genucht</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Lieve dochter zijt te <rhyme label="b" type="f">vreden</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">‘k Sal u tijds genoegh <rhyme label="b" type="f" corresp="#s2:l5">besteden</rhyme>,</l>
        <l id="s2:l7" met="2" type="+-">Toeft wat, toeft <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Daer is noch genoech in <rhyme label="c" type="m" corresp="#s2:l7">stadt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">’t Is nu wel een jaer <rhyme label="a" type="m">geleen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">’t Is nu wel een jaer <rhyme label="a" type="m" corresp="#s3:l1">geleen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Dat ick staegh u heb <rhyme label="a" type="m" corresp="#s3:l1">gebeen</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dat ick staegh u heb <rhyme label="a" type="m" corresp="#s3:l1">gebeen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Dat ghy my met rijcken <rhyme label="b" type="f">Bouwen</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Soudt in eeren laten <rhyme label="b" type="f" corresp="#s3:l5">trouwen</rhyme>,</l>
        <l id="s3:l7" met="2" type="+-">Ay my, ay <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Lieve Moeder staet my <rhyme label="c" type="m" corresp="#s3:l7">by</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maer de moeder heel <rhyme label="a" type="m">ontstelt</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Maer de moeder heel <rhyme label="a" type="m" corresp="#s4:l1">ontstelt</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Seyde: neemt een ander <rhyme label="a" type="m" corresp="#s4:l1">helt</rhyme>;</l>
        <l id="s4:l4" met="4" type="+-">Seyde: neemt een ander <rhyme label="a" type="m" corresp="#s4:l1">helt</rhyme>;</l>
        <l id="s4:l5" met="4" type="+-">Wilt ghy uwe groene <rhyme label="b" type="f">jaren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Voor een Ambachts-man <rhyme label="b" type="f" corresp="#s4:l5">besparen</rhyme>.</l>
        <l id="s4:l7" met="2" type="+-">Toeft wat, toeft <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Daer is noch genoech in <rhyme label="c" type="m" corresp="#s4:l7">stadt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Roosken slaende traen op <rhyme label="a" type="m">traen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Roosken slaende traen op <rhyme label="a" type="m" corresp="#s5:l1">traen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Sprack de moer wel vinnigh <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme>:</l>
        <l id="s5:l4" met="4" type="+-">Sprack de moer wel vinnigh <rhyme label="a" type="m" corresp="#s5:l1">aen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Wilt ghy mijn geluck <rhyme label="b" type="f">ontstelen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Soo sal ick van droefheyt <rhyme label="b" type="f" corresp="#s5:l5">quelen</rhyme>,</l>
        <l id="s5:l7" met="2" type="+-">Ay my, ay <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Lieve Moeder staet my <rhyme label="c" type="m" corresp="#s5:l7">by</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Neen ick kindt, volgt mijnen <rhyme label="a" type="m">raedt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Neen ick kindt, volgt mijnen <rhyme label="a" type="m" corresp="#s6:l1">raedt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Neemt een deftigh <rhyme label="a" type="m" corresp="#s6:l1">Advocaet</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Neemt een deftigh <rhyme label="a" type="m" corresp="#s6:l1">Advocaet</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Oft wel yemandt van der <rhyme label="b" type="f">pennen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Laet doch, bidd’ ick, desen <rhyme label="b" type="f" corresp="#s6:l5">Hennen</rhyme>,</l>
        <l id="s6:l7" met="2" type="+-">Toeft wat, toeft <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Daer is noch genoech in <rhyme label="c" type="m" corresp="#s6:l7">stadt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">‘k Prijs een geestig <rhyme label="a" type="m">Ambachtsman</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">‘k Prijs een geestig <rhyme label="a" type="m" corresp="#s7:l1">Ambachtsman</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die den schouw doen roocken <rhyme label="a" type="m" corresp="#s7:l1">kan</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Die den schouw doen roocken <rhyme label="a" type="m" corresp="#s7:l1">kan</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Die met alderlye <rhyme label="b" type="f">waren</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Kan een pennincxken <rhyme label="b" type="f" corresp="#s7:l5">vergaren</rhyme>:</l>
        <l id="s7:l7" met="2" type="+-">Ay my, ay <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Lieve Moeder staet my <rhyme label="c" type="m" corresp="#s7:l7">by</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Dochter ’t is een eeuwig <rhyme label="a" type="m">kruys</rhyme></l>
        <l id="s8:l2" met="4" type="+-">Dochter ’t is een eeuwig <rhyme label="a" type="m" corresp="#s8:l1">kruys</rhyme></l>
        <l id="s8:l3" met="4" type="+-">Staegh te sitten in het <rhyme label="a" type="m" corresp="#s8:l1">huys</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Staegh te sitten in het <rhyme label="a" type="m" corresp="#s8:l1">huys</rhyme>,</l>
        <l id="s8:l5" met="4" type="+-">Om den Koopman te <rhyme label="b" type="f">verwachten</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">Wilt doch naer wat eelder <rhyme label="b" type="f" corresp="#s8:l5">trachten</rhyme>,</l>
        <l id="s8:l7" met="2" type="+-">Toeft wat, toeft <rhyme label="c" type="m">wat</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Daer is noch genoech in <rhyme label="c" type="m" corresp="#s8:l7">stadt</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Waert niet erger, als een <rhyme label="a" type="m">vrou</rhyme></l>
        <l id="s9:l2" met="4" type="+-">Waert niet erger, als een <rhyme label="a" type="m" corresp="#s9:l1">vrou</rhyme></l>
        <l id="s9:l3" met="4" type="+-">Geen ontfangh en hebben <rhyme label="a" type="m" corresp="#s9:l1">sou</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Geen ontfangh en hebben <rhyme label="a" type="m" corresp="#s9:l1">sou</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Als sy van dees <rhyme label="b" type="f">pennelingen</rhyme></l>
        <l id="s9:l6" met="4" type="+-">Tot een oortjen af moet <rhyme label="b" type="f" corresp="#s9:l5">dringen</rhyme>,</l>
        <l id="s9:l7" met="2" type="+-">Ay my, ay <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Lieve Moeder staet my <rhyme label="c" type="m" corresp="#s9:l7">by</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">’t Is al niet wat ick haer <rhyme label="a" type="m">segh</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">’t Is al niet wat ick haer <rhyme label="a" type="m" corresp="#s10:l1">segh</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">En mijn dochters tijdt gaet <rhyme label="a" type="m" corresp="#s10:l1">wegh</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">En mijn dochters tijdt gaet <rhyme label="a" type="m" corresp="#s10:l1">wegh</rhyme>,</l>
        <l id="s10:l5" met="4" type="+-">Had ik haer met rijcke <rhyme label="b" type="f">Bouwen</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Als ’t hy meynde, laten <rhyme label="b" type="f" corresp="#s10:l5">trouwen</rhyme>,</l>
        <l id="s10:l7" met="2" type="+-">Toeft niet, toeft <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Oft ghy brenght u in ’t <rhyme label="c" type="m" corresp="#s10:l7">verdriet</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Oorlof moeders, die naer <rhyme label="a" type="m">reen</rhyme></l>
        <l id="s11:l2" met="4" type="+-">Oorlof moeders, die naer <rhyme label="a" type="m" corresp="#s11:l1">reen</rhyme></l>
        <l id="s11:l3" met="4" type="+-">Soeckt u dochters te <rhyme label="a" type="m" corresp="#s11:l1">besteen</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Soeckt u dochters te <rhyme label="a" type="m" corresp="#s11:l1">besteen</rhyme>,</l>
        <l id="s11:l5" met="4" type="+-">Wilt naar pen noch degen <rhyme label="b" type="f">kijcken</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Geeftse vry aen haers <rhyme label="b" type="f" corresp="#s11:l5">gelijcken</rhyme>,</l>
        <l id="s11:l7" met="2" type="+-">’t Is tijdt, ’t is <rhyme label="c" type="m">tijdt</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">Oft ghy wort u dochter <rhyme label="c" type="m" corresp="#s11:l7">quijt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
