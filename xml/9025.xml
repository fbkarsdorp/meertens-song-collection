    <tags>
      <tag translation="lament">klaaglied</tag>
      <tag translation="">geestelijk</tag>
      <tag translation="psalm">psalm</tag>
    </tags>
    <melody>Aus tiefer Not schrei ich zu dir</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">ACh hoe Elen'd is onse <rhyme label="a" type="m">Tijt</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al hier op deser <rhyme label="b" type="f">Aerden</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Hoe haest een Mensch daer neder <rhyme label="a" type="m" corresp="#s1:l1">leyt</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">End is van gheener <rhyme label="b" type="f" corresp="#s1:l2">waerden</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Want t'is in desen <rhyme label="c" type="m">Jammerdal</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">All' Moeyt end Arbeyt over <rhyme label="c" type="m" corresp="#s1:l5">al</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Oock alst schijnt wel <rhyme label="d" type="f">ghelucken</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ach Adams Val end <rhyme label="a" type="m">Missedaet</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Sulcx op ons alle <rhyme label="b" type="f">Erven</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">HEer GOdt geeft ghy ons goeden <rhyme label="a" type="m" corresp="#s2:l1">Raet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Anders gaen wy <rhyme label="b" type="f" corresp="#s2:l2">verderven</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Dat wy so blint end seker <rhyme label="c" type="m">zijn</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Midden in Droeffnis end in <rhyme label="c" type="m" corresp="#s2:l5">Pijn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wie macht ghenoech <rhyme label="d" type="f">beclaghen</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">HEer Godt ghy onse Toevlucht <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Dijn hulpe wilt ons <rhyme label="b" type="f">seynden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Want ghy dijn Volck verlatet <rhyme label="a" type="m" corresp="#s3:l1">niet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Die haer tot dy gaen <rhyme label="b" type="f" corresp="#s3:l2">weynden</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Met dijnen Gheeste staet ons <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Een Saelich Eynde gheeft ons <rhyme label="c" type="m" corresp="#s3:l5">bly</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Door JEsum Christum, <rhyme label="d" type="f">Amen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
