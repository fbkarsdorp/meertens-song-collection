    <melody>Och wat lijden</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">‘,,Ghelijck <rhyme label="a" type="f">stadich</rhyme></l>
        <l id="s1:l2" met="4" type="-+">‘,,Inde zee//t'roer loos <rhyme label="b" type="m">schip</rhyme>/</l>
        <l id="s1:l3" met="5" type="+-">‘,,In perijckel sweeft dach ende <rhyme label="c" type="m">nacht</rhyme>;</l>
        <l id="s1:l4" met="2" type="+-">‘,,D'welck smijt <rhyme label="a" type="f" corresp="#s1:l1">schadich</rhyme></l>
        <l id="s1:l5" met="4" type="+-">‘,,Licht in twee// op een <rhyme label="b" type="m" corresp="#s1:l2">clip</rhyme>/</l>
        <l id="s1:l6" met="5" type="+-">‘,,Oft het singht door t'groot onweders <rhyme label="c" type="m" corresp="#s1:l3">cracht</rhyme>/</l>
        <l id="s1:l7" met="4" type="-+">‘,,De baren heffent op/en <rhyme label="d" type="m">neer</rhyme>/</l>
        <l id="s1:l8" met="2" type="+-">‘,,Nu hooghe // uyt <rhyme label="e" type="f">d'ooghe</rhyme>/</l>
        <l id="s1:l9" met="3" type="-+">‘,,Dan schier ten gronde <rhyme label="d" type="m" corresp="#s1:l7">weer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Soo ist <rhyme label="a" type="f">even</rhyme></l>
        <l id="s2:l2" met="4" type="-+">Met den mensch // soo't hier <rhyme label="b" type="m">blijckt</rhyme></l>
        <l id="s2:l3" met="5" type="+-">Aen Eudotia weert <rhyme label="c" type="m">ghelauw'riert</rhyme>/</l>
        <l id="s2:l4" met="2" type="+-">Wyens <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>/</l>
        <l id="s2:l5" met="4" type="+-">Ende wensch // recht <rhyme label="b" type="m" corresp="#s2:l2">ghelijckt</rhyme>/</l>
        <l id="s2:l6" met="5" type="+-">T'schip/ dat door de baren wort <rhyme label="c" type="m" corresp="#s2:l3">ghestiert</rhyme>!</l>
        <l id="s2:l7" met="4" type="-+">Dats door Fortun', diet hoochst / meest <rhyme label="d" type="m">treft</rhyme>/</l>
        <l id="s2:l8" met="2" type="+-">Die weder // worpt <rhyme label="e" type="f">neder</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Al 't ghene sy <rhyme label="d" type="m" corresp="#s2:l7">verheft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Dese <rhyme label="a" type="f">Maghet</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Arm van staet/en <rhyme label="b" type="m">gheslacht</rhyme>/</l>
        <l id="s3:l3" met="5" type="+-">Die Atheneas eerst was <rhyme label="c" type="m">ghenoemt</rhyme>/</l>
        <l id="s3:l4" met="2" type="+-">Heeft <rhyme label="a" type="f" corresp="#s3:l1">behaghet</rhyme></l>
        <l id="s3:l5" met="4" type="+-">Om haer daet // hoogh <rhyme label="b" type="m" corresp="#s3:l2">gheacht</rhyme>/</l>
        <l id="s3:l6" met="5" type="+-">Wijsheyt/deucht/en schoonheyt wijt <rhyme label="c" type="m" corresp="#s3:l3">beroemt</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Den Keyser <rhyme label="d" type="m">Theosiom</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Die houde // en <rhyme label="e" type="f">troude</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Met dese weerde <rhyme label="d" type="m" corresp="#s3:l7">blom</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Als nu <rhyme label="a" type="f">prachtich</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Was in weldt // roem/ en <rhyme label="b" type="m">eer</rhyme></l>
        <l id="s4:l3" met="5" type="+-">Dees die boven wensch verheven <rhyme label="c" type="m">sadt</rhyme>/</l>
        <l id="s4:l4" met="2" type="+-">Die groot <rhyme label="a" type="f" corresp="#s4:l1">machtich</rhyme>/</l>
        <l id="s4:l5" met="4" type="+-">Scheen ghesteldt // tot een <rhyme label="b" type="m" corresp="#s4:l2">leer</rhyme>/</l>
        <l id="s4:l6" met="5" type="+-">Schier op 't hoochste van Fortunas <rhyme label="c" type="m" corresp="#s4:l3">radt</rhyme>/</l>
        <l id="s4:l7" met="4" type="-+">Als Keyserinne boven <rhyme label="d" type="m">al</rhyme></l>
        <l id="s4:l8" met="2" type="+-">Tot haren // seer <rhyme label="e" type="f">swaren</rhyme>/</l>
        <l id="s4:l9" met="3" type="-+">En al te droeven <rhyme label="d" type="m" corresp="#s4:l7">val</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Met <rhyme label="a" type="f">beliefte</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Heeft den Prins // op een <rhyme label="b" type="m">tijt</rhyme></l>
        <l id="s5:l3" met="5" type="+-">Haer/een schoone raere vrucht <rhyme label="c" type="m">vereert</rhyme>/</l>
        <l id="s5:l4" met="2" type="+-">Welcke <rhyme label="a" type="f" corresp="#s5:l1">giefte</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Onversins // heeft vol <rhyme label="b" type="m" corresp="#s5:l2">nijt</rhyme>/</l>
        <l id="s5:l6" met="5" type="+-">s'Keysers min in jelousy <rhyme label="c" type="m" corresp="#s5:l3">verkeert</rhyme>/</l>
        <l id="s5:l7" met="4" type="-+">Midts sy die aen een ander <rhyme label="d" type="m">gaf</rhyme>:</l>
        <l id="s5:l8" met="2" type="+-">,,Hoe slechte // <rhyme label="e" type="f">t'onrechte</rhyme></l>
        <l id="s5:l9" met="3" type="-+">,,Lijt menich smaet en <rhyme label="d" type="m" corresp="#s5:l7">straf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">Desen <rhyme label="a" type="f">goeden</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Heer/ liep ras // tot den <rhyme label="b" type="m">stoel</rhyme></l>
        <l id="s6:l3" met="5" type="+-">Vanden Keyser/en schonck hem den <rhyme label="c" type="m">Vorst</rhyme>:</l>
        <l id="s6:l4" met="2" type="+-">Die <rhyme label="a" type="f" corresp="#s6:l1">vermoeden</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Strackx dat 't was // haren <rhyme label="b" type="m" corresp="#s6:l2">boel</rhyme>/</l>
        <l id="s6:l6" met="5" type="+-">Die sy desen Appel gheven <rhyme label="c" type="m" corresp="#s6:l3">dorst</rhyme>/</l>
        <l id="s6:l7" met="4" type="-+">En naer wat <rhyme label="d" type="m">ondersoechkingh/knap</rhyme>/</l>
        <l id="s6:l8" met="2" type="+-">Verlietfe: // en <rhyme label="e" type="f">stietse</rhyme></l>
        <l id="s6:l9" met="3" type="-+">In droeve <rhyme label="d" type="m" corresp="#s6:l7">ballinckschap</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">,,Leert hier <rhyme label="a" type="f">menschen</rhyme>!</l>
        <l id="s7:l2" met="4" type="-+">,,Dat de snoo // werelt <rhyme label="b" type="m">is</rhyme></l>
        <l id="s7:l3" met="5" type="+-">,,Recht een schaduwe/want als ghy <rhyme label="c" type="m">meynt</rhyme>/</l>
        <l id="s7:l4" met="2" type="+-">,,Dat gh'u <rhyme label="a" type="f" corresp="#s7:l1">wenschen</rhyme></l>
        <l id="s7:l5" met="4" type="+-">,,Hebbet/soo // sijt ghy <rhyme label="b" type="m" corresp="#s7:l2">wis</rhyme></l>
        <l id="s7:l6" met="5" type="+-">,,Niet/ gansch niet/ja min dan niet <rhyme label="c" type="m" corresp="#s7:l3">vercleynt</rhyme>.</l>
        <l id="s7:l7" met="4" type="-+">,,Schout dees vermomde streelster <rhyme label="d" type="m">dan</rhyme></l>
        <l id="s7:l8" met="2" type="+-">,,Dient meere // den <rhyme label="e" type="f">Heere</rhyme></l>
        <l id="s7:l9" met="3" type="-+">,,Diet eeuwich loonen <rhyme label="d" type="m" corresp="#s7:l7">can</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
