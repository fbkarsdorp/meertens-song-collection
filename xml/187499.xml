<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187499</id>
    <date>
      <year-start>1784</year-start>
      <year-end>1784</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Adieu dan Roselinde</melody>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Niemand kan myn <rhyme label="a" type="f">bekooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Als Rozelinde <rhyme label="b" type="m">alleen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik was liever nooyd <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Als brengen uw in <rhyme label="b" type="m" corresp="#s1:l2">Geween</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Gy weet dat ik ben <rhyme label="c" type="f">zwanger</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">En ben van u <rhyme label="d" type="m">bevrugt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En fopt my dog niet <rhyme label="c" type="f" corresp="#s1:l5">langer</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zoo sprak zy met een <rhyme label="d" type="m" corresp="#s1:l6">zugt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hoe bitter valt het <rhyme label="a" type="f">scheiden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">wat droefheid en wat <rhyme label="b" type="m">pyn</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ik hoop Godt zal myn <rhyme label="a" type="f" corresp="#s2:l1">leiden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Op Zee myn Trooster <rhyme label="b" type="m" corresp="#s2:l2">zyn</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Minnaer houd op van <rhyme label="c" type="f">klaegen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Ik ben voor u te <rhyme label="d" type="m">pand</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gy staet in myn <rhyme label="c" type="f" corresp="#s2:l5">behaegen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Daer is myn <rhyme label="d" type="m" corresp="#s2:l6">regterhand</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Vaer wel schoon <rhyme label="a" type="f">Rooselinde</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vaer wel myn liefste <rhyme label="b" type="m">lam</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ik kan geen schoonder <rhyme label="a" type="f" corresp="#s3:l1">vinde</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">In het zoete <rhyme label="b" type="m" corresp="#s3:l2">Amsterdam</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ik heb u belooft te <rhyme label="c" type="f">trouwen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Zoo dra ik koom aen ’t <rhyme label="d" type="m">land</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">En maaken van u een <rhyme label="c" type="f" corresp="#s3:l5">vrouwe</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Zoette liefje houd u <rhyme label="d" type="m" corresp="#s3:l6">Constand</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik gae u hier <rhyme label="a" type="f">verlaeten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Adju dan al te <rhyme label="b" type="m">maal</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">’t Treuren kan niet <rhyme label="a" type="f" corresp="#s4:l1">baeten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Genagt in ’t <rhyme label="b" type="m" corresp="#s4:l2">generael</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Adju dan Vader en <rhyme label="c" type="f">Moeder</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En Susters in ’t <rhyme label="d" type="m">Getal</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat Godt is uw <rhyme label="c" type="f" corresp="#s4:l5">behoeder</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En ons bewaaren <rhyme label="d" type="m" corresp="#s4:l6">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
