    <melody>Ik drink de nieuwe most</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAt Jupiter zijn <rhyme label="a" type="m">Throon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Zijn Scepter, en zijn <rhyme label="a" type="m" corresp="#s1:l1">Kroon</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">Voor u mijn bood, en mijn den Hemel <rhyme label="b" type="m">schonk</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">Die rijk van Goudt en Diamanten <rhyme label="b" type="m" corresp="#s1:l3">blonk</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En zeid': Zie daar, ik <rhyme label="c" type="m">scheid'</rhyme></l>
        <l id="s1:l6" met="2" type="-+">Van al mijn <rhyme label="d" type="f">praale</rhyme>;</l>
        <l id="s1:l7" met="3" type="+-">En mijn <rhyme label="c" type="m" corresp="#s1:l5">lekkerny</rhyme>,</l>
        <l id="s1:l8" met="4" type="-+">En ik zal voort op d'aard' gaan <rhyme label="d" type="f" corresp="#s1:l6">daale</rhyme>;</l>
        <l id="s1:l9" met="3" type="+-">Ja ghy zult, naar <rhyme label="e" type="m">mijn</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Heer in den Hemel <rhyme label="e" type="m" corresp="#s1:l9">zijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik zou, zonder <rhyme label="a" type="m">beraân</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Hem danken, en <rhyme label="a" type="m" corresp="#s2:l1">afslaan</rhyme></l>
        <l id="s2:l3" met="5" type="-+">Zijn stout verzoek, en zeggen; hoor, <rhyme label="b" type="m">Jupijn</rhyme>,</l>
        <l id="s2:l4" met="5" type="-+">U Scepter, Kroon, en Troon, en <rhyme label="b" type="m" corresp="#s2:l3">Ambrozijn</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ja zelfs u Hemel <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">En groote <rhyme label="d" type="f">Godheid</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">Die ghy hebt <rhyme label="c" type="m" corresp="#s2:l5">geboôn</rhyme></l>
        <l id="s2:l8" met="4" type="-+">Voor mijn Goddin, die Venus <rhyme label="d" type="f" corresp="#s2:l6">schoonheidt</rhyme>,</l>
        <l id="s2:l9" met="3" type="+-">Pallas wijs <rhyme label="e" type="m">vernuft</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En Junós praal <rhyme label="e" type="m" corresp="#s2:l9">verbluft</rhyme>;</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En acht ik al als <rhyme label="a" type="m">niet</rhyme>;</l>
        <l id="s3:l2" met="3" type="-+">Want die mijn ziel <rhyme label="a" type="m" corresp="#s3:l1">gebiedt</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">Mijn Amaril, heeft veel meer gaven <rhyme label="b" type="m">schoon</rhyme>;</l>
        <l id="s3:l4" met="5" type="-+">Haar voorhooft is een wit-albaste <rhyme label="b" type="m" corresp="#s3:l3">troon</rhyme>;</l>
        <l id="s3:l5" met="3" type="-+">Haar kroon is 't gulde <rhyme label="c" type="m">hair</rhyme>;</l>
        <l id="s3:l6" met="2" type="-+">Haar schoone <rhyme label="d" type="f">oogjes</rhyme></l>
        <l id="s3:l7" met="3" type="+-">Trotzen Starren <rhyme label="c" type="m" corresp="#s3:l5">klaar</rhyme>;</l>
        <l id="s3:l8" met="4" type="-+">Haar winkbrauws zijn twee <rhyme label="d" type="f" corresp="#s3:l6">Minne-boogjes</rhyme>;</l>
        <l id="s3:l9" met="3" type="+-">Kaakjes Roosjes <rhyme label="e" type="m">zijn</rhyme>;</l>
        <l id="s3:l10" met="3" type="-+">De lipjes <rhyme label="e" type="m" corresp="#s3:l9">koralijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Langs tandjes, trots <rhyme label="a" type="m">yvoor</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Vloeit Nectar, en ik <rhyme label="a" type="m" corresp="#s4:l1">hoor</rhyme></l>
        <l id="s4:l3" met="5" type="-+">Een Hemel-vois, als mijn Nimf speelt of <rhyme label="b" type="m">zingt</rhyme>;</l>
        <l id="s4:l4" met="5" type="-+">Nevens haar ftem uit 't lieve mondtje <rhyme label="b" type="m" corresp="#s4:l3">dringt</rhyme></l>
        <l id="s4:l5" met="3" type="-+"><rhyme label="c" type="m">Adems-viole-geur</rhyme>;</l>
        <l id="s4:l6" met="2" type="-+">Zwierende <rhyme label="d" type="f">speelen</rhyme></l>
        <l id="s4:l7" met="3" type="+-">Blaauwe aders, <rhyme label="c" type="m" corresp="#s4:l5">deur</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Haar sneeu-wit vel, van 't wonder <rhyme label="d" type="f" corresp="#s4:l6">eele</rhyme></l>
        <l id="s4:l9" met="3" type="+-">Ronde halsje <rhyme label="e" type="m">lank</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">En poez'le handtjes <rhyme label="e" type="m" corresp="#s4:l9">rank</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Op Borsjes (Pafos <rhyme label="a" type="m">troon</rhyme>)</l>
        <l id="s5:l2" met="3" type="-+">Staan twee Robijne <rhyme label="a" type="m" corresp="#s5:l1">schoon</rhyme>;</l>
        <l id="s5:l3" met="5" type="-+">Maar 't alderschoonst bedekt haar wit <rhyme label="b" type="m">gewaadt</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Dat ik (als ghy Goddin u zomtijdts <rhyme label="b" type="m" corresp="#s5:l3">baad'</rhyme>)</l>
        <l id="s5:l5" met="3" type="-+">Ter sluik wel heb <rhyme label="c" type="m">aanschouwt</rhyme>:</l>
        <l id="s5:l6" met="2" type="-+">Daar ik <rhyme label="d" type="f">gedooken</rhyme></l>
        <l id="s5:l7" met="3" type="+-">Lag in 't <rhyme label="c" type="m" corresp="#s5:l5">Elzen-woud</rhyme>:</l>
        <l id="s5:l8" met="4" type="-+">'t Was Acteon noit zo <rhyme label="d" type="f" corresp="#s5:l6">gewroken</rhyme>,</l>
        <l id="s5:l9" met="3" type="+-">Van de schoon <rhyme label="e" type="m">Diaan</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Had hy zo meê <rhyme label="e" type="m" corresp="#s5:l9">gedaan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Beeld eens <rhyme label="a" type="m">Pigmalions</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">Laat doch de glans uw's <rhyme label="a" type="m" corresp="#s6:l1">Zons</rhyme></l>
        <l id="s6:l3" met="5" type="-+">Bestraalen my met weêr-mins glooren <rhyme label="b" type="m">al</rhyme>;</l>
        <l id="s6:l4" met="5" type="-+">Ik sweer mijn Diamante liefde <rhyme label="b" type="m" corresp="#s6:l3">zal</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Weêr flonkeren op <rhyme label="c" type="m">uw</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Mijn waarde <rhyme label="d" type="f">tweede</rhyme></l>
        <l id="s6:l7" met="3" type="+-">Ziel: vlucht niet zo <rhyme label="c" type="m" corresp="#s6:l5">schuw</rhyme>,</l>
        <l id="s6:l8" met="4" type="-+">Ay! Amaril, verhoor mijn <rhyme label="d" type="f" corresp="#s6:l6">bede</rhyme>,</l>
        <l id="s6:l9" met="3" type="+-">Neem my in <rhyme label="e" type="m">genaâ</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">En zeg doch eensjes <rhyme label="e" type="m" corresp="#s6:l9">ja</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
