<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184303</id>
    <date>
      <year-start>1785</year-start>
      <year-end>1785</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Hoor Kees mijn vrijer</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Hoor toch, myn <rhyme label="a" type="f">meisje</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Ei blyf een weinig <rhyme label="b" type="m">staan</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Een zoen dit <rhyme label="a" type="f" corresp="#s1:l1">reisje</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En 'k zel je laaten <rhyme label="b" type="m" corresp="#s1:l2">gaan</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Ach wistje, lieve <rhyme label="c" type="m">Griet</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">De droefheid en 't <rhyme label="c" type="m" corresp="#s1:l5">verdriet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Die dagelyks my <rhyme label="d" type="f">kwellen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Gy weigerde my <rhyme label="c" type="m" corresp="#s1:l5">niet</rhyme>.</l>
        <l id="s1:l9" met="2" type="-+">Gy zoudt <rhyme label="d" type="f" corresp="#s1:l7">ontstellen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wel nu dat 's <rhyme label="a" type="f">aartig</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">O Misselyk <rhyme label="b" type="m">fatzoen</rhyme>!</l>
        <l id="s2:l3" met="2" type="-+">Gy zyt wel <rhyme label="a" type="f" corresp="#s2:l1">waardig</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Wat durf gy al <rhyme label="b" type="m" corresp="#s2:l2">vermoên</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Dat ik me om u <rhyme label="c" type="m">ontstel</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">'k Zel strak jou op je <rhyme label="c" type="m" corresp="#s2:l5">vel</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Met deuze beezem <rhyme label="d" type="f">kloppen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Dat het jou heugen <rhyme label="c" type="m" corresp="#s2:l5">zal</rhyme>.</l>
        <l id="s2:l9" met="2" type="-+">Zou jy my <rhyme label="d" type="f" corresp="#s2:l7">foppen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Ei! wat voor <rhyme label="a" type="f">grillen</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Wat wonderlyk <rhyme label="b" type="m">bescheid</rhyme>.</l>
        <l id="s3:l3" met="2" type="-+">Zoudt gy dat <rhyme label="a" type="f" corresp="#s3:l1">willen</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik loof 't niet zoete <rhyme label="b" type="m" corresp="#s3:l2">meid</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Zie Krelis! maar eens <rhyme label="c" type="m">aan</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Zoo'n eerlyk borst te <rhyme label="c" type="m" corresp="#s3:l5">slaan</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Het zou je zeker <rhyme label="d" type="f">deeren</rhyme>.</l>
        <l id="s3:l8" met="3" type="-+">Er steekt gewis geen <rhyme label="c" type="m" corresp="#s3:l5">Haan</rhyme></l>
        <l id="s3:l9" met="2" type="-+">In zulke <rhyme label="d" type="f" corresp="#s3:l7">veêren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Wat raare <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hoe steekje wat de <rhyme label="b" type="m">gek</rhyme>.</l>
        <l id="s4:l3" met="2" type="-+">Ik zal jou <rhyme label="a" type="f" corresp="#s4:l1">leden</rhyme></l>
        <l id="s4:l4" met="3" type="-+">En asserante <rhyme label="b" type="m" corresp="#s4:l2">bek</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Tracteeren, Krelis <rhyme label="c" type="m">buur</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Dat jy, als eek, zoo <rhyme label="c" type="m" corresp="#s4:l5">zuur</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">En niet zo fel zelt <rhyme label="d" type="f">kyken</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Ja bleek gelyk een <rhyme label="c" type="m" corresp="#s4:l5">muur</rhyme></l>
        <l id="s4:l9" met="2" type="-+">Gaan heenen <rhyme label="d" type="f" corresp="#s4:l7">stryken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Wat zegje, <rhyme label="a" type="f">Vryster</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">Heb ik je wel <rhyme label="b" type="m">verstaan</rhyme>.</l>
        <l id="s5:l3" met="2" type="-+">Je praat te <rhyme label="a" type="f" corresp="#s5:l1">byster</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Je ziet me vinnig <rhyme label="b" type="m" corresp="#s5:l2">aan</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Myn slaan dat raadje <rhyme label="c" type="m">niet</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Ik wacht me voor <rhyme label="c" type="m" corresp="#s5:l5">verdriet</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Je zelt me niet <rhyme label="d" type="f">tracteeren</rhyme>.</l>
        <l id="s5:l8" met="3" type="-+">'k Begeer geen booze <rhyme label="c" type="m" corresp="#s5:l5">Griet</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">En jy moogt <rhyme label="d" type="f" corresp="#s5:l7">leeren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
