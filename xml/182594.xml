<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>182594</id>
    <date>
      <year-start>1800</year-start>
      <year-end>1800</year-end>
    </date>
    <tags/>
    <melody/>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Smorgens als hy my kwam <rhyme label="a" type="f">wekke</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Met het krteke van den <rhyme label="b" type="m">dag</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Kwam hy aan myn venster <rhyme label="a" type="f" corresp="#s1:l1">trekken</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Toen ik in myn rust nog <rhyme label="b" type="m" corresp="#s1:l2">lag</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">En ik zy myn lief <rhyme label="c" type="f">gepresen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Opent tog u deur voor <rhyme label="d" type="m">myn</rhyme>;</l>
        <l id="s1:l7" met="4" type="+-">Want de dag is reeds <rhyme label="c" type="f" corresp="#s1:l5">gereezen</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Spoeit u ook dan maar by <rhyme label="d" type="m" corresp="#s1:l6">myn</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Myn lief myn <rhyme label="a" type="f">uitverkooren</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Sprak zy met vevreest <rhyme label="b" type="m">geluid</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Laat u van geen ander <rhyme label="a" type="f" corresp="#s2:l1">bekooren</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Treed nader tot uw <rhyme label="b" type="m" corresp="#s2:l2">bruid</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Ach hoe lieflyk is het <rhyme label="c" type="f">minnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">In de vroege <rhyme label="d" type="m">morgenstond</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Daar hoort men 't gevogelte <rhyme label="c" type="f" corresp="#s2:l5">zingen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Op den aardbol in het <rhyme label="d" type="m" corresp="#s2:l6">rond</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Myn overschoon <rhyme label="a" type="f">annetje</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Sprak myne <rhyme label="b" type="m">Bruidegom</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">wild dees roos op uw boezem <rhyme label="a" type="f" corresp="#s3:l1">zetten</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Ik omhels u <rhyme label="b" type="m" corresp="#s3:l2">wederom</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En zo smaake wy met <rhyme label="c" type="f">elkander</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Deez' lieffelyke <rhyme label="d" type="m">geur</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Zo trouwt Annntje met <rhyme label="c" type="f" corresp="#s3:l5">Leander</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">In het eelste van haar <rhyme label="d" type="m" corresp="#s3:l6">fleur</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Geluk gy <rhyme label="a" type="f">overschoone</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Wel leeft te zaam in <rhyme label="b" type="m">vree</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">wilt beide u liefde <rhyme label="a" type="f" corresp="#s4:l1">toonen</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Dat is myn wensch en <rhyme label="b" type="m" corresp="#s4:l2">bee</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">wilt 't huwelyksliefde <rhyme label="c" type="f">smaaken</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Op het zagte veere <rhyme label="d" type="m">bed</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Daar schoone telgen <rhyme label="c" type="f" corresp="#s4:l5">maaken</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">En leeven <rhyme label="d" type="m" corresp="#s4:l6">onbesmet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Speel nu op Bas en <rhyme label="a" type="f">Fluiten</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Ter eere van dit <rhyme label="b" type="m">paar</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">En wil de vreugd <rhyme label="a" type="f" corresp="#s5:l1">besluiten</rhyme>,,</l>
        <l id="s5:l4" met="4" type="+-">Met 't klinken van de <rhyme label="b" type="m" corresp="#s5:l2">snaar</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Op de bruiloft van <rhyme label="c" type="f">Annette</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Met Leander haare <rhyme label="d" type="m">vriend</rhyme>,</l>
        <l id="s5:l7" met="4" type="+-">wild de vreugd geen paale <rhyme label="c" type="f" corresp="#s5:l5">zetten</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">'t Is hier liefde wat men <rhyme label="d" type="m" corresp="#s5:l6">vindt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
