<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>119304</id>
    <date>
      <year-start>1730</year-start>
      <year-end>1730</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>mythological song</tag>
    </tags>
    <melody>Laat je kinders kruien leren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VOrst Jupyn, der Goden <rhyme label="a" type="f">wonder</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Die met Blixem en met <rhyme label="a" type="f" corresp="#s1:l1">Donder</rhyme></l>
        <l id="s1:l3" met="4" type="+-">Dikmaals raasd en tierd en <rhyme label="b" type="m">woed</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Is by <rhyme label="c" type="f">Juno</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Is by <rhyme label="c" type="f" corresp="#s1:l4">Juno</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Is by Juno altyd <rhyme label="b" type="m" corresp="#s1:l3">goed</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Hy vereerd haar lekk're <rhyme label="a" type="f">Moppen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Hy wil hare kousse <rhyme label="a" type="f" corresp="#s2:l1">stoppen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Want hy mag geen gaaten <rhyme label="b" type="m">zien</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Of zyn wil <rhyme label="c" type="f">moet</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">Of zyn wil <rhyme label="c" type="f" corresp="#s2:l4">moet</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Of zyn wil moet voort <rhyme label="b" type="m" corresp="#s2:l3">geschiên</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Mars die op zyn <rhyme label="a" type="f">Oorlogs-wagen</rhyme></l>
        <l id="s3:l2" met="4" type="+-">Vreest geen scharpe Sabel <rhyme label="a" type="f" corresp="#s3:l1">slagen</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Speeld met Venus om een <rhyme label="b" type="m">hoek</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Wiptem <rhyme label="c" type="f">dyne</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-">Kniptem <rhyme label="c" type="f" corresp="#s3:l4">dyne</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Wiptem dyne zoete <rhyme label="b" type="m" corresp="#s3:l3">koek</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Ja dien plaag van arme <rhyme label="a" type="f">Boeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Streeld het hooft der snoodste <rhyme label="a" type="f" corresp="#s4:l1">Hoeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Met iets lekkers tot <rhyme label="b" type="m">vermaak</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">In het <rhyme label="c" type="f">donker</rhyme>,</l>
        <l id="s4:l5" met="2" type="+-">In het <rhyme label="c" type="f" corresp="#s4:l4">donker</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">In het donker altyd <rhyme label="b" type="m" corresp="#s4:l3">raak</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Vorst Neptunus, wyd van <rhyme label="a" type="f">waffel</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Prikt in 't honderd met zyn <rhyme label="a" type="f" corresp="#s5:l1">gaffel</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">Ziet hy maar een <rhyme label="b" type="m">Water-maagd</rhyme>,</l>
        <l id="s5:l4" met="2" type="+-">Die zyn <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s5:l5" met="2" type="+-">Die zyn <rhyme label="c" type="f" corresp="#s5:l4">oogen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Die zyn oogen maar <rhyme label="b" type="m" corresp="#s5:l3">behaagt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Prikt van agt'ren en van <rhyme label="a" type="f">voren</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Schoon dat Triton op zyn <rhyme label="a" type="f" corresp="#s6:l1">Horen</rhyme></l>
        <l id="s6:l3" met="4" type="+-">Blaast als een Alarm <rhyme label="b" type="m">Trompet</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Zoekt in 't <rhyme label="c" type="f">water</rhyme>,</l>
        <l id="s6:l5" met="2" type="+-">Zoekt in 't <rhyme label="c" type="f" corresp="#s6:l4">water</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Zoekt in 't water Min <rhyme label="b" type="m" corresp="#s6:l3">Banket</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Pluto, opper Vorst der <rhyme label="a" type="f">Helle</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Gaat wel eens uit <rhyme label="a" type="f" corresp="#s7:l1">Totebelle</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Met zyn lieve <rhyme label="b" type="m">Prozerpyn</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Speeld voor <rhyme label="c" type="f">Lootsman</rhyme>,</l>
        <l id="s7:l5" met="2" type="+-">Speeld voor <rhyme label="c" type="f" corresp="#s7:l4">Lootsman</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Speeld voor Lootsman dat mag <rhyme label="b" type="m" corresp="#s7:l3">zyn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Spyt de Nikkers die wat <rhyme label="a" type="f">knorren</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Niemant zal zyn wyf <rhyme label="a" type="f" corresp="#s8:l1">onthorren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dien zyn handel al <rhyme label="b" type="m">verstaat</rhyme>,</l>
        <l id="s8:l4" met="2" type="+-">En graag met <rhyme label="c" type="f">hem</rhyme>,</l>
        <l id="s8:l5" met="2" type="+-">En graag met <rhyme label="c" type="f" corresp="#s8:l4">hem</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">En graag met hem vissen <rhyme label="b" type="m" corresp="#s8:l3">gaat</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Paay Vulkaan, dien manken <rhyme label="a" type="f">horrel</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Na het kluiven van een <rhyme label="a" type="f" corresp="#s9:l1">Borrel</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Komt hy meê by Venus <rhyme label="b" type="m">thuis</rhyme>,</l>
        <l id="s9:l4" met="2" type="+-">En hy speeld <rhyme label="c" type="f">daar</rhyme>,</l>
        <l id="s9:l5" met="2" type="+-">En hy speeld <rhyme label="c" type="f" corresp="#s9:l4">daar</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">En speeld daar knik knelis <rhyme label="b" type="m" corresp="#s9:l3">huis</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Lacht dan om zyn zwaarste <rhyme label="a" type="f">Hamer</rhyme>:</l>
        <l id="s10:l2" met="4" type="+-">Want het zoet in Venus <rhyme label="a" type="f" corresp="#s10:l1">kamer</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Maakt steeds Kermis in zyn <rhyme label="b" type="m">lyf</rhyme>,</l>
        <l id="s10:l4" met="2" type="+-">'t Heeft ook <rhyme label="c" type="f">reden</rhyme>,</l>
        <l id="s10:l5" met="2" type="+-">'t Heeft ook <rhyme label="c" type="f" corresp="#s10:l4">reden</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">'t Heeft ook reden, 't is zyn <rhyme label="b" type="m" corresp="#s10:l3">Wyf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Momus, nimmer lui in 't <rhyme label="a" type="f">spotten</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">Loopt gelyk een hooft der <rhyme label="a" type="f" corresp="#s11:l1">zotten</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Zo hy slegts met een <rhyme label="b" type="m">Godin</rhyme>,</l>
        <l id="s11:l4" met="2" type="+-">Zoekt te <rhyme label="c" type="f">spelen</rhyme>,</l>
        <l id="s11:l5" met="2" type="+-">Zoekt te <rhyme label="c" type="f" corresp="#s11:l4">spelen</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Zoekt te spelen 't spel der <rhyme label="b" type="m" corresp="#s11:l3">Min</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Ja hy trekt een rare <rhyme label="a" type="f">snater</rhyme>,</l>
        <l id="s12:l2" met="4" type="+-">En hy grienikt trots een <rhyme label="a" type="f" corresp="#s12:l1">Sater</rhyme>,</l>
        <l id="s12:l3" met="4" type="+-">En voldoet zyn grage <rhyme label="b" type="m">lust</rhyme>;</l>
        <l id="s12:l4" met="2" type="+-">Dus werd <rhyme label="c" type="f">menig</rhyme>,</l>
        <l id="s12:l5" met="2" type="+-">Dus werd <rhyme label="c" type="f" corresp="#s12:l4">menig</rhyme>,</l>
        <l id="s12:l6" met="4" type="+-">Dus werd menig Maagd <rhyme label="b" type="m" corresp="#s12:l3">gekust</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Dus ziet men veel snaken <rhyme label="a" type="f">zweven</rhyme>,</l>
        <l id="s13:l2" met="4" type="+-">Die zich tot dat werk <rhyme label="a" type="f" corresp="#s13:l1">begeven</rhyme>,</l>
        <l id="s13:l3" met="4" type="+-">Schoon het sluit of niet en <rhyme label="b" type="m">sluit</rhyme>,</l>
        <l id="s13:l4" met="2" type="+-">Maar ten <rhyme label="c" type="f">laatste</rhyme>,</l>
        <l id="s13:l5" met="2" type="+-">Maar ten <rhyme label="c" type="f" corresp="#s13:l4">laatste</rhyme>,</l>
        <l id="s13:l6" met="4" type="+-">Maar ten laaste scheid me 'er <rhyme label="b" type="m" corresp="#s13:l3">uit</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
