  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">
          <rhyme label="a" type="f">COninginne</rhyme>
        </l>
        <l id="s1:l2" met="2" type="-+">Des Hemels, <rhyme label="b" type="m">o</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Verblijdt u heden, <rhyme label="c" type="m">want</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Die men uyt <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Onlanghs <rhyme label="b" type="m" corresp="#s1:l2">soo</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Mismaeckt aen't Cruyce <rhyme label="c" type="m" corresp="#s1:l3">vant</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Genagelt als een <rhyme label="d" type="m">die</rhyme></l>
        <l id="s1:l8" met="3" type="-+">De Wet volbrachte <rhyme label="d" type="m" corresp="#s1:l7">nie</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Heeft nu de doodt <rhyme label="e" type="f">bedurven</rhyme></l>
        <l id="s1:l10" met="3" type="+-">Met haer <rhyme label="f" type="m">ongenae</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">En heeft ons vreed' <rhyme label="e" type="f" corresp="#s1:l9">verwurven</rhyme>,</l>
        <l id="s1:l12" met="2" type="-+"><rhyme label="f" type="m" corresp="#s1:l10">Alleluya</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Hy die ghy <rhyme label="a" type="f">waerdich</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Zijt <rhyme label="b" type="m">gheweest</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Te draghen aen u <rhyme label="c" type="m">borst</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Komt nu heel <rhyme label="a" type="f" corresp="#s2:l1">vaerdich</rhyme>,</l>
        <l id="s2:l5" met="2" type="+-">En <rhyme label="b" type="m" corresp="#s2:l2">gheneest</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Ons droogen ziele <rhyme label="c" type="m" corresp="#s2:l3">dorst</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Hy toont <rhyme label="d" type="m">allegaer</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Nu vijf fonteynen <rhyme label="d" type="m" corresp="#s2:l7">klaer</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">In handen, zijd', en <rhyme label="e" type="f">voeten</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Op dat ons hier <rhyme label="f" type="m">nae</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Niet meer sal dorst <rhyme label="e" type="f" corresp="#s2:l9">ontmoeten</rhyme>,</l>
        <l id="s2:l12" met="2" type="-+"><rhyme label="f" type="m" corresp="#s2:l10">Alleluya</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Dus wilt niet <rhyme label="a" type="f">vreesen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Droeve <rhyme label="b" type="m">Maecht</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En Moeder, om u <rhyme label="c" type="m">Soon</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Hy is <rhyme label="a" type="f" corresp="#s3:l1">verreesen</rhyme>,</l>
        <l id="s3:l5" met="2" type="+-"><rhyme label="b" type="m" corresp="#s3:l2">Onvertsaecht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Wt handen van de <rhyme label="c" type="m" corresp="#s3:l3">Ioo'n</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">'t Graf dat Pilato <rhyme label="d" type="m">had'</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Wel vast toe zegelt, <rhyme label="d" type="m" corresp="#s3:l7">dat</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Heeft hy al weer <rhyme label="e" type="f">gheopent</rhyme></l>
        <l id="s3:l10" met="3" type="+-">Met klaerheden, <rhyme label="f" type="m">ja</rhyme></l>
        <l id="s3:l11" met="3" type="+-">De Wachters die <rhyme label="e" type="f" corresp="#s3:l9">ontloopent</rhyme>,</l>
        <l id="s3:l12" met="2" type="-+"><rhyme label="f" type="m" corresp="#s3:l10">Alleluya</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Dus stelt <rhyme label="a" type="f">bezijden</rhyme></l>
        <l id="s4:l2" met="2" type="-+">Vwen <rhyme label="b" type="m">wee</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">O Vrouwe <rhyme label="c" type="m">sonderlingh</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Hy sal niet <rhyme label="a" type="f" corresp="#s4:l1">lijden</rhyme></l>
        <l id="s4:l5" met="2" type="+-">Als hy <rhyme label="b" type="m" corresp="#s4:l2">dee'</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Doen hy aen't Cruyce <rhyme label="c" type="m" corresp="#s4:l3">hingh</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Hy heeft voor ons <rhyme label="d" type="m">betaelt</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">En alles weer <rhyme label="d" type="m" corresp="#s4:l7">ghehaelt</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Soo hy ons lang' belooft <rhyme label="e" type="f">had</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Want groot was de <rhyme label="f" type="m">scha'</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Die Satan ons berooft <rhyme label="e" type="f" corresp="#s4:l9">had</rhyme>,</l>
        <l id="s4:l12" met="2" type="-+"><rhyme label="f" type="m" corresp="#s4:l10">Alleluya</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
