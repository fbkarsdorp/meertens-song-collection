  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">Ach waarde <rhyme label="a" type="m">Lief</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Ach waarde <rhyme label="a" type="m" corresp="#s1:l1">Lief</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Omhelst my vry noch <rhyme label="b" type="m">eens</rhyme>,</l>
        <l id="s1:l4" met="7" type="-+">Waarde, waarde, waarde, waarde omhelst my vry noch <rhyme label="b" type="m" corresp="#s1:l3">eens</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Nu ik van u moet <rhyme label="c" type="f">scheiden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Daar 't lot my heen zal <rhyme label="c" type="f" corresp="#s1:l5">lyden</rhyme>,</l>
        <l id="s1:l7" met="3" type="+-">Vol droefheid en <rhyme label="b" type="m" corresp="#s1:l3">geweens</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Ga heen myn <rhyme label="a" type="m">ziel</rhyme></l>
        <l id="s2:l2" met="2" type="-+">Ga heen myn <rhyme label="a" type="m" corresp="#s2:l1">ziel</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Nu 't anders niet kan <rhyme label="b" type="m">zyn</rhyme>,</l>
        <l id="s2:l4" met="7" type="-+">Hemel, hemel, hemel, hemel kan 't niet anders <rhyme label="b" type="m" corresp="#s2:l3">zyn</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Hoe zyt gy Go√¥n <rhyme label="c" type="f">verbolgen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Kan ik myn Lief niet <rhyme label="c" type="f" corresp="#s2:l5">volgen</rhyme>,</l>
        <l id="s2:l7" met="3" type="+-">O al te wreede <rhyme label="b" type="m" corresp="#s2:l3">pyn</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Vaar wel myn <rhyme label="a" type="m">hert</rhyme></l>
        <l id="s3:l2" met="2" type="-+">Vaar wel myn <rhyme label="a" type="m" corresp="#s3:l1">hert</rhyme></l>
        <l id="s3:l3" met="3" type="-+">En hoopt een beter <rhyme label="b" type="m">tyd</rhyme>.</l>
        <l id="s3:l4" met="7" type="-+">Lieve leven, lieve leven hoopt een beter <rhyme label="b" type="m" corresp="#s3:l3">tyd</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ik zal u noit <rhyme label="c" type="f">begeeven</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Hoopt tot gy beter <rhyme label="c" type="f" corresp="#s3:l5">leeven</rhyme>,</l>
        <l id="s3:l7" met="3" type="+-">En lot verzekert <rhyme label="b" type="m" corresp="#s3:l3">zyt</rhyme>.</l>
      </lg>
    </lg>
