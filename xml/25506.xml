    <melody>Een meisje op een riviertje zat</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="amusement">vermaak</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wij wilt hooren een nieu <rhyme label="a" type="m">liet</rhyme>?</l>
        <l id="s1:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s1:l3" met="4" type="-+">En dat te sintruyden es <rhyme label="a" type="m" corresp="#s1:l1">gheschiet</rhyme>.</l>
        <l id="s1:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s1:l2">dop</rhyme>.</l>
        <l id="s1:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het viel op een maendach, <rhyme label="a" type="m">hoort</rhyme>!</l>
        <l id="s2:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s2:l3" met="4" type="-+">Die geuzen stonden al voer die <rhyme label="a" type="m" corresp="#s2:l1">port</rhyme>.</l>
        <l id="s2:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s2:l2">dop</rhyme>.</l>
        <l id="s2:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die port was toe, sy en mochten niet <rhyme label="a" type="m">in</rhyme>.</l>
        <l id="s3:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s3:l3" met="4" type="-+">Dies creghen sy eenen quaden <rhyme label="a" type="m" corresp="#s3:l1">sin</rhyme>.</l>
        <l id="s3:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s3:l2">dop</rhyme>.</l>
        <l id="s3:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sij clopten met eenen grammen <rhyme label="a" type="m">moet</rhyme>.</l>
        <l id="s4:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s4:l3" met="4" type="-+">Hoe compt dat men die port niet op en <rhyme label="a" type="m" corresp="#s4:l1">doet</rhyme>?</l>
        <l id="s4:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s4:l2">dop</rhyme>.</l>
        <l id="s4:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doen quam myn heer al van <rhyme label="a" type="m">duras</rhyme>.</l>
        <l id="s5:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s5:l3" met="4" type="-+">En vraechden wi hun heere <rhyme label="a" type="m" corresp="#s5:l1">was</rhyme>?</l>
        <l id="s5:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s5:l2">dop</rhyme>.</l>
        <l id="s5:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Sij seyden wi sijn al van <rhyme label="a" type="m">nassouwen</rhyme>.</l>
        <l id="s6:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s6:l3" met="4" type="-+">Wi willen hier ons concilium <rhyme label="a" type="m" corresp="#s6:l1">houwen</rhyme>.</l>
        <l id="s6:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s6:l2">dop</rhyme>.</l>
        <l id="s6:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Die van sintruyden hebben tot dien <rhyme label="a" type="m">stonden</rhyme></l>
        <l id="s7:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s7:l3" met="4" type="-+">Eenen bode totten biscop <rhyme label="a" type="m" corresp="#s7:l1">ghesonden</rhyme>.</l>
        <l id="s7:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s7:l2">dop</rhyme>.</l>
        <l id="s7:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Die bischop sprack met worden <rhyme label="a" type="m">dat</rhyme>:</l>
        <l id="s8:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s8:l3" met="4" type="-+">'Sintruyden es een rijcxsche <rhyme label="a" type="m" corresp="#s8:l1">stat'</rhyme>.</l>
        <l id="s8:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s8:l2">dop</rhyme>.</l>
        <l id="s8:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Die geuzen hadden quaden <rhyme label="a" type="m">moet</rhyme>.</l>
        <l id="s9:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s9:l3" met="4" type="-+">Sij reden na leuwe al metter <rhyme label="a" type="m" corresp="#s9:l1">spoet</rhyme>.</l>
        <l id="s9:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s9:l2">dop</rhyme>.</l>
        <l id="s9:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Sanderdaechs quam floyon tonser <rhyme label="a" type="m">baten</rhyme>.</l>
        <l id="s10:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s10:l3" met="4" type="-+">En dede die genken binnen <rhyme label="a" type="m" corresp="#s10:l1">laten</rhyme>.</l>
        <l id="s10:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s10:l2">dop</rhyme>.</l>
        <l id="s10:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Sij sworen bij haer eedelheit <rhyme label="a" type="m">coen</rhyme>.</l>
        <l id="s11:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s11:l3" met="4" type="-+">Dat ze nimant en souden <rhyme label="a" type="m" corresp="#s11:l1">mesdoen</rhyme></l>
        <l id="s11:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s11:l2">dop</rhyme>.</l>
        <l id="s11:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Doen riepen die reuaelge metter <rhyme label="a" type="m">macht</rhyme>.</l>
        <l id="s12:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s12:l3" met="4" type="-+">Dits den dach daer wi hebben na <rhyme label="a" type="m" corresp="#s12:l1">ghewacht</rhyme>.</l>
        <l id="s12:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s12:l2">dop</rhyme>.</l>
        <l id="s12:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Floyon riep: 'wi weten <rhyme label="a" type="m">onderstant</rhyme>.</l>
        <l id="s13:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s13:l3" met="4" type="-+">Die connick compt al in ons <rhyme label="a" type="m" corresp="#s13:l1">lant'</rhyme>.</l>
        <l id="s13:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s13:l2">dop</rhyme>.</l>
        <l id="s13:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">En compt den connick emmers <rhyme label="a" type="m">niet</rhyme>.</l>
        <l id="s14:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s14:l3" met="4" type="-+">Soo blijven die cloosterkens int <rhyme label="a" type="m" corresp="#s14:l1">verdriet</rhyme>.</l>
        <l id="s14:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s14:l2">dop</rhyme>.</l>
        <l id="s14:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Sij trocken int clooster binnen den <rhyme label="a" type="m">crocht</rhyme>.</l>
        <l id="s15:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s15:l3" met="4" type="-+">Daer hebben sij loozen raet <rhyme label="a" type="m" corresp="#s15:l1">ghesocht</rhyme>.</l>
        <l id="s15:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s15:l2">dop</rhyme>.</l>
        <l id="s15:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Horen raet sach men corts <rhyme label="a" type="m">openbaren</rhyme>.</l>
        <l id="s16:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s16:l3" met="4" type="-+">Men tracker beelden en <rhyme label="a" type="m" corresp="#s16:l1">altaren</rhyme>.</l>
        <l id="s16:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s16:l2">dop</rhyme>.</l>
        <l id="s16:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">Haer beloofte hebben sij macht <rhyme label="a" type="m">ghegeven</rhyme>.</l>
        <l id="s17:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s17:l3" met="4" type="-+">Sij en hebben sintruyden niet <rhyme label="a" type="m" corresp="#s17:l1">bedreven</rhyme>.</l>
        <l id="s17:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s17:l2">dop</rhyme>.</l>
        <l id="s17:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="4" type="-+">Laet ons bidden den Heer der <rhyme label="a" type="m">heeren</rhyme>.</l>
        <l id="s18:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s18:l3" met="4" type="-+">Dat hun die genken mochten <rhyme label="a" type="m" corresp="#s18:l1">bekeeren</rhyme>.</l>
        <l id="s18:l4" met="2" type="+-">Vive le geux! Dobbe dobbe <rhyme label="b" type="m" corresp="#s18:l2">dop</rhyme>.</l>
        <l id="s18:l5" met="5" type="+-">Vive le geux! Dobbe dobbe <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="4" type="-+">En dij dit liedeken heeft <rhyme label="a" type="m">ghedicht</rhyme>.</l>
        <l id="s19:l2" met="2" type="+-">Vive le <rhyme label="b" type="m">geux</rhyme>!</l>
        <l id="s19:l3" met="4" type="-+">Het was een vroucken van herten <rhyme label="a" type="m" corresp="#s19:l1">licht</rhyme>.</l>
        <l id="s19:l4" met="2" type="+-">Vive le geux! Dobbe. dobbe. <rhyme label="b" type="m" corresp="#s19:l2">dop</rhyme>.</l>
        <l id="s19:l5" met="5" type="+-">Vive le geux! Dobbe. dobbe. <rhyme label="c" type="m">dop</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
