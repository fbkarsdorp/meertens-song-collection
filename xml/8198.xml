    <melody>Hoe zalig zijn de landen</melody>
    <categories>
      <category translation="love and sex">liefde en seks</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DE Koster van dees <rhyme label="a" type="f">Kercke</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Hoe frayen Man hy <rhyme label="b" type="m">was</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Komt met sijn sleutel te <rhyme label="a" type="f" corresp="#s1:l1">wercke</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met een devote <rhyme label="b" type="m" corresp="#s1:l2">tas</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Hy wilde soo garen eens <rhyme label="c" type="f">fudele</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hy wilde soo garen eens <rhyme label="c" type="f" corresp="#s1:l5">fudele</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Hy wilde soo garen eens <rhyme label="c" type="f" corresp="#s1:l5">fudele</rhyme></l>
        <l id="s1:l8" met="3" type="-+">In een besloote <rhyme label="b" type="m" corresp="#s1:l2">kas</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy greep mijn in sijn <rhyme label="a" type="f">arremen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ick vandt hem al <rhyme label="b" type="m">terterck</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Al riep noch ach! <rhyme label="a" type="f" corresp="#s2:l1">armen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En sey 't is in de <rhyme label="b" type="m" corresp="#s2:l2">Kerck</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">hy wilde al even eens <rhyme label="c" type="f">fudelen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">hy wilde al even eens <rhyme label="c" type="f" corresp="#s2:l5">fudelen</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">hy wilde al even eens <rhyme label="c" type="f" corresp="#s2:l5">fudelen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">In mijn bestelde <rhyme label="b" type="m" corresp="#s2:l2">perck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Van Quesel wierde ick <rhyme label="a" type="f">boele</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">En stel mijn in <rhyme label="b" type="m">postuer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In een leun Paters <rhyme label="a" type="f" corresp="#s3:l1">stoele</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">En juyst ter <rhyme label="b" type="m" corresp="#s3:l2">avontuer</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Soo wil hy daer voor <rhyme label="c" type="f">Lamphudelen</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Soo wil hy daer voor <rhyme label="c" type="f" corresp="#s3:l5">Lamphudelen</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Soo wil hy daer voor <rhyme label="c" type="f" corresp="#s3:l5">Lamphudelen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Op dat sijn brandt <rhyme label="b" type="m" corresp="#s3:l2">verkoel</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Mijn beentjens dien hy <rhyme label="a" type="f">leyde</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Op ellicke leuningh <rhyme label="b" type="m">een</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Want dat ick hem niet <rhyme label="a" type="f" corresp="#s4:l1">beyde</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat heeft te recht wel <rhyme label="b" type="m" corresp="#s4:l2">reÃ«n</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Doen raeckten hy aen 't <rhyme label="c" type="f">Lamphulen</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Doen raeckten hy aen 't <rhyme label="c" type="f" corresp="#s4:l5">Lamphulen</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Doen raeckten hy aen 't <rhyme label="c" type="f" corresp="#s4:l5">Lamphulen</rhyme></l>
        <l id="s4:l8" met="3" type="-+">In 't Heylighdom binnens <rhyme label="b" type="m" corresp="#s4:l2">been</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mijn kuysche ziel hy <rhyme label="a" type="f">raeckte</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Terwijl sijn klockjens <rhyme label="b" type="m">slaen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">En op mijn roode <rhyme label="a" type="f" corresp="#s5:l1">lipjens</rhyme></l>
        <l id="s5:l4" met="3" type="-+">'t Ave Mari quam <rhyme label="b" type="m" corresp="#s5:l2">slaen</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Doen waer't niet als <rhyme label="c" type="f">lamphudelen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Doen waer't niet als <rhyme label="c" type="f" corresp="#s5:l5">lamphudelen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Doen waer't niet als <rhyme label="c" type="f" corresp="#s5:l5">lamphudelen</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">En 't Pater noster <rhyme label="b" type="m" corresp="#s5:l2">gedaen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy prevelde, en ick <rhyme label="a" type="f">krevelde</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hy hyghde na sijn <rhyme label="b" type="m">aem</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Hoe seer dat ick my <rhyme label="a" type="f" corresp="#s6:l1">quelde</rhyme>;</l>
        <l id="s6:l4" met="3" type="-+">De Koster riep al <rhyme label="b" type="m" corresp="#s6:l2">aem</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">En 't waer niet als <rhyme label="c" type="f">lamphudelen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En 't waer niet als <rhyme label="c" type="f" corresp="#s6:l5">lamphudelen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">En 't waer niet als <rhyme label="c" type="f" corresp="#s6:l5">lamphudelen</rhyme></l>
        <l id="s6:l8" met="3" type="-+">wat goets, wat quaets te <rhyme label="b" type="m" corresp="#s6:l2">saem</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
