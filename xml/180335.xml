<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>180335</id>
    <date>
      <year-start>1783</year-start>
      <year-end>1783</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Wat is ons al vreugde gegeven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">VRijers wilt gy wel <rhyme label="a" type="f">besinnen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Gaet niet al te los in 't <rhyme label="a" type="f" corresp="#s1:l1">minnen</rhyme>;</l>
        <l id="s1:l3" met="4" type="-+">Ziet eerst wat 'er omme <rhyme label="b" type="m">gaet</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hoe het met u Liefste <rhyme label="b" type="m" corresp="#s1:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Of zy Deugdsaem is van <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Of zy Reeden weet te <rhyme label="a" type="f" corresp="#s2:l1">geven</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Niet te los nog <rhyme label="b" type="m">onbedagt</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Datter niemand om u <rhyme label="b" type="m" corresp="#s2:l3">lagt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Laet u niet aen Goed <rhyme label="a" type="f">verbinden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wilt geen Trouwen <rhyme label="a" type="f" corresp="#s3:l1">onderwinden</rhyme>;</l>
        <l id="s3:l3" met="4" type="-+">Met een Duyfje sonder <rhyme label="b" type="m">Gal</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Die elk houd voor half <rhyme label="b" type="m" corresp="#s3:l3">Mal</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Laet veel liever 't Goedje <rhyme label="a" type="f">varen</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Zoek ook wysheyt in het <rhyme label="a" type="f" corresp="#s4:l1">Paren</rhyme>;</l>
        <l id="s4:l3" met="4" type="-+">'t Goed en doet wel niemant <rhyme label="b" type="m">quaet</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Maer wat is 't een slegte <rhyme label="b" type="m" corresp="#s4:l3">staet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Als de Man alleen moet <rhyme label="a" type="f">waken</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">En beschikken alle <rhyme label="a" type="f" corresp="#s5:l1">saken</rhyme>;</l>
        <l id="s5:l3" met="4" type="-+">Als de Vrouw is dom en <rhyme label="b" type="m">bot</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Altyd even mal en <rhyme label="b" type="m" corresp="#s5:l3">Zot</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Zulje op een Maeltyd <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Wyse man hoe moet gy <rhyme label="a" type="f" corresp="#s6:l1">schromen</rhyme>;</l>
        <l id="s6:l3" met="4" type="-+">Gy hebt u nauwelijks <rhyme label="b" type="m">omgedraeyt</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Of gy vind uw al <rhyme label="b" type="m" corresp="#s6:l3">bekaeyt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Ã´ Dit kan soo dapper <rhyme label="a" type="f">drukken</rhyme>!</l>
        <l id="s7:l2" met="4" type="+-">Nog soo vreest gy 't hert sal <rhyme label="a" type="f" corresp="#s7:l1">bukken</rhyme>;</l>
        <l id="s7:l3" met="4" type="-+">'t Geene dat 'er word <rhyme label="b" type="m">Gebaerd</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Meest al na de Moeders <rhyme label="b" type="m" corresp="#s7:l3">aerd</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Voor gepresen, na <rhyme label="a" type="f">gesongen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Bonte Ouden, bonte <rhyme label="a" type="f" corresp="#s8:l1">Iongen</rhyme>;</l>
        <l id="s8:l3" met="4" type="-+">Wilt gy Leven <rhyme label="b" type="m">ongequelt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Neemt geen Doedje om haer <rhyme label="b" type="m" corresp="#s8:l3">Geld</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">'t Geld en kan geen wysheyt <rhyme label="a" type="f">geven</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Wysheyt is de Ziel van 't <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme>;</l>
        <l id="s9:l3" met="4" type="-+">Ziet wel voor u wat gy <rhyme label="b" type="m">doet</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">Trouwt geen Zots kap om haer <rhyme label="b" type="m" corresp="#s9:l3">Goed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
