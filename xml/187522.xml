<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>187522</id>
    <date>
      <year-start>1693</year-start>
      <year-end>1693</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Aanmerk o aards geslacht</melody>
  <author>Frank A. Metaal</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Gy Doggenaertjes <rhyme label="a" type="m">fier</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Al van Maes-sluys <rhyme label="b" type="f">eerbare</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die met een goet <rhyme label="a" type="m" corresp="#s1:l1">playsier</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Op bracke stroomen <rhyme label="b" type="f" corresp="#s1:l2">varen</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">Heën en weer na u <rhyme label="c" type="m">begeert</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die gronden gaen <rhyme label="d" type="f">besoecken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Om Vis te vangen <rhyme label="e" type="m">jent</rhyme> ,</l>
        <l id="s1:l8" met="3" type="-+">Met Netten of met <rhyme label="d" type="f" corresp="#s1:l6">hoecken</rhyme>:</l>
        <l id="s1:l9" met="3" type="-+">U tot het goede <rhyme label="e" type="m" corresp="#s1:l7">wendt</rhyme>,</l>
        <l id="s1:l10" met="3" type="+-">Godt sal u zijn <rhyme label="e" type="m" corresp="#s1:l7">omtrent</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Want door Godts macht seer <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Soo brenght gy hier te <rhyme label="b" type="f">Lande</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Veel Cabeljau <rhyme label="a" type="m" corresp="#s2:l1">minjoot</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Oock Schelvis <rhyme label="b" type="f" corresp="#s2:l2">veelderhande</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Vleet, Roch en Poon, oock Ballick <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Schol, Schar, en Heyl-bot <rhyme label="d" type="f">goedigh</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Spieringh en <rhyme label="e" type="m">Mackereel</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Oock Haringh <rhyme label="d" type="f" corresp="#s2:l6">overvloedigh</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">Leng’, Pieterman oock <rhyme label="e" type="m" corresp="#s2:l7">veel</rhyme>,</l>
        <l id="s2:l10" met="3" type="+-">Komter al in ’t <rhyme label="e" type="m" corresp="#s2:l7">geheel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Soo dat gy <rhyme label="a" type="m">Sluysche-dal</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Spijst veelderhande <rhyme label="b" type="f">menschen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">In Holland over <rhyme label="a" type="m" corresp="#s3:l1">al</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">In Brabant mee na <rhyme label="b" type="f" corresp="#s3:l2">wenschen</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Oock in Vranckrijck en te <rhyme label="c" type="m">Danswijck</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Doet gyse voeren <rhyme label="d" type="f">vaerdigh</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Met schepen groot en <rhyme label="e" type="m">kleyn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Om te spijsen seer <rhyme label="d" type="f" corresp="#s3:l6">aerdigh</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Veel menschen in ’t <rhyme label="e" type="m" corresp="#s3:l7">gemeyn</rhyme>,</l>
        <l id="s3:l10" met="3" type="+-">Door Godes zegen <rhyme label="e" type="m" corresp="#s3:l7">reyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Want het moet alles <rhyme label="a" type="m">van</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Des Heeren zegen <rhyme label="b" type="f">komen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Soo dat hem niemant <rhyme label="a" type="m" corresp="#s4:l1">kan</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Op den zegen <rhyme label="b" type="f" corresp="#s4:l2">beromen</rhyme>:</l>
        <l id="s4:l5" met="4" type="-+">Want yder leeft, van’t geen Godt <rhyme label="c" type="m">geeft</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Door sijn milde-handt <rhyme label="d" type="f">krachtigh</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Dies laten wy met <rhyme label="e" type="m">vlijt</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Hem prijsen al <rhyme label="d" type="f" corresp="#s4:l6">eendrachtigh</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Om te wesen <rhyme label="e" type="m" corresp="#s4:l7">verblijt</rhyme>,</l>
        <l id="s4:l10" met="3" type="+-">Al in den laetsten <rhyme label="e" type="m" corresp="#s4:l7">tijt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Den Landt-man <rhyme label="a" type="m">excellent</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Moet ick oock lof af <rhyme label="b" type="f">singen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Hy brenght ons seer <rhyme label="a" type="m" corresp="#s5:l1">bekent</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Veele leefbare <rhyme label="b" type="f" corresp="#s5:l2">dingen</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Mellick seer soet, oock Boter <rhyme label="c" type="m">goet</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Kaes, Kooren, Eyers, <rhyme label="d" type="f">Koeyen</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Verckens en <rhyme label="e" type="m">Eende-bout</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Appels, Quee-peeren <rhyme label="d" type="f" corresp="#s5:l6">groeyen</rhyme></l>
        <l id="s5:l9" met="3" type="-+">By hem seer <rhyme label="e" type="m" corresp="#s5:l7">menighfout</rhyme>.</l>
        <l id="s5:l10" met="3" type="+-">Tot ’s menschen <rhyme label="e" type="m" corresp="#s5:l7">onderhout</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Doch hy moet <rhyme label="a" type="m">evenstaegh</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Hem met het wercken <rhyme label="b" type="f">moeyen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Die Vrijsters ick <rhyme label="a" type="m" corresp="#s6:l1">beklaegh</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Want sy moeten de <rhyme label="b" type="f" corresp="#s6:l2">koeyen</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">In ’t natte gras, gaen melcken <rhyme label="c" type="m">ras</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">’s Morgens en ’s avonts <rhyme label="d" type="f">late</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">In onweer, storm en <rhyme label="e" type="m">wint</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Dus wilt den Boer-man <rhyme label="d" type="f" corresp="#s6:l6">late</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">En kiest een Vissers <rhyme label="e" type="m" corresp="#s6:l7">kint</rhyme>,</l>
        <l id="s6:l10" met="3" type="+-">Wert ghy daer van <rhyme label="e" type="m" corresp="#s6:l7">bemint</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Wel Vrijsters wie gy <rhyme label="a" type="m">zijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Mach u een Visscher <rhyme label="b" type="f">beuren</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Die op eer en deught <rhyme label="a" type="m" corresp="#s7:l1">vrijt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En is van <rhyme label="b" type="f" corresp="#s7:l2">goed’humeuren</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Wilt hem voortaen, niet fel <rhyme label="c" type="m">afslaen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Doch wat weyg’righ <rhyme label="d" type="f">wesen</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Want het is maegden <rhyme label="e" type="m">eer</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Al is’t een groote <rhyme label="d" type="f" corresp="#s7:l6">vreese</rhyme>,</l>
        <l id="s7:l9" met="3" type="-+">En doet de knechts <rhyme label="e" type="m" corresp="#s7:l7">hertseer</rhyme>,</l>
        <l id="s7:l10" met="3" type="+-">’t Komt ten lesten wel <rhyme label="e" type="m" corresp="#s7:l7">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
