    <melody>Psalm 006 Datheen</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Hendrik Rintjes</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilt my niet straffen <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die misdaen, hebb’ so <rhyme label="a" type="f" corresp="#s1:l1">seere</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In eenen grammen <rhyme label="b" type="m">sin</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">In uwen Throon <rhyme label="c" type="f">vervaerlijck</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Kastijt my niet soo <rhyme label="c" type="f" corresp="#s1:l4">swaerlijck</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Als ick wel weerdigh <rhyme label="b" type="m" corresp="#s1:l3">ben</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Maer wilt u, Heer, <rhyme label="a" type="f">ontfarmen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">End’ over my <rhyme label="a" type="f" corresp="#s2:l1">erbarmen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ick ben seer swack <rhyme label="b" type="m">altijdt</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wilt my ghesontheyt <rhyme label="c" type="f">gheven</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Want mijn Ziel end’ lijf <rhyme label="c" type="f" corresp="#s2:l4">beven</rhyme></l>
        <l id="s2:l6" met="3" type="-+">In desen mijnen <rhyme label="b" type="m" corresp="#s2:l3">strijt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Mijn geeft hem oock <rhyme label="a" type="f">onstellet</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Swaert verschricken my <rhyme label="a" type="f" corresp="#s3:l1">quellet</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Vreese maeckt my <rhyme label="b" type="m">onvro</rhyme>;</l>
        <l id="s3:l4" met="3" type="-+">O Heere hoogh <rhyme label="c" type="f">ghepresen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Hoe langhe sal ‘t noch <rhyme label="c" type="f" corresp="#s3:l4">wesen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Dat ick moet blijven <rhyme label="b" type="m" corresp="#s3:l3">so</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ach! wilt u tot my <rhyme label="a" type="f">keeren</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Wilt oock van my noch <rhyme label="a" type="f" corresp="#s4:l1">weeren</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Dees benautheyt niet <rhyme label="b" type="m">kleyn</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Seer groot zijn mijn <rhyme label="c" type="f">misdaden</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Maer u louter <rhyme label="c" type="f" corresp="#s4:l4">ghenaden</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Maeckt my Heer, daer van <rhyme label="b" type="m" corresp="#s4:l3">reyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Want in de Doodt seer <rhyme label="a" type="f">wreede</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Wie is ‘t die daer <rhyme label="a" type="f" corresp="#s5:l1">verbreede</rhyme></l>
        <l id="s5:l3" met="3" type="-+">U lof end’ eer <rhyme label="b" type="m">bequaem</rhyme>?</l>
        <l id="s5:l4" met="3" type="-+">Niemant sal in der <rhyme label="c" type="f">Hellen</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Uwen prijs schoon <rhyme label="c" type="f" corresp="#s5:l4">vertellen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Noch dancken uwen <rhyme label="b" type="m" corresp="#s5:l3">naem</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ick ben moed’ end’ <rhyme label="a" type="f">verslaghen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Van gantsch den nacht te <rhyme label="a" type="f" corresp="#s6:l1">klaghen</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">Ick doe swemmen <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Mijn bedde met mijn <rhyme label="c" type="f">weenen</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">End’ mijn legher met <rhyme label="c" type="f" corresp="#s6:l4">eenen</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In mijn Tranen <rhyme label="b" type="m" corresp="#s6:l3">eenpaer</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Mijn ghedaente met <rhyme label="a" type="f">allen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Is nu, Heer, gantsch <rhyme label="a" type="f" corresp="#s7:l1">vervallen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door gheduyrich <rhyme label="b" type="m">gheklach</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">Om dat aen allen <rhyme label="c" type="f">zijden</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Mijn Vyanden <rhyme label="c" type="f" corresp="#s7:l4">verblijden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Voor my met groot <rhyme label="b" type="m" corresp="#s7:l3">ghelach</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Ghy boose wilt nu <rhyme label="a" type="f">wijcken</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ghy wreede <rhyme label="a" type="f" corresp="#s8:l1">desghelijcken</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Vertreckt nu haest van <rhyme label="b" type="m">hier</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Godt heeft mijn truyrich <rhyme label="c" type="f">klaghen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Nae sijn goet <rhyme label="c" type="f" corresp="#s8:l4">welbehaghen</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Verhoort seer <rhyme label="b" type="m" corresp="#s8:l3">goedertier</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Godt en wilt niet <rhyme label="a" type="f">verachten</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Mijn Ghebedt, noch mijn <rhyme label="a" type="f" corresp="#s9:l1">klachten</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Maer hoort my t’sijner <rhyme label="b" type="m">eer</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Mijn beden hem <rhyme label="c" type="f">beweghen</rhyme>,</l>
        <l id="s9:l5" met="3" type="-+">Ick heb van hem <rhyme label="c" type="f" corresp="#s9:l4">verkreghen</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Mijn begheerten, end’ <rhyme label="b" type="m" corresp="#s9:l3">meer</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Daerom zijn nu met <rhyme label="a" type="f">schanden</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Beswaert al mijn <rhyme label="a" type="f" corresp="#s10:l1">Vyanden</rhyme>?</l>
        <l id="s10:l3" met="3" type="-+">Verbaest zijn sy <rhyme label="b" type="m">ghewis</rhyme>:</l>
        <l id="s10:l4" met="3" type="-+">Te rugg’ moeten sy <rhyme label="c" type="f">keeren</rhyme></l>
        <l id="s10:l5" met="3" type="-+">Met schaemten en <rhyme label="c" type="f" corresp="#s10:l4">oneeren</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">Want my Godt soo goet <rhyme label="b" type="m" corresp="#s10:l3">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
