<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>14467</id>
    <date>
      <year-start>1638</year-start>
      <year-end>1638</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>holy song</tag>
    </tags>
    <melody>Aanhoor toch mijn geklaag gij ruiters</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MAria Maeght <rhyme label="a" type="m">eerbaer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Princes van <rhyme label="b" type="f">Hemel-rijcke</rhyme>:</l>
        <l id="s1:l3" met="3" type="-+">Ghy zijt een spieghel <rhyme label="a" type="m" corresp="#s1:l1">klaer</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Heel sonder smet <rhyme label="a" type="m" corresp="#s1:l1">voorwaer</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Boven der Eng'len <rhyme label="a" type="m" corresp="#s1:l1">schaer</rhyme>.</l>
        <l id="s1:l6" met="3" type="-+">Waerom sy oock <rhyme label="a" type="m" corresp="#s1:l1">eenpaer</rhyme></l>
        <l id="s1:l7" met="3" type="-+">U eeren <rhyme label="b" type="f" corresp="#s1:l2">eeuwelijcke</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ghy hebt Adams <rhyme label="a" type="m">gheslacht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">'t Welck Eva had doen <rhyme label="b" type="f">trueren</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Als Hester, wijs <rhyme label="a" type="m" corresp="#s2:l1">bedacht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En Judith, door Godts <rhyme label="a" type="m" corresp="#s2:l1">kracht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Weder in vree <rhyme label="a" type="m" corresp="#s2:l1">ghebracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Voor uw'vrucht groot van <rhyme label="a" type="m" corresp="#s2:l1">macht</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Wt druck, en doodts <rhyme label="b" type="f" corresp="#s2:l2">besueren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Ghy zijt de vrome <rhyme label="a" type="m">Vrouw</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die 't Serpents hooft <rhyme label="b" type="f">vertreden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Heeft, en door 's Hemels <rhyme label="a" type="m" corresp="#s3:l1">douw</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Godts Soon, Edel <rhyme label="a" type="m" corresp="#s3:l1">kersouw</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ghebaert, Maeght sonder <rhyme label="a" type="m" corresp="#s3:l1">rou</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Bleeft Maeght, en sonder <rhyme label="a" type="m" corresp="#s3:l1">flouw</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Bracht, als Abigail, <rhyme label="b" type="f" corresp="#s3:l2">vreden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">U heeft de Son <rhyme label="a" type="m">ghekleedt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Schoon dagheraet <rhyme label="b" type="f">gheresen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Die uyt u wel <rhyme label="a" type="m" corresp="#s4:l1">bereedt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Heel sonder pijn of <rhyme label="a" type="m" corresp="#s4:l1">leet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Quam, om ons druck en <rhyme label="a" type="m" corresp="#s4:l1">sweet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Te drooghen wijdt en <rhyme label="a" type="m" corresp="#s4:l1">breedt</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Met recht wordt ghy <rhyme label="b" type="f" corresp="#s4:l2">ghepresen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ghy blinckt van diep <rhyme label="a" type="m">ootmoet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Hof, schat, fonteyn vol <rhyme label="b" type="f">deughden</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Gheloof, Hoop, Liefden <rhyme label="a" type="m" corresp="#s5:l1">gloet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Is uwen geur, duyf <rhyme label="a" type="m" corresp="#s5:l1">soet</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Ghelijck de Maen den <rhyme label="a" type="m" corresp="#s5:l1">vloedt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Verweckt soo ons <rhyme label="a" type="m" corresp="#s5:l1">ghemoedt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Vervuldt ons ziel met <rhyme label="b" type="f" corresp="#s5:l2">vreughden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Schoon' leli', Jacobs <rhyme label="a" type="m">leer</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Die streckt aen Godt <rhyme label="b" type="f">bezijden</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De Eng'len doen u <rhyme label="a" type="m" corresp="#s6:l1">eer</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Naer uwes Soons <rhyme label="a" type="m" corresp="#s6:l1">begheer</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Slagh-oord', druckt nu toch <rhyme label="a" type="m" corresp="#s6:l1">neer</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Uw' vyanden, die <rhyme label="a" type="m" corresp="#s6:l1">seer</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Groot'lijcks uw' lof <rhyme label="b" type="f" corresp="#s6:l2">benijden</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Maer wy tot 's duyvels <rhyme label="a" type="m">spijt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Willen u altijdt <rhyme label="b" type="f">loven</rhyme>:</l>
        <l id="s7:l3" met="3" type="-+">Al is 't dat sulcks <rhyme label="a" type="m" corresp="#s7:l1">benijdt</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Sijn zaedt <rhyme label="a" type="m" corresp="#s7:l1">vermaledijdt</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Van haer gheweldt ons <rhyme label="a" type="m" corresp="#s7:l1">vrijdt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Vroom Iahel voor ons <rhyme label="a" type="m" corresp="#s7:l1">strijdt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Aenhoort ons arm <rhyme label="b" type="f" corresp="#s7:l2">verschoven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
