<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>23498</id>
    <date>
      <year-start>1598</year-start>
      <year-end>1598</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>anti-beggar song</tag>
    </tags>
    <melody>Ik ben niet als een pluim</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Holland/ Zeeland laet v <rhyme label="a" type="f">vermanen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Tot Peys en Vrede t'is <rhyme label="b" type="m">noot</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Legt af v bloedighe <rhyme label="a" type="f" corresp="#s1:l1">vanen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die ghy voert met nijt seer <rhyme label="b" type="m" corresp="#s1:l2">groot</rhyme>/</l>
        <l id="s1:l5" met="4" type="-+">Ghy en hebt gheen oorsaeck wapen oft <rhyme label="b" type="m" corresp="#s1:l2">cloot</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Te voeren met rebellie <rhyme label="b" type="m" corresp="#s1:l2">bloot</rhyme>/</l>
        <l id="s1:l7" met="4" type="-+">Teghen Philippus uwen Prins <rhyme label="b" type="m" corresp="#s1:l2">minoot</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hy is een Prince <rhyme label="a" type="f">saechtmoedich</rhyme>/</l>
        <l id="s2:l2" met="3" type="-+">Sijn wercken ghetuyghent <rhyme label="b" type="m">wel</rhyme>/</l>
        <l id="s2:l3" met="3" type="-+">Keert weder tot hem <rhyme label="a" type="f" corresp="#s2:l1">spoedich</rhyme>/</l>
        <l id="s2:l4" met="3" type="-+">Drijft wt tyrannich <rhyme label="b" type="m" corresp="#s2:l2">opstel</rhyme>/</l>
        <l id="s2:l5" met="4" type="-+">Waerom blijfdy doch dus <rhyme label="b" type="m" corresp="#s2:l2">rebel</rhyme>/</l>
        <l id="s2:l6" met="4" type="-+">Laet varen den twist t'is <rhyme label="b" type="m" corresp="#s2:l2">kinderspel</rhyme>/</l>
        <l id="s2:l7" met="4" type="-+">Ontfangt v Ouerheyt t'is Godts <rhyme label="b" type="m" corresp="#s2:l2">beuel</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">En wilt niet meer <rhyme label="a" type="f">gheloouen</rhyme>/</l>
        <l id="s3:l2" met="3" type="-+">Dees muytmakers boos en <rhyme label="b" type="m">quaet</rhyme>/</l>
        <l id="s3:l3" met="3" type="-+">Sy souden v gheerne <rhyme label="a" type="f" corresp="#s3:l1">verdoouen</rhyme>/</l>
        <l id="s3:l4" met="3" type="-+">Maer en wilt niet volghen haren <rhyme label="b" type="m" corresp="#s3:l2">raet</rhyme>/</l>
        <l id="s3:l5" met="4" type="-+">Neemt aen den Peys tot uwer <rhyme label="b" type="m" corresp="#s3:l2">baet</rhyme>/</l>
        <l id="s3:l6" met="4" type="-+">Ontfangt dat Edel Conincklijck <rhyme label="b" type="m" corresp="#s3:l2">saet</rhyme>/</l>
        <l id="s3:l7" met="4" type="-+">Wilt van v weeren alle twist en <rhyme label="b" type="m" corresp="#s3:l2">haet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die u noch <rhyme label="a" type="f">persuaderen</rhyme>/</l>
        <l id="s4:l2" met="3" type="-+">Tot Oorloch met <rhyme label="b" type="m">ghewelt</rhyme>/</l>
        <l id="s4:l3" met="3" type="-+">Die en soecken niet dat ghy meucht <rhyme label="a" type="f" corresp="#s4:l1">floreren</rhyme>/</l>
        <l id="s4:l4" met="3" type="-+">Maer sy haken naer goet en <rhyme label="b" type="m" corresp="#s4:l2">ghelt</rhyme>/</l>
        <l id="s4:l5" met="4" type="-+">Met eyghen baet sijn sy <rhyme label="b" type="m" corresp="#s4:l2">verselt</rhyme>/</l>
        <l id="s4:l6" met="4" type="-+">Den armen Boer die wort <rhyme label="b" type="m" corresp="#s4:l2">ghequelt</rhyme>/</l>
        <l id="s4:l7" met="4" type="-+">En den Coopman wort ghekneuelt opt <rhyme label="b" type="m" corresp="#s4:l2">velt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daerom en wilt niet <rhyme label="a" type="f">versmaden</rhyme>/</l>
        <l id="s5:l2" met="3" type="-+">Den Peys u van Godt <rhyme label="b" type="m">ghesant</rhyme>/</l>
        <l id="s5:l3" met="3" type="-+">Drijft wt die valsche <rhyme label="a" type="f" corresp="#s5:l1">raden</rhyme>/</l>
        <l id="s5:l4" met="3" type="-+">Die bederuen v edel <rhyme label="b" type="m" corresp="#s5:l2">Landt</rhyme>/</l>
        <l id="s5:l5" met="4" type="-+">Al hadt ghy noch yemant tot u <rhyme label="b" type="m" corresp="#s5:l2">bystandt</rhyme>/</l>
        <l id="s5:l6" met="4" type="-+">Sy moghen noch worden v <rhyme label="b" type="m" corresp="#s5:l2">vyandt</rhyme>/</l>
        <l id="s5:l7" met="4" type="-+">En bestrijden v aen elcken <rhyme label="b" type="m" corresp="#s5:l2">cant</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dit en is gheen wonder om <rhyme label="a" type="f">hooren</rhyme>/</l>
        <l id="s6:l2" met="3" type="-+">Want rebellie Godt <rhyme label="b" type="m">mishaecht</rhyme>/</l>
        <l id="s6:l3" met="3" type="-+">Ten lesten sal hy hem <rhyme label="a" type="f" corresp="#s6:l1">verstooren</rhyme>/</l>
        <l id="s6:l4" met="3" type="-+">En v Landen sullen worden <rhyme label="b" type="m" corresp="#s6:l2">gheplaecht</rhyme>/</l>
        <l id="s6:l5" met="4" type="-+">Godt is lanckmoedich hy <rhyme label="b" type="m" corresp="#s6:l2">verdraecht</rhyme>/</l>
        <l id="s6:l6" met="4" type="-+">Neemt aen den Peys eer ghyt <rhyme label="b" type="m" corresp="#s6:l2">beclaecht</rhyme>/</l>
        <l id="s6:l7" met="4" type="-+">Soo en wort ghy bedorven of <rhyme label="b" type="m" corresp="#s6:l2">veriaecht</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Dat ghy v meynt te <rhyme label="a" type="f">verlaten</rhyme>/</l>
        <l id="s7:l2" met="3" type="-+">Op v Forten en Waterlanden <rhyme label="b" type="m">wijt</rhyme>/</l>
        <l id="s7:l3" met="3" type="-+">Als Godt wilt ten sal v al niet <rhyme label="a" type="f" corresp="#s7:l1">baten</rhyme>/</l>
        <l id="s7:l4" met="3" type="-+">Dus wilt aflegghen desen bloedighen <rhyme label="b" type="m" corresp="#s7:l2">strijt</rhyme>/</l>
        <l id="s7:l5" met="4" type="-+">Weert wt t'fenijn van haet en <rhyme label="b" type="m" corresp="#s7:l2">nijt</rhyme>/</l>
        <l id="s7:l6" met="4" type="-+">Neemt aen den Peys wie dat ghy <rhyme label="b" type="m" corresp="#s7:l2">sijt</rhyme>/</l>
        <l id="s7:l7" met="4" type="-+">Wilt v bedencken/ want t'is meer dan <rhyme label="b" type="m" corresp="#s7:l2">tijt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Oorlof Hollanders met <rhyme label="a" type="f">desen</rhyme>/</l>
        <l id="s8:l2" met="3" type="-+">En Zeelanders groot en <rhyme label="b" type="m">cleyn</rhyme>/</l>
        <l id="s8:l3" met="3" type="-+">Wilt aenueerden den Pays <rhyme label="a" type="f" corresp="#s8:l1">ghepresen</rhyme>/</l>
        <l id="s8:l4" met="3" type="-+">Weert dees Oorloghen wt v <rhyme label="b" type="m" corresp="#s8:l2">pleyn</rhyme>/</l>
        <l id="s8:l5" met="4" type="-+">Ontfangt uwen Prince met goeder <rhyme label="b" type="m" corresp="#s8:l2">meynt</rhyme>/</l>
        <l id="s8:l6" met="4" type="-+">Sy sal v omhelsen wt liefden <rhyme label="b" type="m" corresp="#s8:l2">reyn</rhyme>/</l>
        <l id="s8:l7" met="4" type="-+">En vergheuen v misdaden <rhyme label="b" type="m" corresp="#s8:l2">ghemeyn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
