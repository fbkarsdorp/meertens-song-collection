<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>190476</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody>Schoon dat ik onder het groen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Viva, Oranje <rhyme label="a" type="m">leeft</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">Men roer' de zilvre <rhyme label="b" type="f">snaaren</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">By Willems bly <rhyme label="b" type="f" corresp="#s1:l2">Verjaaren</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Men zingt, dat alles <rhyme label="a" type="m" corresp="#s1:l1">beeft</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Viva Oranje <rhyme label="a" type="m" corresp="#s1:l1">leeft</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ten spyt der <rhyme label="c" type="f">Democraaten</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En die Oranje <rhyme label="c" type="f" corresp="#s1:l6">haaten</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Zyn wy op d'achtste <rhyme label="d" type="m">Maart</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Met vreugde, zaam <rhyme label="d" type="m" corresp="#s1:l8">vergaard</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Geen duivel, is <rhyme label="e" type="f">gebooren</rhyme>,</l>
        <l id="s1:l11" met="3" type="+-">Die onze vreugd kan <rhyme label="e" type="f" corresp="#s1:l10">stooren</rhyme>,</l>
        <l id="s1:l12" met="3" type="-+">Al was 't Plato <rhyme label="f" type="m">zelf</rhyme>,</l>
        <l id="s1:l13" met="3" type="-+">Uit 't onderaards <rhyme label="f" type="m" corresp="#s1:l12">gewelf</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Viva, Oranje <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ik zie zyn Haaters <rhyme label="b" type="f">trillen</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Terwyl zy staan te <rhyme label="b" type="f" corresp="#s2:l2">drillen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Ja, Oproermaakers <rhyme label="a" type="m" corresp="#s2:l1">beeft</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">Want Vyfde Willem <rhyme label="a" type="m" corresp="#s2:l1">leeft</rhyme>.</l>
        <l id="s2:l6" met="3" type="-+">Hy zal ten allen <rhyme label="c" type="f">tyden</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Uw helsche doel <rhyme label="c" type="f" corresp="#s2:l6">bestryden</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Hy waakt voor Land en <rhyme label="d" type="m">Kerk</rhyme>,</l>
        <l id="s2:l9" met="3" type="-+">En zet uw muitzugt <rhyme label="d" type="m" corresp="#s2:l8">perk</rhyme>:</l>
        <l id="s2:l10" met="3" type="-+">Zyn wysheid en <rhyme label="e" type="f">vermoogen</rhyme>,</l>
        <l id="s2:l11" met="3" type="+-">Fnuikt al uw listig <rhyme label="e" type="f" corresp="#s2:l10">poogen</rhyme>,</l>
        <l id="s2:l12" met="3" type="-+">'t Verwerde Babel <rhyme label="f" type="m">daal'</rhyme>,</l>
        <l id="s2:l13" met="3" type="-+">Voor Willems <rhyme label="f" type="m" corresp="#s2:l12">Zeegenpraal</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Viva Oranje <rhyme label="a" type="m">leeft</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">Dien Vorst, van zoo veel <rhyme label="b" type="f">smarten</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hy leeft in onze <rhyme label="b" type="f" corresp="#s3:l2">harten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">o goede Hemel <rhyme label="a" type="m" corresp="#s3:l1">geeft</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Dat Willem lange <rhyme label="a" type="m" corresp="#s3:l1">leeft</rhyme>.</l>
        <l id="s3:l6" met="3" type="-+">Schenkt hem de Kroon der <rhyme label="c" type="f">Grysheid</rhyme>:</l>
        <l id="s3:l7" met="3" type="-+">Verleen hem Saal'mons <rhyme label="c" type="f" corresp="#s3:l6">wysheid</rhyme>;</l>
        <l id="s3:l8" met="3" type="-+">En Davids <rhyme label="d" type="m">Heldenmoed</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Terwyl zyn vyand <rhyme label="d" type="m" corresp="#s3:l8">woed</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">Wil steeds zyn stamhuis <rhyme label="e" type="f">spaaren</rhyme>,</l>
        <l id="s3:l11" met="3" type="+-">Tot aan 't eind' der <rhyme label="e" type="f" corresp="#s3:l10">Jaaren</rhyme>,</l>
        <l id="s3:l12" met="3" type="-+">Ja blyf Oranle <rhyme label="f" type="m">by</rhyme>,</l>
        <l id="s3:l13" met="3" type="-+">Tot dat geen tyd meer <rhyme label="f" type="m" corresp="#s3:l12">zy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Viva Oranje <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Een Stem die 't hart kan <rhyme label="b" type="f">raaken</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hy zal voor Neêrland <rhyme label="b" type="f" corresp="#s4:l2">waaken</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Schoon hem de hel <rhyme label="a" type="m" corresp="#s4:l1">weêrstreef</rhyme>:,</l>
        <l id="s4:l5" met="3" type="-+">Viva Oranje <rhyme label="a" type="m" corresp="#s4:l1">leeft</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Het Schip der Zeven <rhyme label="c" type="f">Landen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Behoed hy voor 't <rhyme label="c" type="f" corresp="#s4:l6">stranden</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">De felste Staats <rhyme label="d" type="m">Orkaan</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">Blyft Willem <rhyme label="d" type="m" corresp="#s4:l8">wederstaan</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">In 't midden der <rhyme label="e" type="f">gevaaren</rhyme>,</l>
        <l id="s4:l11" met="3" type="+-">Der bulderende <rhyme label="e" type="f" corresp="#s4:l10">Baaren</rhyme>,</l>
        <l id="s4:l12" met="3" type="-+">Is hy den <rhyme label="f" type="m">Toeverlaat</rhyme>,</l>
        <l id="s4:l13" met="3" type="-+">Van Neêrlands vryen <rhyme label="f" type="m" corresp="#s4:l12">Staat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Viva Oranje <rhyme label="a" type="m">leeft</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Zoo lang de <rhyme label="b" type="f">Regenboogen</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Door 's Hemels <rhyme label="b" type="f" corresp="#s5:l2">Alvermogen</rhyme>;</l>
        <l id="s5:l4" met="3" type="-+">Aan ons dat schynsel <rhyme label="a" type="m" corresp="#s5:l1">geeft</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Viva Oranje <rhyme label="a" type="m" corresp="#s5:l1">leeft</rhyme>;</l>
        <l id="s5:l6" met="3" type="-+">Dat teeken in de <rhyme label="c" type="f">wolken</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Vertoont aan alle <rhyme label="c" type="f" corresp="#s5:l6">Volken</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Hoe dat des Heemels <rhyme label="d" type="m">Hand</rhyme>;</l>
        <l id="s5:l9" met="3" type="-+">Oranje houd in <rhyme label="d" type="m" corresp="#s5:l8">stand</rhyme>.</l>
        <l id="s5:l10" met="3" type="-+">Dus blyft (by 't Hof der <rhyme label="e" type="f">Hoven</rhyme>)</l>
        <l id="s5:l11" met="3" type="+-">Altoos Oranje <rhyme label="e" type="f" corresp="#s5:l10">booven</rhyme>,</l>
        <l id="s5:l12" met="3" type="-+">Schoon d'afgrond hem <rhyme label="f" type="m">weêrstreeft</rhyme>,</l>
        <l id="s5:l13" met="3" type="-+">Viva Oranje <rhyme label="f" type="m" corresp="#s5:l12">leeft</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
