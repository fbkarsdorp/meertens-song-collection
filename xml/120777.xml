  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">'k Kan my gelukkig <rhyme label="a" type="f">noemen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Want 'k heb een lieve <rhyme label="b" type="m">meid</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Met recht kan ik haar <rhyme label="a" type="f" corresp="#s1:l1">roemen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">O wat ben ik <rhyme label="b" type="m" corresp="#s1:l2">verblyd</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Haar schoone lieve <rhyme label="c" type="f">oogen</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Zyn altoos even <rhyme label="d" type="m">fier</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Wenkbraauwen net <rhyme label="c" type="f" corresp="#s1:l5">geboogen</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">'t Is alles na de <rhyme label="d" type="m" corresp="#s1:l6">zwier</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Haar neusje net <rhyme label="a" type="f">besneden</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Gelyk haar lieve <rhyme label="b" type="m">kin</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Volmaakt van lyf en <rhyme label="a" type="f" corresp="#s2:l1">leden</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In 't kort, ze is, na myn <rhyme label="b" type="m" corresp="#s2:l2">zin</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Haar mondje klein en <rhyme label="c" type="f">aardig</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Met lipjes als <rhyme label="d" type="m">carmosyn</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">'t Maakt haar <rhyme label="c" type="f" corresp="#s2:l5">beminnenswaardig</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Ze is niet te groot of <rhyme label="d" type="m" corresp="#s2:l6">klein</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Haar boezem zo <rhyme label="a" type="f">bekoorelyk</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Zo blank als leliÃ«n <rhyme label="b" type="m">wit</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met borsjes zo <rhyme label="a" type="f" corresp="#s3:l1">behoorelyk</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Zwart hair gelyk een <rhyme label="b" type="m" corresp="#s3:l2">git</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Haar kleine voetjes en <rhyme label="c" type="f">handen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Zo poeselig en <rhyme label="d" type="m">zacht</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Doen my van liefde <rhyme label="c" type="f" corresp="#s3:l5">branden</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Als ik maar om haar <rhyme label="d" type="m" corresp="#s3:l6">dacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Zy kan zo aardig <rhyme label="a" type="f">dansen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En zingen na de <rhyme label="b" type="m">zwier</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Gelyk de eerste <rhyme label="a" type="f" corresp="#s4:l1">Franschen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dat baard my veel <rhyme label="b" type="m" corresp="#s4:l2">plaizier</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">'k Verlang om haar te <rhyme label="c" type="f">trouwen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">En zy zo wel als <rhyme label="d" type="m">ik</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ze is een der schoonfte <rhyme label="c" type="f" corresp="#s4:l5">vrouwen</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">O 'k ben zo in myn <rhyme label="d" type="m" corresp="#s4:l6">schik</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ik roem op al haar <rhyme label="a" type="f">deugden</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En op haar schoon <rhyme label="b" type="m">gelaat</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Het geen myn strekt tot <rhyme label="a" type="f" corresp="#s5:l1">vreugde</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Maar als zy myn <rhyme label="b" type="m" corresp="#s5:l2">verlaat</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Moet ik van droefheid <rhyme label="c" type="f">sterven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">En daalen in het <rhyme label="d" type="m">graf</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Och laat m' uw gunst <rhyme label="c" type="f" corresp="#s5:l5">verwerven</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">De liefde baart geen <rhyme label="d" type="m" corresp="#s5:l6">straf</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik ben verblyd van <rhyme label="a" type="f">zinnen</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ik heb haar hand <rhyme label="b" type="m">gekust</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Ik voel myn hart van <rhyme label="a" type="f" corresp="#s6:l1">binnen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Vol vreugd en <rhyme label="b" type="m" corresp="#s6:l2">minnelust</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Ik heb tot trouw <rhyme label="c" type="f">gekregen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">De schoonste van der <rhyme label="d" type="m">aard'</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Ben thans niet meer <rhyme label="c" type="f" corresp="#s6:l5">verlegen</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Nog voor geen vrees <rhyme label="d" type="m" corresp="#s6:l6">vervaard</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
