<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21188</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
    </tags>
    <melody>Wanneer de zon het morgenrood</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Florida onder <rhyme label="a" type="m">d'Elsen-schaeuw</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Lagh soetjes <rhyme label="b" type="f">ne'er-ghedoocken</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Terwijl de Son het <rhyme label="a" type="m" corresp="#s1:l1">Hemels-blaeuw</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Was even <rhyme label="b" type="f" corresp="#s1:l2">door-ghebroocken</rhyme>:</l>
        <l id="s1:l5" met="4" type="-+">De vaeck bevingh haer <rhyme label="c" type="m">Ooghe-le'en</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">'t Gheen ick sagh door de Telghjes <rhyme label="c" type="m" corresp="#s1:l5">heen</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">Toen is sy ne'er <rhyme label="d" type="f">gheseghen</rhyme>.</l>
        <l id="s1:l8" met="4" type="-+">En heeft het geurigh kruydt <rhyme label="e" type="m">verplet</rhyme>,</l>
        <l id="s1:l9" met="4" type="-+">Dus lagh mijn Florisaetje <rhyme label="e" type="m" corresp="#s1:l8">met</rhyme></l>
        <l id="s1:l10" met="3" type="-+">Haer Borsjes los <rhyme label="d" type="f" corresp="#s1:l7">ontreghen</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Ick sagh op 't Lely-witte <rhyme label="a" type="m">vel</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Twee purpre Karshes <rhyme label="b" type="f">proncken</rhyme>:</l>
        <l id="s2:l3" met="4" type="-+">Ay, Florida, dacht ick, <rhyme label="a" type="m" corresp="#s2:l1">hoewel</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Kondt ghy mijn borst <rhyme label="b" type="f" corresp="#s2:l2">ontvoncken</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Door 't heete vuyr van uwe <rhyme label="c" type="m">Min</rhyme>:</l>
        <l id="s2:l6" met="4" type="-+">Wat Schyth soudt ghy sijn wreede <rhyme label="c" type="m" corresp="#s2:l5">sin</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Niet tot de Min <rhyme label="d" type="f">beweghen</rhyme>?</l>
        <l id="s2:l8" met="4" type="-+">Had Apoll' u soo leggen <rhyme label="e" type="m">sien</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Hy had sijn Daphne laten <rhyme label="e" type="m" corresp="#s2:l8">vlien</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">En zin in u <rhyme label="d" type="f" corresp="#s2:l7">ghekreghen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Een dartel Musje sprongh om <rhyme label="a" type="m">hoogh</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Keeck ylinghs nae <rhyme label="b" type="f">beneden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Van stonden aen het Diefje <rhyme label="a" type="m" corresp="#s3:l1">vloogh</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Op haer Albaste <rhyme label="b" type="f" corresp="#s3:l2">Leden</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">En pickte met sijn beckje <rhyme label="c" type="m">teer</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Op 't purper-root: wie krenckt mijn <rhyme label="c" type="m" corresp="#s3:l5">eer</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Rieps' met ghesloten <rhyme label="d" type="f">ooghen</rhyme>?</l>
        <l id="s3:l8" met="4" type="-+">Ey Boefje 't is niet wel <rhyme label="e" type="m">ghemickt</rhyme></l>
        <l id="s3:l9" met="4" type="-+">Riep ick hem toe, doen is 't <rhyme label="e" type="m" corresp="#s3:l8">verschrickt</rhyme>,</l>
        <l id="s3:l10" met="3" type="-+">En is weer <rhyme label="d" type="f" corresp="#s3:l7">op-ghevloghen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Soo drae als 't Musje had <rhyme label="a" type="m">ghemerckt</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hoe dat het was <rhyme label="b" type="f">bedroghen</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Soo heeft het we'er sijn moet <rhyme label="a" type="m" corresp="#s4:l1">versterckt</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En quam strackx <rhyme label="b" type="f" corresp="#s4:l2">ne'er-ghevloghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">En dacht, tot lichtingh van mijn <rhyme label="c" type="m">pijn</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Sal ick gaen soecken <rhyme label="c" type="m" corresp="#s4:l5">Medecijn</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Tusschen die purpre <rhyme label="d" type="f">wallen</rhyme>,</l>
        <l id="s4:l8" met="4" type="-+">En suyghen uyt die lieve <rhyme label="e" type="m">mondt</rhyme></l>
        <l id="s4:l9" met="4" type="-+">De Nectar, lavingh van mijn <rhyme label="e" type="m" corresp="#s4:l8">wondt</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Daer d'Hemel mee sou <rhyme label="d" type="f" corresp="#s4:l7">brallen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Dus komt het dart'le Musje <rhyme label="a" type="m">we'er</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van boven ne'er <rhyme label="b" type="f">ghezeghen</rhyme>.</l>
        <l id="s5:l3" met="4" type="-+">Help, roept sy, ach! wie krenckt mijn <rhyme label="a" type="m" corresp="#s5:l1">eer</rhyme>?</l>
        <l id="s5:l4" met="3" type="-+">Ick ben om hulp <rhyme label="b" type="f" corresp="#s5:l2">verleghen</rhyme>;</l>
        <l id="s5:l5" met="4" type="-+">Ach, ach! wie steurt mijn zachte <rhyme label="c" type="m">rust</rhyme>?</l>
        <l id="s5:l6" met="4" type="-+">Houdt op, het is ghenoegh <rhyme label="c" type="m" corresp="#s5:l5">ghekust</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wie red' m' uyt dees <rhyme label="d" type="f">ellende</rhyme>?</l>
        <l id="s5:l8" met="4" type="-+">Dus roept sy, slapend', <rhyme label="e" type="m">overluydt</rhyme>,</l>
        <l id="s5:l9" met="4" type="-+">Ach helpt m', eer dat mijn hartje <rhyme label="e" type="m" corresp="#s5:l8">sluyt</rhyme>,</l>
        <l id="s5:l10" met="3" type="-+">Wie wou mijn eer daer <rhyme label="d" type="f" corresp="#s5:l7">schenden</rhyme>?</l>
      </lg>
    </lg>
  </text>
</song>
