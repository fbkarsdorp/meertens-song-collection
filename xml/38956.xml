<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>38956</id>
    <date>
      <year-start>1710</year-start>
      <year-end>1733</year-end>
    </date>
    <location>
      <orginal-name>brugge</orginal-name>
      <standard-name>Brugge, Belgium</standard-name>
      <coordinates>
        <lon>51.2315785</lon>
        <lat>3.20809951603418</lat>
      </coordinates>
    </location>
    <tags>
      <tag>christmas song</tag>
      <tag>praise</tag>
      <tag>spiritual song</tag>
    </tags>
    <melody>Herders hij is geboren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">O Prins van peys <rhyme label="a" type="f">geboren</rhyme>!</l>
        <l id="s1:l2" met="3" type="-+">O Vorst van <rhyme label="b" type="m">Israel</rhyme>!</l>
        <l id="s1:l3" met="3" type="-+">O koninck <rhyme label="a" type="f" corresp="#s1:l1">uytverkoren</rhyme>!</l>
        <l id="s1:l4" met="3" type="-+">O Godt <rhyme label="b" type="m" corresp="#s1:l2">Emmanuel</rhyme>!</l>
        <l id="s1:l5" met="3" type="-+">O Leytsman, herder <rhyme label="c" type="m">goet</rhyme>!</l>
        <l id="s1:l6" met="3" type="-+">O Kintjen honigh <rhyme label="c" type="m" corresp="#s1:l5">soet</rhyme>!</l>
        <l id="s1:l7" met="3" type="-+">O Soet onnoosel <rhyme label="d" type="m">Lam</rhyme>!</l>
        <l id="s1:l8" met="3" type="-+">O Blom van Iesses <rhyme label="d" type="m" corresp="#s1:l7">stam</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">O Abrahams saet <rhyme label="a" type="f">verheven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">O Godt <rhyme label="b" type="m">Adonai</rhyme>!</l>
        <l id="s2:l3" met="3" type="-+">O Heyl van ’t mensche <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme>!</l>
        <l id="s2:l4" met="3" type="-+">O Stam van <rhyme label="b" type="m" corresp="#s2:l2">Isai</rhyme>!</l>
        <l id="s2:l5" met="3" type="-+">O Davids vrucht, en <rhyme label="c" type="m">vreught</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">O Son die ’t al <rhyme label="c" type="m" corresp="#s2:l5">verheught</rhyme>!</l>
        <l id="s2:l7" met="3" type="-+">O Glans van ’t eeuwigh <rhyme label="d" type="m">licht</rhyme>!</l>
        <l id="s2:l8" met="3" type="-+">O Woordt dat alles <rhyme label="d" type="m" corresp="#s2:l7">sticht</rhyme>!</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">O Wegh tot al ons <rhyme label="a" type="f">wenschen</rhyme>!</l>
        <l id="s3:l2" met="3" type="-+">O Waerheydt, die ’t al <rhyme label="b" type="m">leert</rhyme>!</l>
        <l id="s3:l3" met="3" type="-+">O Leven van ons <rhyme label="a" type="f" corresp="#s3:l1">menschen</rhyme>!</l>
        <l id="s3:l4" met="3" type="-+">O Zee, daer ’t al <rhyme label="b" type="m" corresp="#s3:l2">toekeert</rhyme>!</l>
        <l id="s3:l5" met="3" type="-+">O Macht, die alles <rhyme label="c" type="m">doet</rhyme>!</l>
        <l id="s3:l6" met="3" type="-+">O Liefde vol van <rhyme label="c" type="m" corresp="#s3:l5">goet</rhyme>!</l>
        <l id="s3:l7" met="3" type="-+">O Wijsheydt die ’t al <rhyme label="d" type="m">raeckt</rhyme>!</l>
        <l id="s3:l8" met="3" type="-+">O Macht die alles <rhyme label="d" type="m" corresp="#s3:l7">maeckt</rhyme>!</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">O oorspronck van ons <rhyme label="a" type="f">leven</rhyme>!</l>
        <l id="s4:l2" met="3" type="-+">O Heyl van al dat <rhyme label="b" type="m">leeft</rhyme>!</l>
        <l id="s4:l3" met="3" type="-+">O Kintjen ons <rhyme label="a" type="f" corresp="#s4:l1">geheven</rhyme>!</l>
        <l id="s4:l4" met="3" type="-+">O Goetheydt die ’t al <rhyme label="b" type="m" corresp="#s4:l2">geeft</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">O Oogh die alles <rhyme label="c" type="m">siet</rhyme>!</l>
        <l id="s4:l6" met="3" type="-+">O Macht die ’t al <rhyme label="c" type="m" corresp="#s4:l5">gebiedt</rhyme>!</l>
        <l id="s4:l7" met="3" type="-+">O Al daer ’t al  in <rhyme label="d" type="m">leeft</rhyme>!</l>
        <l id="s4:l8" met="3" type="-+">O Al daer ’t al in <rhyme label="d" type="m" corresp="#s4:l7">sweeft</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Al, die ’t al komt <rhyme label="a" type="f">geven</rhyme>!</l>
        <l id="s5:l2" met="3" type="-+">O Grooten <rhyme label="b" type="m">Messias</rhyme>!</l>
        <l id="s5:l3" met="3" type="-+">O Schrik, die d’hel doet <rhyme label="a" type="f" corresp="#s5:l1">beven</rhyme>!</l>
        <l id="s5:l4" met="3" type="-+">O Heyl die ’t al <rhyme label="b" type="m" corresp="#s5:l2">genas</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">O aller volcken <rhyme label="c" type="m">wensch</rhyme>!</l>
        <l id="s5:l6" met="3" type="-+">O Licht van allen <rhyme label="c" type="m" corresp="#s5:l5">mensch</rhyme>!</l>
        <l id="s5:l7" met="3" type="-+">O Al ons <rhyme label="d" type="m">vroyelijckheydt</rhyme>!</l>
        <l id="s5:l8" met="3" type="-+">O Al ons <rhyme label="d" type="m" corresp="#s5:l7">saligheydt</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">O kleyntjen ons <rhyme label="a" type="f">gegeven</rhyme>!</l>
        <l id="s6:l2" met="3" type="-+">O Aldersoetste <rhyme label="b" type="m">kint</rhyme>!</l>
        <l id="s6:l3" met="3" type="-+">O Leytsman van ons <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme>!</l>
        <l id="s6:l4" met="3" type="-+">O Prins die ons <rhyme label="b" type="m" corresp="#s6:l2">bemint</rhyme>!</l>
        <l id="s6:l5" met="3" type="-+">O Hooghe <rhyme label="c" type="m">Majesteyt</rhyme>!</l>
        <l id="s6:l6" met="3" type="-+">O Groote <rhyme label="c" type="m" corresp="#s6:l5">Almogentheyt</rhyme>!</l>
        <l id="s6:l7" met="3" type="-+">O Goetheydt weert <rhyme label="d" type="m">bemint</rhyme>!</l>
        <l id="s6:l8" met="3" type="-+">O Godt, die wordt een <rhyme label="d" type="m" corresp="#s6:l7">Kindt</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Broodt van ’t eeuwigh <rhyme label="a" type="f">leven</rhyme>!</l>
        <l id="s7:l2" met="3" type="-+">O Broodt dat ’t leven <rhyme label="b" type="m">is</rhyme>!</l>
        <l id="s7:l3" met="3" type="-+">O Manna, recht <rhyme label="a" type="f" corresp="#s7:l1">gegeven</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">O Zielen <rhyme label="b" type="m" corresp="#s7:l2">laefvenis</rhyme>!</l>
        <l id="s7:l5" met="3" type="-+">O Herder, t’samen <rhyme label="c" type="m">spijs</rhyme>!</l>
        <l id="s7:l6" met="3" type="-+">O Teerprijs van ons <rhyme label="c" type="m" corresp="#s7:l5">reys</rhyme>!</l>
        <l id="s7:l7" met="3" type="-+">O Englen weerdigh <rhyme label="d" type="m">broodt</rhyme>!</l>
        <l id="s7:l8" met="3" type="-+">O Broodt dat weert de <rhyme label="d" type="m" corresp="#s7:l7">doodt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O Priester ende <rhyme label="a" type="f">mede</rhyme></l>
        <l id="s8:l2" met="3" type="-+">O Eeuwigh <rhyme label="b" type="m">Offerhand</rhyme>!</l>
        <l id="s8:l3" met="3" type="-+">O Offer van den <rhyme label="a" type="f" corresp="#s8:l1">vrede</rhyme>!</l>
        <l id="s8:l4" met="3" type="-+">O Liefde, vier en <rhyme label="b" type="m" corresp="#s8:l2">brandt</rhyme>!</l>
        <l id="s8:l5" met="3" type="-+">O Wijn die Maeghden <rhyme label="c" type="m">voedt</rhyme>!</l>
        <l id="s8:l6" met="3" type="-+">O Kilck van Godes <rhyme label="c" type="m" corresp="#s8:l5">bloedt</rhyme>!</l>
        <l id="s8:l7" met="3" type="-+">O Terwe van Godts <rhyme label="d" type="m">handt</rhyme>!</l>
        <l id="s8:l8" met="3" type="-+">O Eeuwigh levens <rhyme label="d" type="m" corresp="#s8:l7">pandt</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O Prins, die peys komt <rhyme label="a" type="f">geven</rhyme>!</l>
        <l id="s9:l2" met="3" type="-+">O Eewigh levens <rhyme label="b" type="m">bron</rhyme>!</l>
        <l id="s9:l3" met="3" type="-+">O Stierman van ons <rhyme label="a" type="f" corresp="#s9:l1">leven</rhyme>!</l>
        <l id="s9:l4" met="3" type="-+">O Luyster van de <rhyme label="b" type="m" corresp="#s9:l2">Son</rhyme>!</l>
        <l id="s9:l5" met="3" type="-+">O Soeten <rhyme label="c" type="m">Bruydegom</rhyme>?</l>
        <l id="s9:l6" met="3" type="-+">O Gever van <rhyme label="c" type="m" corresp="#s9:l5">Vrydom</rhyme>!</l>
        <l id="s9:l7" met="3" type="-+">O Prijs der sonden <rhyme label="d" type="m">boet</rhyme>!</l>
        <l id="s9:l8" met="3" type="-+">O Bron van alle <rhyme label="d" type="m" corresp="#s9:l7">goet</rhyme>!</l>
      </lg>
    </lg>
  </text>
</song>
