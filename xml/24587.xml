<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>24587</id>
    <date>
      <year-start>1476</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody/>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">HOege   vrouden synt hierbauen al inder <rhyme label="a" type="m">ewicheit</rhyme></l>
        <l id="s1:l2" met="6" type="-+">daer jhesus syn wtuercoren haer stede heeft <rhyme label="a" type="m" corresp="#s1:l1">bereyt</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Jhesus toe behaegen dat is al myne <rhyme label="a" type="m">begeerte</rhyme></l>
        <l id="s2:l2" met="6" type="-+">Alle   daege daer omme toe steruen dat is hi my wal <rhyme label="a" type="m" corresp="#s2:l1">weert</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Wie wil behaegen die sy daer toe <rhyme label="a" type="m">bereit</rhyme></l>
        <l id="s3:l2" met="6" type="-+">syn herte ledich toe maken van alre <rhyme label="a" type="m" corresp="#s3:l1">ydelheit</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Wie myt ihesus mynne van bynnen is omme <rhyme label="a" type="m">beuaen</rhyme></l>
        <l id="s4:l2" met="6" type="-+">Die mach syn lof wal synnen haer schult waert of <rhyme label="a" type="m" corresp="#s4:l1">gedaen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">Die ihesus mynne draeget die synt wal geheten <rhyme label="a" type="m">vroet</rhyme></l>
        <l id="s5:l2" met="6" type="-+">sie hebben wtuercoeren dat alre beste  <rhyme label="a" type="m" corresp="#s5:l1">guet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="-+">Wie suuer is van herten nae ihesus wille <rhyme label="a" type="m">bereit</rhyme></l>
        <l id="s6:l2" met="6" type="-+">die moet wt mynnen daelen in dieper <rhyme label="a" type="m" corresp="#s6:l1">oetmoedicheit</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="6" type="-+">Die hoeren eygen willen laten Ende ihesus syn   <rhyme label="a" type="m">onderdaen</rhyme></l>
        <l id="s7:l2" met="6" type="-+">gheen tonge en mach wt spreken wat loen sie soelen <rhyme label="a" type="m" corresp="#s7:l1">ontfaen</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="6" type="-+">Die ihesus mynne smaken den alre besten <rhyme label="a" type="m">wyn</rhyme></l>
        <l id="s8:l2" met="6" type="-+">Altyt is hoere begeerte in synen dienste toe <rhyme label="a" type="m" corresp="#s8:l1">syn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="6" type="-+">Jhesus an toescouwen in synre hoeger <rhyme label="a" type="m">moegentheit</rhyme></l>
        <l id="s9:l2" met="6" type="-+">Daer is myne verlangen Och   weer myne siele <rhyme label="a" type="m" corresp="#s9:l1">bereyt</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="6" type="-+">Wat vrouden   mach my hier lusten in desen yamerliken <rhyme label="a" type="m">dael</rhyme></l>
        <l id="s10:l2" met="6" type="-+">myn herte dat lopet van bynen tot ihesus altoe <rhyme label="a" type="m" corresp="#s10:l1">maele</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
