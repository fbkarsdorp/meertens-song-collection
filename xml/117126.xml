  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DAer waren vijf <rhyme label="a" type="f">Bagijntjes</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">die soude gaen vegen de <rhyme label="b" type="m">vloer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Sy vonden een Pape <rhyme label="a" type="f" corresp="#s1:l1">beulingsken</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gebonden al aen een <rhyme label="b" type="m" corresp="#s1:l2">snoer</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Hey wat een deun hadden sy daer <rhyme label="c" type="m">me</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Vijf Bagijntjes die waren <rhyme label="c" type="m" corresp="#s1:l5">geree</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s1:l7">groot</rhyme>,</l>
        <l id="s1:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s1:l7">groot</rhyme>,</l>
        <l id="s1:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Dat eerste <rhyme label="a" type="f">Bagijntje</rhyme></l>
        <l id="s2:l2" met="3" type="-+">dat was’er gekleet in ’t <rhyme label="b" type="m">wit</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Ba seyde sy pots <rhyme label="a" type="f" corresp="#s2:l1">jemeny</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wat voor een ding is <rhyme label="b" type="m" corresp="#s2:l2">dit</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">hey wat een deun had sy daer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Vyf Bagijntjes die waren <rhyme label="c" type="m" corresp="#s2:l5">geree</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s2:l7">groot</rhyme>,</l>
        <l id="s2:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s2:l7">groot</rhyme>,</l>
        <l id="s2:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Dat tweede <rhyme label="a" type="f">Bagijntje</rhyme></l>
        <l id="s3:l2" met="3" type="-+">was gekleet in ’t <rhyme label="b" type="m">root</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ba schaemde u niet <rhyme label="a" type="f" corresp="#s3:l1">Gesusterkens</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Laet Heerooms Beulingske so niet <rhyme label="b" type="m" corresp="#s3:l2">bloot</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">Hey wat een deun hadden sy daer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Vijf Bagijntjes die waren <rhyme label="c" type="m" corresp="#s3:l5">geree</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s3:l7">groot</rhyme>,</l>
        <l id="s3:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s3:l7">groot</rhyme>,</l>
        <l id="s3:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Dat derde <rhyme label="a" type="f">Bagijntje</rhyme></l>
        <l id="s4:l2" met="3" type="-+">dat was’er gekleet in ’t <rhyme label="b" type="m">groen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">ba seyde sy pots <rhyme label="a" type="f" corresp="#s4:l1">jemeny</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Sy nam het beulingske en gaft een <rhyme label="b" type="m" corresp="#s4:l2">soen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Hey wat een deun hadden sy daer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">Vijf Bagijntjes waren <rhyme label="c" type="m" corresp="#s4:l5">geree</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s4:l7">groot</rhyme>,</l>
        <l id="s4:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s4:l7">groot</rhyme>,</l>
        <l id="s4:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Dat vierde <rhyme label="a" type="f">Bagijntje</rhyme></l>
        <l id="s5:l2" met="3" type="-+">die was’er gekleet in ’t <rhyme label="b" type="m">zwart</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">ba seyde sy pots <rhyme label="a" type="f" corresp="#s5:l1">jemeny</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Hoe is dat Pape beulingske so <rhyme label="b" type="m" corresp="#s5:l2">hart</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Hey wat een deun hadden sy daer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Vijf Bagijntjes die waren <rhyme label="c" type="m" corresp="#s5:l5">geree</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s5:l7">groot</rhyme>,</l>
        <l id="s5:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s5:l7">groot</rhyme>,</l>
        <l id="s5:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dat vijfde <rhyme label="a" type="f">Bagijntje</rhyme></l>
        <l id="s6:l2" met="3" type="-+">was de jongste van ’t <rhyme label="b" type="m">Klooster</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">En die kreegh daer <rhyme label="a" type="f" corresp="#s6:l1">appety</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Sy braden het Beulingske op den <rhyme label="b" type="m" corresp="#s6:l2">rooster</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Hey wat een deun hadden sy daer <rhyme label="c" type="m">mee</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Vijf Bagyntjes waren <rhyme label="c" type="m" corresp="#s6:l5">geree</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">En gaede niet <rhyme label="d" type="m">groot</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s6:l7">groot</rhyme>,</l>
        <l id="s6:l9" met="2" type="-+">En gaede niet <rhyme label="d" type="m" corresp="#s6:l7">groot</rhyme>,</l>
        <l id="s6:l10" met="2" type="+-">Kom aenstonts <rhyme label="e" type="m">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
