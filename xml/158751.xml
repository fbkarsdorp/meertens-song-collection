<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>158751</id>
    <date>
      <year-start>1770</year-start>
      <year-end>1780</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Ik heb lange tijd bemind</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik heb lange tyd <rhyme label="a" type="m">bemind</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Een die ik niet wou <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Ik was door de liefde <rhyme label="a" type="m" corresp="#s1:l1">blind</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">En liep gelyk <rhyme label="a" type="m" corresp="#s1:l1">ontzind</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">‘t was om haer zoet <rhyme label="c" type="m">gelaet</rhyme></l>
        <l id="s1:l6" met="3" type="-+">En haer <rhyme label="d" type="f">bevalligheden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Maer ach ik vond geen <rhyme label="c" type="m" corresp="#s1:l5">baet</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Het was al verlooren <rhyme label="c" type="m" corresp="#s1:l5">oraet</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Vermoeyt van het <rhyme label="a" type="m">gevly</rhyme>.</l>
        <l id="s2:l2" met="3" type="-+">Dat myn kwam te <rhyme label="b" type="f">verdrieten</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van streelen en <rhyme label="a" type="m" corresp="#s2:l1">gevly</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Bragt myn in <rhyme label="a" type="m" corresp="#s2:l1">slaverny</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Ik zal dan voor myn <rhyme label="c" type="m">smart</rhyme>!</l>
        <l id="s2:l6" met="3" type="-+">Myn hert tot troost gaen <rhyme label="d" type="f">zoeken</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Indien myn <rhyme label="c" type="m" corresp="#s2:l5">minnepyn</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Een trooster zoude <rhyme label="c" type="m" corresp="#s2:l5">zijn</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Bacchus dat is een <rhyme label="a" type="m">Man</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Die myn in myn <rhyme label="b" type="f">elenden</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Een helper weezen <rhyme label="a" type="m" corresp="#s3:l1">kan</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Als hy geeft glas en <rhyme label="a" type="m" corresp="#s3:l1">kan</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Begeeft uw in de <rhyme label="c" type="m">wyn</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Die het alles kan <rhyme label="d" type="f">verkeeren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Hy doet ons vrolyk <rhyme label="c" type="m" corresp="#s3:l5">zyn</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Als hy geeft roode <rhyme label="c" type="m" corresp="#s3:l5">wyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Gy die dan nog <rhyme label="a" type="m">bemint</rhyme>:</l>
        <l id="s4:l2" met="3" type="-+">En geen liefden komt <rhyme label="b" type="f">verwerven</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Vlugt voor dat dertel <rhyme label="a" type="m" corresp="#s4:l1">kind</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Dat uw zoo zeer <rhyme label="a" type="m" corresp="#s4:l1">verblind</rhyme>;</l>
        <l id="s4:l5" met="3" type="-+">Verheugd uw tot d <rhyme label="c" type="m">ewyn</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Die het alles kan <rhyme label="d" type="f">verkeeren</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Hy doet ons vrolyk <rhyme label="c" type="m" corresp="#s4:l5">zyn</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Verdryft al onze <rhyme label="c" type="m" corresp="#s4:l5">pyn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Drinkt dan uit Bacchus <rhyme label="a" type="m">vat</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Syn aengename <rhyme label="b" type="f">vogten</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Het is zoo een edel <rhyme label="a" type="m" corresp="#s5:l1">nat</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Dat uw zoo milt <rhyme label="a" type="m" corresp="#s5:l1">bespat</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Drinkt in uw <rhyme label="c" type="m">tegenspoed</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Het nat van Bacchus <rhyme label="d" type="f">druyven</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dat zal uw weezen <rhyme label="c" type="m" corresp="#s5:l5">goed</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Syn Teugjes die zyn <rhyme label="c" type="m" corresp="#s5:l5">zoet</rhyme>:</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Ik heb daer nog <rhyme label="a" type="m">onteert</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Een dien ik niet mogt <rhyme label="b" type="f">trouwen</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Haer vader die kwam <rhyme label="a" type="m" corresp="#s6:l1">thuys</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Die maekt een groot <rhyme label="a" type="m" corresp="#s6:l1">gedruys</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">Hebt gy myn dogter <rhyme label="c" type="m">onteert</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Het zal uw wel <rhyme label="d" type="f">berouwen</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Ik zy wel ja myn <rhyme label="c" type="m" corresp="#s6:l5">Heer</rhyme>:</l>
        <l id="s6:l8" met="3" type="-+">Ik heb genomen haer <rhyme label="c" type="m" corresp="#s6:l5">Eer</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">De Vader sprak met <rhyme label="a" type="m">gram</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Laet je jou zoo <rhyme label="b" type="f">bedriegen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Van zulk een snoode <rhyme label="a" type="m" corresp="#s7:l1">Vent</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die je in ‘t minst niet <rhyme label="a" type="m" corresp="#s7:l1">kent</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">De dogter sprak met <rhyme label="c" type="m">spoet</rhyme>:</l>
        <l id="s7:l6" met="3" type="-+">En dat met zoete <rhyme label="d" type="f">reeden</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Ach Vader het deed ‘er myn <rhyme label="c" type="m" corresp="#s7:l5">goed</rhyme>:</l>
        <l id="s7:l8" met="3" type="-+">Het zoene dat is zoo <rhyme label="c" type="m" corresp="#s7:l5">zoet</rhyme>:</l>
      </lg>
    </lg>
  </text>
</song>
