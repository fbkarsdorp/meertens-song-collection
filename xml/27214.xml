  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Int hert ly ick van binnen / <rhyme label="a" type="m">pyn</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Dies heel onuroet die zinnen / <rhyme label="a" type="m" corresp="#s1:l1">zyn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Om v deuchtsaeme <rhyme label="b" type="m">maecht</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die listich hebt <rhyme label="b" type="m" corresp="#s1:l3">geiaecht</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Int vierich net van minnen / <rhyme label="a" type="m" corresp="#s1:l1">myn</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Dat Venus dede spinnen / <rhyme label="a" type="m" corresp="#s1:l1">fyn</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Twert Cupido <rhyme label="c" type="m">geclaecht</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Hoe deerlick ghy myn <rhyme label="c" type="m" corresp="#s1:l7">plaecht</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Gedoocht dat hy mach <rhyme label="d" type="f">schieten</rhyme></l>
        <l id="s1:l10" met="3" type="-+">In v der liefden <rhyme label="e" type="m">schicht</rhyme></l>
        <l id="s1:l11" met="3" type="+-">En laet my doch <rhyme label="d" type="f" corresp="#s1:l9">genieten</rhyme></l>
        <l id="s1:l12" met="3" type="-+">V vriendelick <rhyme label="e" type="m" corresp="#s1:l10">gesicht</rhyme></l>
        <l id="s1:l13" met="2" type="-+">Daer ghy den <rhyme label="f" type="m">brant</rhyme></l>
        <l id="s1:l14" met="2" type="-+">In menich <rhyme label="f" type="m" corresp="#s1:l13">quant</rhyme></l>
        <l id="s1:l15" met="3" type="-+">Wel haestelick mee <rhyme label="e" type="m" corresp="#s1:l10">sticht</rhyme></l>
        <l id="s1:l16" met="2" type="-+">Twelck is <rhyme label="f" type="m" corresp="#s1:l13">playsant</rhyme></l>
        <l id="s1:l17" met="2" type="-+">In v <rhyme label="f" type="m" corresp="#s1:l13">geplant</rhyme></l>
        <l id="s1:l18" met="3" type="-+">En glinstert als een <rhyme label="e" type="m" corresp="#s1:l10">licht</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waer door Ick met begeeren <rhyme label="a" type="m">goet</rhyme></l>
        <l id="s2:l2" met="4" type="-+">V altyt ionstich eeren <rhyme label="a" type="m" corresp="#s2:l1">moet</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Op hoop dat ghy int <rhyme label="b" type="m">ent</rhyme></l>
        <l id="s2:l4" met="3" type="-+">V straffe zinnen <rhyme label="b" type="m" corresp="#s2:l3">went</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Zoo ghy v tot verneeren <rhyme label="a" type="m" corresp="#s2:l1">spoet</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Vreucht zal in my vermeeren <rhyme label="a" type="m" corresp="#s2:l1">soet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">In steede van <rhyme label="c" type="m">elent</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Het welck my houdt <rhyme label="c" type="m" corresp="#s2:l7">verblint</rhyme></l>
        <l id="s2:l9" met="3" type="-+">In d'onbreckelicke <rhyme label="d" type="f">banden</rhyme></l>
        <l id="s2:l10" met="3" type="-+">Ist niet een swaere <rhyme label="e" type="m">last</rhyme></l>
        <l id="s2:l11" met="3" type="+-">Daer my tbegeert in <rhyme label="d" type="f" corresp="#s2:l9">spanden</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Tot v persoon seer <rhyme label="e" type="m" corresp="#s2:l10">vast</rhyme></l>
        <l id="s2:l13" met="2" type="-+">
          <rhyme label="f" type="m">Melancoly</rhyme>
        </l>
        <l id="s2:l14" met="2" type="-+">En vreuchts <rhyme label="f" type="m" corresp="#s2:l13">party</rhyme></l>
        <l id="s2:l15" met="3" type="-+">Dagelickx in my <rhyme label="e" type="m" corresp="#s2:l10">wast</rhyme></l>
        <l id="s2:l16" met="2" type="-+">Dies vindt ick <rhyme label="f" type="m" corresp="#s2:l13">my</rhyme></l>
        <l id="s2:l17" met="2" type="-+">Gedurich <rhyme label="f" type="m" corresp="#s2:l13">by</rhyme></l>
        <l id="s2:l18" met="3" type="-+">De slauerny <rhyme label="e" type="m" corresp="#s2:l10">vergast</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Toont aen my geen moordadich <rhyme label="a" type="m">feit</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Maer bewist v <rhyme label="a" type="m" corresp="#s3:l1">genadicheit</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Als een deuchtsaeme <rhyme label="b" type="m">vrou</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Iont haer minnaer <rhyme label="b" type="m" corresp="#s3:l3">getrou</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Nae troost myn hert gestadich <rhyme label="a" type="m" corresp="#s3:l1">beit</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Dat met druck zeer beladen <rhyme label="a" type="m" corresp="#s3:l1">leit</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Dat steruen sal van <rhyme label="c" type="m">rou</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Ten sy v reyn <rhyme label="c" type="m" corresp="#s3:l7">carcou</rhyme></l>
        <l id="s3:l9" met="3" type="-+">Dat ghy my wilt <rhyme label="d" type="f">geneesen</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Van dat door Venus <rhyme label="e" type="m">kint</rhyme></l>
        <l id="s3:l11" met="3" type="+-">In my hert is <rhyme label="d" type="f" corresp="#s3:l9">gereesen</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Daerom v wel <rhyme label="e" type="m" corresp="#s3:l10">besint</rhyme></l>
        <l id="s3:l13" met="2" type="-+">Waer toe dees <rhyme label="f" type="m">haet</rhyme></l>
        <l id="s3:l14" met="2" type="-+">Dat ghy dus <rhyme label="f" type="m" corresp="#s3:l13">quaet</rhyme></l>
        <l id="s3:l15" met="3" type="-+">Myn vrolickheit <rhyme label="e" type="m" corresp="#s3:l10">verslint</rhyme></l>
        <l id="s3:l16" met="2" type="-+">Daer naest Godt <rhyme label="f" type="m" corresp="#s3:l13">staet</rhyme></l>
        <l id="s3:l17" met="2" type="-+">Myn <rhyme label="f" type="m" corresp="#s3:l13">toeuerlaet</rhyme></l>
        <l id="s3:l18" met="3" type="-+">Op v myn lieff <rhyme label="e" type="m" corresp="#s3:l10">bemindt</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Princes neemt op dees Reden <rhyme label="a" type="m">acht</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Laet hebben myn gebeden <rhyme label="a" type="m" corresp="#s4:l1">cracht</rhyme></l>
        <l id="s4:l3" met="3" type="-+">By v soot mach <rhyme label="b" type="m">geschien</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Myn druck suldy doen <rhyme label="b" type="m" corresp="#s4:l3">vlien</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Om v loff te verbreeden <rhyme label="a" type="m" corresp="#s4:l1">tracht</rhyme></l>
        <l id="s4:l6" met="4" type="-+">In deesen daet geen vreeden <rhyme label="a" type="m" corresp="#s4:l1">slacht</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Maer wilt Ionst t'mynwaert <rhyme label="c" type="m">bien</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Dat ick mach vreucht <rhyme label="c" type="m" corresp="#s4:l7">bespien</rhyme></l>
        <l id="s4:l9" met="3" type="-+">En u weerliefde <rhyme label="d" type="f">verweruen</rhyme></l>
        <l id="s4:l10" met="3" type="-+">Van v met een clein <rhyme label="e" type="m">woort</rhyme></l>
        <l id="s4:l11" met="3" type="+-">Het welck ick licht can <rhyme label="d" type="f" corresp="#s4:l9">eruen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Soo ghy myn clacht <rhyme label="e" type="m" corresp="#s4:l10">verhoort</rhyme></l>
        <l id="s4:l13" met="2" type="-+">Hoop ick dat <rhyme label="f" type="m">dra</rhyme></l>
        <l id="s4:l14" met="2" type="-+">Wt v mont <rhyme label="f" type="m" corresp="#s4:l13">Iae</rhyme></l>
        <l id="s4:l15" met="3" type="-+">Schoon lieff sal coomen <rhyme label="e" type="m" corresp="#s4:l10">voort</rhyme></l>
        <l id="s4:l16" met="2" type="-+">Anders soo <rhyme label="f" type="m" corresp="#s4:l13">lae</rhyme></l>
        <l id="s4:l17" met="2" type="-+">Ick <rhyme label="f" type="m" corresp="#s4:l13">ongenae</rhyme></l>
        <l id="s4:l18" met="3" type="-+">En crig den doot aen <rhyme label="e" type="m" corresp="#s4:l10">boort</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
