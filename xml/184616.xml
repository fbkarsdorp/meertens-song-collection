<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>184616</id>
    <date>
      <year-start>1740</year-start>
      <year-end>1740</year-end>
    </date>
    <location>
      <orginal-name>leuven</orginal-name>
      <standard-name>Leuven, Belgium</standard-name>
      <coordinates>
        <lon>50.879202</lon>
        <lat>4.7011675</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Bénissez le seigneur suprême</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">GEbenedydt den Heer van <rhyme label="a" type="f">boven</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Gy kleyn Gevogelt in uw' <rhyme label="b" type="m">nest</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">In't koele wout singht om te <rhyme label="b" type="m" corresp="#s1:l2">best</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Godt moeten wy hier <rhyme label="a" type="f" corresp="#s1:l1">loven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Singht Nachtegaels, bemindt den <rhyme label="a" type="f">Heere</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Of al-te-saem, oft <rhyme label="b" type="m">overhandt</rhyme>;</l>
        <l id="s2:l3" met="4" type="-+">En dat den Echo van een <rhyme label="b" type="m" corresp="#s2:l2">kant</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Antwoordt: bemindt den <rhyme label="a" type="f" corresp="#s2:l1">Heere</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Droef Tortel-duyfken, stilt uw <rhyme label="a" type="f">klaegen</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Gebenedydt Godt, wonder <rhyme label="b" type="m">soet</rhyme>:</l>
        <l id="s3:l3" met="4" type="-+">Veel meer als gy, ick suchten <rhyme label="b" type="m" corresp="#s3:l2">moet</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Om d'ontrouw, die ick <rhyme label="a" type="f" corresp="#s3:l1">draege</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Gy Schaepkens doet de vreese <rhyme label="a" type="f">wycken</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">En looft den Herder, die u <rhyme label="b" type="m">weydt</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Siet hy in my uw <rhyme label="b" type="m" corresp="#s4:l2">soetigheydt</rhyme>?</l>
        <l id="s4:l4" met="3" type="-+">Ick groot verschil laet <rhyme label="a" type="f" corresp="#s4:l1">blycken</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gy weste windt, blaest sonder <rhyme label="a" type="f">stooren</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Gebenedydt Godt op het <rhyme label="b" type="m">Veldt</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">Soo menigh oogenblick men <rhyme label="b" type="m" corresp="#s5:l2">telt</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Doet synen Lof ons <rhyme label="a" type="f" corresp="#s5:l1">hooren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Kleyn Beexken, die het veldt doet <rhyme label="a" type="f">bloeyen</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Gebenedydt den Heer met <rhyme label="b" type="m">my</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Gelyck uw waeter loopt <rhyme label="b" type="m" corresp="#s6:l2">voorby</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Voorby sal alles <rhyme label="a" type="f" corresp="#s6:l1">vloeyen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Op dees schoon plaets geeft alles <rhyme label="a" type="f">Vruchten</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Ick sien hier Bloemen, en soet <rhyme label="b" type="m">kruyt</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Ick onvruchtbaeren boom moet <rhyme label="b" type="m" corresp="#s7:l2">uyt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Roep ick in menigh <rhyme label="a" type="f" corresp="#s7:l1">suchten</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Sier, wat dat ick hier wil gaen <rhyme label="a" type="f">schryven</rhyme>,</l>
        <l id="s8:l2" met="4" type="-+">Diep in de schors van desen <rhyme label="b" type="m">Boom</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Ontrent dees stille <rhyme label="b" type="m" corresp="#s8:l2">waeter-stroom</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Godt moet verwondert <rhyme label="a" type="f" corresp="#s8:l1">blyven</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Ick sien de schoone sterre <rhyme label="a" type="f">schynen</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Die vroegh en laet ons geeft haer <rhyme label="b" type="m">licht</rhyme>:</l>
        <l id="s9:l3" met="4" type="-+">O Godt, thoont oock uw klaer <rhyme label="b" type="m" corresp="#s9:l2">gesicht</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">En doet den doeck <rhyme label="a" type="f" corresp="#s9:l1">verdwynen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">O soeten luyster, schoone <rhyme label="a" type="f">bloemen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Die voor een dagh wy op sien <rhyme label="b" type="m">staen</rhyme>:</l>
        <l id="s10:l3" met="4" type="-+">Misschien eer gy, moet ick <rhyme label="b" type="m" corresp="#s10:l2">vergaen</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Ah, waer op kan ick <rhyme label="a" type="f" corresp="#s10:l1">roemen</rhyme>!</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">De blixems, en de <rhyme label="a" type="f">donder-slaegen</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">Te kennen geven syne <rhyme label="b" type="m">macht</rhyme>:</l>
        <l id="s11:l3" met="4" type="-+">Godt kan den sondaer door syn <rhyme label="b" type="m" corresp="#s11:l2">kracht</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Tot asschen toe <rhyme label="a" type="f" corresp="#s11:l1">verslaegen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Onstilbaer Zee, onstuyme <rhyme label="a" type="f">baeren</rhyme>,</l>
        <l id="s12:l2" met="4" type="-+">Ick moet wel beven voor den <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s12:l3" met="4" type="-+">Als gloeyt syn rasérny een <rhyme label="b" type="m" corresp="#s12:l2">keer</rhyme>,</l>
        <l id="s12:l4" met="3" type="-+">En sal hy oock niet <rhyme label="a" type="f" corresp="#s12:l1">spaeren</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">Dat in hun loop de snel <rhyme label="a" type="f">rievieren</rhyme>,</l>
        <l id="s13:l2" met="4" type="-+">(Riep ick in mynen drift een <rhyme label="b" type="m">keer</rhyme>)</l>
        <l id="s13:l3" met="4" type="-+">Ist dat ick u vergram, ô <rhyme label="b" type="m" corresp="#s13:l2">Heer</rhyme>,</l>
        <l id="s13:l4" met="3" type="-+">Sich naer hun oorspronck <rhyme label="a" type="f" corresp="#s13:l1">stieren</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Rievieren loopt uyt al uw' <rhyme label="a" type="f">krachten</rhyme>,</l>
        <l id="s14:l2" met="4" type="-+">En keert naer uwen oorspronck <rhyme label="b" type="m">weer</rhyme>,</l>
        <l id="s14:l3" met="4" type="-+">Herneemt den vloet, en daelt niet <rhyme label="b" type="m" corresp="#s14:l2">neer</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Ick sondigh dagh en <rhyme label="a" type="f" corresp="#s14:l1">nachten</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">Dat Sonn', en Maen, dat al die <rhyme label="a" type="f">leven</rhyme>,</l>
        <l id="s15:l2" met="4" type="-+">Dat al de vruchten op het <rhyme label="b" type="m">veldt</rhyme>,</l>
        <l id="s15:l3" met="4" type="-+">Dat al de Visschen, die men <rhyme label="b" type="m" corresp="#s15:l2">telt</rhyme>,</l>
        <l id="s15:l4" met="3" type="-+">En schepsels lof u <rhyme label="a" type="f" corresp="#s15:l1">geven</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Gelyck men siet naer't waeter <rhyme label="a" type="f">haeken</rhyme></l>
        <l id="s16:l2" met="4" type="-+">Den Hert, heel dorstigh en <rhyme label="b" type="m">vermoeyt</rhyme>;</l>
        <l id="s16:l3" met="4" type="-+">Tot u, ô Heer, myn ziel soo <rhyme label="b" type="m" corresp="#s16:l2">spoeyt</rhyme>,</l>
        <l id="s16:l4" met="3" type="-+">Laet my u soetheydt <rhyme label="a" type="f" corresp="#s16:l1">smaeken</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="4" type="-+">O Godt, de hôop' van alle <rhyme label="a" type="f">gader</rhyme>,</l>
        <l id="s17:l2" met="4" type="-+">Ey thoont my uw' <rhyme label="b" type="m">bermhertigheydt</rhyme>;</l>
        <l id="s17:l3" met="4" type="-+">k'Ben sondaer vol <rhyme label="b" type="m" corresp="#s17:l2">ondanckbaerheydt</rhyme>,</l>
        <l id="s17:l4" met="3" type="-+">Maer gy syt mynen <rhyme label="a" type="f" corresp="#s17:l1">Vader</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
