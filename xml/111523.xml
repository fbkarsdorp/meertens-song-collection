    <categories>
      <category translation="children">kinderen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">JUffertje, 'k bid eens <rhyme label="a" type="m">segt</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s1:l3" met="3" type="-+">En biegd my eens <rhyme label="a" type="m" corresp="#s1:l1">opregt</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Wenst gy niet nevens <rhyme label="c" type="m">my</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s1:l2">la</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Te trouwen? Om eens te <rhyme label="c" type="m" corresp="#s1:l5">zyn</rhyme>,</l>
        <l id="s1:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s1:l2">za</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik loof het zeker <rhyme label="a" type="m">wel</rhyme>,</l>
        <l id="s2:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s2:l3" met="3" type="-+">Is 't zo? kom an ik <rhyme label="a" type="m" corresp="#s2:l1">zel</rhyme>,</l>
        <l id="s2:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s2:l2">la</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">U toonen als een <rhyme label="c" type="m">Man</rhyme>,</l>
        <l id="s2:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s2:l2">la</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Dat ik maar deftig <rhyme label="c" type="m" corresp="#s2:l5">kan</rhyme>,</l>
        <l id="s2:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s2:l2">za</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Maar hoe! word gy <rhyme label="a" type="m">beschaamd</rhyme>!</l>
        <l id="s3:l2" met="2" type="+-">Ta la <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s3:l3" met="3" type="-+">Dat 's iets dat niet <rhyme label="a" type="m" corresp="#s3:l1">betaamd</rhyme>,</l>
        <l id="s3:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s3:l2">la</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Och, och, ick zie het <rhyme label="c" type="m">wel</rhyme>,</l>
        <l id="s3:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s3:l2">la</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Gy weet noch niets van 't <rhyme label="c" type="m" corresp="#s3:l5">Spel</rhyme>,</l>
        <l id="s3:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s3:l2">za</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Waar 't u bekend <rhyme label="a" type="m">Juffrouw</rhyme>,</l>
        <l id="s4:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s4:l3" met="3" type="-+">Gelyk aan my; gy <rhyme label="a" type="m" corresp="#s4:l1">zou</rhyme>,</l>
        <l id="s4:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s4:l2">la</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Seggen, kom nog eens <rhyme label="c" type="m">an</rhyme>,</l>
        <l id="s4:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s4:l2">la</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Het zoete deuntje <rhyme label="c" type="m" corresp="#s4:l5">van</rhyme>,</l>
        <l id="s4:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s4:l2">za</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Denkt g dat 'k u <rhyme label="a" type="m">bedrieg</rhyme>,</l>
        <l id="s5:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s5:l3" met="3" type="-+">Of ik hier wat sta en <rhyme label="a" type="m" corresp="#s5:l1">lieg</rhyme>:</l>
        <l id="s5:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s5:l2">la</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Neen zeker ryge <rhyme label="c" type="m">by</rhyme>,</l>
        <l id="s5:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s5:l2">la</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Geen zoeter vreugd, als <rhyme label="c" type="m" corresp="#s5:l5">in</rhyme>,</l>
        <l id="s5:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s5:l2">za</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maar geeft gy geen <rhyme label="a" type="m">geloof</rhyme>,</l>
        <l id="s6:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s6:l3" met="3" type="-+">En blyft gy voor my <rhyme label="a" type="m" corresp="#s6:l1">doof</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s6:l2">la</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Smaakt gy nooit 't zoet der <rhyme label="c" type="m">Min</rhyme>,</l>
        <l id="s6:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s6:l2">la</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Het welk dat huysvest <rhyme label="c" type="m" corresp="#s6:l5">in</rhyme>,</l>
        <l id="s6:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s6:l2">za</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Daarom ziet wat gy <rhyme label="a" type="m">doet</rhyme>,</l>
        <l id="s7:l2" met="2" type="+-">Ta, la, <rhyme label="b" type="m">la</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">Verwerp dog niet het <rhyme label="a" type="m" corresp="#s7:l1">zoet</rhyme>,</l>
        <l id="s7:l4" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s7:l2">la</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Het welk gy <rhyme label="c" type="m">Leonoor</rhyme>,</l>
        <l id="s7:l6" met="2" type="+-">Ta, la, <rhyme label="b" type="m" corresp="#s7:l2">la</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Niet krygen kund als <rhyme label="c" type="m" corresp="#s7:l5">door</rhyme>,</l>
        <l id="s7:l8" met="2" type="+-">Houp za <rhyme label="b" type="m" corresp="#s7:l2">za</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
