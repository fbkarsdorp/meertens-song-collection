  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AFgodinne van mijn <rhyme label="a" type="f">leeven</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Boven-aardsche <rhyme label="b" type="m">Maagde-siel</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Cierlijk Nymfje, hoog <rhyme label="a" type="f" corresp="#s1:l1">verheeven</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Deugde-bag, die my <rhyme label="b" type="m" corresp="#s1:l2">beviel</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Eerbaar weesen na <rhyme label="c" type="f">behooren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Fraeye schoon- en <rhyme label="d" type="m">aardig-heyd</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Goede, vroome <rhyme label="c" type="f" corresp="#s1:l5">uytverkooren</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Heemelrijk my <rhyme label="d" type="m" corresp="#s1:l6">toe-bereyd</rhyme>,</l>
        <l id="s1:l9" met="4" type="+-">Heemelrijk my <rhyme label="d" type="m" corresp="#s1:l6">toe-bereyd</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ionge bloem vol <rhyme label="a" type="f">seedigheeden</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Kroone-draagster van mijn <rhyme label="b" type="m">hart</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Luyster onser Eew en <rhyme label="a" type="f" corresp="#s2:l1">Steeden</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Meesteresse van mijn <rhyme label="b" type="m" corresp="#s2:l2">smart</rhyme>,</l>
        <l id="s2:l5" met="4" type="+-">Noord-star van verliefde <rhyme label="c" type="f">sinnen</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Oogen-troost voor my <rhyme label="d" type="m">gesond</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Parel waardig om te <rhyme label="c" type="f" corresp="#s2:l5">minnen</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Quetster van gewenste <rhyme label="d" type="m" corresp="#s2:l6">wond</rhyme>,</l>
        <l id="s2:l9" met="4" type="+-">Quetster van gewenste <rhyme label="d" type="m" corresp="#s2:l6">wond</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Reyne, wijse, soeten <rhyme label="a" type="f">Engel</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Schoone, lieve, waarde <rhyme label="b" type="m">Bout</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Toon uw Min, daar ik na <rhyme label="a" type="f" corresp="#s3:l1">hengel</rhyme>.</l>
        <l id="s3:l4" met="4" type="+-">U, so lang men 't leeven <rhyme label="b" type="m" corresp="#s3:l2">houd</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Wil ik kuys en vast <rhyme label="c" type="f">beminnen</rhyme>.</l>
        <l id="s3:l6" met="4" type="+-">Xal, dat sweer ik siel en <rhyme label="d" type="m">lijf</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Yslijk in de Hel <rhyme label="c" type="f" corresp="#s3:l5">verslinnen</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Zo ik u niet trow en <rhyme label="d" type="m" corresp="#s3:l6">blijf</rhyme>.</l>
        <l id="s3:l9" met="4" type="+-">Zo ik u niet trow en <rhyme label="d" type="m" corresp="#s3:l6">blijf</rhyme>.</l>
      </lg>
