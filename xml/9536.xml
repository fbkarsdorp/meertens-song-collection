<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9536</id>
    <date>
      <year-start>1650</year-start>
      <year-end>1650</year-end>
    </date>
    <location>
      <orginal-name>medemblik</orginal-name>
      <standard-name>Medemblik, The Netherlands</standard-name>
      <coordinates>
        <lon>52.768691</lon>
        <lat>5.106195</lat>
      </coordinates>
    </location>
    <tags>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Als mijn lief zich had wedergekeerd</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">ALs mijne <rhyme label="a" type="f">Lieve</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sich weder had' <rhyme label="b" type="m">gekeert</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">'t Mijnen <rhyme label="a" type="f" corresp="#s1:l1">gherieve</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Soo mijne Ziel <rhyme label="b" type="m" corresp="#s1:l2">begeerd'</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">'k Seyd' hy is de <rhyme label="c" type="m">mijn</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">En ick ben de <rhyme label="c" type="m" corresp="#s1:l5">sijn</rhyme>:</l>
        <l id="s1:l7" met="2" type="-+">Het lichte 't <rhyme label="d" type="f">Ghesichte</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">'t Gewichte der <rhyme label="c" type="m" corresp="#s1:l5">pijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Sijn stadigh <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Van my, als sijne <rhyme label="b" type="m">Bruyt</rhyme>,</l>
        <l id="s2:l3" met="2" type="-+">Wil ick <rhyme label="a" type="f" corresp="#s2:l1">besinnen</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En roepen <rhyme label="b" type="m" corresp="#s2:l2">over-luyt</rhyme>;</l>
        <l id="s2:l5" met="3" type="+-">Al heb ick <rhyme label="c" type="m">misdaen</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Noch sal hy <rhyme label="c" type="m" corresp="#s2:l5">voortaen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">De sporen <rhyme label="d" type="f">Verkoren</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">Als voren <rhyme label="c" type="m" corresp="#s2:l5">beslaen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Laet sonder <rhyme label="a" type="f">wachten</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Ons gaen te velden <rhyme label="b" type="m">he'en</rhyme>,</l>
        <l id="s3:l3" met="2" type="-+">Lief, en <rhyme label="a" type="f" corresp="#s3:l1">vernachten</rhyme></l>
        <l id="s3:l4" met="3" type="-+">In Dorpe dicht by <rhyme label="b" type="m" corresp="#s3:l2">een</rhyme>;</l>
        <l id="s3:l5" met="3" type="+-">Om 't werckje te <rhyme label="c" type="m">sien</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Van <rhyme label="c" type="m" corresp="#s3:l5">d'Ackere-lien</rhyme>:</l>
        <l id="s3:l7" met="2" type="-+">De wegen, Met <rhyme label="d" type="f">regen</rhyme></l>
        <l id="s3:l8" met="2" type="-+">En zegen <rhyme label="c" type="m" corresp="#s3:l5">ghemien</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Laet onse <rhyme label="a" type="f">voeten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Tot in het <rhyme label="b" type="m">Bergen-hof</rhyme></l>
        <l id="s4:l3" met="2" type="-+">Den Wijn-stock <rhyme label="a" type="f" corresp="#s4:l1">groeten</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">En soecken <rhyme label="b" type="m" corresp="#s4:l2">Druyve-lof</rhyme>.</l>
        <l id="s4:l5" met="3" type="+-">Besien het <rhyme label="c" type="m">ghesnoey</rhyme></l>
        <l id="s4:l6" met="3" type="+-">En 't <rhyme label="c" type="m" corresp="#s4:l5">Boome-gebloey</rhyme>:</l>
        <l id="s4:l7" met="2" type="-+">De koppen, De <rhyme label="d" type="f">knoppen</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">De kroppen, 't <rhyme label="c" type="m" corresp="#s4:l5">ghegroey</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">'t Wassen der <rhyme label="a" type="f">Kercken</rhyme></l>
        <l id="s5:l2" met="3" type="-+">In trou en in <rhyme label="b" type="m">ghetal</rhyme>,</l>
        <l id="s5:l3" met="2" type="-+">Soo vast <rhyme label="a" type="f" corresp="#s5:l1">verstercken</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Dan onse Ziele <rhyme label="b" type="m" corresp="#s5:l2">sal</rhyme>;</l>
        <l id="s5:l5" met="3" type="+-">Dat geene sich <rhyme label="c" type="m">scahem</rhyme></l>
        <l id="s5:l6" met="3" type="+-">'t Vereene te <rhyme label="c" type="m" corresp="#s5:l5">saem</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Van binnen In <rhyme label="d" type="f">sinnen</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">En minnen <rhyme label="c" type="m" corresp="#s5:l5">bequaem</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Dan sal ick <rhyme label="a" type="f">geven</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Mijn over-soete <rhyme label="b" type="m">min</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Dan sal ick <rhyme label="a" type="f" corresp="#s6:l1">leven</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met een vernoeghde <rhyme label="b" type="m" corresp="#s6:l2">sin</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">'t Geluckigh <rhyme label="c" type="m">gebouw</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Van onse <rhyme label="c" type="m" corresp="#s6:l5">gehouw</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Geresen, <rhyme label="d" type="f">Ghepresen</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Sal wesen <rhyme label="c" type="m" corresp="#s6:l5">ghetrouw</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">De <rhyme label="a" type="f">Dudaim-bloemen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Van aenghename <rhyme label="b" type="m">reuck</rhyme>,</l>
        <l id="s7:l3" met="2" type="-+">Deuchd' wijse <rhyme label="a" type="f" corresp="#s7:l1">noemen</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Die legghen in de <rhyme label="b" type="m" corresp="#s7:l2">meuck</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Ey! sie voor de <rhyme label="c" type="m">deur</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Is allerley <rhyme label="c" type="m" corresp="#s7:l5">geur</rhyme></l>
        <l id="s7:l7" met="2" type="-+">Der luchten, <rhyme label="d" type="f">Genuchten</rhyme></l>
        <l id="s7:l8" met="2" type="-+">Van vruchten, <rhyme label="c" type="m" corresp="#s7:l5">gescheur</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Siet allen <rhyme label="a" type="f">desen</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Heb ick'er <rhyme label="b" type="m">wegh-geleyt</rhyme>,</l>
        <l id="s8:l3" met="2" type="-+">Datter <rhyme label="a" type="f" corresp="#s8:l1">ghepresen</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Zy uwe <rhyme label="b" type="m" corresp="#s8:l2">gunstigheyt</rhyme>.</l>
        <l id="s8:l5" met="3" type="+-">Ia dat het <rhyme label="c" type="m">gheral</rhyme></l>
        <l id="s8:l6" met="3" type="+-">Van 't heyligh <rhyme label="c" type="m" corresp="#s8:l5">geschal</rhyme>,</l>
        <l id="s8:l7" met="2" type="-+">Ter eere Des <rhyme label="d" type="f">Heere</rhyme></l>
        <l id="s8:l8" met="2" type="-+">Vermeere, by <rhyme label="c" type="m" corresp="#s8:l5">al</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
