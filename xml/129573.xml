<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>129573</id>
    <date>
      <year-start>1708</year-start>
      <year-end>1708</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>dialog song</tag>
      <tag>secular song</tag>
    </tags>
    <melody>Wel Mieke stout</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="-+">‘Wel, mieken <rhyme label="a" type="m">stout</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Gy hebt u tijd <rhyme label="b" type="f">versleten</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En door de min <rhyme label="b" type="f" corresp="#s1:l2">vergeten</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Het welck u wel <rhyme label="a" type="m" corresp="#s1:l1">roud</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">Dat gy van dezen <rhyme label="c" type="m">post</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Niet en kont zijn <rhyme label="c" type="m" corresp="#s1:l5">verlost</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Dat heeft u van zijn <rhyme label="d" type="f">leven</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Soo meenigen traen <rhyme label="c" type="m" corresp="#s1:l5">gekost</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="-+">Wat droevig <rhyme label="a" type="m">kruis</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Elenden en wat <rhyme label="b" type="f">plagen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Moet gy nu al <rhyme label="b" type="f" corresp="#s2:l2">verdragen</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Hier in dit <rhyme label="a" type="m" corresp="#s2:l1">Spinhuis</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Want Water en droog <rhyme label="c" type="m">Brood</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Dat krijgt gy in den <rhyme label="c" type="m" corresp="#s2:l5">nood</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Om uwen dorst te <rhyme label="d" type="f">laven</rhyme></l>
        <l id="s2:l8" met="3" type="-+">En den Honger <rhyme label="c" type="m" corresp="#s2:l5">groot</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="-+">Nu zit gy <rhyme label="a" type="m">daar</rhyme></l>
        <l id="s3:l2" met="3" type="-+">In een zoo groot <rhyme label="b" type="f">verlangen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Met tranen op de <rhyme label="b" type="f" corresp="#s3:l2">wangen</rhyme>,</l>
        <l id="s3:l4" met="3" type="+-">En maakt groot <rhyme label="a" type="m" corresp="#s3:l1">misbaer</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">En gy, ellendig <rhyme label="c" type="m">Dier</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Sit daar zonder <rhyme label="c" type="m" corresp="#s3:l5">playzier</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Gy plagt te <rhyme label="d" type="f">dommineeren</rhyme></l>
        <l id="s3:l8" met="3" type="-+">En naemt uwen <rhyme label="c" type="m" corresp="#s3:l5">zwier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="-+">Den zoeten <rhyme label="a" type="m">tijdt</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Die meenig mens doet <rhyme label="b" type="f">branden</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Die hebt gy nu voor <rhyme label="b" type="f" corresp="#s4:l2">handen</rhyme>.</l>
        <l id="s4:l4" met="3" type="+-">Denckt eens wat een <rhyme label="a" type="m" corresp="#s4:l1">spijt</rhyme>!</l>
        <l id="s4:l5" met="3" type="-+">En uw jonge jeugt <rhyme label="c" type="m">vergaat</rhyme></l>
        <l id="s4:l6" met="3" type="-+">In desen droeven <rhyme label="c" type="m" corresp="#s4:l5">staet</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Soo dat nog lang moet <rhyme label="d" type="f">dueren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Gy wort <rhyme label="c" type="m" corresp="#s4:l5">desperaat</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="-+">Gy plagt te <rhyme label="a" type="m">zijn</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Heele dag op Venus <rhyme label="b" type="f">Schoolen</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Met Bassen en <rhyme label="b" type="f" corresp="#s5:l2">Fiolen</rhyme>.</l>
        <l id="s5:l4" met="3" type="+-">Is het niet een <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme>!</l>
        <l id="s5:l5" met="3" type="-+">En gy, elendig <rhyme label="c" type="m">dier</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Sit hier nou sonder <rhyme label="c" type="m" corresp="#s5:l5">plaisier</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Gy plagt te <rhyme label="d" type="f">dommineren</rhyme></l>
        <l id="s5:l8" met="3" type="-+">En naemt u <rhyme label="c" type="m" corresp="#s5:l5">pleysier</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="-+">Gy, meisje <rhyme label="a" type="m">wijs</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">‘t Is voor u quade <rhyme label="b" type="f">plagen</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Dat gy waert alle <rhyme label="b" type="f" corresp="#s6:l2">dagen</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Dronken, sat en <rhyme label="a" type="m" corresp="#s6:l1">stijf</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">En alle uwen <rhyme label="c" type="m">zin</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Die trekt u tot de <rhyme label="c" type="m" corresp="#s6:l5">Min</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Die gy nu al moet <rhyme label="d" type="f">derven</rhyme></l>
        <l id="s6:l8" met="3" type="-+">Met een droeven <rhyme label="c" type="m" corresp="#s6:l5">zin</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="-+">Is ‘t niet een <rhyme label="a" type="m">pyn</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Dat gy moet zitten <rhyme label="b" type="f">hiere</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Daar gy soo plagt te <rhyme label="b" type="f" corresp="#s7:l2">zwiere</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Met de Jongmans <rhyme label="a" type="m" corresp="#s7:l1">fijn</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">En al uwen <rhyme label="c" type="m">zin</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Die treckt u tot de <rhyme label="c" type="m" corresp="#s7:l5">min</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Die gy nu al moet <rhyme label="d" type="f">derven</rhyme></l>
        <l id="s7:l8" met="3" type="-+">Met een droeve <rhyme label="c" type="m" corresp="#s7:l5">zin</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="2" type="-+">Wel, schoone <rhyme label="a" type="m">maegt</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Ik bid, houd op van <rhyme label="b" type="f">klagen</rhyme>.</l>
        <l id="s8:l3" met="3" type="-+">Ik zal in korte <rhyme label="b" type="f" corresp="#s8:l2">dagen</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Helpen van de <rhyme label="a" type="m" corresp="#s8:l1">plaeg</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Want u <rhyme label="c" type="m">lieftaligheid</rhyme></l>
        <l id="s8:l6" met="3" type="-+">Heeft mijn nu haast   <rhyme label="c" type="m" corresp="#s8:l5">verleid</rhyme>:</l>
        <l id="s8:l7" met="3" type="-+">Ik zal u dienaar <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s8:l8" met="3" type="-+">Tot de dood ons <rhyme label="c" type="m" corresp="#s8:l5">scheid</rhyme>.’</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="2" type="-+">‘Ach, Jongman <rhyme label="a" type="m">fijn</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Woud gy mijn verlosser <rhyme label="b" type="f">zijn</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Ik wou u by nacht en <rhyme label="b" type="f" corresp="#s9:l2">dagen</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Altijd danckbaer <rhyme label="a" type="m" corresp="#s9:l1">zijn</rhyme>.</l>
        <l id="s9:l5" met="3" type="-+">Als gy mijn hier uit <rhyme label="c" type="m">trouwt</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Wilt gy niet zijn <rhyme label="c" type="m" corresp="#s9:l5">benouwt</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Het sal u waardig <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s9:l8" met="3" type="-+">Duysent Guldens <rhyme label="c" type="m" corresp="#s9:l5">Goud</rhyme>.’</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="2" type="-+">‘Mieke, schoone   <rhyme label="a" type="m">blom</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Kom, mijne wel <rhyme label="b" type="f">beminde</rhyme>,</l>
        <l id="s10:l3" met="3" type="-+">Ik zal mijn aen u <rhyme label="b" type="f" corresp="#s10:l2">verbinde</rhyme></l>
        <l id="s10:l4" met="3" type="+-">En zijn u <rhyme label="a" type="m" corresp="#s10:l1">Bruidegom</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Komt, geeft mijn maer u <rhyme label="c" type="m">hand</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">U trouw tot <rhyme label="c" type="m" corresp="#s10:l5">onderpand</rhyme>,</l>
        <l id="s10:l7" met="3" type="-+">Mijn Engel, mijn <rhyme label="d" type="f">beminde</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">Schoonste van het <rhyme label="c" type="m" corresp="#s10:l5">Landt</rhyme>.’</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="2" type="-+">‘Oorlof, nu <rhyme label="a" type="m">ziet</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Gy blijft hier in <rhyme label="b" type="f">elenden</rhyme>.</l>
        <l id="s11:l3" met="3" type="-+">Voor u en zijn geen <rhyme label="b" type="f" corresp="#s11:l2">Vrienden</rhyme>,</l>
        <l id="s11:l4" met="3" type="+-">Het is al om <rhyme label="a" type="m" corresp="#s11:l1">niet</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Het is met u <rhyme label="c" type="m">gedaen</rhyme>:</l>
        <l id="s11:l6" met="3" type="-+">Gy moet U vuyl <rhyme label="c" type="m" corresp="#s11:l5">lighaam</rhyme></l>
        <l id="s11:l7" met="3" type="-+">In droefheid en <rhyme label="d" type="f">elenden</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Soo versleijten <rhyme label="c" type="m" corresp="#s11:l5">gaen</rhyme>.’</l>
      </lg>
    </lg>
  </text>
</song>
