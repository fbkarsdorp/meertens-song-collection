<song>
  <meta>
    <id>34430</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>prayer song</tag>
      <tag>Maria song</tag>
    </tags>
    <melody>Kindje zoet en uitverkoren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">WEest ghegroet Maria <rhyme label="a" type="f">reyne</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Vol van gratie <rhyme label="b" type="m">altijdt</rhyme>;</l>
        <l id="s1:l3" met="4" type="+-">Weest ghegroet suyver <rhyme label="a" type="f" corresp="#s1:l1">fonteyne</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Onsen toevlucht inden <rhyme label="b" type="m" corresp="#s1:l2">strijdt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Weest ghegroet Maria <rhyme label="c" type="m">soet</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Die ghebaert hebt t'hooghste <rhyme label="c" type="m" corresp="#s1:l5">goet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Weerde Bruydt van Godt <rhyme label="a" type="f">Almachtigh</rhyme></l>
        <l id="s2:l2" met="4" type="+-">Die u uytverkooren <rhyme label="b" type="m">heeft</rhyme>;</l>
        <l id="s2:l3" met="4" type="+-">Uwe macht die is soo <rhyme label="a" type="f" corresp="#s2:l1">krachtigh</rhyme></l>
        <l id="s2:l4" met="4" type="+-">Dat g'aen dooden t'leven <rhyme label="b" type="m" corresp="#s2:l2">gheeft</rhyme></l>
        <l id="s2:l5" met="4" type="+-">En den Heer met u <rhyme label="c" type="m">altijdt</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Heeft u van d'erf-sond' <rhyme label="c" type="m" corresp="#s2:l5">bevrijdt</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Sijt ghebenedijdt <rhyme label="a" type="f">verheven</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Boven all' Vrouwen <rhyme label="b" type="m">gheslaght</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Uwe vrucht daer oock <rhyme label="a" type="f" corresp="#s3:l1">beneven</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Sy gheheylight door Godts <rhyme label="b" type="m" corresp="#s3:l2">kracht</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">Sijt altijdt <rhyme label="c" type="m">ghebenedijdt</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Nu, tot inder <rhyme label="c" type="m" corresp="#s3:l5">eeuwighijdt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Maria Princes <rhyme label="a" type="f">verkooren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Moeder vanden Schepper <rhyme label="b" type="m">al</rhyme></l>
        <l id="s4:l3" met="4" type="+-">Bidt voor ons die sijn <rhyme label="a" type="f" corresp="#s4:l1">verlooren</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Door den droeven Adams <rhyme label="b" type="m" corresp="#s4:l2">val</rhyme>:</l>
        <l id="s4:l5" met="4" type="+-">Nu, en in onss' laeste <rhyme label="c" type="m">ur</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Weest ons eenen vasten <rhyme label="c" type="m" corresp="#s4:l5">muer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
