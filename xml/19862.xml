    <tags>
      <tag standard-name="geestelijk lied" translation="spiritual song">geestelijk lied</tag>
    </tags>
    <melody>Windje daar het bos af drilt</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  <author>Johan van Born</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">WEl op, laet ons uyt visschen <rhyme label="a" type="m">gaen</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Naer de Visschen <rhyme label="a" type="m" corresp="#s1:l1">onberaen</rhyme>,</l>
        <l id="s1:l3" met="2" type="+-">Die stom <rhyme label="b" type="f">swemmen</rhyme></l>
        <l id="s1:l4" met="2" type="+-">Vol <rhyme label="b" type="f" corresp="#s1:l3">beklemmen</rhyme></l>
        <l id="s1:l5" met="4" type="+-">In de wilde woeste <rhyme label="c" type="m">zee</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Sonder te weten van haer <rhyme label="c" type="m" corresp="#s1:l5">wee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Daer d'een golve d'ander <rhyme label="a" type="m">stoot</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En daer buldren winden <rhyme label="a" type="m" corresp="#s2:l1">groot</rhyme>,</l>
        <l id="s2:l3" met="2" type="+-">Die haer <rhyme label="b" type="f">stranden</rhyme></l>
        <l id="s2:l4" met="2" type="+-">Doen op <rhyme label="b" type="f" corresp="#s2:l3">sanden</rhyme></l>
        <l id="s2:l5" met="4" type="+-">Daer hy blijven <rhyme label="c" type="m">waterloos</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">En versticken voort <rhyme label="c" type="m" corresp="#s2:l5">altoos</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Och dat sietmen int <rhyme label="a" type="m">gemeen</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Groote Visschen slocken <rhyme label="a" type="m" corresp="#s3:l1">kleen</rhyme>,</l>
        <l id="s3:l3" met="2" type="+-">En de <rhyme label="b" type="f">Snoecken</rhyme></l>
        <l id="s3:l4" met="2" type="+-">Altijt <rhyme label="b" type="f" corresp="#s3:l3">soecken</rhyme></l>
        <l id="s3:l5" met="4" type="+-">In haer holle kaecken <rhyme label="c" type="m">wreet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Die happent Vischken eer het <rhyme label="c" type="m" corresp="#s3:l5">weet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Werpet van de Netten <rhyme label="a" type="m">uyt</rhyme></l>
        <l id="s4:l2" met="4" type="+-">Op de rechte zy der <rhyme label="a" type="m" corresp="#s4:l1">Schuyt</rhyme>,</l>
        <l id="s4:l3" met="2" type="+-">Ghy sult <rhyme label="b" type="f">krijgen</rhyme></l>
        <l id="s4:l4" met="2" type="+-">Sulcken <rhyme label="b" type="f" corresp="#s4:l3">rijgen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Sulcken grooten hoop van <rhyme label="c" type="m">Vis</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Dat ghy sult sien wie by u <rhyme label="c" type="m" corresp="#s4:l5">is</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Christus de Heere vol <rhyme label="a" type="m">genaet</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">Die sijn schepsel niet <rhyme label="a" type="m" corresp="#s5:l1">verlaet</rhyme>,</l>
        <l id="s5:l3" met="2" type="+-">
          <rhyme label="b" type="f">Predikanten</rhyme>
        </l>
        <l id="s5:l4" met="2" type="+-">Alle <rhyme label="b" type="f" corresp="#s5:l3">kanten</rhyme></l>
        <l id="s5:l5" met="4" type="+-">Sendet uyt, die vangen <rhyme label="c" type="m">ras</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Die sondaer die int wilde <rhyme label="c" type="m" corresp="#s5:l5">was</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Haer net is het heyligh <rhyme label="a" type="m">woort</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Dat seer verre wort <rhyme label="a" type="m" corresp="#s6:l1">gehoort</rhyme></l>
        <l id="s6:l3" met="2" type="+-">O <rhyme label="b" type="f">geluckigh</rhyme>,</l>
        <l id="s6:l4" met="2" type="+-">End <rhyme label="b" type="f" corresp="#s6:l3">ondruckigh</rhyme></l>
        <l id="s6:l5" met="4" type="+-">Is dien Sondaer door <rhyme label="c" type="m">gewis</rhyme></l>
        <l id="s6:l6" met="4" type="+-">Die in Gods Net gevangen <rhyme label="c" type="m" corresp="#s6:l5">is</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy sal dorsten <rhyme label="a" type="m">nimmermeer</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Maer bevrijt zijn van <rhyme label="a" type="m" corresp="#s7:l1">verseer</rhyme></l>
        <l id="s7:l3" met="2" type="+-">In den <rhyme label="b" type="f">Vijver</rhyme></l>
        <l id="s7:l4" met="2" type="+-">Dorst <rhyme label="b" type="f" corresp="#s7:l3">verdrijver</rhyme>,</l>
        <l id="s7:l5" met="4" type="+-">Van des Lams dierbaere <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s7:l6" met="4" type="+-">Sal steets verquickt zijn sijn <rhyme label="c" type="m" corresp="#s7:l5">gemoed</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
