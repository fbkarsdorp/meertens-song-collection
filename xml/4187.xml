<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>4187</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Mennistenzusje</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">GElijk de bomen, <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">In alle <rhyme label="b" type="f">warme-landen</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Die steeds zijn schoon, en <rhyme label="a" type="m" corresp="#s1:l1">groen</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Ook op de heetste <rhyme label="b" type="f" corresp="#s1:l2">stranden</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">So groend mijn <rhyme label="c" type="m">jeugdig-hart</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">In liefdelijke <rhyme label="d" type="m">Min</rhyme>:</l>
        <l id="s1:l7" met="3" type="-+">So blijft mijn ziel <rhyme label="c" type="m" corresp="#s1:l5">verward</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">In 't geen ik dus <rhyme label="d" type="m" corresp="#s1:l6">begin</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ik voele dach, en <rhyme label="a" type="m">nacht</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Dees' <rhyme label="b" type="f">over-soete-vlagen</rhyme>:</l>
        <l id="s2:l3" met="3" type="-+">Den boom van ons <rhyme label="a" type="m" corresp="#s2:l1">geslacht</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wil liefdens-vruchten <rhyme label="b" type="f" corresp="#s2:l2">dragen</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Geen Winter, oyt so <rhyme label="c" type="m">koud</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Geen Somer, even <rhyme label="d" type="m">heet</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat ik niet wensch <rhyme label="c" type="m" corresp="#s2:l5">getroud</rhyme>:</l>
        <l id="s2:l8" met="3" type="-+">Met innig <rhyme label="d" type="m" corresp="#s2:l6">harten-leet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Geen tijt, geen plaats, geen <rhyme label="a" type="m">volk</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Doet my, u Lief <rhyme label="b" type="f">vergeten</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Schoon wel een <rhyme label="a" type="m" corresp="#s3:l1">donk're-wolk</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">My is om 'thoofd <rhyme label="b" type="f" corresp="#s3:l2">geseten</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Geen Zee, geen <rhyme label="c" type="m">ongemak</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Geen <rhyme label="d" type="m">giftig-Africa</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Geen wind, geen stromen, <rhyme label="c" type="m" corresp="#s3:l5">brak</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Geen ruymte: waar ik <rhyme label="d" type="m" corresp="#s3:l6">ga</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Ik leef dan <rhyme label="a" type="m">(waardste-Pand</rhyme>)</l>
        <l id="s4:l2" met="3" type="-+">In hoop, en groot <rhyme label="b" type="f">verlangen</rhyme>:</l>
        <l id="s4:l3" met="3" type="-+">En wensche (van u <rhyme label="a" type="m" corresp="#s4:l1">hand</rhyme>)</l>
        <l id="s4:l4" met="3" type="-+">Het leven te <rhyme label="b" type="f" corresp="#s4:l2">ontfangen</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Want leef ik sonder <rhyme label="c" type="m">u</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">So ben ik, <rhyme label="d" type="m">levend-dood</rhyme>:</l>
        <l id="s4:l7" met="3" type="-+">Dies, weest voor my niet <rhyme label="c" type="m" corresp="#s4:l5">schuw</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Maar helpt my uyt <rhyme label="d" type="m" corresp="#s4:l6">dees'nood</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">My voerd den <rhyme label="a" type="m">Dollephijn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">(Als Arion) door 't <rhyme label="b" type="f">water</rhyme>:</l>
        <l id="s5:l3" met="3" type="-+">Neptuni <rhyme label="a" type="m" corresp="#s5:l1">nacht-gordijn</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ontvoud, met luyd <rhyme label="b" type="f" corresp="#s5:l2">geklater</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">AEoli (heel <rhyme label="c" type="m">vergramd</rhyme>)</l>
        <l id="s5:l6" met="3" type="-+">Ontsteld den <rhyme label="d" type="m">Oceaan</rhyme>:</l>
        <l id="s5:l7" met="3" type="-+">Die dreygd my staag, en <rhyme label="c" type="m" corresp="#s5:l5">vlamd</rhyme></l>
        <l id="s5:l8" met="3" type="-+">My in den grond te <rhyme label="d" type="m" corresp="#s5:l6">slaan</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Mijn Hengst rend, in het <rhyme label="a" type="m">oog</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Der Gallen, en der <rhyme label="b" type="f">Britten</rhyme>:</l>
        <l id="s6:l3" met="3" type="-+">En daald, en ryst <rhyme label="a" type="m" corresp="#s6:l1">om-hoog</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Ik blijf gestadig <rhyme label="b" type="f" corresp="#s6:l2">sitten</rhyme>:</l>
        <l id="s6:l5" met="3" type="-+">En sweef door Zee, en <rhyme label="c" type="m">Lucht</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">So ménig <rhyme label="d" type="m">honderd-mijl</rhyme>:</l>
        <l id="s6:l7" met="3" type="-+">En sie (an-merkens <rhyme label="c" type="m" corresp="#s6:l5">vrucht</rhyme>)</l>
        <l id="s6:l8" met="3" type="-+">Veel wonders <rhyme label="d" type="m" corresp="#s6:l6">onderwijl</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nu koom ik by den <rhyme label="a" type="m">Moor</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">En onder Phoebi <rhyme label="b" type="f">stralen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Door 't <rhyme label="a" type="m" corresp="#s7:l1">ongebaande-spoor</rhyme>:</l>
        <l id="s7:l4" met="3" type="-+">So moet dit lichaam <rhyme label="b" type="f" corresp="#s7:l2">dwalen</rhyme>:</l>
        <l id="s7:l5" met="3" type="-+">Hier, voor de vuyst van <rhyme label="c" type="m">Mars</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Daar, voor het <rhyme label="d" type="m">drie-tand-Spits</rhyme>:</l>
        <l id="s7:l7" met="3" type="-+">Nu, drijf ik <rhyme label="c" type="m" corresp="#s7:l5">over-dwars</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Dan, sien ik weêr wat <rhyme label="d" type="m" corresp="#s7:l6">wits</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Nu swerf ik Oost, dan <rhyme label="a" type="m">West</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Nu Zuyden, en dan <rhyme label="b" type="f">Noorden</rhyme>:</l>
        <l id="s8:l3" met="3" type="-+">Gevaar, en nood, is 't <rhyme label="a" type="m" corresp="#s8:l1">best</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Van dat my <rhyme label="b" type="f" corresp="#s8:l2">toe-behoorden</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Dus vlieg ik om dit <rhyme label="c" type="m">Rond</rhyme>:</l>
        <l id="s8:l6" met="3" type="-+">Geen plaats, geen <rhyme label="d" type="m">vaste-steé</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Ik oyt op d' Aarde <rhyme label="c" type="m" corresp="#s8:l5">vond</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Noch in de <rhyme label="d" type="m" corresp="#s8:l6">woeste-Zee</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Maar siet: (ô! <rhyme label="a" type="m">Lieve-Maagd</rhyme>)</l>
        <l id="s9:l2" met="3" type="-+">Ik draag u <rhyme label="b" type="f">beeldenissie</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">(Die my alleen <rhyme label="a" type="m" corresp="#s9:l1">behaagd</rhyme>:</l>
        <l id="s9:l4" met="3" type="-+">Dat niemand (acht ik) <rhyme label="b" type="f" corresp="#s9:l2">gisse</rhyme>)</l>
        <l id="s9:l5" met="3" type="-+">Door 't blaau asurig <rhyme label="c" type="m">veld</rhyme>:</l>
        <l id="s9:l6" met="3" type="-+">Door bos, langs berg, en <rhyme label="d" type="m">dal</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Tot ik (in u <rhyme label="c" type="m" corresp="#s9:l5">geweld</rhyme>)</l>
        <l id="s9:l8" met="3" type="-+">Mijn leven eynden <rhyme label="d" type="m" corresp="#s9:l6">sal</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Gy zijt mijn ander, <rhyme label="a" type="m">Ik</rhyme>:</l>
        <l id="s10:l2" met="3" type="-+">Gy zijt mijn Helft, mijn <rhyme label="b" type="f">eygen</rhyme>:</l>
        <l id="s10:l3" met="3" type="-+">Gy kunt mijn ziel, van <rhyme label="a" type="m" corresp="#s10:l1">schrik</rhyme>,</l>
        <l id="s10:l4" met="3" type="-+">Tot waare blijdschap <rhyme label="b" type="f" corresp="#s10:l2">neygen</rhyme>:</l>
        <l id="s10:l5" met="3" type="-+">Ik draag u, en gy <rhyme label="c" type="m">torst</rhyme></l>
        <l id="s10:l6" met="3" type="-+">Mijn ogen, hert, en <rhyme label="d" type="m">sin</rhyme>:</l>
        <l id="s10:l7" met="3" type="-+">Ontsluyt dan (Lief) u <rhyme label="c" type="m" corresp="#s10:l5">borst</rhyme>,</l>
        <l id="s10:l8" met="3" type="-+">En laat mijn ziel daar <rhyme label="d" type="m" corresp="#s10:l6">in</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">So sal noch plaats, noch <rhyme label="a" type="m">tijd</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Noch ruymt, ons kunnen <rhyme label="b" type="f">scheyden</rhyme>:</l>
        <l id="s11:l3" met="3" type="-+">So sal Ik (waar gy <rhyme label="a" type="m" corresp="#s11:l1">zijt</rhyme>,)</l>
        <l id="s11:l4" met="3" type="-+">En gy my weer <rhyme label="b" type="f" corresp="#s11:l2">geleyden</rhyme>,</l>
        <l id="s11:l5" met="3" type="-+">Den tijd terwijl Ik <rhyme label="c" type="m">swerf</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Van d' een, op d' ander <rhyme label="d" type="m">kust</rhyme>:</l>
        <l id="s11:l7" met="3" type="-+">En duysend-doden <rhyme label="c" type="m" corresp="#s11:l5">sterf</rhyme>,</l>
        <l id="s11:l8" met="3" type="-+">Tot dat Ik by u <rhyme label="d" type="m" corresp="#s11:l6">rust</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
