<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3680</id>
    <date>
      <year-start>1566</year-start>
      <year-end>1566</year-end>
    </date>
    <location>
      <orginal-name>heidelberg</orginal-name>
      <standard-name>Heidelberg, Germany</standard-name>
      <coordinates>
        <lon>49.4093582</lon>
        <lat>8.694724</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>Psalm 077 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ICK hebb' myn stem <rhyme label="a" type="f">opgheheuen</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">End myn schreyen daer <rhyme label="a" type="f" corresp="#s1:l1">beneuen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Tot v Heer' end nae v' <rhyme label="b" type="m">Woort</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Hebt ghy my voormaels <rhyme label="b" type="m" corresp="#s1:l3">verhoort</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">In mynen noot end <rhyme label="c" type="f">benauwen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Stondt op v al myn <rhyme label="c" type="f" corresp="#s1:l5">vertrauwen</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Ick hebb' myn handen Heer <rhyme label="d" type="m">goet</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Tot v gestreckt met <rhyme label="d" type="m" corresp="#s1:l7">demoet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Myn siel in dit groot <rhyme label="a" type="f">verlanghen</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">En wil gheenen troost <rhyme label="a" type="f" corresp="#s2:l1">ontfanghen</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Iae als ick aen v denck' <rhyme label="b" type="m">Heer</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Bedroeft ben ick des te <rhyme label="b" type="m" corresp="#s2:l3">meer</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">Als hebb'ick van gantscher <rhyme label="c" type="f">herten</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Ghebeden in angst end <rhyme label="c" type="f" corresp="#s2:l5">smerten</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">So blyft doch myn hert' <rhyme label="d" type="m">eenpaer</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Vol benaudtheits end angsts <rhyme label="d" type="m" corresp="#s2:l7">swaer</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Ghy hebt my O Heer <rhyme label="a" type="f">ghenaedigh</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Wacker ghehouden <rhyme label="a" type="f" corresp="#s3:l1">ghestaedigh</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Ick ben so krachtloos daer <rhyme label="b" type="m">van</rhyme>,</l>
        <l id="s3:l4" met="4" type="+-">Dat ick schier niet spreken <rhyme label="b" type="m" corresp="#s3:l3">kan</rhyme>.</l>
        <l id="s3:l5" met="4" type="+-">Als ick met druck ben <rhyme label="c" type="f">doersneden</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Ghedenck ick des tydts <rhyme label="c" type="f" corresp="#s3:l5">voorleden</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">D'oude jaeren dagh end <rhyme label="d" type="m">nacht</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">Werden van my <rhyme label="d" type="m" corresp="#s3:l7">ouerdacht</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dat schoon lieflick spel der <rhyme label="a" type="f">snaeren</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Kan wt mynen sin niet <rhyme label="a" type="f" corresp="#s4:l1">vaeren</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Myn hert' vol smerten <rhyme label="b" type="m">voorwaer</rhyme>,</l>
        <l id="s4:l4" met="4" type="+-">Ghedenckt daer aen voor end <rhyme label="b" type="m" corresp="#s4:l3">naer</rhyme>.</l>
        <l id="s4:l5" met="4" type="+-">Daer nae tracht ick t'allen <rhyme label="c" type="f">stonden</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Of ick dat eynd mocht' <rhyme label="c" type="f" corresp="#s4:l5">doergronden</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Deses dinghs: end dat <rhyme label="d" type="m">verstaen</rhyme>,</l>
        <l id="s4:l8" met="4" type="+-">Dies vangh' ick so myn klacht' <rhyme label="d" type="m" corresp="#s4:l7">aen</rhyme>:</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Sal my Godt altyt <rhyme label="a" type="f">versteken</rhyme>?</l>
        <l id="s5:l2" met="4" type="+-">Is hy gantsch van my <rhyme label="a" type="f" corresp="#s5:l1">gheweken</rhyme>?</l>
        <l id="s5:l3" met="4" type="+-">Is my nu in <rhyme label="b" type="m">eewicheyt</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Gheen ghenaede meer <rhyme label="b" type="m" corresp="#s5:l3">bereydt</rhyme>?</l>
        <l id="s5:l5" met="4" type="+-">Sal Gods goedtheit hoogh' <rhyme label="c" type="f">ghepresen</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">Hier met nu gantschlick wt <rhyme label="c" type="f" corresp="#s5:l5">wesen</rhyme>?</l>
        <l id="s5:l7" met="4" type="+-">Sullen sijn beloften <rhyme label="d" type="m">fyn</rhyme>,</l>
        <l id="s5:l8" met="4" type="+-">Hier met gantsch ten eynde <rhyme label="d" type="m" corresp="#s5:l7">sijn</rhyme>?</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Heeft Godt t'mywaert gaer <rhyme label="a" type="f">vergheten</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">Sijner goetheit <rhyme label="a" type="f" corresp="#s6:l1">onghemeten</rhyme>?</l>
        <l id="s6:l3" met="4" type="+-">Gaet nu sijne <rhyme label="b" type="m">toornicheit</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Bouen sijn' <rhyme label="b" type="m" corresp="#s6:l3">barmherticheit</rhyme>?</l>
        <l id="s6:l5" met="4" type="+-">Alsulcks sprack ick tot dien <rhyme label="c" type="f">tyden</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Myn Godt wilt dat ick sal <rhyme label="c" type="f" corresp="#s6:l5">lyden</rhyme>,</l>
        <l id="s6:l7" met="4" type="+-">Hy heeft verwisselt sijn <rhyme label="d" type="m">handt</rhyme>,</l>
        <l id="s6:l8" met="4" type="+-">Die my voortyts gaf <rhyme label="d" type="m" corresp="#s6:l7">bystandt</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Daer nae wierdt ick oock <rhyme label="a" type="f">indachtigh</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Der wonderdaeden Gods <rhyme label="a" type="f" corresp="#s7:l1">krachtigh</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die hy voormaels heeft <rhyme label="b" type="m">ghedaen</rhyme>,</l>
        <l id="s7:l4" met="4" type="+-">Deser ded' ick oock <rhyme label="b" type="m" corresp="#s7:l3">vermaen</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Maer als ick so ghingh <rhyme label="c" type="f">aenmercken</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Sijn heerlicke <rhyme label="c" type="f" corresp="#s7:l5">wonderwercken</rhyme>,</l>
        <l id="s7:l7" met="4" type="+-">End' all' sijn doen groot end <rhyme label="d" type="m">kleyn</rhyme>,</l>
        <l id="s7:l8" met="4" type="+-">Dan sprack ick by my <rhyme label="d" type="m" corresp="#s7:l7">alleyn</rhyme>:</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">O Godt heyligh sijn v' <rhyme label="a" type="f">weghen</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Niemant kan v'spreken <rhyme label="a" type="f" corresp="#s8:l1">teghen</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Waer isser breedt ofte <rhyme label="b" type="m">wydt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Een alsulck Godt als ghy <rhyme label="b" type="m" corresp="#s8:l3">sijt</rhyme>?</l>
        <l id="s8:l5" met="4" type="+-">Ghy laett uwe daeden <rhyme label="c" type="f">blycken</rhyme>,</l>
        <l id="s8:l6" met="4" type="+-">Doer macht niet om <rhyme label="c" type="f" corresp="#s8:l5">verghelycken</rhyme>,</l>
        <l id="s8:l7" met="4" type="+-">Ghy doet die heydnen vol <rhyme label="d" type="m">pracht</rhyme>,</l>
        <l id="s8:l8" met="4" type="+-">Ondervinden uwe <rhyme label="d" type="m" corresp="#s8:l7">kracht</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Ghy hebt doer v' handt <rhyme label="a" type="f">almachtigh</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">V' volck verlost, Heere <rhyme label="a" type="f" corresp="#s9:l1">krachtigh</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Die kindren des Iacobs <rhyme label="b" type="m">goedt</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">End des vromen Iosephs <rhyme label="b" type="m" corresp="#s9:l3">vroedt</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Vwe vrees' is, Heer <rhyme label="c" type="f">ghekomen</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Ouer al die <rhyme label="c" type="f" corresp="#s9:l5">waterstromen</rhyme>,</l>
        <l id="s9:l7" met="4" type="+-">Voor v' Maiesteit <rhyme label="d" type="m">terstondt</rhyme>,</l>
        <l id="s9:l8" met="4" type="+-">Beefde Heer de diep' <rhyme label="d" type="m" corresp="#s9:l7">afgrondt</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Die wolcken seer dicht <rhyme label="a" type="f">ghesloten</rhyme>,</l>
        <l id="s10:l2" met="4" type="+-">Hebben water <rhyme label="a" type="f" corresp="#s10:l1">wtgheghoten</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">End des donders groot <rhyme label="b" type="m">gheluyt</rhyme>,</l>
        <l id="s10:l4" met="4" type="+-">Streckt hem in den wolcken <rhyme label="b" type="m" corresp="#s10:l3">wt</rhyme>.</l>
        <l id="s10:l5" met="4" type="+-">Die vuerstraelen hen <rhyme label="c" type="f">wtspreyden</rhyme>,</l>
        <l id="s10:l6" met="4" type="+-">Donderslaeghen hen <rhyme label="c" type="f" corresp="#s10:l5">wtbreyden</rhyme>,</l>
        <l id="s10:l7" met="4" type="+-">De blixem met den schyn <rhyme label="d" type="m">fel</rhyme>,</l>
        <l id="s10:l8" met="4" type="+-">Ontstack t'gansch ęerdtrycke <rhyme label="d" type="m" corresp="#s10:l7">snel</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Den ęerdtbodem sagh men <rhyme label="a" type="f">beuen</rhyme>,</l>
        <l id="s11:l2" met="4" type="+-">End ghy gaeft eenen wegh <rhyme label="a" type="f" corresp="#s11:l1">euen</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Doer t'Męer tot uws volcks <rhyme label="b" type="m">oorboor</rhyme>,</l>
        <l id="s11:l4" met="4" type="+-">Daer gheen voetpadt was te <rhyme label="b" type="m" corresp="#s11:l3">voor</rhyme>.</l>
        <l id="s11:l5" met="4" type="+-">Ghy leydet v' volck <rhyme label="c" type="f">bequaeme</rhyme>,</l>
        <l id="s11:l6" met="4" type="+-">Als een kudde schaepen <rhyme label="c" type="f" corresp="#s11:l5">t'saeme</rhyme>,</l>
        <l id="s11:l7" met="4" type="+-">Doer Mosis end Aarons <rhyme label="d" type="m">handt</rhyme>,</l>
        <l id="s11:l8" met="4" type="+-">In dat schoon beloefde <rhyme label="d" type="m" corresp="#s11:l7">landt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
