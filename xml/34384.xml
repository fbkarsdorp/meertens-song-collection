      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">AMarilli mijn <rhyme label="a" type="f">uyt-vercooren</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">Ghy sijt aen my door liefden <rhyme label="a" type="f" corresp="#s1:l1">gebooren</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">U soet-straelende flackerich <rhyme label="b" type="f">wesen</rhyme></l>
        <l id="s1:l4" met="4" type="+-">Can mijn smerten alleenich <rhyme label="b" type="f" corresp="#s1:l3">genesen</rhyme>;</l>
        <l id="s1:l5" met="4" type="+-">Mijn Meestersse,, <rhyme label="c" type="f">Verstroostersse</rhyme>,</l>
        <l id="s1:l6" met="6" type="+-">Die mijn perse,, Siet verversse,, Wilt <rhyme label="c" type="f" corresp="#s1:l5">dienersse</rhyme></l>
        <l id="s1:l7" met="2" type="-+">U dienaer dan <rhyme label="d" type="f">gheve</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Dat hy eeuwelijck met u <rhyme label="d" type="f" corresp="#s1:l7">leve</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Ghy sijt weerder als Iuno te <rhyme label="a" type="f">loven</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Ghy gaet Venus en Pallas te <rhyme label="a" type="f" corresp="#s2:l1">boven</rhyme>,</l>
        <l id="s2:l3" met="4" type="+-">Waert dat Paris den appel moest <rhyme label="b" type="f">schencken</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Hy sou om Amarilli nu <rhyme label="b" type="f" corresp="#s2:l3">dencken</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Herderinne,, Die ick <rhyme label="c" type="f">minne</rhyme>,,</l>
        <l id="s2:l6" met="6" type="+-">Laet my winne,, Mijn vrindinne,, t'Hert van <rhyme label="c" type="f" corresp="#s2:l5">binne</rhyme></l>
        <l id="s2:l7" met="2" type="-+">Door brandige <rhyme label="d" type="f">liefde</rhyme></l>
        <l id="s2:l8" met="4" type="+-">Uwe schoonicheydt my door <rhyme label="d" type="f" corresp="#s2:l7">cliefde</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Weest Princersse gheen Daphne int <rhyme label="a" type="f">vlieden</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Siet Apollo sijn ionst u comt <rhyme label="a" type="f" corresp="#s3:l1">bieden</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">t'Is de cracht vande gout straele <rhyme label="b" type="f">schichten</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Die my t'hert met u liefde <rhyme label="b" type="f" corresp="#s3:l3">doorvlichten</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">Mijn beminde,, Die ick <rhyme label="c" type="f">siende</rhyme></l>
        <l id="s3:l6" met="6" type="+-">My verblinde,, Wilt my binde,, Dat ick <rhyme label="c" type="f" corresp="#s3:l5">inde</rhyme></l>
        <l id="s3:l7" met="2" type="-+">Ghedurighe <rhyme label="d" type="f">banden</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Met u liefde in liefde mach <rhyme label="d" type="f" corresp="#s3:l7">branden</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Schoonste Maeghet wie mach u <rhyme label="a" type="f">ghelijcken</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">Voor u Thetis en Philida <rhyme label="a" type="f" corresp="#s4:l1">wijcken</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Ick en can u geen lof meer <rhyme label="b" type="f">bewijsen</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Want mijn tongh is te swack u te <rhyme label="b" type="f" corresp="#s4:l3">prijsen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Amarillis,, Boven <rhyme label="c" type="f">Philis</rhyme></l>
        <l id="s4:l6" met="6" type="+-">My u ziel is,, Mijnen wil is,, (Die noyt stil <rhyme label="c" type="f" corresp="#s4:l5">is</rhyme>)</l>
        <l id="s4:l7" met="2" type="-+">Door liefde te <rhyme label="d" type="f">blijven</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Eene ziele te saem in twee <rhyme label="d" type="f" corresp="#s4:l7">lijven</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
