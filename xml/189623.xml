    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">MEysje, zoete <rhyme label="a" type="m">Sus</rhyme>,</l>
        <l id="s1:l2" met="2" type="+-">Geef een <rhyme label="a" type="m" corresp="#s1:l1">Kusch</rhyme>,</l>
        <l id="s1:l3" met="2" type="-+">Myn lieve <rhyme label="b" type="f">Gekje</rhyme>,</l>
        <l id="s1:l4" met="2" type="+-">Met je <rhyme label="b" type="f" corresp="#s1:l3">Bekje</rhyme>,</l>
        <l id="s1:l5" met="2" type="+-">Of ik <rhyme label="b" type="f" corresp="#s1:l3">Trekje</rhyme>,</l>
        <l id="s1:l6" met="2" type="+-">Weer als <rhyme label="a" type="m" corresp="#s1:l1">Flus</rhyme>,</l>
        <l id="s1:l7" met="2" type="+-">Naar my <rhyme label="c" type="m">toe</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">En kusch my <rhyme label="c" type="m" corresp="#s1:l7">moe</rhyme>.</l>
      </lg>
