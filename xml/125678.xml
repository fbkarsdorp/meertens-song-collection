        <l id="s1:l4" met="2" type="+-">Haar Schoonheyd, Ten toon <rhyme label="c" type="m">leyd</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Voor my, die haar <rhyme label="b" type="m" corresp="#s1:l3">Mind</rhyme></l>
