    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Ik ben hier <rhyme label="a" type="f">gekomen</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Ter woon in deese <rhyme label="b" type="m">wyk</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Een Apteeker van <rhyme label="a" type="f" corresp="#s1:l1">Rome</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Gestudeert te <rhyme label="b" type="m" corresp="#s1:l2">Harderwyk</rhyme>;</l>
        <l id="s1:l5" met="3" type="-+">Ik koom u <rhyme label="c" type="m">ontbieden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Voor alle de <rhyme label="c" type="m" corresp="#s1:l5">lieden</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">En ik toon hier mijn <rhyme label="d" type="m">kunst</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Versoek u om de <rhyme label="d" type="m" corresp="#s1:l7">gunst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wilt gy ook <rhyme label="a" type="f">purgeren</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Komt vry maar tot mijn <rhyme label="b" type="m">in</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Voor de dames en de <rhyme label="a" type="f" corresp="#s2:l1">Heeren</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Heb ik een goed <rhyme label="b" type="m" corresp="#s2:l2">begin</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Rabarber en kruyde <rhyme label="c" type="m">schoon</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Amandelen zoet ten <rhyme label="c" type="m" corresp="#s2:l5">toon</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Salappen Wortelen <rhyme label="d" type="m">schoon</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Voor de gemeene <rhyme label="d" type="m" corresp="#s2:l7">persoon</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Siroop om te <rhyme label="a" type="f">zweeren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Vlier en <rhyme label="b" type="m">Kamilleblom</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hebt gy geen smaak in ‘t <rhyme label="a" type="f" corresp="#s3:l1">eeten</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ik bid u tot my <rhyme label="b" type="m" corresp="#s3:l2">kom</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Of hebt gy de koortse <rhyme label="c" type="m">snel</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gebruikt mijn kina <rhyme label="c" type="m" corresp="#s3:l5">wel</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Gy zult ‘er genesen <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Zonder smert of <rhyme label="d" type="m" corresp="#s3:l7">pyn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Hebt gy hoesten of <rhyme label="a" type="f">teeringm</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Of hebt gy een kwade <rhyme label="b" type="m">maag</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Hier ziet gy ‘er mijn <rhyme label="a" type="f" corresp="#s4:l1">neering</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Treet en weest niet <rhyme label="b" type="m" corresp="#s4:l2">traag</rhyme>:</l>
        <l id="s4:l5" met="3" type="-+">Ook heb ik hooft <rhyme label="c" type="m">plyster</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Voor dames en <rhyme label="c" type="m" corresp="#s4:l5">vryster</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Ook salf voor den <rhyme label="d" type="m">brant</rhyme>;</l>
        <l id="s4:l8" met="3" type="-+">Balsem voor de <rhyme label="d" type="m" corresp="#s4:l7">winterhand</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Hier mee wil ik <rhyme label="a" type="f">besluiten</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Mijn winkel hebt gy <rhyme label="b" type="m">gesien</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Maar nu wagt ik op <rhyme label="a" type="f" corresp="#s5:l1">duiten</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Die ik heb <rhyme label="b" type="m" corresp="#s5:l2">verdient</rhyme>;</l>
        <l id="s5:l5" met="3" type="-+">Ik wensch u <rhyme label="c" type="m">allegaar</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Veel zegen in het <rhyme label="c" type="m" corresp="#s5:l5">jaar</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Dat gy gesegent mag <rhyme label="d" type="m">zijn</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">Dat is de wens van <rhyme label="d" type="m" corresp="#s5:l7">mijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
