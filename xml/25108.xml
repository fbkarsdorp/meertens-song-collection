<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>25108</id>
    <date>
      <year-start>1500</year-start>
      <year-end>1500</year-end>
    </date>
    <tags>
      <tag>christmas song</tag>
      <tag>Maria song</tag>
    </tags>
    <melody>Wie schoon zo is het hierboven</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Laet ons met vollen <rhyme label="a" type="f">chore</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Singen nuwen <rhyme label="b" type="m">sanc</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Want god es ons <rhyme label="a" type="f" corresp="#s1:l1">geboren</rhyme></l>
        <l id="s1:l4" met="2" type="-+">Een <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Dies moet hy hebben <rhyme label="b" type="m" corresp="#s1:l2">danc</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wy mogen wel vrolic <rhyme label="a" type="f">leuen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Ic segge v wel waer <rhyme label="b" type="m">of</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Want god heeft ons <rhyme label="a" type="f" corresp="#s2:l1">gegeuen</rhyme></l>
        <l id="s2:l4" met="2" type="-+">Den sone <rhyme label="c" type="m">sijn</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dies moet hy hebben <rhyme label="b" type="m" corresp="#s2:l2">lof</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy nam wt goeder <rhyme label="a" type="f">minnen</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Onse menscheit <rhyme label="b" type="m">an</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Hets recht dat elc met <rhyme label="a" type="f" corresp="#s3:l1">sinne</rhyme></l>
        <l id="s3:l4" met="2" type="-+">Dit <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Loeft als hy best <rhyme label="b" type="m" corresp="#s3:l2">can</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Wel hem die suete ende <rhyme label="a" type="f">sachte</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dit kindeken antieren <rhyme label="b" type="m">sal</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Want wy met reynen <rhyme label="a" type="f" corresp="#s4:l1">gedachte</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Dit <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Wiecht hy louet <rhyme label="b" type="m" corresp="#s4:l2">al</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Wy willen dit kindeken <rhyme label="a" type="f">wermen</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Met onser minnen <rhyme label="b" type="m">gloet</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Soe sal hy onser <rhyme label="a" type="f" corresp="#s5:l1">ontfermen</rhyme></l>
        <l id="s5:l4" met="2" type="-+">Huut minnen <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Gaf hy doer ons sijn <rhyme label="b" type="m" corresp="#s5:l2">bloet</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Laet ons dit kindeken <rhyme label="a" type="f">baden</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Met onsen tranen <rhyme label="b" type="m">nat</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Ende bidden om <rhyme label="a" type="f" corresp="#s6:l1">genade</rhyme></l>
        <l id="s6:l4" met="2" type="-+">Met herten <rhyme label="c" type="m">fijn</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Ons werdes wel te <rhyme label="b" type="m" corresp="#s6:l2">bat</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O rose sonder <rhyme label="a" type="f">dooren</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Bouen de lelien <rhyme label="b" type="m">suet</rhyme></l>
        <l id="s7:l3" met="3" type="-+">God was van v <rhyme label="a" type="f" corresp="#s7:l1">geboren</rhyme></l>
        <l id="s7:l4" met="2" type="-+">Dats wel <rhyme label="c" type="m">aenscijn</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Doer v groet <rhyme label="b" type="m" corresp="#s7:l2">oetmoet</rhyme></l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">O maria lief <rhyme label="a" type="f">moeder</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Ende maget <rhyme label="b" type="m">reen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">By v wert god ons <rhyme label="a" type="f" corresp="#s8:l1">broeder</rhyme></l>
        <l id="s8:l4" met="2" type="-+">V <rhyme label="c" type="m">kindekijn</rhyme></l>
        <l id="s8:l5" met="3" type="-+">Verlost ons al <rhyme label="b" type="m" corresp="#s8:l2">gemeyn</rhyme></l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">O vrouwe der doecht <rhyme label="a" type="f">gewone</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Wilt altijt sijn <rhyme label="b" type="m">bereyt</rhyme></l>
        <l id="s9:l3" met="3" type="-+">Te bidden uwen <rhyme label="a" type="f" corresp="#s9:l1">sone</rhyme></l>
        <l id="s9:l4" met="2" type="-+">Dat sijn <rhyme label="c" type="m">aenschijn</rhyme></l>
        <l id="s9:l5" met="3" type="-+">Ons niet en werde <rhyme label="b" type="m" corresp="#s9:l2">ontseit</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
