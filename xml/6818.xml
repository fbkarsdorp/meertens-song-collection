<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>6818</id>
    <date>
      <year-start>1637</year-start>
      <year-end>1637</year-end>
    </date>
    <location>
      <orginal-name>emmerick</orginal-name>
      <standard-name>Emmerik, Germany</standard-name>
      <coordinates>
        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>Verbum caro factum est</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">In den Cyrckel van desen <rhyme label="a" type="m">Iaer</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Wort ons dat leven <rhyme label="a" type="m" corresp="#s1:l1">openbaer</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Iesus is geboren <rhyme label="a" type="m" corresp="#s1:l1">klaer</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s1:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s1:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Een Fonteyn heel snel en <rhyme label="a" type="m">klaer</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">Heeft den Mensch van sijn <rhyme label="a" type="m" corresp="#s2:l1">misdaet</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Die doot verliest haer kracht <rhyme label="a" type="m" corresp="#s2:l1">voorwaer</rhyme></l>
        <l id="s2:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s2:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s2:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Een stem brengt ons die klare <rhyme label="a" type="m">Son</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Die Son brengt ons den Godes <rhyme label="a" type="m" corresp="#s3:l1">Soon</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Nochtans so blijft sy een bloemke <rhyme label="a" type="m" corresp="#s3:l1">schoon</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s3:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s3:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Al van een suyver Maegdeken <rhyme label="a" type="m">reyn</rhyme></l>
        <l id="s4:l2" met="4" type="-+">Die opperste Konincks dochter <rhyme label="a" type="m" corresp="#s4:l1">fijn</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Wordt gegeven des hemels <rhyme label="a" type="m" corresp="#s4:l1">schijn</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s4:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s4:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">O Salige Iongfrou <rhyme label="a" type="m">excelent</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Die heylige vrucht ws Lichaems <rhyme label="a" type="m" corresp="#s5:l1">jent</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Neemt wech die bloedige s’werelts <rhyme label="a" type="m" corresp="#s5:l1">krenckt</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s5:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s5:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">In eender Cribbe wort Iesus <rhyme label="a" type="m">gelacht</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Die stomme beesten naemen hem <rhyme label="a" type="m" corresp="#s6:l1">acht</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Een kint gebooren al in der <rhyme label="a" type="m" corresp="#s6:l1">nacht</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s6:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s6:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">O Herderkens loopt met grooten <rhyme label="a" type="m">vliet</rhyme></l>
        <l id="s7:l2" met="4" type="-+">V schaepkens nu niet aen en <rhyme label="a" type="m" corresp="#s7:l1">siet</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Godt wort geboren hier int <rhyme label="a" type="m" corresp="#s7:l1">verdriet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s7:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s7:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Drie Coningen uyt <rhyme label="a" type="m">Orienten</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Sy brochten Iesum haer <rhyme label="a" type="m" corresp="#s8:l1">presenten</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Sy bidden mit gebogen <rhyme label="a" type="m" corresp="#s8:l1">knyen</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s8:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s8:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Op dese schoone Eedelen <rhyme label="a" type="m">tijt</rhyme></l>
        <l id="s9:l2" met="4" type="-+">Wort alle menschen hert <rhyme label="a" type="m" corresp="#s9:l1">verblijt</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Geopent wert ons dat <rhyme label="a" type="m" corresp="#s9:l1">hemelrijck</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s9:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s9:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Gebooren wert Godt in s’werelts <rhyme label="a" type="m">groen</rhyme></l>
        <l id="s10:l2" met="4" type="-+">Dat sonder eenich Mensche <rhyme label="a" type="m" corresp="#s10:l1">toedoen</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Al uyt een edel mensche <rhyme label="a" type="m" corresp="#s10:l1">schoon</rhyme></l>
        <l id="s10:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s10:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s10:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Den achtsten dach werdt Iesus <rhyme label="a" type="m">besneden</rhyme></l>
        <l id="s11:l2" met="4" type="-+">Het bloet liep uyt sijn teere <rhyme label="a" type="m" corresp="#s11:l1">leden</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">Den naem werdt hem geven <rhyme label="a" type="m" corresp="#s11:l1">mede</rhyme></l>
        <l id="s11:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s11:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s11:l4">Maria</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">O Heer in uwen hoogen <rhyme label="a" type="m">throon</rhyme></l>
        <l id="s12:l2" met="4" type="-+">Ghy sijt die eewige Godes <rhyme label="a" type="m" corresp="#s12:l1">Soon</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Lof, eer, soo moet u altoos <rhyme label="a" type="m" corresp="#s12:l1">sijn</rhyme></l>
        <l id="s12:l4" met="4" type="-+">Al van der Maget <rhyme label="b" type="m">Maria</rhyme>.</l>
        <l id="s12:l5" met="7" type="-+">Verbum caro factum est, de virgine <rhyme label="b" type="m" corresp="#s12:l4">Maria</rhyme>,</l>
      </lg>
    </lg>
  </text>
</song>
