    <melody/>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  <author>Philippus Jennyn</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">EEn Maghet <rhyme label="a" type="f">wt-verkooren</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">(Seer langh' verwacht hier <rhyme label="a" type="f" corresp="#s1:l1">vooren</rhyme>)</l>
        <l id="s1:l3" met="3" type="-+">Heeft nu haer Soon <rhyme label="a" type="f" corresp="#s1:l1">ghebooren</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">Die hemelijcke <rhyme label="b" type="f">schaeren</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">In Godes jonst' <rhyme label="b" type="f" corresp="#s1:l4">ervaeren</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Ons brenghen dese <rhyme label="b" type="f" corresp="#s1:l4">maeren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Al is hy <rhyme label="a" type="f">d'aldermeesten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Verkleent wat min, als <rhyme label="a" type="f" corresp="#s2:l1">geesten</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Light tusschen stomme <rhyme label="a" type="f" corresp="#s2:l1">beesten</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">Tot het beschudt der <rhyme label="b" type="f">winden</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Hy laet sijn Lichaem <rhyme label="b" type="f" corresp="#s2:l4">binden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En in de doecxkens <rhyme label="b" type="f" corresp="#s2:l4">winden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Hy schreyt (en naer mijn <rhyme label="a" type="f">dincken</rhyme>)</l>
        <l id="s3:l2" met="3" type="-+">Dat Kindeken wilt <rhyme label="a" type="f" corresp="#s3:l1">drincken</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Maria gaet hem <rhyme label="a" type="f" corresp="#s3:l1">schincken</rhyme>.</l>
        <l id="s3:l4" met="3" type="-+">Die hemelijcke <rhyme label="b" type="f">bloesem</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Zuyght wt een Maghets <rhyme label="b" type="f" corresp="#s3:l4">boesem</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Den Goddelijcken <rhyme label="b" type="f" corresp="#s3:l4">droesem</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">In dese aerdsche <rhyme label="a" type="f">palen</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Hy komt wt 'shemels <rhyme label="a" type="f" corresp="#s4:l1">salen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Al onse schuldt <rhyme label="a" type="f" corresp="#s4:l1">betalen</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Van wt den den schoot sijns <rhyme label="b" type="f">Vaders</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Hy komt tot sijn <rhyme label="b" type="f" corresp="#s4:l4">versmaders</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Hy komt tot sijn <rhyme label="b" type="f" corresp="#s4:l4">verraders</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Die eeuwigh is, <rhyme label="a" type="f">waerachtigh</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Rijck, edel, ende <rhyme label="a" type="f" corresp="#s5:l1">krachtigh</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Wilt wesen als <rhyme label="a" type="f" corresp="#s5:l1">onmachtigh</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Hy komt hier naeckt <rhyme label="b" type="f">ghevaeren</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Om sich te <rhyme label="b" type="f" corresp="#s5:l4">openbaeren</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Aerm, als de <rhyme label="b" type="f" corresp="#s5:l4">bedelaeren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Hy komt wt 'shemels <rhyme label="a" type="f">stede</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">En brenght de menschen <rhyme label="a" type="f" corresp="#s6:l1">mede</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Den langh-ghewenschten <rhyme label="a" type="f" corresp="#s6:l1">Vrede</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Door liefde voort <rhyme label="b" type="f">ghedreven</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Hy komt voor ons hiet <rhyme label="b" type="f" corresp="#s6:l4">gheven</rhyme></l>
        <l id="s6:l6" met="3" type="-+">Den tol van't eeuwigh <rhyme label="b" type="f" corresp="#s6:l4">leven</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Het is dan recht, en <rhyme label="a" type="f">reden</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Dat Godt, nu hier <rhyme label="a" type="f" corresp="#s7:l1">beneden</rhyme></l>
        <l id="s7:l3" met="3" type="-+">In dorpen, ende <rhyme label="a" type="f" corresp="#s7:l1">steden</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Van op den dagh van <rhyme label="b" type="f">heden</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">In alle <rhyme label="b" type="f" corresp="#s7:l4">eeuwigheden</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Zy vyerighlijck <rhyme label="b" type="f" corresp="#s7:l4">aenbeden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laet ons met hert, en <rhyme label="a" type="f">sinnen</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Wtwendigh, en van <rhyme label="a" type="f" corresp="#s8:l1">binnen</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Dat soete Kindt <rhyme label="a" type="f" corresp="#s8:l1">herminnen</rhyme>:</l>
        <l id="s8:l4" met="3" type="-+">Het welcke <rhyme label="b" type="f">kortelinghen</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Door soete <rhyme label="b" type="f" corresp="#s8:l4">minne-stringhen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Ons tot de deught komt <rhyme label="b" type="f" corresp="#s8:l4">dwinghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
