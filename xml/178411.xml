<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>178411</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <tags/>
    <melody/>
  <author>Anonymous</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="1" type="-+"><rhyme label="a" type="m">Vry-heid</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Die het Volk in 't Juk <rhyme label="a" type="m" corresp="#s1:l1">leid</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Burger Tweedragt <rhyme label="a" type="m" corresp="#s1:l1">uitbreid</rhyme>.</l>
        <l id="s1:l4" met="5" type="+-">Doemen wy, als braave <rhyme label="b" type="f">Batavieren</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Laat Traiters het Land <rhyme label="b" type="f" corresp="#s1:l4">bestieren</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Nog vlegten wy Eer <rhyme label="b" type="f" corresp="#s1:l4">Laurieren</rhyme>,</l>
        <l id="s1:l7" met="4" type="+-">Om het hooft van Neerlands <rhyme label="c" type="m">Vorst</rhyme>,</l>
        <l id="s1:l8" met="4" type="+-">Die 's Lands zorg gewillig <rhyme label="c" type="m" corresp="#s1:l7">torst</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="1" type="-+"><rhyme label="a" type="m">Moord-Rot</rhyme>!</l>
        <l id="s2:l2" met="3" type="-+">Die met Staat en Prins <rhyme label="a" type="m" corresp="#s2:l1">spot</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Lacht maar vry om ons <rhyme label="a" type="m" corresp="#s2:l1">lot</rhyme>:</l>
        <l id="s2:l4" met="5" type="+-">Ons, die voor de Vryheid zyn <rhyme label="b" type="f">gebooren</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Eer ge onze moed zult <rhyme label="b" type="f" corresp="#s2:l4">smooren</rhyme>,</l>
        <l id="s2:l6" met="4" type="+-">Gaat het Vaderland <rhyme label="b" type="f" corresp="#s2:l4">verlooren</rhyme>,</l>
        <l id="s2:l7" met="4" type="+-">Drygt ons dan met <rhyme label="c" type="m">dwing'landy</rhyme>,</l>
        <l id="s2:l8" met="4" type="+-">Haast vegt elk Bataav' zig <rhyme label="c" type="m" corresp="#s2:l7">vry</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="1" type="-+">
          <rhyme label="a" type="m">Nooit-zult</rhyme>
        </l>
        <l id="s3:l2" met="3" type="-+">Gy, Tyrannen! de <rhyme label="a" type="m" corresp="#s3:l1">onschult</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Hoe gy op haar <rhyme label="a" type="m" corresp="#s3:l1">aanbrult</rhyme>,</l>
        <l id="s3:l4" met="5" type="+-">Door uw lastertong voor u-zien <rhyme label="b" type="f">bukken</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En, als gy Haar wilt <rhyme label="b" type="f" corresp="#s3:l4">drukken</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Dan slaan wy u aan <rhyme label="b" type="f" corresp="#s3:l4">stukken</rhyme>,</l>
        <l id="s3:l7" met="4" type="+-">Voor Verraders is geen <rhyme label="c" type="m">quartier</rhyme>,</l>
        <l id="s3:l8" met="4" type="+-">By een eerlyk <rhyme label="c" type="m" corresp="#s3:l7">Batavier</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="1" type="-+"><rhyme label="a" type="m">Waagt-niet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Dat ge ons trots het hooft <rhyme label="a" type="m" corresp="#s4:l1">bied</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Of boosaardig <rhyme label="a" type="m" corresp="#s4:l1">aanziet</rhyme>,</l>
        <l id="s4:l4" met="5" type="+-">Want wy zullen U wat anders <rhyme label="b" type="f">leeren</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Dan Land en <rhyme label="b" type="f" corresp="#s4:l4">Burger-Heeren</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Met Wapens te <rhyme label="b" type="f" corresp="#s4:l4">trotseeren</rhyme>,</l>
        <l id="s4:l7" met="4" type="+-">Die Oranje mint sterft <rhyme label="c" type="m">voor</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Staat en Prins: of slaat 'er <rhyme label="c" type="m" corresp="#s4:l7">door</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
