<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>9976</id>
    <date>
      <year-start>1649</year-start>
      <year-end>1649</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Je suis le plus ferme amant</melody>
  <author>Isaak A. van Vaerlen en Jan Jansz. van Asten</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">ICk draegh d'aldertrouste <rhyme label="a" type="m">Min</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Van die 'k in de werelt <rhyme label="a" type="m" corresp="#s1:l1">vin</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Ick ben de tweede <rhyme label="b" type="m">Paris</rhyme>,</l>
        <l id="s1:l4" met="3" type="+-">Mijn Heleen' heet <rhyme label="c" type="f">Maeyken</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Hare moeder te <rhyme label="b" type="m" corresp="#s1:l3">Paris</rhyme></l>
        <l id="s1:l6" met="3" type="+-">speedt daer 't <rhyme label="c" type="f" corresp="#s1:l4">Papegaeyken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Maeykens soetheydt heeft een <rhyme label="a" type="m">val</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">'t Is als Honigh sonder <rhyme label="a" type="m" corresp="#s2:l1">gal</rhyme>.</l>
        <l id="s2:l3" met="4" type="+-">Haer Neus, haer Mondt, en haer <rhyme label="b" type="m">Oogh</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">Altijdt <rhyme label="c" type="f">Disteleren</rhyme>.</l>
        <l id="s2:l5" met="4" type="+-">'k Sie deurgaens 't geen niet en <rhyme label="b" type="m" corresp="#s2:l3">doogh</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Aen haer vuyle <rhyme label="c" type="f" corresp="#s2:l4">kleeren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Doen ick eerst haer schoonheyt <rhyme label="a" type="m">sagh</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Kreegh ick sulck een grooten <rhyme label="a" type="m" corresp="#s3:l1">lach</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Om dees' overschoone <rhyme label="b" type="m">Vrouw'</rhyme></l>
        <l id="s3:l4" met="3" type="+-">Die wel licht <rhyme label="c" type="f">ontstellen</rhyme>,</l>
        <l id="s3:l5" met="4" type="+-">En tot liefd' beweghen <rhyme label="b" type="m" corresp="#s3:l3">souw'</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Den Poortier der <rhyme label="c" type="f" corresp="#s3:l4">Hellen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Haer scheel aensight is heel <rhyme label="a" type="m">gaef</rhyme>.</l>
        <l id="s4:l2" met="4" type="+-">Van couleur ghelijck een <rhyme label="a" type="m" corresp="#s4:l1">Raef</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Uyen is soo kraghtigh <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Als haer Roode <rhyme label="c" type="f">Ooghen</rhyme>,</l>
        <l id="s4:l5" met="4" type="+-">Elck een wordt (die haer <rhyme label="b" type="m" corresp="#s4:l3">besiet</rhyme>)</l>
        <l id="s4:l6" met="3" type="+-">Tot schreyen <rhyme label="c" type="f" corresp="#s4:l4">bewoghen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Gheen schoonheydt soo <rhyme label="a" type="m">onghemeen</rhyme></l>
        <l id="s5:l2" met="4" type="+-">(Soo van Aensight als van <rhyme label="a" type="m" corresp="#s5:l1">Leen</rhyme>)</l>
        <l id="s5:l3" met="4" type="+-">Niemant oyt ter Wereldt <rhyme label="b" type="m">vondt</rhyme>,</l>
        <l id="s5:l4" met="3" type="+-">Of sal konnen <rhyme label="c" type="f">vinden</rhyme>,</l>
        <l id="s5:l5" met="4" type="+-">'t Backes lijckt (heel dick en <rhyme label="b" type="m" corresp="#s5:l3">rondt</rhyme>)</l>
        <l id="s5:l6" met="3" type="+-">Een van de vier <rhyme label="c" type="f" corresp="#s5:l4">winden</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Indien sy my wierdt <rhyme label="a" type="m">ontschaeckt</rhyme>,</l>
        <l id="s6:l2" met="4" type="+-">En afhandigh wierdt <rhyme label="a" type="m" corresp="#s6:l1">ghemaeckt</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">Soo d'Orpheus is <rhyme label="b" type="m">gheschiedt</rhyme>,</l>
        <l id="s6:l4" met="3" type="+-">Moch ick my <rhyme label="c" type="f">verkloecken</rhyme>,</l>
        <l id="s6:l5" met="4" type="+-">Ende gaen haer nerghens <rhyme label="b" type="m" corresp="#s6:l3">niet</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Als by Pluto <rhyme label="c" type="f" corresp="#s6:l4">soecken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
