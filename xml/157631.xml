<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>157631</id>
    <date>
      <year-start>1659</year-start>
      <year-end>1659</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Cette cruelle</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="5" type="-+">HOe dickmael siet men dit <rhyme label="a" type="f">gebeuren</rhyme>,</l>
        <l id="s1:l2" met="5" type="-+">Dat menich Dochter wort <rhyme label="b" type="m">gevrijt</rhyme>,</l>
        <l id="s1:l3" met="5" type="-+">En dat de Minnaer staegh moet <rhyme label="a" type="f" corresp="#s1:l1">treuren</rhyme>,</l>
        <l id="s1:l4" met="5" type="-+">En raeckt sijn soete Nimphje <rhyme label="b" type="m" corresp="#s1:l2">quijt</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">Schoon dat sijn <rhyme label="c" type="f">hope</rhyme></l>
        <l id="s1:l6" met="3" type="+-">Hem gaf goeden <rhyme label="d" type="m">moedt</rhyme>,</l>
        <l id="s1:l7" met="2" type="-+">Om snel te <rhyme label="c" type="f" corresp="#s1:l5">loopen</rhyme></l>
        <l id="s1:l8" met="3" type="+-">Naer 't gewenste <rhyme label="d" type="m" corresp="#s1:l6">soet</rhyme>,</l>
        <l id="s1:l9" met="3" type="-+">Dat hy nu missen <rhyme label="d" type="m" corresp="#s1:l6">moet</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="5" type="-+">Hoe dickmael schuylter in het <rhyme label="a" type="f">minnen</rhyme>,</l>
        <l id="s2:l2" met="5" type="-+">In 't heymelijck een blauwe <rhyme label="b" type="m">scheen</rhyme>,</l>
        <l id="s2:l3" met="5" type="-+">Hoe vaeck soo dwalen al de <rhyme label="a" type="f" corresp="#s2:l1">sinnen</rhyme></l>
        <l id="s2:l4" met="5" type="-+">Mistroostigh t'samen onder <rhyme label="b" type="m" corresp="#s2:l2">een</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Schier sonder <rhyme label="c" type="f">ruste</rhyme></l>
        <l id="s2:l6" met="3" type="+-">Leeft men in de <rhyme label="d" type="m">Min</rhyme>,</l>
        <l id="s2:l7" met="2" type="-+">De soete <rhyme label="c" type="f" corresp="#s2:l5">lusten</rhyme>,</l>
        <l id="s2:l8" met="3" type="+-">Neemt het hert en <rhyme label="d" type="m" corresp="#s2:l6">sin</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Met al zijn krachten <rhyme label="d" type="m" corresp="#s2:l6">in</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="5" type="-+">Dan wenst de soet <rhyme label="a" type="f">Harderinne</rhyme></l>
        <l id="s3:l2" met="5" type="-+">Eens weder by haer lief te <rhyme label="b" type="m">zijn</rhyme>,</l>
        <l id="s3:l3" met="5" type="-+">En wederom de oude <rhyme label="a" type="f" corresp="#s3:l1">minne</rhyme></l>
        <l id="s3:l4" met="5" type="-+">Te plegen, en niet meer in <rhyme label="b" type="m" corresp="#s3:l2">schijn</rhyme></l>
        <l id="s3:l5" met="2" type="-+">Veel stuerheyt <rhyme label="c" type="f">toonen</rhyme>,</l>
        <l id="s3:l6" met="3" type="+-">Maer met <rhyme label="d" type="m">trouwigheyt</rhyme>,</l>
        <l id="s3:l7" met="2" type="-+">Haer Minnaer <rhyme label="c" type="f" corresp="#s3:l5">loonen</rhyme>,</l>
        <l id="s3:l8" met="3" type="+-">Als sy wort <rhyme label="d" type="m" corresp="#s3:l6">gevleyt</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Door Liefd' vol <rhyme label="d" type="m" corresp="#s3:l6">soetigheyt</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="5" type="-+">Waerom ghy teere <rhyme label="a" type="f">Harderinnen</rhyme>,</l>
        <l id="s4:l2" met="5" type="-+">Waerom soo straf als ghy 't niet <rhyme label="b" type="m">meent</rhyme>,</l>
        <l id="s4:l3" met="5" type="-+">Waerom te veynsen in het <rhyme label="a" type="f" corresp="#s4:l1">minnen</rhyme>,</l>
        <l id="s4:l4" met="5" type="-+">Te toonen u schier als <rhyme label="b" type="m" corresp="#s4:l2">versteent</rhyme>?</l>
        <l id="s4:l5" met="2" type="-+">Hoe dat wy <rhyme label="c" type="f">vleyen</rhyme></l>
        <l id="s4:l6" met="3" type="+-">Aen u soete <rhyme label="d" type="m">Ieught</rhyme>,</l>
        <l id="s4:l7" met="2" type="-+">'t Is in het <rhyme label="c" type="f" corresp="#s4:l5">scheyen</rhyme></l>
        <l id="s4:l8" met="3" type="+-">Maer een schijn van <rhyme label="d" type="m" corresp="#s4:l6">vreught</rhyme>,</l>
        <l id="s4:l9" met="3" type="-+">En vol van <rhyme label="d" type="m" corresp="#s4:l6">ongeneught</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="5" type="-+">Hoort waerde Nimph, en soete <rhyme label="a" type="f">susje</rhyme>,</l>
        <l id="s5:l2" met="5" type="-+">Schiet doch een soete straeltjen <rhyme label="b" type="m">uyt</rhyme>,</l>
        <l id="s5:l3" met="5" type="-+">En gunt my neffens dien een <rhyme label="a" type="f" corresp="#s5:l1">kusje</rhyme>,</l>
        <l id="s5:l4" met="5" type="-+">Want ick u Lief, en ghy mijn <rhyme label="b" type="m" corresp="#s5:l2">Bruyt</rhyme></l>
        <l id="s5:l5" met="2" type="-+">Sult heden <rhyme label="c" type="f">wesen</rhyme>,</l>
        <l id="s5:l6" met="3" type="+-">Soo het u <rhyme label="d" type="m">behaeght</rhyme>,</l>
        <l id="s5:l7" met="2" type="-+">Godin <rhyme label="c" type="f" corresp="#s5:l5">gepresen</rhyme></l>
        <l id="s5:l8" met="3" type="+-">My niet langer <rhyme label="d" type="m" corresp="#s5:l6">plaeght</rhyme>,</l>
        <l id="s5:l9" met="3" type="-+">Maer weder-liefde <rhyme label="d" type="m" corresp="#s5:l6">draeght</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="5" type="-+">De Bruylofts-Kroon die sal u <rhyme label="a" type="f">cieren</rhyme>,</l>
        <l id="s6:l2" met="5" type="-+">Gelijck als een Godinne <rhyme label="b" type="m">schoon</rhyme>,</l>
        <l id="s6:l3" met="5" type="-+">Men sal u heusheyt en <rhyme label="a" type="f" corresp="#s6:l1">manieren</rhyme></l>
        <l id="s6:l4" met="5" type="-+">Vertoonen op een Gouden <rhyme label="b" type="m" corresp="#s6:l2">Throon</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Daer sult ghy <rhyme label="c" type="f">wesen</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Mijn beminde <rhyme label="d" type="m">Bruyt</rhyme>,</l>
        <l id="s6:l7" met="2" type="-+">Mijn <rhyme label="c" type="f" corresp="#s6:l5">uyt-gelesen</rhyme>,</l>
        <l id="s6:l8" met="3" type="+-">En door <rhyme label="d" type="m" corresp="#s6:l6">Liefds-besluyt</rhyme>,</l>
        <l id="s6:l9" met="3" type="-+">In Liefde munten <rhyme label="d" type="m" corresp="#s6:l6">uyt</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
