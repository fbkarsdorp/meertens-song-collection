<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>7263</id>
    <date>
      <year-start>1630</year-start>
      <year-end>1640</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>craft song</tag>
    </tags>
    <melody>O welig Amsterdam</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aurora brenght den <rhyme label="a" type="m">dach</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Licht Phebus dat ick <rhyme label="a" type="m" corresp="#s1:l1">mach</rhyme></l>
        <l id="s1:l3" met="3" type="+-">Haest de Waghen <rhyme label="b" type="m">sien</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met Paerden op de <rhyme label="c" type="m">gangh</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Komt Voerman ick <rhyme label="c" type="m" corresp="#s1:l4">verlangh</rhyme>,</l>
        <l id="s1:l6" met="3" type="+-">Mijn Hartjen <rhyme label="b" type="m" corresp="#s1:l3">ontwien</rhyme>,</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Wat hoor ick daer op <rhyme label="a" type="m">straet</rhyme>?</l>
        <l id="s2:l2" met="3" type="-+">Ist niet de Waghen? <rhyme label="a" type="m" corresp="#s2:l1">Iae’t</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Die soo luyde <rhyme label="b" type="m">raest</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Wel Voerman benje <rhyme label="c" type="m">daer</rhyme>?</l>
        <l id="s2:l5" met="3" type="-+">Volck setje by <rhyme label="c" type="m" corresp="#s2:l4">malkaer</rhyme>,</l>
        <l id="s2:l6" met="3" type="+-">Het is seper <rhyme label="b" type="m" corresp="#s2:l3">haest</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Nou setje daer te <rhyme label="a" type="m">deech</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De plaets is ginder <rhyme label="a" type="m" corresp="#s3:l1">leech</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Hoe verdringhje <rhyme label="b" type="m">my</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Ey setje liever <rhyme label="c" type="m">voor</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Ick voegh my daer ick <rhyme label="c" type="m" corresp="#s3:l4">hoor</rhyme></l>
        <l id="s3:l6" met="3" type="+-">Aen mijn Liefkens <rhyme label="b" type="m" corresp="#s3:l3">zy</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nou slaet aen Voerman <rhyme label="a" type="m">rap</rhyme></l>
        <l id="s4:l2" met="3" type="-+">De strenghene hanghen <rhyme label="a" type="m" corresp="#s4:l1">slap</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Van jou eene <rhyme label="b" type="m">Paert</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Ey lieve hoort mijn <rhyme label="c" type="m">bee</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Rijt flus niet diep in <rhyme label="c" type="m" corresp="#s4:l4">Zee</rhyme>,</l>
        <l id="s4:l6" met="3" type="+-">‘k Ben soo licht <rhyme label="b" type="m" corresp="#s4:l3">vervaert</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Daer komt een <rhyme label="a" type="m">Somerblaegh</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Nou teghen, dats een <rhyme label="a" type="m" corresp="#s5:l1">plaegh</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Drijft wech watte <rhyme label="b" type="m">lap</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Mijn dunckt ick voel al <rhyme label="c" type="m">nat</rhyme>:</l>
        <l id="s5:l5" met="3" type="-+">Ey Voerman leent mee <rhyme label="c" type="m" corresp="#s5:l4">wat</rhyme></l>
        <l id="s5:l6" met="3" type="+-">De waghenaere <rhyme label="b" type="m" corresp="#s5:l3">Kap</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Maets rijtje nae u <rhyme label="a" type="m">sin</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Soo ’t flus der Duynen <rhyme label="a" type="m" corresp="#s6:l1">in</rhyme></l>
        <l id="s6:l3" met="3" type="+-">Immers wesen <rhyme label="b" type="m">most</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Segh, waerom queelje <rhyme label="c" type="m">niet</rhyme>?</l>
        <l id="s6:l5" met="3" type="-+">Ick bidje singht een <rhyme label="c" type="m" corresp="#s6:l4">Liet</rhyme></l>
        <l id="s6:l6" met="3" type="+-">Wt verheuchder <rhyme label="b" type="m" corresp="#s6:l3">dorst</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Nou Voerman niet soo <rhyme label="a" type="m">seer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Rijt fasjes nae mijn <rhyme label="a" type="m" corresp="#s7:l1">leer</rhyme>,</l>
        <l id="s7:l3" met="3" type="+-">Dat ick soeter <rhyme label="b" type="m">vaer</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">Dats wel ghehotse <rhyme label="c" type="m">dorst</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Ik wort van daegh <rhyme label="c" type="m" corresp="#s7:l4">gehotst</rhyme></l>
        <l id="s7:l6" met="3" type="+-">Voor het heele <rhyme label="b" type="m" corresp="#s7:l3">Iaer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
