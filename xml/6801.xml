        <lon>51.8322137</lon>
        <lat>6.2428283</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="kerstlied" translation="Christmas song">kerstlied</tag>
    </tags>
    <melody>O zalig heilig Bethlehem</melody>
    <categories>
      <category translation="seasons and annual events">jaarkring</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O salich heylich <rhyme label="a" type="m">Bethlehem</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Onder veel duysent <rhyme label="b" type="f">wtverkoren</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Vereert boven <rhyme label="a" type="m" corresp="#s1:l1">Ierusalem</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Van Iesus is in u <rhyme label="b" type="f" corresp="#s1:l2">geboren</rhyme>.</l>
        <l id="s1:l5" met="4" type="-+">Vereert boven <rhyme label="a" type="m" corresp="#s1:l1">Ierusalem</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Van Iesus is in u <rhyme label="b" type="f" corresp="#s1:l2">geboren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">O Betlehem kleyn grote <rhyme label="a" type="m">Stadt</rhyme>,</l>
        <l id="s2:l2" met="4" type="-+">L;eyn van begrijp, maer groot van <rhyme label="b" type="f">weerden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Gy sijt dat aller eelste <rhyme label="a" type="m" corresp="#s2:l1">vat</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">En aller rijckste Stadt der <rhyme label="b" type="f" corresp="#s2:l2">Eerden</rhyme>.</l>
        <l id="s2:l5" met="4" type="-+">Gy sijt dat aller eelste <rhyme label="a" type="m" corresp="#s2:l1">vat</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">En aller rijckste Stadt der <rhyme label="b" type="f" corresp="#s2:l2">Eerden</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Verheucht u dan o <rhyme label="a" type="m">Israel</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Hoe mochtmen u blijder bootschap <rhyme label="b" type="f">bringen</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Tot u so komt <rhyme label="a" type="m" corresp="#s3:l1">Emanuel</rhyme></l>
        <l id="s3:l4" met="4" type="-+">Wilt wt der sonden slaep <rhyme label="b" type="f" corresp="#s3:l2">ontspringen</rhyme>.</l>
        <l id="s3:l5" met="4" type="-+">Tot u so komt <rhyme label="a" type="m" corresp="#s3:l1">Emanuel</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Wilt wt der sonden slaep <rhyme label="b" type="f" corresp="#s3:l2">ontspringen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">O koningh Christe prince <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Hoe werdt ghy hier als nu <rhyme label="b" type="f">gevonden</rhyme></l>
        <l id="s4:l3" met="4" type="-+">In hoy, in stroo, in sulcker <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">In arme doecxkens teer <rhyme label="b" type="f" corresp="#s4:l2">gewonden</rhyme>.</l>
        <l id="s4:l5" met="4" type="-+">In hoy, in stroo, in sulcker <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme>,</l>
        <l id="s4:l6" met="4" type="-+">In arme doecxkens teer <rhyme label="b" type="f" corresp="#s4:l2">gewonden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Gy hebt dat firmament <rhyme label="a" type="m">gemaeckt</rhyme>,</l>
        <l id="s5:l2" met="4" type="-+">Daer u loven des hemels <rhyme label="b" type="f">geesten</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Maer nu geheel bloot en <rhyme label="a" type="m" corresp="#s5:l1">naeckt</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Ligt gy int midden van den <rhyme label="b" type="f" corresp="#s5:l2">beesten</rhyme>.</l>
        <l id="s5:l5" met="4" type="-+">Maer nu geheel bloot en <rhyme label="a" type="m" corresp="#s5:l1">naeckt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">Ligt gy int midden van den <rhyme label="b" type="f" corresp="#s5:l2">beesten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ghy wert geboren in een <rhyme label="a" type="m">Stal</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Niemant bekent in der nacht <rhyme label="b" type="f">stille</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Die Engel songen over <rhyme label="a" type="m" corresp="#s6:l1">al</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Peys met den menschen van goeden <rhyme label="b" type="f" corresp="#s6:l2">wille</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Die Engel songen over <rhyme label="a" type="m" corresp="#s6:l1">al</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Peys met den menschen van goeden <rhyme label="b" type="f" corresp="#s6:l2">wille</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">O macgtich Godt, o Iesu <rhyme label="a" type="m">soet</rhyme>,</l>
        <l id="s7:l2" met="4" type="-+">Wat liefd heeft u daer toe <rhyme label="b" type="f">getrocken</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Dat ghy aenneemt ons fleisch en <rhyme label="a" type="m" corresp="#s7:l1">bloet</rhyme></l>
        <l id="s7:l4" met="4" type="-+">Om ons tot u alsoo te <rhyme label="b" type="f" corresp="#s7:l2">locken</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Dat ghy aenneemt ons fleisch en <rhyme label="a" type="m" corresp="#s7:l1">bloet</rhyme></l>
        <l id="s7:l6" met="4" type="-+">Om ons tot u alsoo te <rhyme label="b" type="f" corresp="#s7:l2">locken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Kompt tot dit kint ghy Adams <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s8:l2" met="4" type="-+">Hoe kunt ghy noch die werelt <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Siet hoe Iesus sich met u <rhyme label="a" type="m" corresp="#s8:l1">verbindt</rhyme></l>
        <l id="s8:l4" met="4" type="-+">Offert hem gansch u hert en <rhyme label="b" type="f" corresp="#s8:l2">sinnen</rhyme>.</l>
        <l id="s8:l5" met="4" type="-+">Siet hoe Iesus sich met u <rhyme label="a" type="m" corresp="#s8:l1">verbindt</rhyme></l>
        <l id="s8:l6" met="4" type="-+">Offert hem gansch u hert en <rhyme label="b" type="f" corresp="#s8:l2">sinnen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
