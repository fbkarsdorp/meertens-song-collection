  <author>Jacob Claesz</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">AEnmerckt met vlijt wel Godes <rhyme label="a" type="m">woort</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Met neersticheyt <rhyme label="a" type="m" corresp="#s1:l1">aenhoort</rhyme></l>
        <l id="s1:l3" met="3" type="-+">En vierich gaet oock <rhyme label="a" type="m" corresp="#s1:l1">voort</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Den Heer al met <rhyme label="a" type="m" corresp="#s1:l1">accoort</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Wilt loven <rhyme label="a" type="m" corresp="#s1:l1">onversmoort</rhyme></l>
        <l id="s1:l6" met="3" type="-+">In rechte <rhyme label="a" type="m" corresp="#s1:l1">wederboort</rhyme>:</l>
        <l id="s1:l7" met="4" type="-+">Wilt hem na gaen hoort dit <rhyme label="b" type="m">belijt</rhyme></l>
        <l id="s1:l8" met="4" type="-+">Want het u ziel is groot <rhyme label="b" type="m" corresp="#s1:l7">profijt</rhyme></l>
        <l id="s1:l9" met="3" type="-+">Om na eeuwich te <rhyme label="c" type="f">leven</rhyme></l>
        <l id="s1:l10" met="4" type="-+">Met Christo d'Heer <rhyme label="b" type="m" corresp="#s1:l7">ghebenedijt</rhyme>,</l>
        <l id="s1:l11" met="4" type="+-">Hem eert en dient met alle <rhyme label="b" type="m" corresp="#s1:l7">vlijt</rhyme></l>
        <l id="s1:l12" met="3" type="-+">Soo sal hy ruste <rhyme label="c" type="f" corresp="#s1:l9">gheven</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Den Heer die moet hier sijn <rhyme label="a" type="m">gheeert</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sijn lof moet sijn <rhyme label="a" type="m" corresp="#s2:l1">vermeert</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">In hem <rhyme label="a" type="m" corresp="#s2:l1">glorificeert</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">Na zijn wille <rhyme label="a" type="m" corresp="#s2:l1">useert</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Versaeckt en <rhyme label="a" type="m" corresp="#s2:l1">persiqueert</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">En uwen moet <rhyme label="a" type="m" corresp="#s2:l1">verneert</rhyme></l>
        <l id="s2:l7" met="4" type="-+">Want Godt de Heer alleen hooch <rhyme label="b" type="m">is</rhyme>,</l>
        <l id="s2:l8" met="4" type="-+">Een Schepper van alles <rhyme label="b" type="m" corresp="#s2:l7">ghewis</rhyme></l>
        <l id="s2:l9" met="3" type="-+">Looft hem al sonder <rhyme label="c" type="f">sneven</rhyme>,</l>
        <l id="s2:l10" met="4" type="-+">V wil laet zijn in hem vry <rhyme label="b" type="m" corresp="#s2:l7">fris</rhyme>,</l>
        <l id="s2:l11" met="4" type="+-">Soo sal u vreuchd' niet moghen <rhyme label="b" type="m" corresp="#s2:l7">mis</rhyme></l>
        <l id="s2:l12" met="3" type="-+">Naemaels wert ghy <rhyme label="c" type="f" corresp="#s2:l9">verheven</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Wie hem verneert seyt Christus <rhyme label="a" type="m">claer</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die wert int <rhyme label="a" type="m" corresp="#s3:l1">openbaer</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Naemaels verhoocht tis <rhyme label="a" type="m" corresp="#s3:l1">waer</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Dus Christen <rhyme label="a" type="m" corresp="#s3:l1">allegaer</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Al valt het dat  vleys <rhyme label="a" type="m" corresp="#s3:l1">zwaer</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Door strijt tsy hier of <rhyme label="a" type="m" corresp="#s3:l1">daer</rhyme></l>
        <l id="s3:l7" met="4" type="-+">Soo stelt u cloeck en <rhyme label="b" type="m">fijn</rhyme></l>
        <l id="s3:l8" met="4" type="-+">Teghen des werelts <rhyme label="b" type="m" corresp="#s3:l7">lijn</rhyme>,</l>
        <l id="s3:l9" met="3" type="-+">Doort gheloof wilt <rhyme label="c" type="f">verwinnen</rhyme>,</l>
        <l id="s3:l10" met="4" type="-+">Alle boosheyt in desen <rhyme label="b" type="m" corresp="#s3:l7">tijn</rhyme>,</l>
        <l id="s3:l11" met="4" type="+-">Wacht u al voor eenich qua <rhyme label="b" type="m" corresp="#s3:l7">schijn</rhyme></l>
        <l id="s3:l12" met="3" type="-+">Wilt Godt alleen <rhyme label="c" type="f" corresp="#s3:l9">beminnen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Prinslijcke vrienden <rhyme label="a" type="m">delicaet</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Die door dat eeuwich <rhyme label="a" type="m" corresp="#s4:l1">zaet</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Zijt vry van s'Duyvels <rhyme label="a" type="m" corresp="#s4:l1">smaet</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Dus nu in eenen <rhyme label="a" type="m" corresp="#s4:l1">graet</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Soo laet ons Christus <rhyme label="a" type="m" corresp="#s4:l1">raet</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Nu gaen al inder <rhyme label="a" type="m" corresp="#s4:l1">maet</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">En troulijck blijven <rhyme label="b" type="m">staen</rhyme></l>
        <l id="s4:l8" met="4" type="-+">Sonder daer af te <rhyme label="b" type="m" corresp="#s4:l7">gaen</rhyme></l>
        <l id="s4:l9" met="3" type="-+">Soo sullen wy <rhyme label="c" type="f">ontfanghen</rhyme></l>
        <l id="s4:l10" met="4" type="-+">De croon des levens hoort <rhyme label="b" type="m" corresp="#s4:l7">t'vermaen</rhyme>,</l>
        <l id="s4:l11" met="4" type="+-">Die sullen Gods kinderen <rhyme label="b" type="m" corresp="#s4:l7">ontfaen</rhyme></l>
        <l id="s4:l12" met="3" type="-+">Loof Godt met blyde <rhyme label="c" type="f" corresp="#s4:l9">sanghen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
