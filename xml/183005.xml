<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>183005</id>
    <date>
      <year-start>1788</year-start>
      <year-end>1788</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Sinjeur de Schelde</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f">Oranje</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Geteeld uit Nassauws <rhyme label="b" type="m">bloed</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Verzwagerd aan <rhyme label="a" type="f" corresp="#s1:l1">Germanje</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Smaakt na veel zuur weêr 't <rhyme label="b" type="m" corresp="#s1:l2">zoet</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">Zich Kerk en Staat te <rhyme label="c" type="f">wyden</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Is deeze Held van <rhyme label="d" type="m">zins</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Hy zal het Land <rhyme label="c" type="f" corresp="#s1:l5">bevryden</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Vivat! lang leeft de <rhyme label="d" type="m" corresp="#s1:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Hebt dank ô Heeren <rhyme label="a" type="f">Staaten</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Die deez' <rhyme label="b" type="m">Oranje-Held</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Tot spyt van die hem <rhyme label="a" type="f" corresp="#s2:l1">haaten</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">In voorigen luister <rhyme label="b" type="m" corresp="#s2:l2">steld</rhyme>.</l>
        <l id="s2:l5" met="3" type="-+">Die snoode staan te <rhyme label="c" type="f">vreezen</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Te kyken hier en <rhyme label="d" type="m">gins</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Met schaamte in 't oog te <rhyme label="c" type="f" corresp="#s2:l5">leesen</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Vivat! Vivat! de <rhyme label="d" type="m" corresp="#s2:l6">Prins</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">God liet ons niet <rhyme label="a" type="f">bezwyken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">In onzen bangen <rhyme label="b" type="m">nood</rhyme>;</l>
        <l id="s3:l3" met="3" type="-+">Laat ons niet van hem <rhyme label="a" type="f" corresp="#s3:l1">wyken</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Looft, looft hem; klein en <rhyme label="b" type="m" corresp="#s3:l2">groot</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Wild vreugde lichten <rhyme label="c" type="f">branden</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Gelyk men is van <rhyme label="d" type="m">zins</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">Roept nu door Stad en <rhyme label="c" type="f" corresp="#s3:l5">Landen</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">Vivat! lang leeft de <rhyme label="d" type="m" corresp="#s3:l6">Prins</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Nu jaagen alle <rhyme label="a" type="f">Braaven</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Van ons <rhyme label="b" type="m">Gemeenebest</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">De snoode, die als <rhyme label="a" type="f" corresp="#s4:l1">raaven</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Ons dreigden uit hun <rhyme label="b" type="m" corresp="#s4:l2">nest</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Die ons wil <rhyme label="c" type="f">massacreeren</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Zal vluchten hier en <rhyme label="d" type="m">gins</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Wy mogen <rhyme label="c" type="f" corresp="#s4:l5">triumpheeren</rhyme>,</l>
        <l id="s4:l8" met="3" type="-+">Vivat! vivat! de <rhyme label="d" type="m" corresp="#s4:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Der Pruissen wakkre <rhyme label="a" type="f">Koning</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Erbarm zich over <rhyme label="b" type="m">ons</rhyme>;</l>
        <l id="s5:l3" met="3" type="-+">Oranje, (wat <rhyme label="a" type="f" corresp="#s5:l1">vertooning</rhyme>!)</l>
        <l id="s5:l4" met="3" type="-+">Geeft 't booze rot de <rhyme label="b" type="m" corresp="#s5:l2">bons</rhyme>?</l>
        <l id="s5:l5" met="3" type="-+">Hoord Pauken en <rhyme label="c" type="f">Trompetten</rhyme>:</l>
        <l id="s5:l6" met="3" type="-+">Nu speelen hier en <rhyme label="d" type="m">gins</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">Wie zal de vreugd <rhyme label="c" type="f" corresp="#s5:l5">beletten</rhyme>?</l>
        <l id="s5:l8" met="3" type="-+">Vivat! lang leeft de <rhyme label="d" type="m" corresp="#s5:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">De Leeuw schrikt nu voor 't <rhyme label="a" type="f">kraaijen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Van 't slegte rot niet <rhyme label="b" type="m">meer</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">De Oranje-vlaggen <rhyme label="a" type="f" corresp="#s6:l1">waaijen</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Tot Vyfde Willems <rhyme label="b" type="m" corresp="#s6:l2">eer</rhyme>.</l>
        <l id="s6:l5" met="3" type="-+">Men zweert op nieuw hem <rhyme label="c" type="f">trouwe</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">En zingt vast hier en <rhyme label="d" type="m">gins</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Wilhelmus van <rhyme label="c" type="f" corresp="#s6:l5">Nassouwe</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Vivat! Vivat! de <rhyme label="d" type="m" corresp="#s6:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Van Hof en Tooren <rhyme label="a" type="f">transsen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Waaid nu de <rhyme label="b" type="m">Oranje-vlag</rhyme>;</l>
        <l id="s7:l3" met="3" type="-+">Wat onverwachte <rhyme label="a" type="f" corresp="#s7:l1">glansen</rhyme>!</l>
        <l id="s7:l4" met="3" type="-+">ô Aangenaame <rhyme label="b" type="m" corresp="#s7:l2">dag</rhyme>?</l>
        <l id="s7:l5" met="3" type="-+">God zegen de Eed'le <rhyme label="c" type="f">Mannen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Die lustig zyn van <rhyme label="d" type="m">zins</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Dit Volk te <rhyme label="c" type="f" corresp="#s7:l5">verbannen</rhyme>.</l>
        <l id="s7:l8" met="3" type="-+">Vivat! Vivat! de <rhyme label="d" type="m" corresp="#s7:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Men ziet Oranje <rhyme label="a" type="f">treeden</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Van groot en klein <rhyme label="b" type="m">begroet</rhyme>;</l>
        <l id="s8:l3" met="3" type="-+">In Neêrland vryje <rhyme label="a" type="f" corresp="#s8:l1">Steeden</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Herleeft weêr de oude <rhyme label="b" type="m" corresp="#s8:l2">moed</rhyme>,</l>
        <l id="s8:l5" met="3" type="-+">Om Vryheid te <rhyme label="c" type="f">onderschraagen</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Gelyk wy zyn van <rhyme label="d" type="m">zins</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">Elk roept in 's <rhyme label="c" type="f" corresp="#s8:l5">Gravenhaagen</rhyme>,</l>
        <l id="s8:l8" met="3" type="-+">Vivat! lang leeft de <rhyme label="d" type="m" corresp="#s8:l6">Prins</rhyme>!</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">Speeld Klokken en <rhyme label="a" type="f">Kartouwe</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">In Stad, op Dorp, aan 't <rhyme label="b" type="m">Strand</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Wilhelmus van <rhyme label="a" type="f" corresp="#s9:l1">Nassouwe</rhyme>,</l>
        <l id="s9:l4" met="3" type="-+">De Wellust van ons <rhyme label="b" type="m" corresp="#s9:l2">Land</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Word door de <rhyme label="c" type="f">Burgerheeren</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Verwellekomt <rhyme label="d" type="m">alzins</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">Nu mag hy <rhyme label="c" type="f" corresp="#s9:l5">triumpheeren</rhyme>!</l>
        <l id="s9:l8" met="3" type="-+">Gods zegen volg den <rhyme label="d" type="m" corresp="#s9:l6">Prins</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
