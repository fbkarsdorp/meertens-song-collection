<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>13889</id>
    <date>
      <year-start>1646</year-start>
      <year-end>1646</year-end>
    </date>
    <location>
      <orginal-name>haarlem</orginal-name>
      <standard-name>Haarlem, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3885317</lon>
        <lat>4.6388048</lat>
      </coordinates>
    </location>
    <tags>
      <tag>farewell song</tag>
      <tag>praise</tag>
    </tags>
    <melody>Phoebus is lang over de zee</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IOnghe Luyden datjet <rhyme label="a" type="m">wist</rhyme></l>
        <l id="s1:l2" met="4" type="+-">Wat een Vrouw ick heb <rhyme label="b" type="f">ghekreghen</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">‘k Wed dat jy jou strackx <rhyme label="a" type="m" corresp="#s1:l1">bepist</rhyme>,</l>
        <l id="s1:l4" met="4" type="+-">Lachen moetje wel ter <rhyme label="b" type="f" corresp="#s1:l2">deghen</rhyme>,</l>
        <l id="s1:l5" met="4" type="+-">Om mijn sonder <rhyme label="c" type="m">dat</rhyme>:</l>
        <l id="s1:l6" met="4" type="+-">Want sy heeft gheen gal of <rhyme label="c" type="m" corresp="#s1:l5">spat</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Van stijfheydt isse <rhyme label="c" type="m" corresp="#s1:l5">radt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">’t Lichaem is in als <rhyme label="a" type="m">volmaeckt</rhyme>,</l>
        <l id="s2:l2" met="4" type="+-">Venus kander niet by <rhyme label="b" type="f">lijcken</rhyme>:</l>
        <l id="s2:l3" met="4" type="+-">Sy ist die de schoonheydt <rhyme label="a" type="m" corresp="#s2:l1">raeckct</rhyme>,</l>
        <l id="s2:l4" met="4" type="+-">Al de wereldt moet haer <rhyme label="b" type="f" corresp="#s2:l2">wijcken</rhyme>:</l>
        <l id="s2:l5" met="4" type="+-">Want haer hooft dat is een <rhyme label="c" type="m">hooft</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Dat een yeders hertje <rhyme label="c" type="m" corresp="#s2:l5">rooft</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">En al ‘t ghesicht <rhyme label="c" type="m" corresp="#s2:l5">verdooft</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Want het is ontrent so <rhyme label="a" type="m">groot</rhyme>,</l>
        <l id="s3:l2" met="4" type="+-">Als men ‘t juyst perfeckt wil <rhyme label="b" type="f">weten</rhyme>,</l>
        <l id="s3:l3" met="4" type="+-">Als de grootste <rhyme label="a" type="m" corresp="#s3:l1">Botter-vloot</rhyme></l>
        <l id="s3:l4" met="4" type="+-">Die oyt Huys-man heeft <rhyme label="b" type="f" corresp="#s3:l2">versleten</rhyme>:</l>
        <l id="s3:l5" met="4" type="+-">En haer Ooghjes, datjet <rhyme label="c" type="m">weet</rhyme>,</l>
        <l id="s3:l6" met="4" type="+-">Zijn als ‘t gat van een <rhyme label="c" type="m" corresp="#s3:l5">Secreet</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">‘t Is yemandt lief of <rhyme label="c" type="m" corresp="#s3:l5">leet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Siet haer Marmer-wanghen <rhyme label="a" type="m">an</rhyme>,</l>
        <l id="s4:l2" met="4" type="+-">En het voorhooft noyt <rhyme label="b" type="f">ghepresen</rhyme>.</l>
        <l id="s4:l3" met="4" type="+-">Mijn dunckt dat het seper <rhyme label="a" type="m" corresp="#s4:l1">kan</rhyme></l>
        <l id="s4:l4" met="4" type="+-">Wel een Wereldts-kaertje <rhyme label="b" type="f" corresp="#s4:l2">wesen</rhyme></l>
        <l id="s4:l5" met="4" type="+-">Voor de slechte, domme <rhyme label="c" type="m">Lien</rhyme>,</l>
        <l id="s4:l6" met="4" type="+-">Om Landt, Steden in te <rhyme label="c" type="m" corresp="#s4:l5">sien</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Dat kan seer licht <rhyme label="c" type="m" corresp="#s4:l5">gheschien</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">’t Neusjen is heel fraey <rhyme label="a" type="m">besneen</rhyme>,</l>
        <l id="s5:l2" met="4" type="+-">‘t Hanght vol schoone roode <rhyme label="b" type="f">Ioopen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">‘t Is ghespaut by-nae in <rhyme label="a" type="m" corresp="#s5:l1">tween</rhyme>,</l>
        <l id="s5:l4" met="4" type="+-">Wieder jonghs wil, mach wat <rhyme label="b" type="f" corresp="#s5:l2">koopen</rhyme>:</l>
        <l id="s5:l5" met="4" type="+-">Dese Vrouw wind op een <rhyme label="c" type="m">Iaer</rhyme></l>
        <l id="s5:l6" met="4" type="+-">Met haer Bloesems <rhyme label="c" type="m" corresp="#s5:l5">allegaer</rhyme></l>
        <l id="s5:l7" met="3" type="-+">De Kost wel voor een <rhyme label="c" type="m" corresp="#s5:l5">Paer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">En haer Mondt die is <rhyme label="a" type="m">bequaem</rhyme></l>
        <l id="s6:l2" met="4" type="+-">Om een Schuer-deur af te <rhyme label="b" type="f">maecken</rhyme>:</l>
        <l id="s6:l3" met="4" type="+-">En het Kinnetje met <rhyme label="a" type="m" corresp="#s6:l1">naem</rhyme>,</l>
        <l id="s6:l4" met="4" type="+-">Kan by-nae haer Hart-kuyl <rhyme label="b" type="f" corresp="#s6:l2">raecken</rhyme>:</l>
        <l id="s6:l5" met="4" type="+-">Goedt om slacht-tijdt uyt te <rhyme label="c" type="m">gaen</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Wie maer Klaeuwen wil of <rhyme label="c" type="m" corresp="#s6:l5">slaen</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Die spreeckt dees Schoonste <rhyme label="c" type="m" corresp="#s6:l5">aen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">’t Halsjen is soo proper <rhyme label="a" type="m">vet</rhyme>,</l>
        <l id="s7:l2" met="4" type="+-">Als de alderdickste <rhyme label="b" type="f">Kasen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Zoo jy op haer Borsten <rhyme label="a" type="m" corresp="#s7:l1">let</rhyme></l>
        <l id="s7:l4" met="4" type="+-">Schijnen sy twee groote <rhyme label="b" type="f" corresp="#s7:l2">Blasen</rhyme>.</l>
        <l id="s7:l5" met="4" type="+-">Ruym drie Ell’ is zy in’t <rhyme label="c" type="m">rondt</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">En sy weeght acht hondert <rhyme label="c" type="m" corresp="#s7:l5">pondt</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Wanneerse is <rhyme label="c" type="m" corresp="#s7:l5">ghesondt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Watter nou noch dient <rhyme label="a" type="m">gheseydt</rhyme>,</l>
        <l id="s8:l2" met="4" type="+-">Zoo vanachter als van <rhyme label="b" type="f">voren</rhyme>,</l>
        <l id="s8:l3" met="4" type="+-">Dat denckt yder met <rhyme label="a" type="m" corresp="#s8:l1">bescheydt</rhyme>,</l>
        <l id="s8:l4" met="4" type="+-">Hoe het zijn mocht nae <rhyme label="b" type="f" corresp="#s8:l2">behooren</rhyme>;</l>
        <l id="s8:l5" met="4" type="+-">Och! ick zwijgh, en segh niet <rhyme label="c" type="m">meer</rhyme>:</l>
        <l id="s8:l6" met="4" type="+-">Want ick lach mijn Buyck soo <rhyme label="c" type="m" corresp="#s8:l5">seer</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">En val schier los om <rhyme label="c" type="m" corresp="#s8:l5">veer</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Wilder yemandt dese <rhyme label="a" type="m">Bruydt</rhyme>,</l>
        <l id="s9:l2" met="4" type="+-">Die kan komen en <rhyme label="b" type="f">versoecken</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Ick versieder aen een <rhyme label="a" type="m" corresp="#s9:l1">duyt</rhyme>,</l>
        <l id="s9:l4" met="4" type="+-">En wil haer noch laten <rhyme label="b" type="f" corresp="#s9:l2">doecken</rhyme>,</l>
        <l id="s9:l5" met="4" type="+-">Alsoo mooy ‘k en weet niet <rhyme label="c" type="m">hoe</rhyme>,</l>
        <l id="s9:l6" met="4" type="+-">Trots een opghepronckte <rhyme label="c" type="m" corresp="#s9:l5">Koe</rhyme>:</l>
        <l id="s9:l7" met="3" type="-+">Ick bense seper <rhyme label="c" type="m" corresp="#s9:l5">moe</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
