<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>20714</id>
    <date>
      <year-start>1664</year-start>
      <year-end>1664</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>Maria song</tag>
    </tags>
    <melody>J'aimerai en ce village</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="+-">AVe Vrouw der <rhyme label="a" type="f">Vrouwen</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Godes waerdste <rhyme label="b" type="m">Bruyd</rhyme>!</l>
        <l id="s1:l3" met="3" type="+-">Die geboeyt doet <rhyme label="a" type="f" corresp="#s1:l1">houwen</rhyme></l>
        <l id="s1:l4" met="3" type="+-">D' Helsch' vervloeckte <rhyme label="b" type="m" corresp="#s1:l2">Guyt</rhyme>;</l>
        <l id="s1:l5" met="3" type="+-">Door wien haer <rhyme label="c" type="f">bekooren</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Eva liet in 't <rhyme label="d" type="m">Pa-ra-dijs</rhyme>:</l>
        <l id="s1:l7" met="3" type="+-">O! Die <rhyme label="c" type="f" corresp="#s1:l5">uytverkoore</rhyme></l>
        <l id="s1:l8" met="4" type="+-">Zijt Ghy, waerdigh lof, en <rhyme label="d" type="m" corresp="#s1:l6">prijs</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="+-">Gratiâ Tu <rhyme label="a" type="f">plena</rhyme></l>
        <l id="s2:l2" met="3" type="+-">In den hooghsten <rhyme label="b" type="m">graed</rhyme>;</l>
        <l id="s2:l3" met="3" type="+-">Tu salutis <rhyme label="a" type="f" corresp="#s2:l1">vena</rhyme>,</l>
        <l id="s2:l4" met="3" type="+-">En ons' <rhyme label="b" type="m" corresp="#s2:l2">toeverlaet</rhyme>.</l>
        <l id="s2:l5" met="3" type="+-">Ghy zijt <rhyme label="c" type="f">Koninginne</rhyme></l>
        <l id="s2:l6" met="4" type="+-">Van het schoone <rhyme label="d" type="m">Hemelrijck</rhyme>:</l>
        <l id="s2:l7" met="3" type="+-">'s Hemels <rhyme label="c" type="f" corresp="#s2:l5">Keyserinne</rhyme>!</l>
        <l id="s2:l8" met="4" type="+-">Geen in hoogheyd uws <rhyme label="d" type="m" corresp="#s2:l6">gelijck</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="+-">Dominus est <rhyme label="a" type="f">Tecum</rhyme>,</l>
        <l id="s3:l2" met="3" type="+-">Alderreynste <rhyme label="b" type="m">Maeght</rhyme>!</l>
        <l id="s3:l3" met="3" type="+-">Utinam &amp; <rhyme label="a" type="f" corresp="#s3:l1">Mecum</rhyme>!</l>
        <l id="s3:l4" met="3" type="+-">Die U bystandt <rhyme label="b" type="m" corresp="#s3:l2">vraeght</rhyme>.</l>
        <l id="s3:l5" met="3" type="+-">Alderliefste <rhyme label="c" type="f">Moeder</rhyme>!</l>
        <l id="s3:l6" met="4" type="+-">Alderwaerdste <rhyme label="d" type="m">Voorsprakers</rhyme>!</l>
        <l id="s3:l7" met="3" type="+-">Weest by ons' <rhyme label="c" type="f" corresp="#s3:l5">behoeder</rhyme></l>
        <l id="s3:l8" met="4" type="+-">Mijner saecke <rhyme label="d" type="m" corresp="#s3:l6">Middelers</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="+-">Tu es <rhyme label="a" type="f">Benedicta</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Onder 't <rhyme label="b" type="m">vrouw-geslacht</rhyme>:</l>
        <l id="s4:l3" met="3" type="+-">Nobis Tu <rhyme label="a" type="f" corresp="#s4:l1">Relicta</rhyme></l>
        <l id="s4:l4" met="3" type="+-">In dees duyst're <rhyme label="b" type="m" corresp="#s4:l2">nacht</rhyme>:</l>
        <l id="s4:l5" met="3" type="+-">O quàm <rhyme label="c" type="f">Benedictus</rhyme></l>
        <l id="s4:l6" met="4" type="+-">Jesus uwes lichaems <rhyme label="d" type="m">vrucht</rhyme>,</l>
        <l id="s4:l7" met="3" type="+-">Qui ut <rhyme label="c" type="f" corresp="#s4:l5">derelictus</rhyme></l>
        <l id="s4:l8" met="4" type="+-">Hingh aen 't Kruys voor ons <rhyme label="d" type="m" corresp="#s4:l6">beducht</rhyme>,</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="+-">Moeder Goods <rhyme label="a" type="f">Maria</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Zonne-schoone <rhyme label="b" type="m">vrouw</rhyme>!</l>
        <l id="s5:l3" met="3" type="+-">Sis salutis <rhyme label="a" type="f" corresp="#s5:l1">via</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Ons, die zijn in <rhyme label="b" type="m" corresp="#s5:l2">rouw</rhyme>.</l>
        <l id="s5:l5" met="3" type="+-">Bidt voor ons op <rhyme label="c" type="f">heden</rhyme>,</l>
        <l id="s5:l6" met="4" type="+-">En voortaen ten allen <rhyme label="d" type="m">tijd</rhyme>;</l>
        <l id="s5:l7" met="3" type="+-">Laet door uw' <rhyme label="c" type="f" corresp="#s5:l5">gebeden</rhyme></l>
        <l id="s5:l8" met="4" type="+-">Naemaels wesen ons <rhyme label="d" type="m" corresp="#s5:l6">verblijdt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="+-">Ah! nè sis in <rhyme label="a" type="f">morâ</rhyme>,</l>
        <l id="s6:l2" met="3" type="+-">Helpt ons stonden <rhyme label="b" type="m">aen</rhyme>!</l>
        <l id="s6:l3" met="3" type="+-">Sis in mortis <rhyme label="a" type="f" corresp="#s6:l1">horâ</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Ons, een held're <rhyme label="b" type="m" corresp="#s6:l2">Maen</rhyme>.</l>
        <l id="s6:l5" met="3" type="+-">Bidt des hemels <rhyme label="c" type="f">Koningh</rhyme>,</l>
        <l id="s6:l6" met="4" type="+-">Dat Hy onse sond' <rhyme label="d" type="m">vergeeft</rhyme>;</l>
        <l id="s6:l7" met="3" type="+-">Brenght in 's hemels <rhyme label="c" type="f" corresp="#s6:l5">wooningh</rhyme></l>
        <l id="s6:l8" met="4" type="+-">Ons by God, waer Gh' eeuwigh <rhyme label="d" type="m" corresp="#s6:l6">leeft</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="+-">Maget! wilt <rhyme label="a" type="f">ontfermen</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Ons in onse <rhyme label="b" type="m">noot</rhyme>.</l>
        <l id="s7:l3" met="3" type="+-">Wilt ons' ziel <rhyme label="a" type="f" corresp="#s7:l1">beschermen</rhyme></l>
        <l id="s7:l4" met="3" type="+-">In ons bitt're <rhyme label="b" type="m" corresp="#s7:l2">dood</rhyme>.</l>
        <l id="s7:l5" met="3" type="+-">Antwoord hier op, <rhyme label="c" type="f">Amen</rhyme>,</l>
        <l id="s7:l6" met="4" type="+-">Seght, O Moeder! 't sal <rhyme label="d" type="m">geschien</rhyme>:</l>
        <l id="s7:l7" met="3" type="+-">Soo sult Gh' ons te <rhyme label="c" type="f" corresp="#s7:l5">samen</rhyme></l>
        <l id="s7:l8" met="4" type="+-">Lieve Moeder Goods <rhyme label="d" type="m" corresp="#s7:l6">vervlien</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
