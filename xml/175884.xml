    <tags>
      <tag translation="Geuzen song">geuzenlied</tag>
      <tag translation="religious song">geestelijk lied</tag>
    </tags>
    <melody>Aux plaisirs aux délices bergères</melody>
    <categories>
      <category translation="politics and history">politiek en geschiedenis</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Wrede Maraen met u helsche <rhyme label="a" type="f">spoken</rhyme></l>
        <l id="s1:l2" met="4" type="-+">Genegen tot branden moorden <rhyme label="a" type="f" corresp="#s1:l1">smoken</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Schrickt voor Orangie met sijne <rhyme label="b" type="f">Helde</rhyme></l>
        <l id="s1:l4" met="4" type="-+">Die haer als Leeuwen toonen te <rhyme label="b" type="f" corresp="#s1:l3">velde</rhyme>,</l>
        <l id="s1:l5" met="3" type="-+">En schaecken uwe kroon, en u <rhyme label="c" type="f">glory</rhyme></l>
        <l id="s1:l6" met="4" type="-+">Singende nu Victory, <rhyme label="c" type="f" corresp="#s1:l5">Victory</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Vlugt Cordua met u speck na <rhyme label="a" type="f">Spanjen</rhyme></l>
        <l id="s2:l2" met="4" type="-+">De seventien Susters laet <rhyme label="a" type="f" corresp="#s2:l1">Orangien</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Want u afgodische <rhyme label="b" type="f">Papen-heyningh</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Die is gevallen tegen haer <rhyme label="b" type="f" corresp="#s2:l3">meyningh</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Dies wy weer stellen in ons <rhyme label="c" type="f">History</rhyme></l>
        <l id="s2:l6" met="4" type="-+">Ons Prins Hendrick Victory, <rhyme label="c" type="f" corresp="#s2:l5">Victory</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Die Grol, en Wesel, Bossche <rhyme label="a" type="f">Maeght</rhyme>,</l>
        <l id="s3:l2" met="4" type="-+">Die Vloot en Schanssen u heeft <rhyme label="a" type="f" corresp="#s3:l1">ontjaegt</rhyme></l>
        <l id="s3:l3" met="4" type="-+">En die (o schant in 't gesicht van <rhyme label="b" type="f">allen</rhyme>)</l>
        <l id="s3:l4" met="4" type="-+">Bedwongh de trotse Maestrichtse <rhyme label="b" type="f" corresp="#s3:l3">wallen</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Die wy tot sijn eeuwige <rhyme label="c" type="f">memory</rhyme></l>
        <l id="s3:l6" met="4" type="-+">Singen dit Lied, Victory, <rhyme label="c" type="f" corresp="#s3:l5">Victory</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Al die op Ruyteren en <rhyme label="a" type="f">Soldaten</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Op wagens en Paerden haer <rhyme label="a" type="f" corresp="#s4:l1">verlaten</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">En 't schepsel boven den Schepper <rhyme label="b" type="f">eere</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Moet-willigh haer kanten tegen <rhyme label="b" type="f" corresp="#s4:l3">d'Heere</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Maer die op hem bout, noyt en verloor <rhyme label="c" type="f">hy</rhyme></l>
        <l id="s4:l6" met="4" type="-+">Maer had altijt Victory, <rhyme label="c" type="f" corresp="#s4:l5">Victory</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Want wanneer oyt met hare <rhyme label="a" type="f">gebeden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">De Christen bend den Hemel <rhyme label="a" type="f" corresp="#s5:l1">bestreden</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Heeft haren God den God der <rhyme label="b" type="f">Heyscharen</rhyme></l>
        <l id="s5:l4" met="4" type="-+">Als sijn Oog-appel haer gaen <rhyme label="b" type="f" corresp="#s5:l3">bewaren</rhyme></l>
        <l id="s5:l5" met="3" type="-+">En in drucx plaetse voor haar verkoor <rhyme label="c" type="f">hy</rhyme></l>
        <l id="s5:l6" met="4" type="-+">Steeds vreugde-sang Victory, <rhyme label="c" type="f" corresp="#s5:l5">Victory</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Als wy nu weder ons <rhyme label="a" type="f">Offerhanden</rhyme>,</l>
        <l id="s6:l2" met="4" type="-+">Van dank tot aen den Hemel doen <rhyme label="a" type="f" corresp="#s6:l1">branden</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Soo sal onse God noyt van ons <rhyme label="b" type="f">wijcken</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Maer voegen by ons de Spaense <rhyme label="b" type="f" corresp="#s6:l3">rijken</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Tot sijns naems eer en groote <rhyme label="c" type="f">glory</rhyme></l>
        <l id="s6:l6" met="4" type="-+">Looft en dankt God, Victory, <rhyme label="c" type="f" corresp="#s6:l5">Victory</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
