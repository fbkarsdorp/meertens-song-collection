    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Swijg en houje mongt digt <rhyme label="a" type="m">toe</rhyme>,</l>
        <l id="s1:l2" met="4" type="+-">'k Benje malle praat al <rhyme label="a" type="m" corresp="#s1:l1">moe</rhyme>.</l>
        <l id="s1:l3" met="4" type="+-">Maar, ik sie men mortje <rhyme label="b" type="f">komen</rhyme>.</l>
        <l id="s1:l4" met="4" type="+-">Sie wel veur je wat je <rhyme label="c" type="m">segt</rhyme>:</l>
        <l id="s1:l5" met="4" type="+-">Het se jou gebruy   <rhyme label="b" type="f" corresp="#s1:l3">vernomen</rhyme>,</l>
        <l id="s1:l6" met="4" type="+-">Staat et hier te byster <rhyme label="c" type="m" corresp="#s1:l4">slegt</rhyme>.</l>
      </lg>
    </lg>
