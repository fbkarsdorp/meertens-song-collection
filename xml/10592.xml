  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">DRaech ick mijn liefd' <rhyme label="a" type="f">verborgen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ter eeren die liefste <rhyme label="b" type="m">mijn</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ick laet Fyole <rhyme label="a" type="f" corresp="#s1:l1">sorgen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">My hindert verdriet noch <rhyme label="b" type="m" corresp="#s1:l2">pijn</rhyme></l>
        <l id="s1:l5" met="4" type="-+">De liefste staet int hert <rhyme label="c" type="m">gheprent</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ick wil haer dit Bekerken <rhyme label="d" type="f">schencken</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Op dat sy mijn liefde <rhyme label="c" type="m" corresp="#s1:l5">ghedenckt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Ick sal haer dit Bekerken <rhyme label="a" type="f">gheven</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Daer toe mijn sin en <rhyme label="b" type="m">moet</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Sy doet my vrolijck <rhyme label="a" type="f" corresp="#s2:l1">leven</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Sy verquickt alle mijn <rhyme label="b" type="m" corresp="#s2:l2">bloet</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Sy is mijn hoep en <rhyme label="c" type="m">toeverlaet</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Mijn lachen en al mijn <rhyme label="d" type="f">treuren</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Gantselijck in haer <rhyme label="c" type="m" corresp="#s2:l5">staet</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Schoon Lief dits v <rhyme label="a" type="f">gheschoncken</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Ter liefde die ick v <rhyme label="b" type="m">draech</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Laet mijn liefd' v hert <rhyme label="a" type="f" corresp="#s3:l1">ontfoncken</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Na v ick stadelijck <rhyme label="b" type="m" corresp="#s3:l2">jaech</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Soo wert ons liefde aen een <rhyme label="c" type="m">gheknocht</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Hay laet ons vrolijck <rhyme label="d" type="f">wesen</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Schoon lief dits v <rhyme label="c" type="m" corresp="#s3:l5">ghebrocht</rhyme>.</l>
      </lg>
    </lg>
  </text>
