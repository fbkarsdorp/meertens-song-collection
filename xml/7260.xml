    <melody/>
    <categories>
      <category translation="narratives">verhalen</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Des hadde een Swave een <rhyme label="a" type="m">dochterlijn</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Sy en woude hem niet langer <rhyme label="b" type="f">dienen</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Rock en Mantel woude sy <rhyme label="c" type="m">haen</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Twee schoenen met smalle <rhyme label="b" type="f" corresp="#s1:l2">riemen</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s1:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Des wil dy Rock en Mantel <rhyme label="a" type="m">haen</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Twee schoenen met smalle <rhyme label="b" type="f">riemen</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Soo gaet ’t Ausburch inde <rhyme label="c" type="m">Stadt</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Ende groote Heeren <rhyme label="b" type="f" corresp="#s2:l2">dienen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s2:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Doen sy t’Ausburch binnen <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Al op de enghe <rhyme label="b" type="f">straten</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Sy vraeghden nae den besten <rhyme label="c" type="m">Wijn</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Daer ruyters en Landts-knechten <rhyme label="b" type="f" corresp="#s3:l2">saten</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s3:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Doen sy inder stoven <rhyme label="a" type="m">quam</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Men bracht haer eens te <rhyme label="b" type="f">drincken</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Sy liet haer ooghen omme <rhyme label="c" type="m">gaen</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Den Beecker liet sy <rhyme label="b" type="f" corresp="#s4:l2">sincken</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s4:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sy namen die teerlinck in de <rhyme label="a" type="m">hant</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Sy begosten daer te <rhyme label="b" type="f">speelen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Die daer de meeste ooghen <rhyme label="c" type="m">wierp</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sliep hy der schooder <rhyme label="b" type="f" corresp="#s5:l2">Vrouwen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s5:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy nam schoon Elsselijn by der <rhyme label="a" type="m">handt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">By haer Sneeu witte <rhyme label="b" type="f">handen</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Hy leydese daer hy een bedde <rhyme label="c" type="m">van’t</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Dat groene wout ten <rhyme label="b" type="f" corresp="#s6:l2">eynde</rhyme>.</l>
        <l id="s6:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s6:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Hy sitten haer neder op de <rhyme label="a" type="m">banck</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Sy en dede niet dan <rhyme label="b" type="f">weenen</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Noch hebbe ick stoute Broeders <rhyme label="c" type="m">drie</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Eene rijcken Vader soo <rhyme label="b" type="f" corresp="#s7:l2">heyme</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s7:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Wat hebdy stoute Broeders <rhyme label="a" type="m">drie</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Eenen rijcken Vader soo <rhyme label="b" type="f">heyme</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Soo meuchdy dit Iaer een huerlinck <rhyme label="c" type="m">zijn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">En trecken met my <rhyme label="b" type="f" corresp="#s8:l2">alleyne</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s8:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Och Broeder sprack sy <rhyme label="a" type="m">Broeder</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Du riestu aen mijn <rhyme label="b" type="f">eere</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Hadde my dat een ander <rhyme label="c" type="m">ghedaen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Hy soudet my wederom <rhyme label="b" type="f" corresp="#s9:l2">keeren</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s9:l1">Elsselijn</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Die dit Liedeken eerstwerf <rhyme label="a" type="m">sanck</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Hy hevet gaerne <rhyme label="b" type="f">ghesonghen</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">Een vroom Lants-knecht is hy <rhyme label="c" type="m">genaemt</rhyme></l>
        <l id="s10:l4" met="3" type="-+">t’Ausborch binnen der <rhyme label="b" type="f" corresp="#s10:l2">stede</rhyme></l>
        <l id="s10:l5" met="4" type="-+">Iae schoon isser mijn <rhyme label="a" type="m" corresp="#s10:l1">Elsselijn</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
