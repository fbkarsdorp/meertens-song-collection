    <melody>Hansje sneed het koren</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Tis wonder wat de meisies <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Om eenen man te <rhyme label="b" type="f">krijgen</rhyme></l>
        <l id="s1:l3" met="4" type="-+">sij gaen te dans op trijpte <rhyme label="a" type="m" corresp="#s1:l1">schoen</rhyme></l>
        <l id="s1:l4" met="3" type="-+">sij leeren stuepen en <rhyme label="b" type="f" corresp="#s1:l2">rigen</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Die rijcke dochters laet Ick <rhyme label="c" type="m">staen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Voerwaer sij hebbende <rhyme label="d" type="f">machte</rhyme></l>
        <l id="s1:l7" met="4" type="-+">maer meisies die uit dienen <rhyme label="c" type="m" corresp="#s1:l5">gaen</rhyme></l>
        <l id="s1:l8" met="3" type="-+">die willen den nadel <rhyme label="d" type="f" corresp="#s1:l6">slachte</rhyme></l>
        <l id="s1:l9" met="4" type="-+">sij gaen hoevaerdich oever <rhyme label="e" type="m">straet</rhyme></l>
        <l id="s1:l10" met="3" type="-+">gereegen smael van <rhyme label="f" type="f">leeden</rhyme></l>
        <l id="s1:l11" met="4" type="+-">haer haerken fraei gevlechtet <rhyme label="e" type="m" corresp="#s1:l9">staet</rhyme></l>
        <l id="s1:l12" met="3" type="-+">sij kuenen soe proeper <rhyme label="f" type="f" corresp="#s1:l10">treeden</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Sij draegen mouwen van <rhyme label="a" type="m">daemast</rhyme></l>
        <l id="s2:l2" met="3" type="-+">met silleveren <rhyme label="b" type="f">knoopen</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Daer op een baeyen die fraoij <rhyme label="a" type="m" corresp="#s2:l1">past</rhyme></l>
        <l id="s2:l4" met="3" type="-+">en die staen voer <rhyme label="b" type="f" corresp="#s2:l2">oepen</rhyme></l>
        <l id="s2:l5" met="4" type="-+">een voerschoet die van half <rhyme label="c" type="m">veset</rhyme></l>
        <l id="s2:l6" met="3" type="-+">geboert met sije <rhyme label="d" type="f">banden</rhyme></l>
        <l id="s2:l7" met="4" type="-+">haer lobbeiges staen fraeij <rhyme label="c" type="m" corresp="#s2:l5">geset</rhyme></l>
        <l id="s2:l8" met="3" type="-+">gestift al om haer <rhyme label="d" type="f" corresp="#s2:l6">hande</rhyme></l>
        <l id="s2:l9" met="4" type="-+">sij sitten sundaechs In de duer <rhyme label="e" type="m">blij</rhyme></l>
        <l id="s2:l10" met="3" type="-+">en proncken op de <rhyme label="f" type="f">straete</rhyme></l>
        <l id="s2:l11" met="4" type="+-">Komt daer een goedt gesel <rhyme label="e" type="m" corresp="#s2:l9">voerbij</rhyme></l>
        <l id="s2:l12" met="3" type="-+">hij moeter een veder <rhyme label="f" type="f" corresp="#s2:l10">laeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Den een heeft haer een <rhyme label="a" type="m">langenues</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Den ander Is haer te <rhyme label="b" type="f">mager</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Den derden en Is niet <rhyme label="a" type="m" corresp="#s3:l1">aemoerues</rhyme></l>
        <l id="s3:l4" met="3" type="-+">den vierden Is een <rhyme label="b" type="f" corresp="#s3:l2">hoerJager</rhyme></l>
        <l id="s3:l5" met="4" type="-+">Wat haer meester of vrouwe <rhyme label="c" type="m">raen</rhyme></l>
        <l id="s3:l6" met="3" type="-+">het en dunck haer niet te <rhyme label="d" type="f">duegen</rhyme></l>
        <l id="s3:l7" met="4" type="-+">een goedt gesel die wert <rhyme label="c" type="m" corresp="#s3:l5">voersmaet</rhyme></l>
        <l id="s3:l8" met="3" type="-+">soe worden sij dan <rhyme label="d" type="f" corresp="#s3:l6">bedroegen</rhyme></l>
        <l id="s3:l9" met="4" type="-+">het geeft die meisies alsoe <rhyme label="e" type="m">nieu</rhyme></l>
        <l id="s3:l10" met="3" type="-+">als sij van schaemele vrijers <rhyme label="f" type="f">hoeren</rhyme></l>
        <l id="s3:l11" met="4" type="+-">sij trecken den mondt maer alsoe <rhyme label="e" type="m" corresp="#s3:l9">slim</rhyme></l>
        <l id="s3:l12" met="3" type="-+">bijkans tot aen haer <rhyme label="f" type="f" corresp="#s3:l10">ooren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Sij vraegen niet nae moer of <rhyme label="a" type="m">vaer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">sij loepen al om te <rhyme label="b" type="f">houwen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">tsij meisies van veertien of viftien <rhyme label="a" type="m" corresp="#s4:l1">Jaer</rhyme></l>
        <l id="s4:l4" met="3" type="-+">sij willen weesen <rhyme label="b" type="f" corresp="#s4:l2">vrouwen</rhyme></l>
        <l id="s4:l5" met="4" type="-+">sij neemen een geselleken <rhyme label="c" type="m">Ient</rhyme></l>
        <l id="s4:l6" met="3" type="-+">die lustich gaedt <rhyme label="d" type="f">bracgeeren</rhyme></l>
        <l id="s4:l7" met="4" type="-+">groet werck en Is hij niet <rhyme label="c" type="m" corresp="#s4:l5">gewent</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Wat baedt hem dan sin <rhyme label="d" type="f" corresp="#s4:l6">kleeren</rhyme></l>
        <l id="s4:l9" met="4" type="-+">bocksen mantel wort <rhyme label="e" type="m">voerset</rhyme></l>
        <l id="s4:l10" met="3" type="-+">haer vrienden maechden <rhyme label="f" type="f">voerbolgen</rhyme></l>
        <l id="s4:l11" met="4" type="+-">haer vrou en haer meesters kneken <rhyme label="e" type="m" corresp="#s4:l9">vet</rhyme></l>
        <l id="s4:l12" met="3" type="-+">die en mach haer dan niet <rhyme label="f" type="f" corresp="#s4:l10">volgen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Sij krijgen het huis vol kinderen <rhyme label="a" type="m">vrij</rhyme></l>
        <l id="s5:l2" met="3" type="-+">het wijf stinckt naeder <rhyme label="b" type="f">pisse</rhyme></l>
        <l id="s5:l3" met="4" type="-+">Wanneer het kint bescheeten <rhyme label="a" type="m" corresp="#s5:l1">Is</rhyme></l>
        <l id="s5:l4" met="3" type="-+">sij en kant nau <rhyme label="b" type="f" corresp="#s5:l2">gewisschen</rhyme></l>
        <l id="s5:l5" met="4" type="-+">die armen bloet die klout sin <rhyme label="c" type="m">hoeft</rhyme></l>
        <l id="s5:l6" met="3" type="-+">hem dunckt hij Is <rhyme label="d" type="f">bedroegen</rhyme></l>
        <l id="s5:l7" met="4" type="-+">dat wiJf sidt daer so naeckt en <rhyme label="c" type="m" corresp="#s5:l5">bloet</rhyme></l>
        <l id="s5:l8" met="3" type="-+">die muts hanckt oever haer <rhyme label="d" type="f" corresp="#s5:l6">oegen</rhyme>.</l>
        <l id="s5:l9" met="4" type="-+">haer fieren ganck haer schoen van <rhyme label="e" type="m">leen</rhyme></l>
        <l id="s5:l10" met="3" type="-+">het wordt haer al <rhyme label="f" type="f">benoemen</rhyme></l>
        <l id="s5:l11" met="4" type="+-">sij gaedt alwaer haer den ruck aen <rhyme label="e" type="m" corresp="#s5:l9">tween</rhyme></l>
        <l id="s5:l12" met="3" type="-+">men souder snachs of <rhyme label="f" type="f" corresp="#s5:l10">droomen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
