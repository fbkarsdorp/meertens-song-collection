    <melody>Mijn heer van Valkenstein</melody>
    <categories>
      <category translation="formal genres">formele genres</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">IEsus riep sijn beminde <rhyme label="a" type="m">bruyt</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Coemt haestelijc sonder <rhyme label="b" type="f">beyden</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Ende doet mi op ws herten <rhyme label="c" type="m">duer</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ick spreke v gherne <rhyme label="b" type="f" corresp="#s1:l2">alleyne</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Dat maechdeken had haer lief haest <rhyme label="a" type="m">ghehoort</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Sy stelde haer haest vander <rhyme label="b" type="f">straten</rhyme></l>
        <l id="s2:l3" met="4" type="-+">Si dede op haers herten <rhyme label="c" type="m">duere</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Haren brudegom heeft si in <rhyme label="b" type="f" corresp="#s2:l2">gelaten</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Doen si hem in ghelaten <rhyme label="a" type="m">had</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Hi ghinc haer so minnelijc toe <rhyme label="b" type="f">spreken</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Segt mij mijn wtuercoren <rhyme label="c" type="m">bruyt</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Waer om hebdy mi soe dich <rhyme label="b" type="f" corresp="#s3:l2">vergheten</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Dat ick v lief vergheten <rhyme label="a" type="m">heb</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat doet mijn coude <rhyme label="b" type="f">minne</rhyme></l>
        <l id="s4:l3" met="4" type="-+">Ick heb mi seluen veel te <rhyme label="c" type="m">lief</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Daer om en can ick v niet <rhyme label="b" type="f" corresp="#s4:l2">kinnen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hebdy v seluen lieuer dan <rhyme label="a" type="m">mi</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Daer toe en hebdy gheen <rhyme label="b" type="f">verwe</rhyme></l>
        <l id="s5:l3" met="4" type="-+">En dede mijn godlijc <rhyme label="c" type="m">onderstant</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Ghi sout moeten <rhyme label="b" type="f" corresp="#s5:l2">bederuen</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Ic ghelooue lief heer v woerden sijn <rhyme label="a" type="m">waer</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Voer v en is gheen dinck <rhyme label="b" type="f">verholen</rhyme></l>
        <l id="s6:l3" met="4" type="-+">Ick ben v arm verdoelde <rhyme label="c" type="m">scaep</rhyme></l>
        <l id="s6:l4" met="3" type="-+">En laet mi niet <rhyme label="b" type="f" corresp="#s6:l2">verloren</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Lief kint dat en soudick niet gaerne <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Leeft altijt met <rhyme label="b" type="f">raden</rhyme></l>
        <l id="s7:l3" met="4" type="-+">Een suuer / reyn / ootmoedich <rhyme label="c" type="m">herte</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En wil ick gheen tijt <rhyme label="b" type="f" corresp="#s7:l2">versmaden</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Lieue heer v woerden hebben mi <rhyme label="a" type="m">gheraect</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Nochtans soe blijft mijn herte <rhyme label="b" type="f">beuanghen</rhyme></l>
        <l id="s8:l3" met="4" type="-+">Al heb ick menich goed <rhyme label="c" type="m">opset</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ick ben seer haest <rhyme label="b" type="f" corresp="#s8:l2">gheuangen</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Lief kint dat is v eygen <rhyme label="a" type="m">scult</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Dat doet v quade <rhyme label="b" type="f">nature</rhyme></l>
        <l id="s9:l3" met="4" type="-+">Wanneer ick v proeue met <rhyme label="c" type="m">teghenspoet</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Soe wort v herte dicwils <rhyme label="b" type="f" corresp="#s9:l2">droeue</rhyme></l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Ic sie wel wat v let lief <rhyme label="a" type="m">kint</rhyme></l>
        <l id="s10:l2" met="3" type="-+">Ghi gheuet spel al <rhyme label="b" type="f">verloren</rhyme></l>
        <l id="s10:l3" met="4" type="-+">Als ic v onttrecke mijn <rhyme label="c" type="m">soeticheyt</rhyme></l>
        <l id="s10:l4" met="3" type="-+">Ende legghe v suer te <rhyme label="b" type="f" corresp="#s10:l2">voren</rhyme></l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">Lieue heer dat en is geen <rhyme label="a" type="m">ongelijck</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Ghi legt mi al int <rhyme label="b" type="f">suere</rhyme></l>
        <l id="s11:l3" met="4" type="-+">Ick ete so node dat garsten <rhyme label="c" type="m">broot</rhyme></l>
        <l id="s11:l4" met="3" type="-+">Dat crenct mijn arm <rhyme label="b" type="f" corresp="#s11:l2">nature</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="-+">Ghi en sijt niet beter dan Dauid en <rhyme label="a" type="m">was</rhyme></l>
        <l id="s12:l2" met="3" type="-+">Hy en heeft gheen ander broot <rhyme label="b" type="f">gheten</rhyme></l>
        <l id="s12:l3" met="4" type="-+">Hi weende so menighen heeten <rhyme label="c" type="m">traen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Voer alle sijn vore gheleden <rhyme label="b" type="f" corresp="#s12:l2">ghebreken</rhyme></l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="-+">O heere dat en is gheen <rhyme label="a" type="m">onghelijck</rhyme></l>
        <l id="s13:l2" met="3" type="-+">Ghi hadt Dauid sijin herteken <rhyme label="b" type="f">beuanghen</rhyme></l>
        <l id="s13:l3" met="4" type="-+">Met uwer <rhyme label="c" type="m">tegenwoerdicheit</rhyme></l>
        <l id="s13:l4" met="3" type="-+">Hi en conde v niet <rhyme label="b" type="f" corresp="#s13:l2">ontganghen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="-+">Sijt te vreden mijn wtuercoren <rhyme label="a" type="m">bruyt</rhyme></l>
        <l id="s14:l2" met="3" type="-+">Kiest mi tot eenen <rhyme label="b" type="f">lieue</rhyme></l>
        <l id="s14:l3" met="4" type="-+">Ende blijft mi ghetrouwe als Dauid <rhyme label="c" type="m">dede</rhyme></l>
        <l id="s14:l4" met="3" type="-+">Ick aen sal v niet <rhyme label="b" type="f" corresp="#s14:l2">verlaten</rhyme></l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="-+">O heere ic sout so gerne <rhyme label="a" type="m">doen</rhyme></l>
        <l id="s15:l2" met="3" type="-+">Conde ic v heel <rhyme label="b" type="f">verweruen</rhyme></l>
        <l id="s15:l3" met="4" type="-+">Al sout mi costen vleesch en <rhyme label="c" type="m">bloet</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Ick sal mi leeren <rhyme label="b" type="f" corresp="#s15:l2">steruen</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="4" type="-+">Dit lieken is gedicht ter <rhyme label="a" type="m">eeren</rhyme></l>
        <l id="s16:l2" met="3" type="-+">Van alle ionghe <rhyme label="b" type="f">maechden</rhyme></l>
        <l id="s16:l3" met="4" type="-+">God gheue hen gracie in haren <rhyme label="c" type="m">strijt</rhyme></l>
        <l id="s16:l4" met="3" type="-+">Dat is moeten volherdich <rhyme label="b" type="f" corresp="#s16:l2">bliuen</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
