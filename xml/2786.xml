    <tags>
      <tag standard-name="bespiegelend lied" translation="reflective song">bespiegelend lied</tag>
    </tags>
    <melody>Kom allen gij Venus' diertjes</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Hoert toe gy Jonge <rhyme label="a" type="f">vryers</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ick selt u gaen <rhyme label="b" type="f">voertellen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Tsy vrysters ofte <rhyme label="a" type="f" corresp="#s1:l1">vryers</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Ofte eenige <rhyme label="b" type="f" corresp="#s1:l2">Jongesellen</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Hoert toe Ick salt u <rhyme label="b" type="f" corresp="#s1:l2">singen</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Hoert toe nae dit <rhyme label="c" type="m">bediet</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Sy geeue malkander <rhyme label="b" type="f" corresp="#s1:l2">een</rhyme></l>
        <l id="s1:l8" met="3" type="-+">en anders en volcht daer <rhyme label="c" type="m" corresp="#s1:l6">niet</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Aldeese Jonge <rhyme label="a" type="f">vryers</rhyme></l>
        <l id="s2:l2" met="3" type="-+">sy gaen gelyck de <rhyme label="b" type="f">heere</rhyme></l>
        <l id="s2:l3" met="3" type="-+">saeterdachs op der <rhyme label="a" type="f" corresp="#s2:l1">straete</rhyme></l>
        <l id="s2:l4" met="3" type="-+">om haer soetelief te <rhyme label="b" type="f" corresp="#s2:l2">spreeken</rhyme></l>
        <l id="s2:l5" met="3" type="-+">hoemenser dan <rhyme label="b" type="f" corresp="#s2:l2">beneeue</rhyme></l>
        <l id="s2:l6" met="3" type="-+">gelyck men daegelicks <rhyme label="c" type="m">siet</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Sy geeue malkander <rhyme label="b" type="f" corresp="#s2:l2">een</rhyme></l>
        <l id="s2:l8" met="3" type="-+">en anders en volcht daer <rhyme label="c" type="m" corresp="#s2:l6">niet</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Aldeese Jonge <rhyme label="a" type="f">vryers</rhyme></l>
        <l id="s3:l2" met="3" type="-+">sy moegent haer wel <rhyme label="b" type="f">beklaege</rhyme></l>
        <l id="s3:l3" met="3" type="-+">van nu tot deeser <rhyme label="a" type="f" corresp="#s3:l1">uere</rhyme></l>
        <l id="s3:l4" met="3" type="-+">en voert tot allen <rhyme label="b" type="f" corresp="#s3:l2">daege</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Dat doen ons goe <rhyme label="b" type="f" corresp="#s3:l2">maechde</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Ick weet wel hoese <rhyme label="c" type="m">hiet</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Sy geeue malkander <rhyme label="b" type="f" corresp="#s3:l2">een</rhyme></l>
        <l id="s3:l8" met="3" type="-+">en anders en volcht daer <rhyme label="c" type="m" corresp="#s3:l6">niet</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Prinse Jonge <rhyme label="a" type="f">Jeuchde</rhyme></l>
        <l id="s4:l2" met="3" type="-+">myn hertge wil myn <rhyme label="b" type="f">schuere</rhyme></l>
        <l id="s4:l3" met="3" type="-+">myn lief die Ick <rhyme label="a" type="f" corresp="#s4:l1">beminne</rhyme></l>
        <l id="s4:l4" met="3" type="-+">en mach myn niet <rhyme label="b" type="f" corresp="#s4:l2">gebuere</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Daerom Is dat ick <rhyme label="b" type="f" corresp="#s4:l2">truere</rhyme></l>
        <l id="s4:l6" met="3" type="-+">en leef In swaer <rhyme label="c" type="m">voerdriet</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Sy geeue malkander <rhyme label="b" type="f" corresp="#s4:l2">een</rhyme></l>
        <l id="s4:l8" met="3" type="-+">en anders en volcht daer <rhyme label="c" type="m" corresp="#s4:l6">niet</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Gy prinse en gy <rhyme label="a" type="f">heere</rhyme></l>
        <l id="s5:l2" met="3" type="-+">wilt dit ten beste <rhyme label="b" type="f">keere</rhyme></l>
        <l id="s5:l3" met="3" type="-+">soe goede roesies <rhyme label="a" type="f" corresp="#s5:l1">spruitender</rhyme></l>
        <l id="s5:l4" met="3" type="-+">alser gaen ter <rhyme label="b" type="f" corresp="#s5:l2">neere</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dick willer niet om <rhyme label="b" type="f" corresp="#s5:l2">truere</rhyme></l>
        <l id="s5:l6" met="3" type="-+">noch leeue in swaer <rhyme label="c" type="m">(voerdriet</rhyme>?)</l>
        <l id="s5:l7" met="3" type="-+">Sy geeue malkander <rhyme label="b" type="f" corresp="#s5:l2">een</rhyme></l>
        <l id="s5:l8" met="3" type="-+">en anders en volcht daer <rhyme label="c" type="m" corresp="#s5:l6">niet</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
