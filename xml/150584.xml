  <meta>
    <id>150584</id>
    <date>
      <year-start>1630</year-start>
      <year-end>1630</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>Psalm 107 Datheen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">LOff Gode. die en goedt <rhyme label="a" type="f">is</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">En goedt is sonder <rhyme label="b" type="m">endt</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">Spreeck yeder die behoedt <rhyme label="a" type="f" corresp="#s1:l1">is</rhyme></l>
        <l id="s1:l4" met="3" type="-+">En door hem uyt <rhyme label="b" type="m" corresp="#s1:l2">ellendt</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">uyt ellendt daer de <rhyme label="c" type="m">handt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Sijns vyants hem me’e <rhyme label="d" type="f">quelden</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Eer Godt in vrijen <rhyme label="c" type="m" corresp="#s1:l5">standt</rhyme></l>
        <l id="s1:l8" met="3" type="-+">Sijn’ dienstb’re schouders <rhyme label="d" type="f" corresp="#s1:l6">stelden</rhyme>:</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Die uyt de slaefsche <rhyme label="a" type="f">banden</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Geredt door Godes <rhyme label="b" type="m">kracht</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">Een lieven <rhyme label="a" type="f" corresp="#s2:l1">vader-lande</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Met vreuchden is <rhyme label="b" type="m" corresp="#s2:l2">herbracht</rhyme>:</l>
        <l id="s2:l5" met="3" type="-+">Herbracht van West en <rhyme label="c" type="m">Oost</rhyme></l>
        <l id="s2:l6" met="3" type="-+">En alle ’swerelts <rhyme label="d" type="f">hoecken</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">(Die Godt heeft tot sijn <rhyme label="c" type="m" corresp="#s2:l5">troost</rhyme></l>
        <l id="s2:l8" met="3" type="-+">Hoeft elders geen te <rhyme label="d" type="f" corresp="#s2:l6">soecken</rhyme>.)</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Sy doolden langs der <rhyme label="a" type="f">heyden</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Onseker waer het <rhyme label="b" type="m">padt</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ter goeder herbergh <rhyme label="a" type="f" corresp="#s3:l1">leyden</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Doch vonden plaets noch <rhyme label="b" type="m" corresp="#s3:l2">stadt</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Door feyl van dranck en <rhyme label="c" type="m">broodt</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Was’t datse schier <rhyme label="d" type="f">versmachten</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Haer stem socht Godt in <rhyme label="c" type="m" corresp="#s3:l5">noodt</rhyme>,</l>
        <l id="s3:l8" met="3" type="-+">En hy verhoord’ haer <rhyme label="d" type="f" corresp="#s3:l6">klachten</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">(Hoe soet na vele <rhyme label="a" type="f">smerten</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Bevalt een ruymer <rhyme label="b" type="m">tijdt</rhyme>!)</l>
        <l id="s4:l3" met="3" type="-+">Hy maeckten hare <rhyme label="a" type="f" corresp="#s4:l1">herten</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De nare bangheydt <rhyme label="b" type="m" corresp="#s4:l2">quijt</rhyme>.</l>
        <l id="s4:l5" met="3" type="-+">Haer aff-gedwaelden <rhyme label="c" type="m">pas</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Bracht hy op rechte <rhyme label="d" type="f">wegen</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Waer door haer oogen <rhyme label="c" type="m" corresp="#s4:l5">ras</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Gesicht van wooning <rhyme label="d" type="f" corresp="#s4:l6">kregen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Der sulcker hert en <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Verplicht Godts gunst ten <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s5:l3" met="3" type="-+">En wonderheydts <rhyme label="a" type="f" corresp="#s5:l1">verkonden</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Den mensch tot <rhyme label="b" type="m" corresp="#s5:l2">onderwijs</rhyme>.</l>
        <l id="s5:l5" met="3" type="-+">Want hy heeft haer <rhyme label="c" type="m">vermaeckt</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Met soete <rhyme label="d" type="f">zadigingen</rhyme></l>
        <l id="s5:l7" met="3" type="-+">En ’thongrigh hert <rhyme label="c" type="m" corresp="#s5:l5">genaeckt</rhyme></l>
        <l id="s5:l8" met="3" type="-+">Met vol’t der goeder <rhyme label="d" type="f" corresp="#s5:l6">dingen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Sy die gekerckert <rhyme label="a" type="f">lagen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">In endeloosen <rhyme label="b" type="m">nacht</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">In’t schad des doodts; <rhyme label="a" type="f" corresp="#s6:l1">verslagen</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Met ysers <rhyme label="b" type="m" corresp="#s6:l2">over-kracht’t</rhyme>;</l>
        <l id="s6:l5" met="3" type="-+">(Om ’twanck’len in Godts <rhyme label="c" type="m">re’en</rhyme></l>
        <l id="s6:l6" met="3" type="-+">En sijnes raedts <rhyme label="d" type="f">verachten</rhyme>)</l>
        <l id="s6:l7" met="3" type="-+">Die, in ellendt <rhyme label="c" type="m" corresp="#s6:l5">vertre’en</rhyme>,</l>
        <l id="s6:l8" met="3" type="-+">Noch troost noch hulp <rhyme label="d" type="f" corresp="#s6:l6">verwachten</rhyme>:</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Angst deed’ haer stem Godt <rhyme label="a" type="f">soecken</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Hy heefts’ in noodt <rhyme label="b" type="m">geredt</rhyme>.</l>
        <l id="s7:l3" met="3" type="-+">En uyt de duystre <rhyme label="a" type="f" corresp="#s7:l1">hoecken</rhyme></l>
        <l id="s7:l4" met="3" type="-+">En doodts gevaer <rhyme label="b" type="m" corresp="#s7:l2">geset</rhyme>.</l>
        <l id="s7:l5" met="3" type="-+">Sijn all’-vermogen <rhyme label="c" type="m">handt</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Om haer den druck <rhyme label="d" type="f">t’ont-rucken</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Brack kluyster, boey, en <rhyme label="c" type="m" corresp="#s7:l5">bandt</rhyme></l>
        <l id="s7:l8" met="3" type="-+">En allen prang in <rhyme label="d" type="f" corresp="#s7:l6">stucken</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Der sulcker hert en <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Verplicht Godts gunst ten <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s8:l3" met="3" type="-+">En wonderheydts <rhyme label="a" type="f" corresp="#s8:l1">verkonden</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">Den Mensch tot <rhyme label="b" type="m" corresp="#s8:l2">onderwijs</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">De poorten van <rhyme label="c" type="m">metael</rhyme></l>
        <l id="s8:l6" met="3" type="-+">De ys’re <rhyme label="d" type="f">grendel-sloten</rhyme></l>
        <l id="s8:l7" met="3" type="-+">(Tot lichting van haer <rhyme label="c" type="m" corresp="#s8:l5">quael</rhyme>)</l>
        <l id="s8:l8" met="3" type="-+">Heeft hy ontwee <rhyme label="d" type="f" corresp="#s8:l6">gestooten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="3" type="-+">De dwase, van Godts <rhyme label="a" type="f">wegen</rhyme></l>
        <l id="s9:l2" met="3" type="-+">Een on-padt <rhyme label="b" type="m">in-gegaen</rhyme>,</l>
        <l id="s9:l3" met="3" type="-+">Doe sy Godts roede <rhyme label="a" type="f" corresp="#s9:l1">kregen</rhyme></l>
        <l id="s9:l4" met="3" type="-+">Om ’tquaedt van hun <rhyme label="b" type="m" corresp="#s9:l2">gedaen</rhyme>;</l>
        <l id="s9:l5" met="3" type="-+">Doe hare ziel <rhyme label="c" type="m">’tgenot</rhyme></l>
        <l id="s9:l6" met="3" type="-+">Van alle spijs <rhyme label="d" type="f">versaeckten</rhyme>,</l>
        <l id="s9:l7" met="3" type="-+">En sy (soo trofse <rhyme label="c" type="m" corresp="#s9:l5">Godt</rhyme>)</l>
        <l id="s9:l8" met="3" type="-+">De deur des doodts <rhyme label="d" type="f" corresp="#s9:l6">genaeckten</rhyme>:</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="3" type="-+">Angst wasser en <rhyme label="a" type="f">verftautheydt</rhyme>.</l>
        <l id="s10:l2" met="3" type="-+">Wat toe-vlucht? Hare <rhyme label="b" type="m">stem</rhyme></l>
        <l id="s10:l3" met="3" type="-+">Socht Godt in dees <rhyme label="a" type="f" corresp="#s10:l1">benautheydt</rhyme>;</l>
        <l id="s10:l4" met="3" type="-+">En ’troepen raeckten <rhyme label="b" type="m" corresp="#s10:l2">hem</rhyme>.</l>
        <l id="s10:l5" met="3" type="-+">Hy sandt sijn troostlijck <rhyme label="c" type="m">woordt</rhyme>:</l>
        <l id="s10:l6" met="3" type="-+">De plaegh wierd <rhyme label="d" type="f">wechgenomen</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">Hy lietse van doodts <rhyme label="c" type="m" corresp="#s10:l5">poort</rhyme></l>
        <l id="s10:l8" met="3" type="-+">Ten lieven leven <rhyme label="d" type="f" corresp="#s10:l6">komen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="3" type="-+">Der sulcker hert en <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s11:l2" met="3" type="-+">Verplicht Godts gunst ten <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s11:l3" met="3" type="-+">En wonderheydts <rhyme label="a" type="f" corresp="#s11:l1">verkonden</rhyme>,</l>
        <l id="s11:l4" met="3" type="-+">Den Mensch tot <rhyme label="b" type="m" corresp="#s11:l2">onderwijs</rhyme>.</l>
        <l id="s11:l5" met="3" type="-+">Volkomen loff en <rhyme label="c" type="m">danck</rhyme></l>
        <l id="s11:l6" met="3" type="-+">Zy ’toffer datse <rhyme label="d" type="f">slachten</rhyme>,</l>
        <l id="s11:l7" met="3" type="-+">Waer van de klare <rhyme label="c" type="m" corresp="#s11:l5">klanck</rhyme></l>
        <l id="s11:l8" met="3" type="-+">Godts doen prent’ in <rhyme label="d" type="f" corresp="#s11:l6">gedachten</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="3" type="-+">Sy die op Zee met <rhyme label="a" type="f">kielen</rhyme>,</l>
        <l id="s12:l2" met="3" type="-+">Ter sneller vaert <rhyme label="b" type="m">gebouwt</rhyme>,</l>
        <l id="s12:l3" met="3" type="-+">Sich geven, en haer <rhyme label="a" type="f" corresp="#s12:l1">zielen</rhyme></l>
        <l id="s12:l4" met="3" type="-+">Betrouwen in een <rhyme label="b" type="m" corresp="#s12:l2">Hout</rhyme>,</l>
        <l id="s12:l5" met="3" type="-+">(Gewoont en noodt maeckt <rhyme label="c" type="m">koen</rhyme>)</l>
        <l id="s12:l6" met="3" type="-+">Om haer begrepen <rhyme label="d" type="f">handel</rhyme></l>
        <l id="s12:l7" met="3" type="-+">Vast wijdt en zijdt te <rhyme label="c" type="m" corresp="#s12:l5">doen</rhyme></l>
        <l id="s12:l8" met="3" type="-+">Door velen <rhyme label="d" type="f" corresp="#s12:l6">water-wandel</rhyme>:</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="3" type="-+">Sy sien int diep de <rhyme label="a" type="f">wercken</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">De wond’ren van den <rhyme label="b" type="m">Heer</rhyme>.</l>
        <l id="s13:l3" met="3" type="-+">(Versoecken doet best <rhyme label="a" type="f" corresp="#s13:l1">mercken</rhyme></l>
        <l id="s13:l4" met="3" type="-+">En ’toogh gaet boven <rhyme label="b" type="m" corresp="#s13:l2">leer</rhyme>.)</l>
        <l id="s13:l5" met="3" type="-+">Hy spreeckt: en ’tsterck <rhyme label="c" type="m">geblas</rhyme></l>
        <l id="s13:l6" met="3" type="-+">Der op-geresen <rhyme label="d" type="f">winden</rhyme></l>
        <l id="s13:l7" met="3" type="-+">Houdt stal, en laet sich <rhyme label="c" type="m" corresp="#s13:l5">ras</rhyme></l>
        <l id="s13:l8" met="3" type="-+">Sijn’ Heer’ ten dienste <rhyme label="d" type="f" corresp="#s13:l6">vinden</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="3" type="-+">De zee verheft haer <rhyme label="a" type="f">baren</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Door wind-kracht gram <rhyme label="b" type="m">gemaeckt</rhyme>,</l>
        <l id="s14:l3" met="3" type="-+">Dies ’tschip, en die’r in <rhyme label="a" type="f" corresp="#s14:l1">varen</rhyme>,</l>
        <l id="s14:l4" met="3" type="-+">Dan hel, nu hemel <rhyme label="b" type="m" corresp="#s14:l2">raeckt</rhyme>.</l>
        <l id="s14:l5" met="3" type="-+">Noodt, noodt! de ad’ren <rhyme label="c" type="m">slaen</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Het aensicht is aen’t <rhyme label="d" type="f">bleecken</rhyme>,</l>
        <l id="s14:l7" met="3" type="-+">Elck siet sijn doodt vast <rhyme label="c" type="m" corresp="#s14:l5">aen</rhyme></l>
        <l id="s14:l8" met="3" type="-+">En ’thert is hem <rhyme label="d" type="f" corresp="#s14:l6">ont-weken</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="3" type="-+">Sy draeyen en sy <rhyme label="a" type="f">rollen</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Geen die sich vesten <rhyme label="b" type="m">kan</rhyme>,</l>
        <l id="s15:l3" met="3" type="-+">Maer moeten <rhyme label="a" type="f" corresp="#s15:l1">suyssebollen</rhyme></l>
        <l id="s15:l4" met="3" type="-+">Gelijck een droncken <rhyme label="b" type="m" corresp="#s15:l2">man</rhyme>.</l>
        <l id="s15:l5" met="3" type="-+">Haer wijsheydt is op <rhyme label="c" type="m">niet</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Des stuyrmans konst ten <rhyme label="d" type="f">ende</rhyme>.</l>
        <l id="s15:l7" met="3" type="-+">Daer ’toogh geen uytkomst <rhyme label="c" type="m" corresp="#s15:l5">siet</rhyme></l>
        <l id="s15:l8" met="3" type="-+">Is ’thert in hooghst’ <rhyme label="d" type="f" corresp="#s15:l6">ellende</rhyme>.</l>
      </lg>
      <lg id="s16" type="stanza">
        <l id="s16:l1" met="3" type="-+">Wat raedt in sulcke <rhyme label="a" type="f">nooden</rhyme>?</l>
        <l id="s16:l2" met="3" type="-+">Sy roepen tot den <rhyme label="b" type="m">Heer</rhyme>.</l>
        <l id="s16:l3" met="3" type="-+">En hy (de grootst’ der <rhyme label="a" type="f" corresp="#s16:l1">Goden</rhyme>)</l>
        <l id="s16:l4" met="3" type="-+">Ont-angt en redtse <rhyme label="b" type="m" corresp="#s16:l2">we’er</rhyme>.</l>
        <l id="s16:l5" met="3" type="-+">Den storm-blas doet hy <rhyme label="c" type="m">staen</rhyme>,</l>
        <l id="s16:l6" met="3" type="-+">De baren laeghte <rhyme label="d" type="f">krijgen</rhyme>,</l>
        <l id="s16:l7" met="3" type="-+">Den zee-toorn <rhyme label="c" type="m" corresp="#s16:l5">overgaen</rhyme>,</l>
        <l id="s16:l8" met="3" type="-+">De schorre keelen <rhyme label="d" type="f" corresp="#s16:l6">swijgen</rhyme>.</l>
      </lg>
      <lg id="s17" type="stanza">
        <l id="s17:l1" met="3" type="-+">Het stillen van het <rhyme label="a" type="f">weder</rhyme></l>
        <l id="s17:l2" met="3" type="-+">Bestilt des vaerders <rhyme label="b" type="m">hert</rhyme>,</l>
        <l id="s17:l3" met="3" type="-+">Dat, (angstigh en ter <rhyme label="a" type="f" corresp="#s17:l1">neder</rhyme></l>
        <l id="s17:l4" met="3" type="-+">In d’affgrondt van de <rhyme label="b" type="m" corresp="#s17:l2">smert</rhyme>)</l>
        <l id="s17:l5" met="3" type="-+">Door vrees gestolt, <rhyme label="c" type="m">ontdooyt</rhyme>,</l>
        <l id="s17:l6" met="3" type="-+">En voelt sijn kracht <rhyme label="d" type="f">ontslaven</rhyme></l>
        <l id="s17:l7" met="3" type="-+">Te meer het weder <rhyme label="c" type="m" corresp="#s17:l5">mooyt</rhyme>.</l>
        <l id="s17:l8" met="3" type="-+">’tSchip krijght gewenschte <rhyme label="d" type="f" corresp="#s17:l6">haven</rhyme>.</l>
      </lg>
      <lg id="s18" type="stanza">
        <l id="s18:l1" met="3" type="-+">Der sulcker hert en <rhyme label="a" type="f">monden</rhyme></l>
        <l id="s18:l2" met="3" type="-+">Verplicht Godts gunst ten <rhyme label="b" type="m">prijs</rhyme></l>
        <l id="s18:l3" met="3" type="-+">En wonderheydts <rhyme label="a" type="f" corresp="#s18:l1">verkonden</rhyme>,</l>
        <l id="s18:l4" met="3" type="-+">Den mensch tot <rhyme label="b" type="m" corresp="#s18:l2">onderwijs</rhyme>.</l>
        <l id="s18:l5" met="3" type="-+">Sy moeten voor den <rhyme label="c" type="m">Raedt</rhyme></l>
        <l id="s18:l6" met="3" type="-+">Godts hoogen loff doen <rhyme label="d" type="f">hooren</rhyme>,</l>
        <l id="s18:l7" met="3" type="-+">En sijne <rhyme label="c" type="m" corresp="#s18:l5">wonder-daedt</rhyme></l>
        <l id="s18:l8" met="3" type="-+">In grooter scharen <rhyme label="d" type="f" corresp="#s18:l6">ooren</rhyme>.</l>
      </lg>
      <lg id="s19" type="stanza">
        <l id="s19:l1" met="3" type="-+">Hy is het die, na <rhyme label="a" type="f">waerde</rhyme>,</l>
        <l id="s19:l2" met="3" type="-+">Des boos-doens halven <rhyme label="b" type="m">plaeght</rhyme>,</l>
        <l id="s19:l3" met="3" type="-+">Soo dat oock selfs de <rhyme label="a" type="f" corresp="#s19:l1">Aerde</rhyme></l>
        <l id="s19:l4" met="3" type="-+">Bewoonders misdaedt <rhyme label="b" type="m" corresp="#s19:l2">draeght</rhyme>.</l>
        <l id="s19:l5" met="3" type="-+">Daer vocht was geeft hy <rhyme label="c" type="m">drooght</rhyme>.</l>
        <l id="s19:l6" met="3" type="-+">Al is de Bouman <rhyme label="d" type="f">wacker</rhyme></l>
        <l id="s19:l7" met="3" type="-+">’tIs niet al watmen <rhyme label="c" type="m" corresp="#s19:l5">pooght</rhyme></l>
        <l id="s19:l8" met="3" type="-+">Op een verdoemden <rhyme label="d" type="f" corresp="#s19:l6">acker</rhyme>.</l>
      </lg>
      <lg id="s20" type="stanza">
        <l id="s20:l1" met="3" type="-+">En we’er is hy’t die <rhyme label="a" type="f">zegen</rhyme></l>
        <l id="s20:l2" met="3" type="-+">Op’t ongezegent <rhyme label="b" type="m">geeft</rhyme>;</l>
        <l id="s20:l3" met="3" type="-+">Die’t woest’, omvocht <rhyme label="a" type="f" corresp="#s20:l1">verlegen</rhyme>,</l>
        <l id="s20:l4" met="3" type="-+">Des vochts de ruymte <rhyme label="b" type="m" corresp="#s20:l2">geeft</rhyme>;</l>
        <l id="s20:l5" met="3" type="-+">Die’t dor en dorstigh <rhyme label="c" type="m">landt</rhyme></l>
        <l id="s20:l6" met="3" type="-+">Begaeft en laeft met <rhyme label="d" type="f">stroomen</rhyme>.</l>
        <l id="s20:l7" met="3" type="-+">(Dus geeft de selve <rhyme label="c" type="m" corresp="#s20:l5">handt</rhyme></l>
        <l id="s20:l8" met="3" type="-+">Die eerstmael had <rhyme label="d" type="f" corresp="#s20:l6">genomen</rhyme>.)</l>
      </lg>
      <lg id="s21" type="stanza">
        <l id="s21:l1" met="3" type="-+">Het volck, dat heen en <rhyme label="a" type="f">weder</rhyme></l>
        <l id="s21:l2" met="3" type="-+">Eerst swerfd’ en kommer <rhyme label="b" type="m">had</rhyme>,</l>
        <l id="s21:l3" met="3" type="-+">Geeft sich aldaer ter <rhyme label="a" type="f" corresp="#s21:l1">neder</rhyme>,</l>
        <l id="s21:l4" met="3" type="-+">En ’twordt te met een <rhyme label="b" type="m" corresp="#s21:l2">Stadt</rhyme>.</l>
        <l id="s21:l5" met="3" type="-+">Men bout, men zaeyt, men <rhyme label="c" type="m">plant</rhyme>,</l>
        <l id="s21:l6" met="3" type="-+">De wijnstock wordt <rhyme label="d" type="f">gesteken</rhyme>,</l>
        <l id="s21:l7" met="3" type="-+">De vruchten houden <rhyme label="c" type="m" corresp="#s21:l5">standt</rhyme>,</l>
        <l id="s21:l8" met="3" type="-+">Men sietter niets <rhyme label="d" type="f" corresp="#s21:l6">ontbreken</rhyme>:</l>
      </lg>
      <lg id="s22" type="stanza">
        <l id="s22:l1" met="3" type="-+">Nature laet sich <rhyme label="a" type="f">vlijen</rhyme></l>
        <l id="s22:l2" met="3" type="-+">Ter vrucht, op bergh en <rhyme label="b" type="m">dal</rhyme>.</l>
        <l id="s22:l3" met="3" type="-+">Godts zegen doetse <rhyme label="a" type="f" corresp="#s22:l1">dijen</rhyme></l>
        <l id="s22:l4" met="3" type="-+">Ten talleloosen <rhyme label="b" type="m" corresp="#s22:l2">tal</rhyme>.</l>
        <l id="s22:l5" met="3" type="-+">’tVee (dat ten teel sich <rhyme label="c" type="m">spoedt</rhyme>)</l>
        <l id="s22:l6" met="3" type="-+">En weet van geen <rhyme label="d" type="f">vermind’ren</rhyme></l>
        <l id="s22:l7" met="3" type="-+">(Daer Godt de handt <rhyme label="c" type="m" corresp="#s22:l5">toe-doet</rhyme></l>
        <l id="s22:l8" met="3" type="-+">Kan niemandt spoedt <rhyme label="d" type="f" corresp="#s22:l6">verhind’ren</rhyme>.)</l>
      </lg>
      <lg id="s23" type="stanza">
        <l id="s23:l1" met="3" type="-+">Maer alsmen nu door <rhyme label="a" type="f">zatheydt</rhyme></l>
        <l id="s23:l2" met="3" type="-+">Des levens gang <rhyme label="b" type="m">verset</rhyme></l>
        <l id="s23:l3" met="3" type="-+">En ’tGodt-geseg geen vat <rhyme label="a" type="f" corresp="#s23:l1">heyt</rhyme></l>
        <l id="s23:l4" met="3" type="-+">Op’t hert te dick en <rhyme label="b" type="m" corresp="#s23:l2">vet</rhyme>;</l>
        <l id="s23:l5" met="3" type="-+">En Godt Tyrannen <rhyme label="c" type="m">geeft</rhyme></l>
        <l id="s23:l6" met="3" type="-+">Die ’svolcks getal <rhyme label="d" type="f">verdunnen</rhyme>,</l>
        <l id="s23:l7" met="3" type="-+">Wier handt verned’ring <rhyme label="c" type="m" corresp="#s23:l5">geeft</rhyme></l>
        <l id="s23:l8" met="3" type="-+">En wat voor quaedt sy <rhyme label="d" type="f" corresp="#s23:l6">kunnen</rhyme>:</l>
      </lg>
      <lg id="s24" type="stanza">
        <l id="s24:l1" met="3" type="-+">Als angst sit in <rhyme label="a" type="f">gedachten</rhyme></l>
        <l id="s24:l2" met="3" type="-+">En alle lusten <rhyme label="b" type="m">smalt</rhyme>;</l>
        <l id="s24:l3" met="3" type="-+">Als smaedtheydt en <rhyme label="a" type="f" corresp="#s24:l1">verachten</rhyme></l>
        <l id="s24:l4" met="3" type="-+">Op groote luyden <rhyme label="b" type="m" corresp="#s24:l2">valt</rhyme>;</l>
        <l id="s24:l5" met="3" type="-+">Als hyse <rhyme label="c" type="m">padeloos</rhyme></l>
        <l id="s24:l6" met="3" type="-+">In’t ydel heen laet <rhyme label="d" type="f">swerven</rhyme>,</l>
        <l id="s24:l7" met="3" type="-+">En ’thert geen troost <rhyme label="c" type="m" corresp="#s24:l5">altoos</rhyme></l>
        <l id="s24:l8" met="3" type="-+">Noch hope kan <rhyme label="d" type="f" corresp="#s24:l6">verwerven</rhyme>:</l>
      </lg>
      <lg id="s25" type="stanza">
        <l id="s25:l1" met="3" type="-+">Dan, dan sal hy noch <rhyme label="a" type="f">richten</rhyme></l>
        <l id="s25:l2" met="3" type="-+">Des armen ned’righ <rhyme label="b" type="m">hoofdt</rhyme></l>
        <l id="s25:l3" met="3" type="-+">En hem het pack <rhyme label="a" type="f" corresp="#s25:l1">aff-lichten</rhyme></l>
        <l id="s25:l4" met="3" type="-+">Dat vreuchd-smaeck had <rhyme label="b" type="m" corresp="#s25:l2">verdooft</rhyme>.</l>
        <l id="s25:l5" met="3" type="-+">Gelijck het vruchtb’re <rhyme label="c" type="m">Vee</rhyme></l>
        <l id="s25:l6" met="3" type="-+">Sal hy sijn huys doen <rhyme label="d" type="f">wassen</rhyme>.</l>
        <l id="s25:l7" met="3" type="-+">Voor Godt wijckt alle <rhyme label="c" type="m" corresp="#s25:l5">wee</rhyme>,</l>
        <l id="s25:l8" met="3" type="-+">Sijn handt kan alles <rhyme label="d" type="f" corresp="#s25:l6">passen</rhyme>.</l>
      </lg>
      <lg id="s26" type="stanza">
        <l id="s26:l1" met="3" type="-+">De vrome siet Godts <rhyme label="a" type="f">wercken</rhyme>,</l>
        <l id="s26:l2" met="3" type="-+">En heefter blijschap <rhyme label="b" type="m">om</rhyme>;</l>
        <l id="s26:l3" met="3" type="-+">De boose moettet <rhyme label="a" type="f" corresp="#s26:l1">mercken</rhyme>,</l>
        <l id="s26:l4" met="3" type="-+">En staet verbaest en <rhyme label="b" type="m" corresp="#s26:l2">stom</rhyme>.</l>
        <l id="s26:l5" met="3" type="-+">Wie wijs is, sla des <rhyme label="c" type="m">ga</rhyme>’:</l>
        <l id="s26:l6" met="3" type="-+">En hy sal duyd’lijck <rhyme label="d" type="f">weten</rhyme></l>
        <l id="s26:l7" met="3" type="-+">Dat onses Godts <rhyme label="c" type="m" corresp="#s26:l5">gena</rhyme>’</l>
        <l id="s26:l8" met="3" type="-+">Is groot en <rhyme label="d" type="f" corresp="#s26:l6">ongemeten</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
