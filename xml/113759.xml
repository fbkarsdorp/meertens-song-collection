<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>113759</id>
    <date>
      <year-start>1645</year-start>
      <year-end>1645</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags/>
    <melody></melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">IEsv, ghever van den <rhyme label="a" type="m">wet</rhyme>,</l>
        <l id="s1:l2" met="3" type="+-">Welck is doch de <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s1:l3" met="4" type="+-">Dat ghy heylich, <rhyme label="a" type="m" corresp="#s1:l1">onbesmet</rhyme></l>
        <l id="s1:l4" met="3" type="+-">Moest oock zijn <rhyme label="b" type="f" corresp="#s1:l2">besneden</rhyme>?</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Moest soo vroegh den scherpen <rhyme label="a" type="m">steen</rhyme></l>
        <l id="s2:l2" met="3" type="+-">U teer lichaem <rhyme label="b" type="f">wonden</rhyme>?</l>
        <l id="s2:l3" met="4" type="+-">Moest ghy soo vroegh met <rhyme label="a" type="m" corresp="#s2:l1">gheween</rhyme></l>
        <l id="s2:l4" met="3" type="+-">Sneven voor ons <rhyme label="b" type="f" corresp="#s2:l2">sonden</rhyme>?</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Moest den Heer als sijnen <rhyme label="a" type="m">knecht</rhyme></l>
        <l id="s3:l2" met="3" type="+-">'tSlaven teecken <rhyme label="b" type="f">draghen</rhyme>?</l>
        <l id="s3:l3" met="4" type="+-">Moest ghy voor ons staen te <rhyme label="a" type="m" corresp="#s3:l1">recht</rhyme></l>
        <l id="s3:l4" met="3" type="+-">In u kindtse <rhyme label="b" type="f" corresp="#s3:l2">daghen</rhyme>?</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Noemen dan men u gaen <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s4:l2" met="3" type="+-">Iesvm wel met <rhyme label="b" type="f">reden</rhyme>,</l>
        <l id="s4:l3" met="4" type="+-">Die ghenoechsaem voor ons <rhyme label="a" type="m" corresp="#s4:l1">al</rhyme></l>
        <l id="s4:l4" met="3" type="+-">Hebt alree <rhyme label="b" type="f" corresp="#s4:l2">gheleden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">'Twas ghenoech aen eenen <rhyme label="a" type="m">traen</rhyme>,</l>
        <l id="s5:l2" met="3" type="+-">Om 't al te <rhyme label="b" type="f">betalen</rhyme>,</l>
        <l id="s5:l3" met="4" type="+-">D' eerste wonde cost <rhyme label="a" type="m" corresp="#s5:l1">ontslaen</rhyme></l>
        <l id="s5:l4" met="3" type="+-">Ons van alle <rhyme label="b" type="f" corresp="#s5:l2">qualen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Maer u liefde wou te <rhyme label="a" type="m">dier</rhyme></l>
        <l id="s6:l2" met="3" type="+-">Coopen af de <rhyme label="b" type="f">sonde</rhyme>,</l>
        <l id="s6:l3" met="4" type="+-">'Twas te weynich voor u <rhyme label="a" type="m" corresp="#s6:l1">vier</rhyme></l>
        <l id="s6:l4" met="3" type="+-">Met des' eene <rhyme label="b" type="f" corresp="#s6:l2">wonde</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Maer dit druppen van u <rhyme label="a" type="m">bloedt</rhyme></l>
        <l id="s7:l2" met="3" type="+-">Moest den reghen <rhyme label="b" type="f">comen</rhyme>,</l>
        <l id="s7:l3" met="4" type="+-">Die den Vaders grammen <rhyme label="a" type="m" corresp="#s7:l1">moedt</rhyme></l>
        <l id="s7:l4" met="3" type="+-">Eerst heeft wegh <rhyme label="b" type="f" corresp="#s7:l2">ghenomen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">Nu hoor ick u droef <rhyme label="a" type="m">gheschrey</rhyme>,</l>
        <l id="s8:l2" met="3" type="+-">Ick sien vol van <rhyme label="b" type="f">tranen</rhyme></l>
        <l id="s8:l3" met="4" type="+-">U onnoosel ooghskens <rhyme label="a" type="m" corresp="#s8:l1">bey</rhyme>,</l>
        <l id="s8:l4" met="3" type="+-">Die alree my <rhyme label="b" type="f" corresp="#s8:l2">manen</rhyme>,</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Manen dat u <rhyme label="a" type="m">weder-min</rhyme></l>
        <l id="s9:l2" met="3" type="+-">Sou mijn hert <rhyme label="b" type="f">betalen</rhyme>,</l>
        <l id="s9:l3" met="4" type="+-">Och oft diep in mijnen <rhyme label="a" type="m" corresp="#s9:l1">sin</rhyme></l>
        <l id="s9:l4" met="3" type="+-">Dit gheheysch quaem <rhyme label="b" type="f" corresp="#s9:l2">dalen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Leert my, Iesv, doch <rhyme label="a" type="m">verstaen</rhyme></l>
        <l id="s10:l2" met="3" type="+-">'Tgroot vier uwer <rhyme label="b" type="f">minnen</rhyme>,</l>
        <l id="s10:l3" met="4" type="+-">Leert my door u wonde <rhyme label="a" type="m" corresp="#s10:l1">gaen</rhyme></l>
        <l id="s10:l4" met="3" type="+-">Tot u hertjen <rhyme label="b" type="f" corresp="#s10:l2">binnen</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Leert dat ghy voor my <rhyme label="a" type="m">alleen</rhyme></l>
        <l id="s11:l2" met="3" type="+-">Leet aen alle <rhyme label="b" type="f">zijden</rhyme>,</l>
        <l id="s11:l3" met="4" type="+-">Van u eerste kindts <rhyme label="a" type="m" corresp="#s11:l1">gheween</rhyme></l>
        <l id="s11:l4" met="3" type="+-">Tot u bitter <rhyme label="b" type="f" corresp="#s11:l2">lijden</rhyme>,</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">Leert my dat ick om u <rhyme label="a" type="m">smert</rhyme></l>
        <l id="s12:l2" met="3" type="+-">Gherne oock wil <rhyme label="b" type="f">lijden</rhyme></l>
        <l id="s12:l3" met="4" type="+-">Om u liefde gansch mijn <rhyme label="a" type="m" corresp="#s12:l1">hert</rhyme></l>
        <l id="s12:l4" met="3" type="+-">Neerstich gae <rhyme label="b" type="f" corresp="#s12:l2">besnijden</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">Dat ick 't al eens om u <rhyme label="a" type="m">laet</rhyme>,</l>
        <l id="s13:l2" met="3" type="+-">En m' aen u gae <rhyme label="b" type="f">binden</rhyme></l>
        <l id="s13:l3" met="4" type="+-">Salich oock mijn selven <rhyme label="a" type="m" corresp="#s13:l1">haet</rhyme>,</l>
        <l id="s13:l4" met="3" type="+-">Om u eens te <rhyme label="b" type="f" corresp="#s13:l2">vinden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
