<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>21967</id>
    <date>
      <year-start>1580</year-start>
      <year-end>1580</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag>scriptural song</tag>
      <tag>praise</tag>
    </tags>
    <melody>Lofzang van Maria</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">MIjn siel maeckt groot den <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">47. Mijn geest verheugt sich <rhyme label="a" type="m" corresp="#s1:l1">seer</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">In God' mijn' Heylant <rhyme label="b" type="f">goedigh</rhyme>.</l>
        <l id="s1:l4" met="3" type="-+">48. Mits zijn oogh' gaede <rhyme label="c" type="m">slaet</rhyme>.</l>
        <l id="s1:l5" met="3" type="-+">De kleynicheyt end' <rhyme label="c" type="m" corresp="#s1:l4">smaet</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Van zijn dienst, maegt <rhyme label="b" type="f" corresp="#s1:l3">ootmoedigh</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">* Want siet, nu voortaen, <rhyme label="a" type="m">sal</rhyme></l>
        <l id="s2:l2" met="3" type="-+">De werelt ouer <rhyme label="a" type="m" corresp="#s2:l1">al</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">My ghelucksaligh <rhyme label="b" type="f">noemen</rhyme>.</l>
        <l id="s2:l4" met="3" type="-+">49. Om dat God groot van <rhyme label="c" type="m">macht</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Aen my vast heeft <rhyme label="c" type="m" corresp="#s2:l4">volbracht</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Een heerlijck werck om <rhyme label="b" type="f" corresp="#s2:l3">roemen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">* Sijn naem end' groot <rhyme label="a" type="m">gherucht</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Is heyligh end' <rhyme label="a" type="m" corresp="#s3:l1">ghedught</rhyme>:</l>
        <l id="s3:l3" met="3" type="-+">Sijn goedicheyt sal <rhyme label="b" type="f">dueren</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Van gheslacht tot <rhyme label="c" type="m">gheslacht</rhyme>:</l>
        <l id="s3:l5" met="3" type="-+">Aen dien, die met <rhyme label="c" type="m" corresp="#s3:l4">aendacht</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Des Heeren vrees' <rhyme label="b" type="f" corresp="#s3:l3">naespueren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">* Met synen erm seer <rhyme label="a" type="m">sterck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Heeft hy ghedaen een <rhyme label="a" type="m" corresp="#s4:l1">werck</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Dat groot'lijck is te <rhyme label="b" type="f">achten</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Want hy heeft oyt end' <rhyme label="c" type="m">oyt</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">D'hooveerdighen <rhyme label="c" type="m" corresp="#s4:l4">verstroyt</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">In haers herten <rhyme label="b" type="f" corresp="#s4:l3">ghedachten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">* Der machtighen <rhyme label="a" type="m">ghewelt</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Heeft hy nu ne'er <rhyme label="a" type="m" corresp="#s5:l1">ghevelt</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">End' uyt den stoel <rhyme label="b" type="f">ghedreuen</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">Daer teghen heeft zijn <rhyme label="c" type="m">handt</rhyme>,</l>
        <l id="s5:l5" met="3" type="-+">Der kleynen snooden <rhyme label="c" type="m" corresp="#s5:l4">standt</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">In eeren hoogh <rhyme label="b" type="f" corresp="#s5:l3">verheuen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">* Die leden honghers <rhyme label="a" type="m">noot</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Heeft hy versaedt met <rhyme label="a" type="m" corresp="#s6:l1">broot</rhyme>,</l>
        <l id="s6:l3" met="3" type="-+">Gherustelijck end' <rhyme label="b" type="f">vredigh</rhyme>:</l>
        <l id="s6:l4" met="3" type="-+">Maer die in <rhyme label="c" type="m">ouervloet</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Besaten ghelt end' <rhyme label="c" type="m" corresp="#s6:l4">goet</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Versonden bloot end' <rhyme label="b" type="f" corresp="#s6:l3">ledigh</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">* End' Israel zijn <rhyme label="a" type="m">kindt</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Beschermt: end' sick <rhyme label="a" type="m" corresp="#s7:l1">besindt</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Sijnr' eewighe <rhyme label="b" type="f">ghenade</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">So hys' hadd' <rhyme label="c" type="m">toeghesegt</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Abraham zynen <rhyme label="c" type="m" corresp="#s7:l4">knecht</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">End' nae hem synen <rhyme label="b" type="f" corresp="#s7:l3">rade</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
