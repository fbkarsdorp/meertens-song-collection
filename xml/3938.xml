<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>3938</id>
    <date>
      <year-start>1539</year-start>
      <year-end>1539</year-end>
    </date>
    <location>
      <orginal-name>antwerpen</orginal-name>
      <standard-name>Antwerp, Belgium</standard-name>
      <coordinates>
        <lon>51.2211097</lon>
        <lat>4.3997081</lat>
      </coordinates>
    </location>
    <tags>
      <tag standard-name="klaaglied" translation="lamentation">klaaglied</tag>
      <tag standard-name="devoot lied" translation="devotional song">devoot lied</tag>
    </tags>
    <melody>Aanhoor toch mijn geklaag gij ruiters</melody>
    <categories>
      <category translation="emotions">emoties</category>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Aenhoort doch mijn <rhyme label="a" type="m">gheclach</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Ghi hertekens veruult met <rhyme label="b" type="f">minnen</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Ic truere nacht ende <rhyme label="a" type="m" corresp="#s1:l1">dach</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Dwelck ick wel claghen <rhyme label="a" type="m" corresp="#s1:l1">mach</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Aylacy noyt sulcken <rhyme label="a" type="m" corresp="#s1:l1">verdrach</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Tes mi een swaer <rhyme label="a" type="m" corresp="#s1:l1">gheclach</rhyme></l>
        <l id="s1:l7" met="3" type="-+">Dat ick dese droefheyt <rhyme label="b" type="f" corresp="#s1:l2">kinne</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Och menigen heeten <rhyme label="a" type="m">traen</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Heeft oueruloten mijn <rhyme label="b" type="f">wangen</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Och om dat ick soe <rhyme label="a" type="m" corresp="#s2:l1">saen</rhyme></l>
        <l id="s2:l4" met="3" type="-+">Mijn lief af moet <rhyme label="a" type="m" corresp="#s2:l1">gaen</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Want scande is <rhyme label="a" type="m" corresp="#s2:l1">tvermaen</rhyme></l>
        <l id="s2:l6" met="3" type="-+">Och dat sonde is <rhyme label="a" type="m" corresp="#s2:l1">ghedaen</rhyme></l>
        <l id="s2:l7" met="3" type="-+">Dat maect mijn herte so <rhyme label="b" type="f" corresp="#s2:l2">bange</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Had ick die duecht <rhyme label="a" type="m">ghekint</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Die hi mi heeft <rhyme label="b" type="f">bewesen</rhyme></l>
        <l id="s3:l3" met="3" type="-+">Ick hadde hem wel <rhyme label="a" type="m" corresp="#s3:l1">bemint</rhyme></l>
        <l id="s3:l4" met="3" type="-+">Ende niet gheacht den <rhyme label="a" type="m" corresp="#s3:l1">wint</rhyme>.</l>
        <l id="s3:l5" met="3" type="-+">Die therte dick heeft <rhyme label="a" type="m" corresp="#s3:l1">verblint</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Och hadde ick dit <rhyme label="a" type="m" corresp="#s3:l1">versint</rhyme></l>
        <l id="s3:l7" met="3" type="-+">Mijn siel waer al <rhyme label="b" type="f" corresp="#s3:l2">ghenesen</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Die liefde was so <rhyme label="a" type="m">groot</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Mer die hem heeft <rhyme label="b" type="f">ghedwongen</rhyme></l>
        <l id="s4:l3" met="3" type="-+">Te comen inder <rhyme label="a" type="m" corresp="#s4:l1">doot</rhyme></l>
        <l id="s4:l4" met="3" type="-+">Voer mi die was in <rhyme label="a" type="m" corresp="#s4:l1">noot</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Ende oock van duechden <rhyme label="a" type="m" corresp="#s4:l1">bloot</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Om dat wi om sviants <rhyme label="a" type="m" corresp="#s4:l1">poot</rhyme></l>
        <l id="s4:l7" met="3" type="-+">Souden worden <rhyme label="b" type="f" corresp="#s4:l2">ghedwongen</rhyme></l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Ons goets en hoefde hi <rhyme label="a" type="m">niet</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Die rijck is bouen <rhyme label="b" type="f">maten</rhyme></l>
        <l id="s5:l3" met="3" type="-+">Noch wast diet wel <rhyme label="a" type="m" corresp="#s5:l1">besiet</rhyme></l>
        <l id="s5:l4" met="3" type="-+">Sijn liefde die hem wel <rhyme label="a" type="m" corresp="#s5:l1">riet</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Dat hi hem cruycen <rhyme label="a" type="m" corresp="#s5:l1">liet</rhyme></l>
        <l id="s5:l6" met="3" type="-+">Ende niet vaet mijn <rhyme label="a" type="m" corresp="#s5:l1">bediet</rhyme></l>
        <l id="s5:l7" met="3" type="-+">Dan tot onser grooter <rhyme label="b" type="f" corresp="#s5:l2">baten</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Wie hadde oyt sulck <rhyme label="a" type="m">verstant</rhyme></l>
        <l id="s6:l2" met="3" type="-+">Dat hi daer mede <rhyme label="b" type="f">bekinde</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Gods grooter liefden <rhyme label="a" type="m" corresp="#s6:l1">brant</rhyme></l>
        <l id="s6:l4" met="3" type="-+">Die hi hadde / doen hi <rhyme label="a" type="m" corresp="#s6:l1">sant</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Dat alder weertste <rhyme label="a" type="m" corresp="#s6:l1">pant</rhyme></l>
        <l id="s6:l6" met="3" type="-+">In dit allendich <rhyme label="a" type="m" corresp="#s6:l1">lant</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Blijct wel dat hi ons <rhyme label="b" type="f" corresp="#s6:l2">minde</rhyme></l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="3" type="-+">Met ons wast eirst al <rhyme label="a" type="m">quaet</rhyme></l>
        <l id="s7:l2" met="3" type="-+">Eer God die wet <rhyme label="b" type="f">volbrachte</rhyme></l>
        <l id="s7:l3" met="3" type="-+">Des vyants dootlijck <rhyme label="a" type="m" corresp="#s7:l1">saet</rhyme></l>
        <l id="s7:l4" met="3" type="-+">Was ons aldoen en <rhyme label="a" type="m" corresp="#s7:l1">laet</rhyme></l>
        <l id="s7:l5" met="3" type="-+">Ende onser en was gheenen <rhyme label="a" type="m" corresp="#s7:l1">raet</rhyme></l>
        <l id="s7:l6" met="3" type="-+">Nochtans verghinc den <rhyme label="a" type="m" corresp="#s7:l1">haet</rhyme></l>
        <l id="s7:l7" met="3" type="-+">Doer liefde die dat <rhyme label="b" type="f" corresp="#s7:l2">wrachte</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="3" type="-+">Laet ons dan alle <rhyme label="a" type="m">ghemeyn</rhyme></l>
        <l id="s8:l2" met="3" type="-+">Int eynde weder <rhyme label="b" type="f">keeren</rhyme></l>
        <l id="s8:l3" met="3" type="-+">Want hi verbeyt <rhyme label="a" type="m" corresp="#s8:l1">certeyn</rhyme></l>
        <l id="s8:l4" met="3" type="-+">Ons allen groot ende <rhyme label="a" type="m" corresp="#s8:l1">cleyn</rhyme>.</l>
        <l id="s8:l5" met="3" type="-+">Om ons te maken <rhyme label="a" type="m" corresp="#s8:l1">reyn</rhyme>.</l>
        <l id="s8:l6" met="3" type="-+">Ende om des hemels <rhyme label="a" type="m" corresp="#s8:l1">pleyn</rhyme></l>
        <l id="s8:l7" met="3" type="-+">Te gheuen tot sijnder <rhyme label="b" type="f" corresp="#s8:l2">eeren</rhyme></l>
      </lg>
    </lg>
  </text>
</song>
