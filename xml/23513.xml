    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">Borghers wilt moet <rhyme label="a" type="f">aenveerden</rhyme>/</l>
        <l id="s1:l2" met="3" type="-+">En gheeft God lof en <rhyme label="b" type="m">danck</rhyme>/</l>
        <l id="s1:l3" met="3" type="-+">Dat Brugghe de Stadt vul <rhyme label="a" type="f" corresp="#s1:l1">weerden</rhyme>/</l>
        <l id="s1:l4" met="3" type="-+">Noch is wt groot <rhyme label="b" type="m" corresp="#s1:l2">bedwanck</rhyme>:</l>
        <l id="s1:l5" met="3" type="-+">Zoomen sterck heeft <rhyme label="c" type="f">beseuen</rhyme>/</l>
        <l id="s1:l6" met="3" type="-+">In Jullet doemen <rhyme label="d" type="m">t'Jaer</rhyme>/</l>
        <l id="s1:l7" met="3" type="-+">Vichthien hondert heeft <rhyme label="c" type="f" corresp="#s1:l5">gheschreuen</rhyme>/</l>
        <l id="s1:l8" met="3" type="-+">Neghenen t'zeuentich <rhyme label="d" type="m" corresp="#s1:l6">naer</rhyme>.</l>
      </lg>
    </lg>
