<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22698</id>
    <date>
      <year-start>1576</year-start>
      <year-end>1582</year-end>
    </date>
    <tags>
      <tag translation="prayer song">gebedslied</tag>
    </tags>
    <melody>Een lied eerbaar</melody>
    <categories>
      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="6" type="-+">Myn sinnekens, heb Ick, O Godt, op u <rhyme label="a" type="m">gheleydt</rhyme>:</l>
        <l id="s1:l2" met="6" type="-+">Na u Ick wens, want ghy my seer wel <rhyme label="a" type="m" corresp="#s1:l1">bereydt</rhyme>.</l>
        <l id="s1:l3" met="2" type="-+">Doch niet en <rhyme label="a" type="m" corresp="#s1:l1">beydt</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">My te <rhyme label="b" type="f">aenhooren</rhyme>,</l>
        <l id="s1:l5" met="2" type="-+">My te <rhyme label="b" type="f" corresp="#s1:l4">aenhooren</rhyme>:</l>
        <l id="s1:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s1:l4">wtvercoren</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="6" type="-+">Och Heer mijn Godt, hoe sijn mijn Vianden dus <rhyme label="a" type="m">veel</rhyme>,</l>
        <l id="s2:l2" met="6" type="-+">Om u lijd' Ick spot, sy stellen haer teghen my <rhyme label="a" type="m" corresp="#s2:l1">gheheel</rhyme>:</l>
        <l id="s2:l3" met="2" type="-+">Ghy sijt mijn <rhyme label="a" type="m" corresp="#s2:l1">deel</rhyme>:</l>
        <l id="s2:l4" met="2" type="-+">Staet my nu <rhyme label="b" type="f">voren</rhyme>,</l>
        <l id="s2:l5" met="2" type="-+">Staet my nu <rhyme label="b" type="f" corresp="#s2:l4">voren</rhyme>:</l>
        <l id="s2:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s2:l4">wtvercoren</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="6" type="-+">Die Viandt quaet, slaet mijn leven te <rhyme label="a" type="m">grondt</rhyme>,</l>
        <l id="s3:l2" met="6" type="-+">Seer desperaet, maeckt hy my t'aller <rhyme label="a" type="m" corresp="#s3:l1">stondt</rhyme>:</l>
        <l id="s3:l3" met="2" type="-+">O rooder <rhyme label="a" type="m" corresp="#s3:l1">mondt</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Biet my dijn <rhyme label="b" type="f">ooren</rhyme>,</l>
        <l id="s3:l5" met="2" type="-+">Biet my dijn <rhyme label="b" type="f" corresp="#s3:l4">ooren</rhyme>:</l>
        <l id="s3:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s3:l4">wtvercoren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="6" type="-+">Druck heeft mijn vleysch, aen allen syden <rhyme label="a" type="m">bevaen</rhyme>,</l>
        <l id="s4:l2" met="6" type="-+">Van swaer ghepeys, en kan Ick my niet <rhyme label="a" type="m" corresp="#s4:l1">ontslaen</rhyme>:</l>
        <l id="s4:l3" met="2" type="-+">Wilt my <rhyme label="a" type="m" corresp="#s4:l1">bystaen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Of Ick blijf <rhyme label="b" type="f">verloren</rhyme>,</l>
        <l id="s4:l5" met="2" type="-+">Of Ick blijf <rhyme label="b" type="f" corresp="#s4:l4">verloren</rhyme>:</l>
        <l id="s4:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s4:l4">wtvercoren</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="6" type="-+">Mijn hert seer beeft, mijn kraft my geheel <rhyme label="a" type="m">ontgaet</rhyme>,</l>
        <l id="s5:l2" met="6" type="-+">T'licht my begeeft, deur't weenen van mijn oogen <rhyme label="a" type="m" corresp="#s5:l1">quaet</rhyme>:</l>
        <l id="s5:l3" met="2" type="-+">Mijn Ziel <rhyme label="a" type="m" corresp="#s5:l1">beraet</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Sy schijnt te <rhyme label="b" type="f">smoren</rhyme>,</l>
        <l id="s5:l5" met="2" type="-+">Sy schijnt te <rhyme label="b" type="f" corresp="#s5:l4">smoren</rhyme>:</l>
        <l id="s5:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s5:l4">wtvercoren</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="6" type="-+">Prins den Ick kies, maeckt u op tot mijn hulpe <rhyme label="a" type="m">snel</rhyme>,</l>
        <l id="s6:l2" met="6" type="-+">Brenght voort die spies, teghens mijn Vervolgers <rhyme label="a" type="m" corresp="#s6:l1">fel</rhyme>,</l>
        <l id="s6:l3" met="2" type="-+">Breeckt haer <rhyme label="a" type="m" corresp="#s6:l1">opstel</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Welck is <rhyme label="b" type="f">gheschoren</rhyme>,</l>
        <l id="s6:l5" met="2" type="-+">Welck is <rhyme label="b" type="f" corresp="#s6:l4">gheschoren</rhyme>:</l>
        <l id="s6:l6" met="5" type="-+">Want boven al sijt ghy mijn <rhyme label="b" type="f" corresp="#s6:l4">wtvercoren</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
