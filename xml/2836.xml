  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Ick sprack mijn liefie In geen <rhyme label="a" type="f">maent</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Soe deede Icker oeck in <rhyme label="b" type="m">twee</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Soe deede Icker oeck in <rhyme label="a" type="f" corresp="#s1:l1">twee</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Jae lief <rhyme label="b" type="m" corresp="#s1:l2">Jae</rhyme></l>
        <l id="s1:l5" met="5" type="-+">Soe deede Icker oeck In <rhyme label="b" type="m" corresp="#s1:l2">twee</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Schoen liefie komt bij <rhyme label="a" type="f">daege</rhyme></l>
        <l id="s2:l2" met="3" type="-+">by nacht en waeck Ick <rhyme label="b" type="m">niet</rhyme></l>
        <l id="s2:l3" met="4" type="-+">en of gij mij dan In schande <rhyme label="a" type="f" corresp="#s2:l1">liet</rhyme></l>
        <l id="s2:l4" met="3" type="-+">mijn oever <rhyme label="b" type="m" corresp="#s2:l2">soetelief</rhyme></l>
        <l id="s2:l5" met="5" type="-+">Jae lief Jae mijn over <rhyme label="b" type="m" corresp="#s2:l2">soetelief</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Lief of gij mijn dan In schande <rhyme label="a" type="f">liet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Wat sout u <rhyme label="b" type="m">baeten</rhyme></l>
        <l id="s3:l3" met="4" type="-+">en ben ick u dan niet rijcke <rhyme label="a" type="f" corresp="#s3:l1">nouch</rhyme></l>
        <l id="s3:l4" met="3" type="-+">gij muech mijn <rhyme label="b" type="m" corresp="#s3:l2">laeten</rhyme></l>
        <l id="s3:l5" met="5" type="-+">Jae lief Jae gij muecht mij <rhyme label="b" type="m" corresp="#s3:l2">laten</rhyme></l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Wat laete lief wat <rhyme label="a" type="f">laeten</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Dat laeten is <rhyme label="b" type="m">gedaen</rhyme></l>
        <l id="s4:l3" met="4" type="-+">De son die schint Int <rhyme label="a" type="f" corresp="#s4:l1">water</rhyme></l>
        <l id="s4:l4" met="3" type="-+">De min Is al <rhyme label="b" type="m" corresp="#s4:l2">voergaen</rhyme></l>
        <l id="s4:l5" met="5" type="-+">Jae lief Jae de min Is al <rhyme label="b" type="m" corresp="#s4:l2">voergaen</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Mijn soetelief Is drie mitges <rhyme label="a" type="f">rijcker</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dan ick sellever <rhyme label="b" type="m">ben</rhyme></l>
        <l id="s5:l3" met="4" type="-+">soe wil Icker ses gaen <rhyme label="a" type="f" corresp="#s5:l1">winnen</rhyme></l>
        <l id="s5:l4" met="3" type="-+">soe ben Ick boeven <rhyme label="b" type="m" corresp="#s5:l2">hem</rhyme></l>
        <l id="s5:l5" met="5" type="-+">Jae lief Jae soe ben Ick boeven <rhyme label="b" type="m" corresp="#s5:l2">hem</rhyme></l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Soe wil Icker ses gaen <rhyme label="a" type="f">winnen</rhyme></l>
        <l id="s6:l2" met="3" type="-+">en drie <rhyme label="b" type="m">voerspaeren</rhyme></l>
        <l id="s6:l3" met="4" type="-+">hadieu mijn oever <rhyme label="a" type="f" corresp="#s6:l1">soetelief</rhyme></l>
        <l id="s6:l4" met="3" type="-+">wel moetie <rhyme label="b" type="m" corresp="#s6:l2">vaeren</rhyme></l>
        <l id="s6:l5" met="5" type="-+">Jae lief Jae wel moetie <rhyme label="b" type="m" corresp="#s6:l2">vaeren</rhyme>.</l>
      </lg>
    </lg>
