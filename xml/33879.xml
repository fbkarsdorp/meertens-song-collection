  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="2" type="+-">JEsus die <rhyme label="a" type="m">klam</rhyme></l>
        <l id="s1:l2" met="3" type="-+">Op eenen hoogen <rhyme label="b" type="f">Berge</rhyme>,</l>
        <l id="s1:l3" met="3" type="-+">En als hy daer neer <rhyme label="c" type="m">sat</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Tot hem aen <rhyme label="a" type="m" corresp="#s1:l1">quam</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Het vollik, om te <rhyme label="b" type="f" corresp="#s1:l2">verge</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Die waren swak, en <rhyme label="c" type="m" corresp="#s1:l3">mat</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Die doof, die stom, die <rhyme label="d" type="m">blindt</rhyme>,</l>
        <l id="s1:l8" met="3" type="-+">Ook kreupelen, en <rhyme label="e" type="f">manken</rhyme>:</l>
        <l id="s1:l9" met="4" type="-+">Soo ras hy deese voor hem <rhyme label="d" type="m" corresp="#s1:l7">vindt</rhyme>,</l>
        <l id="s1:l10" met="3" type="-+">Geneesd' hy al die <rhyme label="e" type="f" corresp="#s1:l8">kranken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="2" type="+-">Dus waren <rhyme label="a" type="m">sy</rhyme></l>
        <l id="s2:l2" met="3" type="-+">Verwondert <rhyme label="b" type="f">altemale</rhyme>,</l>
        <l id="s2:l3" met="3" type="-+">En hartelijk <rhyme label="c" type="m">verblijdt</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">Die waren <rhyme label="a" type="m" corresp="#s2:l1">vry</rhyme></l>
        <l id="s2:l5" met="3" type="-+">Gemaekt van hare <rhyme label="b" type="f" corresp="#s2:l2">quale</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Van al onheyl <rhyme label="c" type="m" corresp="#s2:l3">bevrijdt</rhyme>:</l>
        <l id="s2:l7" met="3" type="-+">Nu spreekt den stommen <rhyme label="d" type="m">man</rhyme>,</l>
        <l id="s2:l8" met="3" type="-+">Heel recht soo gaet den <rhyme label="e" type="f">manken</rhyme>,</l>
        <l id="s2:l9" met="4" type="-+">Heel klaerlijk sien den blinden <rhyme label="d" type="m" corresp="#s2:l7">kan</rhyme>,</l>
        <l id="s2:l10" met="3" type="-+">Waer over sy Godt <rhyme label="e" type="f" corresp="#s2:l8">danken</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="2" type="+-">Jesus <rhyme label="a" type="m">aensagh</rhyme></l>
        <l id="s3:l2" met="3" type="-+">Den meenighte der <rhyme label="b" type="f">Scharen</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Ontfermde over <rhyme label="c" type="m">haer</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">De derden <rhyme label="a" type="m" corresp="#s3:l1">dagh</rhyme></l>
        <l id="s3:l5" met="3" type="-+">Wast nu dat sy daer <rhyme label="b" type="f" corresp="#s3:l2">waren</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Men vondt geen eeten <rhyme label="c" type="m" corresp="#s3:l3">daer</rhyme>;</l>
        <l id="s3:l7" met="3" type="-+">Het sou niet sijn te <rhyme label="d" type="m">deegh</rhyme></l>
        <l id="s3:l8" met="3" type="-+">Soo 't huys te late <rhyme label="e" type="f">wijken</rhyme>,</l>
        <l id="s3:l9" met="4" type="-+">Daer moght 'er sommigh <rhyme label="d" type="m" corresp="#s3:l7">onderweegh</rhyme></l>
        <l id="s3:l10" met="3" type="-+">Van hongers-noodt <rhyme label="e" type="f" corresp="#s3:l8">beswijken</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="2" type="+-">Veel sijn van <rhyme label="a" type="m">veer</rhyme></l>
        <l id="s4:l2" met="3" type="-+">Tot hier toe komen <rhyme label="b" type="f">reysen</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">En sijn <rhyme label="c" type="m">Discipulen</rhyme></l>
        <l id="s4:l4" met="2" type="-+">Seggen, o <rhyme label="a" type="m" corresp="#s4:l1">Heer</rhyme>,</l>
        <l id="s4:l5" met="3" type="-+">Wy weten niet te <rhyme label="b" type="f" corresp="#s4:l2">peysen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Voor deese <rhyme label="c" type="m" corresp="#s4:l3">mennighten</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Genogh broodt in <rhyme label="d" type="m">d'Woestijn</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Te vinden, dat sy <rhyme label="e" type="f">alle</rhyme></l>
        <l id="s4:l9" met="4" type="-+">Versadight souden mogen <rhyme label="d" type="m" corresp="#s4:l7">sijn</rhyme>,</l>
        <l id="s4:l10" met="3" type="-+">Soo groot is het <rhyme label="e" type="f" corresp="#s4:l8">getalle</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="2" type="+-">Seght hoe veel <rhyme label="a" type="m">broodt</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Dat gy nogh hebt te <rhyme label="b" type="f">geeven</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Seght mijn hoe veel der <rhyme label="c" type="m">is</rhyme>?</l>
        <l id="s5:l4" met="2" type="-+">Te nauwer <rhyme label="a" type="m" corresp="#s5:l1">noodt</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Soo hebben wy 'er <rhyme label="b" type="f" corresp="#s5:l2">seeven</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Met nogh een luttel <rhyme label="c" type="m" corresp="#s5:l3">vis</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Hy datelijk <rhyme label="d" type="m">beveeldt</rhyme>,</l>
        <l id="s5:l8" met="3" type="-+">'t Volk neer te sitten <rhyme label="e" type="f">laten</rhyme>:</l>
        <l id="s5:l9" met="4" type="-+">Na Dank-segginge seydt hy, <rhyme label="d" type="m" corresp="#s5:l7">deeldt</rhyme></l>
        <l id="s5:l10" met="3" type="-+">Het om aen die daer <rhyme label="e" type="f" corresp="#s5:l8">saten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="2" type="+-">s' Hebben <rhyme label="a" type="m">gedeelt</rhyme></l>
        <l id="s6:l2" met="3" type="-+">De Brooden aen de <rhyme label="b" type="f">Scharen</rhyme>;</l>
        <l id="s6:l3" met="3" type="-+">Na dat sy sijn <rhyme label="c" type="m">versaedt</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Hy haer <rhyme label="a" type="m" corresp="#s6:l1">beveelt</rhyme>,</l>
        <l id="s6:l5" met="3" type="-+">Het overschot te <rhyme label="b" type="f" corresp="#s6:l2">garen</rhyme>.</l>
        <l id="s6:l6" met="3" type="-+">Sy hebben vol <rhyme label="c" type="m" corresp="#s6:l3">gelaedt</rhyme></l>
        <l id="s6:l7" met="3" type="-+">Wel seven <rhyme label="d" type="m">korreven</rhyme>.</l>
        <l id="s6:l8" met="3" type="-+">Vier-duysent was 'er <rhyme label="e" type="f">t'samen</rhyme>,</l>
        <l id="s6:l9" met="4" type="-+">Behalven wijfs en <rhyme label="d" type="m" corresp="#s6:l7">kinderen</rhyme>,</l>
        <l id="s6:l10" met="3" type="-+">Die al mee eeten <rhyme label="e" type="f" corresp="#s6:l8">quamen</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="2" type="+-">O goede <rhyme label="a" type="m">Heer</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">By u is 't wel te <rhyme label="b" type="f">weesen</rhyme>,</l>
        <l id="s7:l3" met="3" type="-+">Gy sijt soo mildt en <rhyme label="c" type="m">goedt</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Al 't wee, en <rhyme label="a" type="m" corresp="#s7:l1">seer</rhyme>,</l>
        <l id="s7:l5" met="3" type="-+">Gy kondt en wilt  <rhyme label="b" type="f" corresp="#s7:l2">geneesen</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Wanneer wy maer sijn <rhyme label="c" type="m" corresp="#s7:l3">vroet</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Gy geeft ons daegh'lijks <rhyme label="d" type="m">broodt</rhyme>,</l>
        <l id="s7:l8" met="3" type="-+">Gy sijt soo <rhyme label="e" type="f">liberale</rhyme>,</l>
        <l id="s7:l9" met="4" type="-+">Dat gy besorght voor alle <rhyme label="d" type="m" corresp="#s7:l7">noodt</rhyme></l>
        <l id="s7:l10" met="3" type="-+">Ons menschen al te <rhyme label="e" type="f" corresp="#s7:l8">male</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
