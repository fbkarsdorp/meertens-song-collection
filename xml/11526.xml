<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>11526</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>amsterdam</orginal-name>
      <standard-name>Amsterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>52.3745403</lon>
        <lat>4.89797550561798</lat>
      </coordinates>
    </location>
    <tags>
      <tag>love song</tag>
      <tag>narrative song</tag>
    </tags>
    <melody>Ne vous offensez madame</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">O Stuurse Wind! die noch door <rhyme label="a" type="f">smeken</rhyme>,</l>
        <l id="s1:l2" met="4" type="-+">Noch bidden te beweghen <rhyme label="b" type="m">zijt</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Laet mijn gesucht dijn moed wat <rhyme label="a" type="f" corresp="#s1:l1">weeken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Weest niet so vvreed meer t'alle <rhyme label="b" type="m" corresp="#s1:l2">tijt</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Het swerrik sweeft, de wolcken <rhyme label="a" type="f">swieren</rhyme></l>
        <l id="s2:l2" met="4" type="-+">O Boreas! door uwe <rhyme label="b" type="m">kracht</rhyme></l>
        <l id="s2:l3" met="4" type="-+">De zee verschrickt my met sijn <rhyme label="a" type="f" corresp="#s2:l1">gieren</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Den Hemel toont hem altijdt <rhyme label="b" type="m" corresp="#s2:l2">macht</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">'t Is seven dagen nu <rhyme label="a" type="f">alreede</rhyme></l>
        <l id="s3:l2" met="4" type="-+">Datmen noch Son, noch bleecke <rhyme label="b" type="m">Maen</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Het blaeuwe Velt heeft sien <rhyme label="a" type="f" corresp="#s3:l1">betreden</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Of aen ons kimme <rhyme label="b" type="m" corresp="#s3:l2">onder-gaen</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De harde Rotsen, laes! die <rhyme label="a" type="f">tuigen</rhyme>,</l>
        <l id="s4:l2" met="4" type="-+">Ay Asiaensche Hero <rhyme label="b" type="m">soet</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">Dat sich het Strand niet zat kan <rhyme label="a" type="f" corresp="#s4:l1">suigen</rhyme></l>
        <l id="s4:l4" met="4" type="-+">Van myne droeve <rhyme label="b" type="m" corresp="#s4:l2">tranen-vloet</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Hoe menighmael ben ick <rhyme label="a" type="f">getreden</rhyme></l>
        <l id="s5:l2" met="4" type="-+">Naer Hellespontus schorre <rhyme label="b" type="m">Strand</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Mijn beenen vaeck in 't vvater <rhyme label="a" type="f" corresp="#s5:l1">gleden</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Doch aers'lingh ruckt icks' vveer te <rhyme label="b" type="m" corresp="#s5:l2">landt</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hoe dickwils vvend' ick mijn <rhyme label="a" type="f">gesichte</rhyme></l>
        <l id="s6:l2" met="4" type="-+">Na Festos, tot mijn 't hart <rhyme label="b" type="m">besweeck</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Daer ickje vande Toren 't <rhyme label="a" type="f" corresp="#s6:l1">lichten</rhyme></l>
        <l id="s6:l4" met="4" type="-+">Dijns fackels, die na mijn <rhyme label="b" type="m" corresp="#s6:l2">uytkeeck</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Tot drymael puften ick de <rhyme label="a" type="f">stemmen</rhyme></l>
        <l id="s7:l2" met="4" type="-+">Des dulle zee en strenge <rhyme label="b" type="m">vvint</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">En vvilde na u Bake <rhyme label="a" type="f" corresp="#s7:l1">swemmen</rhyme>.</l>
        <l id="s7:l4" met="4" type="-+">Dan, laes! 't vvas vruchteloos <rhyme label="b" type="m" corresp="#s7:l2">versint</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">'t Getuymel vande gryse <rhyme label="a" type="f">baren</rhyme></l>
        <l id="s8:l2" met="4" type="-+">My fluckx te rugge deysen <rhyme label="b" type="m">deedt</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">En dwongh my 't leven te <rhyme label="a" type="f" corresp="#s8:l1">bewaren</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Ia krachteloos te rugge <rhyme label="b" type="m" corresp="#s8:l2">smeet</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">Besorghde Vrouw! staeckt doch u <rhyme label="a" type="f">klaghjes</rhyme>,</l>
        <l id="s9:l2" met="4" type="-+">Ick sal soo haest het vveer <rhyme label="b" type="m">besaet</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Het holle vvater soet en <rhyme label="a" type="f" corresp="#s9:l1">saghjes</rhyme></l>
        <l id="s9:l4" met="4" type="-+">Door-schuyven, dan nu is 't geen <rhyme label="b" type="m" corresp="#s9:l2">raet</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">En schoon de vvint laet hem niet <rhyme label="a" type="f">toomen</rhyme>,</l>
        <l id="s10:l2" met="4" type="-+">Maer blyvet even stuurs en <rhyme label="b" type="m">straf</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">So sal nochtans ick tot u <rhyme label="a" type="f" corresp="#s10:l1">komen</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Al souw de zee vvorden mijn <rhyme label="b" type="m" corresp="#s10:l2">graf</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="-+">En of 't geviel dat ick <rhyme label="a" type="f">versmoorde</rhyme>,</l>
        <l id="s11:l2" met="4" type="-+">In 't vvater, door te stout <rhyme label="b" type="m">bestaen</rhyme>,</l>
        <l id="s11:l3" met="4" type="-+">En ghy mijn diepe suchten <rhyme label="a" type="f" corresp="#s11:l1">hoorde</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Wilt aen u Oever mijn Lijck <rhyme label="b" type="m" corresp="#s11:l2">ontfaen</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
