  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Herderkens laet ons naer Betlehem <rhyme label="a" type="f">trecken</rhyme>,</l>
        <l id="s1:l2" met="2" type="-+">Met blij <rhyme label="b" type="m">gheschal</rhyme>,</l>
        <l id="s1:l3" met="4" type="-+">Laet ons, ons ghebuerkens op <rhyme label="a" type="f" corresp="#s1:l1">wecken</rhyme>,</l>
        <l id="s1:l4" met="2" type="-+">Hier rondom <rhyme label="b" type="m" corresp="#s1:l2">al</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Den Enghel heeft ons komen <rhyme label="c" type="f">verclaren</rhyme>,</l>
        <l id="s1:l6" met="2" type="-+">Een blijde <rhyme label="d" type="m">vreught</rhyme>,</l>
        <l id="s1:l7" met="4" type="-+">Een Maghet heeft ons komen te <rhyme label="c" type="f" corresp="#s1:l5">baren</rhyme>,</l>
        <l id="s1:l8" met="2" type="-+">Den Schepper <rhyme label="d" type="m" corresp="#s1:l6">verheught</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Maer Tyter ick had by naer <rhyme label="a" type="f">vergheten</rhyme>,</l>
        <l id="s2:l2" met="2" type="-+">Te draghen wat <rhyme label="b" type="m">me</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">Mischien en is daer niet om <rhyme label="a" type="f" corresp="#s2:l1">t’eten</rhyme>,</l>
        <l id="s2:l4" met="2" type="-+">In Betlehem <rhyme label="b" type="m" corresp="#s2:l2">ste</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Ick sal wat Melcxken mede <rhyme label="c" type="f">draghen</rhyme>,</l>
        <l id="s2:l6" met="2" type="-+">Haelt ghy wat <rhyme label="d" type="m">broodt</rhyme>,</l>
        <l id="s2:l7" met="4" type="-+">Wat Eyerkens om te <rhyme label="c" type="f" corresp="#s2:l5">behaghen</rhyme>,</l>
        <l id="s2:l8" met="2" type="-+">T’kindt in den <rhyme label="d" type="m" corresp="#s2:l6">noodt</rhyme>,</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Coridon gaet ghy met leghe <rhyme label="a" type="f">handen</rhyme>,</l>
        <l id="s3:l2" met="2" type="-+">Naer Betlehem <rhyme label="b" type="m">toe</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">Doet dat niet want dat waere een <rhyme label="a" type="f" corresp="#s3:l1">schande</rhyme>,</l>
        <l id="s3:l4" met="2" type="-+">Ghevuerken wel <rhyme label="b" type="m" corresp="#s3:l2">hoe</rhyme>,</l>
        <l id="s3:l5" met="4" type="-+">En kunde ghy dat niet <rhyme label="c" type="f">bedencken</rhyme>,</l>
        <l id="s3:l6" met="2" type="-+">Dat als men <rhyme label="d" type="m">gaet</rhyme>,</l>
        <l id="s3:l7" met="4" type="-+">In’t kinderbedt sonder <rhyme label="c" type="f" corresp="#s3:l5">gheschencken</rhyme>,</l>
        <l id="s3:l8" met="2" type="-+">Niet wel en <rhyme label="d" type="m" corresp="#s3:l6">staet</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Philis ghy draeght soo groote <rhyme label="a" type="f">sorghen</rhyme>,</l>
        <l id="s4:l2" met="2" type="-+">K’en ben niet <rhyme label="b" type="m">bodt</rhyme>,</l>
        <l id="s4:l3" met="4" type="-+">K’heb onder mijn casack <rhyme label="a" type="f" corresp="#s4:l1">verborghen</rhyme>,</l>
        <l id="s4:l4" met="2" type="-+">Eenen <rhyme label="b" type="m" corresp="#s4:l2">pap-pot</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">K’heb oock in mijnen buydel <rhyme label="c" type="f">ghesteken</rhyme>,</l>
        <l id="s4:l6" met="2" type="-+">Wat Suycker en <rhyme label="d" type="m">Cruydt</rhyme>,</l>
        <l id="s4:l7" met="4" type="-+">Dat ick dat niet en had <rhyme label="c" type="f" corresp="#s4:l5">gheweten</rhyme>,</l>
        <l id="s4:l8" met="2" type="-+">Dat waer een <rhyme label="d" type="m" corresp="#s4:l6">cluydt</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Coragie laet ons niet blijven <rhyme label="a" type="f">praeten</rhyme>,</l>
        <l id="s5:l2" met="2" type="-+">Maer laet ons <rhyme label="b" type="m">gaen</rhyme>,</l>
        <l id="s5:l3" met="4" type="-+">Want onsen tijt sal ons <rhyme label="a" type="f" corresp="#s5:l1">verlaten</rhyme>,</l>
        <l id="s5:l4" met="2" type="-+">Ke siet eens <rhyme label="b" type="m" corresp="#s5:l2">aen</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Daer staet den stal vol <rhyme label="c" type="f">clarigheden</rhyme>,</l>
        <l id="s5:l6" met="2" type="-+">Een Enghelsche <rhyme label="d" type="m">schaer</rhyme>,</l>
        <l id="s5:l7" met="4" type="-+">Singhen den mensch Peys ende <rhyme label="c" type="f" corresp="#s5:l5">vrede</rhyme>,</l>
        <l id="s5:l8" met="2" type="-+">Met vreughden <rhyme label="d" type="m" corresp="#s5:l6">daer</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Is het niet om verwondert te <rhyme label="a" type="f">wesen</rhyme>,</l>
        <l id="s6:l2" met="2" type="-+">Dat Godt wordt <rhyme label="b" type="m">mensch</rhyme>,</l>
        <l id="s6:l3" met="4" type="-+">Dat hy komt uyt sijnen Croon <rhyme label="a" type="f" corresp="#s6:l1">gheresen</rhyme>,</l>
        <l id="s6:l4" met="2" type="-+">Naer onsen <rhyme label="b" type="m" corresp="#s6:l2">wensch</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Kost hy niet hebben voor sijn <rhyme label="c" type="f">wooningh</rhyme>,</l>
        <l id="s6:l6" met="2" type="-+">Een Koninckx <rhyme label="d" type="m">Paleys</rhyme>,</l>
        <l id="s6:l7" met="4" type="-+">Ghelijck aen eenen grooten <rhyme label="c" type="f" corresp="#s6:l5">Koninck</rhyme>,</l>
        <l id="s6:l8" met="2" type="-+">Is sijnen <rhyme label="d" type="m" corresp="#s6:l6">heys</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">Wie sal nu eerst in’t Stalleken <rhyme label="a" type="f">treden</rhyme>,</l>
        <l id="s7:l2" met="2" type="-+">Tyter, gheeft Philips de <rhyme label="b" type="m">eer</rhyme>,</l>
        <l id="s7:l3" met="4" type="-+">Want sy ons ghenoot heeft om <rhyme label="a" type="f" corresp="#s7:l1">t’aenbidden</rhyme>,</l>
        <l id="s7:l4" met="2" type="-+">Dit kindeken <rhyme label="b" type="m" corresp="#s7:l2">teer</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Philis, maeckt gheen gerucht komt stillekens <rhyme label="c" type="f">binnen</rhyme></l>
        <l id="s7:l6" met="2" type="-+">Het kindt dat <rhyme label="d" type="m">slaept</rhyme>,</l>
        <l id="s7:l7" met="4" type="-+">Dat ghy dat kindeken vol <rhyme label="c" type="f" corresp="#s7:l5">Minnen</rhyme>,</l>
        <l id="s7:l8" met="2" type="-+">Niet wacker en <rhyme label="d" type="m" corresp="#s7:l6">maeckt</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Willecom Herders komt ghy <rhyme label="a" type="f">vereeren</rhyme>,</l>
        <l id="s8:l2" met="2" type="-+">Mijn kindt <rhyme label="b" type="m">minnoot</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Al licht hy hier van leden <rhyme label="a" type="f" corresp="#s8:l1">teere</rhyme>,</l>
        <l id="s8:l4" met="2" type="-+">Hy is seer <rhyme label="b" type="m" corresp="#s8:l2">groot</rhyme>,</l>
        <l id="s8:l5" met="4" type="-+">Een Koninckx Soon sijnen <rhyme label="c" type="f">Heer-Vader</rhyme>,</l>
        <l id="s8:l6" met="2" type="-+">Is Godt van <rhyme label="d" type="m">al</rhyme>,</l>
        <l id="s8:l7" met="4" type="-+">Komt Herderkens aenbidt hem te <rhyme label="c" type="f" corresp="#s8:l5">gader</rhyme>,</l>
        <l id="s8:l8" met="2" type="-+">Met blij <rhyme label="d" type="m" corresp="#s8:l6">gheschal</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="-+">K’waer blij dat ick had me <rhyme label="a" type="f">ghenomen</rhyme>,</l>
        <l id="s9:l2" met="2" type="-+">Mijn <rhyme label="b" type="m">Moeselken</rhyme>,</l>
        <l id="s9:l3" met="4" type="-+">Om te vereeren ben ick <rhyme label="a" type="f" corresp="#s9:l1">ghekomen</rhyme>,</l>
        <l id="s9:l4" met="2" type="-+">Dit <rhyme label="b" type="m" corresp="#s9:l2">Kindeken</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Wy bringhen Melck om papken te <rhyme label="c" type="f">koken</rhyme>,</l>
        <l id="s9:l6" met="2" type="-+">En Suycker daer <rhyme label="d" type="m">by</rhyme>,</l>
        <l id="s9:l7" met="4" type="-+">Hebdy gheen houdt om vierken te <rhyme label="c" type="f" corresp="#s9:l5">stoken</rhyme>,</l>
        <l id="s9:l8" met="2" type="-+">Seght het aen <rhyme label="d" type="m" corresp="#s9:l6">my</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="-+">Mijn moerken die sal fluskens <rhyme label="a" type="f">komen</rhyme>,</l>
        <l id="s10:l2" met="2" type="-+">Met <rhyme label="b" type="m">luyerkens</rhyme>,</l>
        <l id="s10:l3" met="4" type="-+">S’en haddet soo gou niet <rhyme label="a" type="f" corresp="#s10:l1">vernomen</rhyme>,</l>
        <l id="s10:l4" met="2" type="-+">Als <rhyme label="b" type="m" corresp="#s10:l2">d’Herderkens</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">K’sal segghen dat sy wat houtien moet <rhyme label="c" type="f">brenghen</rhyme>,</l>
        <l id="s10:l6" met="2" type="-+">Om t’kindeken <rhyme label="d" type="m">teer</rhyme>,</l>
        <l id="s10:l7" met="4" type="-+">Sijn ledekens te vieren het briest soo <rhyme label="c" type="f" corresp="#s10:l5">strenghen</rhyme>,</l>
        <l id="s10:l8" met="2" type="-+">Dan kom ick <rhyme label="d" type="m" corresp="#s10:l6">weer</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
