<song>
  <meta>
    <id>117779</id>
    <date>
      <year-start>1741</year-start>
      <year-end>1741</year-end>
    </date>
    <location>
      <orginal-name>leiden</orginal-name>
      <standard-name>Leiden, The Netherlands</standard-name>
      <coordinates>
        <lon>52.1598645</lon>
        <lat>4.4824271</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Op een tijd op een tijd</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="+-">Maakje klaar, maakje <rhyme label="a" type="m">klaar</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De Kalfs-kop die is <rhyme label="a" type="m" corresp="#s1:l1">gaar</rhyme>,</l>
        <l id="s1:l3" met="3" type="+-">Ey hoor hem eens <rhyme label="b" type="f">koken</rhyme>,</l>
        <l id="s1:l4" met="4" type="-+">Myn Maagje danst op dat <rhyme label="c" type="m">geluyd</rhyme></l>
        <l id="s1:l5" met="4" type="-+">De kop die wil de Ketel <rhyme label="c" type="m" corresp="#s1:l4">uyt</rhyme></l>
        <l id="s1:l6" met="3" type="-+">Ik heb hem al <rhyme label="b" type="f" corresp="#s1:l3">geroken</rhyme>.</l>
        <l id="s1:l7" met="3" type="-+">Ik heb hem al <rhyme label="b" type="f" corresp="#s1:l3">geroken</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="+-">Dats 'er een, dats 'er <rhyme label="a" type="m">een</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Ter Wereld schoonder <rhyme label="a" type="m" corresp="#s2:l1">geen</rhyme>,</l>
        <l id="s2:l3" met="3" type="+-">Dat yder moet <rhyme label="b" type="f">belyen</rhyme>,</l>
        <l id="s2:l4" met="4" type="-+">Ontgin hem maar eens heen en <rhyme label="c" type="m">weer</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">En denk dat uyt een anders <rhyme label="c" type="m" corresp="#s2:l4">Leer</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Goed riemen is te <rhyme label="b" type="f" corresp="#s2:l3">snyen</rhyme>.</l>
        <l id="s2:l7" met="3" type="-+">Goed riemen is te <rhyme label="b" type="f" corresp="#s2:l3">snyen</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="+-">Was 'er maar, was 'er <rhyme label="a" type="m">maar</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Een Fret of <rhyme label="a" type="m" corresp="#s3:l1">Avegaar</rhyme>,</l>
        <l id="s3:l3" met="3" type="+-">Om de Oogen uyt te <rhyme label="b" type="f">boren</rhyme>,</l>
        <l id="s3:l4" met="4" type="-+">Maar deze Lepel is ook <rhyme label="c" type="m">goed</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">Houd op u Tafelbord of <rhyme label="c" type="m" corresp="#s3:l4">Hoed</rhyme>,</l>
        <l id="s3:l6" met="3" type="-+">Kan u 't Gezigt <rhyme label="b" type="f" corresp="#s3:l3">bekoren</rhyme>.</l>
        <l id="s3:l7" met="3" type="-+">Kan u 't Gezigt <rhyme label="b" type="f" corresp="#s3:l3">bekoren</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="+-">Dit 's zyn Tong, dit 's zyn <rhyme label="a" type="m">Tong</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Daar hy 't Muziek me <rhyme label="a" type="m" corresp="#s4:l1">zong</rhyme>,</l>
        <l id="s4:l3" met="3" type="+-">Terwyl zyn Makkers <rhyme label="b" type="f">dansten</rhyme>,</l>
        <l id="s4:l4" met="4" type="-+">Wat dunkje van dat <rhyme label="c" type="m">Kakebeen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Daar zit de beste Knap om <rhyme label="c" type="m" corresp="#s4:l4">heen</rhyme>,</l>
        <l id="s4:l6" met="3" type="-+">Die wy ons leven <rhyme label="b" type="f" corresp="#s4:l3">schransten</rhyme>.</l>
        <l id="s4:l7" met="3" type="-+">Die wy ons leven <rhyme label="b" type="f" corresp="#s4:l3">schransten</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="+-">Wie zou niet, wie zou <rhyme label="a" type="m">niet</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">Als hy 't Verhemelt <rhyme label="a" type="m" corresp="#s5:l1">ziet</rhyme>,</l>
        <l id="s5:l3" met="3" type="+-">Een stukje daar van <rhyme label="b" type="f">wenssen</rhyme>,</l>
        <l id="s5:l4" met="4" type="-+">Wat zyn de Herssens <rhyme label="c" type="m">hagel-wit</rhyme>,</l>
        <l id="s5:l5" met="4" type="-+">Zie of 'er ook een Ky in <rhyme label="c" type="m" corresp="#s5:l4">zit</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Als in de meeste <rhyme label="b" type="f" corresp="#s5:l3">Menschen</rhyme>.</l>
        <l id="s5:l7" met="3" type="-+">Als in de meeste <rhyme label="b" type="f" corresp="#s5:l3">Menschen</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="+-">Blank en mals, blank en <rhyme label="a" type="m">mals</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Is hy gelyk een <rhyme label="a" type="m" corresp="#s6:l1">Hals</rhyme>,</l>
        <l id="s6:l3" met="3" type="+-">En Prammen van een <rhyme label="b" type="f">Meysje</rhyme>,</l>
        <l id="s6:l4" met="4" type="-+">Men roept vry op de Klieren <rhyme label="c" type="m">schrik</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">En aard de schoonste <rhyme label="c" type="m" corresp="#s6:l4">Zwezerik</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Ik bidje Proef een <rhyme label="b" type="f" corresp="#s6:l3">rysje</rhyme>.</l>
        <l id="s6:l7" met="3" type="-+">Ik bidje Proef een <rhyme label="b" type="f" corresp="#s6:l3">rysje</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="+-">Eêle Kost, eêle <rhyme label="a" type="m">Kost</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Uyt Spanje is <rhyme label="a" type="m" corresp="#s7:l1">gelost</rhyme></l>
        <l id="s7:l3" met="3" type="+-">Geen Koppen kan op <rhyme label="b" type="f">wegen</rhyme>,</l>
        <l id="s7:l4" met="4" type="-+">Indien Spekjan de lugt maar <rhyme label="c" type="m">rook</rhyme>,</l>
        <l id="s7:l5" met="4" type="-+">Hy zou gewis zyn Lap en <rhyme label="c" type="m" corresp="#s7:l4">Pook</rhyme>,</l>
        <l id="s7:l6" met="3" type="-+">Aan deze Kop <rhyme label="b" type="f" corresp="#s7:l3">vervegen</rhyme>.</l>
        <l id="s7:l7" met="3" type="-+">Aan deze Kop <rhyme label="b" type="f" corresp="#s7:l3">vervegen</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="+-">d' Indiaan, d' <rhyme label="a" type="m">Indiaan</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Liet al zyn Kruyden <rhyme label="a" type="m" corresp="#s8:l1">staan</rhyme>,</l>
        <l id="s8:l3" met="3" type="+-">Indien hy meê mogt <rhyme label="b" type="f">eeten</rhyme>,</l>
        <l id="s8:l4" met="4" type="-+">Hy schonk aan ons zyn <rhyme label="c" type="m">Gogeltas</rhyme></l>
        <l id="s8:l5" met="4" type="-+">Zyne Piessan en de <rhyme label="c" type="m" corresp="#s8:l4">Annas</rhyme>,</l>
        <l id="s8:l6" met="3" type="-+">Zou hy wel haast <rhyme label="b" type="f" corresp="#s8:l3">vergeten</rhyme>.</l>
        <l id="s8:l7" met="3" type="-+">Zou hy wel haast <rhyme label="b" type="f" corresp="#s8:l3">vergeten</rhyme>.</l>
      </lg>
      <lg id="s9" type="stanza">
        <l id="s9:l1" met="4" type="+-">Gans en Kip, Gans en <rhyme label="a" type="m">Kip</rhyme>,</l>
        <l id="s9:l2" met="3" type="-+">Kapoen en <rhyme label="a" type="m" corresp="#s9:l1">Watersnip</rhyme>,</l>
        <l id="s9:l3" met="3" type="+-">Wie zou niet na je <rhyme label="b" type="f">talen</rhyme>,</l>
        <l id="s9:l4" met="4" type="-+">Geen Pluym-gediert, Wild of <rhyme label="c" type="m">Tam</rhyme>,</l>
        <l id="s9:l5" met="4" type="-+">Geen Osse-tong, geen <rhyme label="c" type="m" corresp="#s9:l4">Hane-kam</rhyme>,</l>
        <l id="s9:l6" met="3" type="-+">Mag by de Koppen <rhyme label="b" type="f" corresp="#s9:l3">halen</rhyme>.</l>
        <l id="s9:l7" met="3" type="-+">Mag by de Koppen <rhyme label="b" type="f" corresp="#s9:l3">halen</rhyme>.</l>
      </lg>
      <lg id="s10" type="stanza">
        <l id="s10:l1" met="4" type="+-">Hy is op, hy is <rhyme label="a" type="m">op</rhyme>,</l>
        <l id="s10:l2" met="3" type="-+">Die schoonder <rhyme label="a" type="m" corresp="#s10:l1">Kalfs-kop</rhyme>,</l>
        <l id="s10:l3" met="3" type="+-">En in ons Lyf <rhyme label="b" type="f">begraven</rhyme>,</l>
        <l id="s10:l4" met="4" type="-+">Kom laat ons met de <rhyme label="c" type="m">Koele-wyn</rhyme>,</l>
        <l id="s10:l5" met="4" type="-+">Zyn Uytvaard nu gedagtig <rhyme label="c" type="m" corresp="#s10:l4">zyn</rhyme>,</l>
        <l id="s10:l6" met="3" type="-+">En onze Keeltjes <rhyme label="b" type="f" corresp="#s10:l3">laven</rhyme>.</l>
        <l id="s10:l7" met="3" type="-+">En onze Keeltjes <rhyme label="b" type="f" corresp="#s10:l3">laven</rhyme>.</l>
      </lg>
      <lg id="s11" type="stanza">
        <l id="s11:l1" met="4" type="+-">Beste-maat, <rhyme label="a" type="m">Beste-maat</rhyme>,</l>
        <l id="s11:l2" met="3" type="-+">Ey hoor eens hoe hy <rhyme label="a" type="m" corresp="#s11:l1">Blaat</rhyme>,</l>
        <l id="s11:l3" met="3" type="+-">Om <rhyme label="b" type="f">Muskadelle-droppen</rhyme>,</l>
        <l id="s11:l4" met="4" type="-+">Al ben ik aan myn twede <rhyme label="c" type="m">Fles</rhyme>,</l>
        <l id="s11:l5" met="4" type="-+">Kom doenwe nog een teug vyf <rhyme label="c" type="m" corresp="#s11:l4">zes</rhyme>,</l>
        <l id="s11:l6" met="3" type="-+">Ter eeren van de <rhyme label="b" type="f" corresp="#s11:l3">Koppen</rhyme>.</l>
        <l id="s11:l7" met="3" type="-+">Ter eeren van de <rhyme label="b" type="f" corresp="#s11:l3">Koppen</rhyme>.</l>
      </lg>
      <lg id="s12" type="stanza">
        <l id="s12:l1" met="4" type="+-">ô Hoe zoet! ô hoe <rhyme label="a" type="m">zoet</rhyme>!</l>
        <l id="s12:l2" met="3" type="-+">Verheugd de Wyn het <rhyme label="a" type="m" corresp="#s12:l1">Bloed</rhyme>,</l>
        <l id="s12:l3" met="3" type="+-">Der vriendelyke <rhyme label="b" type="f">Snaken</rhyme>,</l>
        <l id="s12:l4" met="4" type="-+">Maar hoe, wat hoor ik voor <rhyme label="c" type="m">gewoel</rhyme>?</l>
        <l id="s12:l5" met="4" type="-+">Of druypt 'er ymand door de <rhyme label="c" type="m" corresp="#s12:l4">Stoel</rhyme>?</l>
        <l id="s12:l6" met="3" type="-+">My dunkt de Sporten <rhyme label="b" type="f" corresp="#s12:l3">kraken</rhyme>.</l>
        <l id="s12:l7" met="3" type="-+">My dunkt de Sporten <rhyme label="b" type="f" corresp="#s12:l3">kraken</rhyme>.</l>
      </lg>
      <lg id="s13" type="stanza">
        <l id="s13:l1" met="4" type="+-">'t Is geen nood, 't Is geen <rhyme label="a" type="m">nood</rhyme>,</l>
        <l id="s13:l2" met="3" type="-+">De Man is nog niet <rhyme label="a" type="m" corresp="#s13:l1">dood</rhyme>,</l>
        <l id="s13:l3" met="3" type="+-">Hy staat weer op zyn <rhyme label="b" type="f">Benen</rhyme>:</l>
        <l id="s13:l4" met="4" type="-+">Maar zagt, wie is 'er buyten <rhyme label="c" type="m">aam</rhyme>,</l>
        <l id="s13:l5" met="4" type="-+">En spreekt 'er woorden van een <rhyme label="c" type="m" corresp="#s13:l4">Vaam</rhyme>,</l>
        <l id="s13:l6" met="3" type="-+">Ik hoor zo byster <rhyme label="b" type="f" corresp="#s13:l3">stenen</rhyme>.</l>
        <l id="s13:l7" met="3" type="-+">Ik hoor zo byster <rhyme label="b" type="f" corresp="#s13:l3">stenen</rhyme>.</l>
      </lg>
      <lg id="s14" type="stanza">
        <l id="s14:l1" met="4" type="+-">Laat ons gaan, laat ons <rhyme label="a" type="m">gaan</rhyme>,</l>
        <l id="s14:l2" met="3" type="-+">Het Heerschop is <rhyme label="a" type="m" corresp="#s14:l1">voldaan</rhyme>,</l>
        <l id="s14:l3" met="3" type="+-">En 't Maantje schynd nog <rhyme label="b" type="f">helder</rhyme>,</l>
        <l id="s14:l4" met="4" type="-+">Wy hebben altemaal de <rhyme label="c" type="m">Vragt</rhyme>:</l>
        <l id="s14:l5" met="4" type="-+">Nu vrienden, 'k wensje goeden <rhyme label="c" type="m" corresp="#s14:l4">Nagt</rhyme>,</l>
        <l id="s14:l6" met="3" type="-+">Maar val niet in de <rhyme label="b" type="f" corresp="#s14:l3">Kelder</rhyme>.</l>
        <l id="s14:l7" met="3" type="-+">Maar val niet in de <rhyme label="b" type="f" corresp="#s14:l3">Kelder</rhyme>.</l>
      </lg>
      <lg id="s15" type="stanza">
        <l id="s15:l1" met="4" type="+-">Is dit Feest, is dit <rhyme label="a" type="m">Feest</rhyme>,</l>
        <l id="s15:l2" met="3" type="-+">Vol van Snakery <rhyme label="a" type="m" corresp="#s15:l1">geweest</rhyme>,</l>
        <l id="s15:l3" met="3" type="+-">Tot geen Verwyt kan 't <rhyme label="b" type="f">strekken</rhyme>,</l>
        <l id="s15:l4" met="4" type="-+">Want na de Taal van <rhyme label="c" type="m">Diogeen</rhyme>,</l>
        <l id="s15:l5" met="4" type="-+">Zo is d' Wereld maar <rhyme label="c" type="m" corresp="#s15:l4">alleen</rhyme>,</l>
        <l id="s15:l6" met="3" type="-+">Een grote Kooy vol <rhyme label="b" type="f" corresp="#s15:l3">Gekken</rhyme>.</l>
        <l id="s15:l7" met="3" type="-+">Een grote Kooy vol <rhyme label="b" type="f" corresp="#s15:l3">Gekken</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
