      <category translation="religion">godsdienst</category>
    </categories>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">Dochter hoe syt gy dus <rhyme label="a" type="m">voerstoert</rhyme></l>
        <l id="s1:l2" met="3" type="-+">en dus bedroeft van <rhyme label="b" type="m">moet</rhyme></l>
        <l id="s1:l3" met="4" type="-+">Daer u een vrijer komt aen <rhyme label="a" type="m" corresp="#s1:l1">boert</rhyme></l>
        <l id="s1:l4" met="3" type="-+">Die machtich Is van <rhyme label="b" type="m" corresp="#s1:l2">goedt</rhyme></l>
        <l id="s1:l5" met="4" type="-+">Daer gij hem eerst waert <rhyme label="c" type="m">toegedaen</rhyme></l>
        <l id="s1:l6" met="4" type="+-">Dat gij hem alree <rhyme label="d" type="m">voersmaet</rhyme></l>
        <l id="s1:l7" met="2" type="-+">en soe <rhyme label="d" type="m" corresp="#s1:l6">verlaet</rhyme>/</l>
        <l id="s1:l8" met="3" type="-+">en hiet hem <rhyme label="c" type="m" corresp="#s1:l5">dueregaen</rhyme></l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">Waerom syt gy dus <rhyme label="a" type="m">bedocht</rhyme></l>
        <l id="s2:l2" met="3" type="-+">gij sijt voewaer niet <rhyme label="b" type="m">moey</rhyme></l>
        <l id="s2:l3" met="4" type="-+">tis omt goedt dat gy wert <rhyme label="a" type="m" corresp="#s2:l1">besocht</rhyme></l>
        <l id="s2:l4" met="3" type="-+">tis voerwaer al quae <rhyme label="b" type="m" corresp="#s2:l2">roeij</rhyme></l>
        <l id="s2:l5" met="4" type="-+">Laet u moet daelen In korten <rhyme label="c" type="m">tijt</rhyme></l>
        <l id="s2:l6" met="4" type="+-">eer die vrijers haer <rhyme label="d" type="m">beraen</rhyme></l>
        <l id="s2:l7" met="2" type="-+">en elders <rhyme label="d" type="m" corresp="#s2:l6">gaen</rhyme>/</l>
        <l id="s2:l8" met="3" type="-+">tis al een groet <rhyme label="c" type="m" corresp="#s2:l5">spijt</rhyme></l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Daerom weest vrij van taele <rhyme label="a" type="m">soet</rhyme></l>
        <l id="s3:l2" met="3" type="-+">alsmen u op eer <rhyme label="b" type="m">begeert</rhyme></l>
        <l id="s3:l3" met="4" type="-+">Eert gijt seil laeten daeln <rhyme label="a" type="m" corresp="#s3:l1">moet</rhyme></l>
        <l id="s3:l4" met="3" type="-+">en te syn <rhyme label="b" type="m" corresp="#s3:l2">geblaemeert</rhyme></l>
        <l id="s3:l5" met="4" type="-+">maer als de vinck is op de <rhyme label="c" type="m">til</rhyme></l>
        <l id="s3:l6" met="4" type="+-">Dat gy hem dan wel <rhyme label="d" type="m">onthaelt</rhyme></l>
        <l id="s3:l7" met="2" type="-+">eert u dan <rhyme label="d" type="m" corresp="#s3:l6">faelt</rhyme></l>
        <l id="s3:l8" met="3" type="-+">en hy wech vliegen <rhyme label="c" type="m" corresp="#s3:l5">wil</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">Prinse lief doch voer u <rhyme label="a" type="m">siet</rhyme></l>
        <l id="s4:l2" met="3" type="-+">eer dat het is te <rhyme label="b" type="m">laet</rhyme></l>
        <l id="s4:l3" met="4" type="-+">het nae klaegen sal baeten <rhyme label="a" type="m" corresp="#s4:l1">niet</rhyme></l>
        <l id="s4:l4" met="3" type="-+">en gy sult sin <rhyme label="b" type="m" corresp="#s4:l2">voersmaet</rhyme></l>
        <l id="s4:l5" met="4" type="-+">Daerom weest doch van sinne <rhyme label="c" type="m">vroet</rhyme></l>
        <l id="s4:l6" met="4" type="+-">eert Int Jaer van eenen <rhyme label="d" type="m">Is</rhyme></l>
        <l id="s4:l7" met="2" type="-+">sijt Dit <rhyme label="d" type="m" corresp="#s4:l6">gewis</rhyme></l>
        <l id="s4:l8" met="3" type="-+">Want gyt Dan vraegen <rhyme label="c" type="m" corresp="#s4:l5">moet</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
