<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>22250</id>
    <date>
      <year-start>1565</year-start>
      <year-end>1565</year-end>
    </date>
    <location>
      <orginal-name>frankfurt [=wesel?]</orginal-name>
      <standard-name>Wesel, Germany</standard-name>
      <coordinates>
        <lon>51.6576909</lon>
        <lat>6.617087</lat>
      </coordinates>
    </location>
    <tags>
      <tag>psalm</tag>
    </tags>
    <melody>O Herr wer wird Wohnungen</melody>
  </meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="4" type="-+">DE Heer die sprack in synen hoochsten <rhyme label="a" type="m">Troon</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">Tot Christum mijnen <rhyme label="b" type="f">Heere</rhyme>:</l>
        <l id="s1:l3" met="4" type="-+">Ghy zijt mijnen eenighen gheborn <rhyme label="a" type="m" corresp="#s1:l1">Soon</rhyme>:</l>
        <l id="s1:l4" met="3" type="-+">U behoort toe de Godlijcke <rhyme label="b" type="f" corresp="#s1:l2">eere</rhyme>,</l>
        <l id="s1:l5" met="4" type="-+">Daeromme tot mijn rechte handt u <rhyme label="c" type="m">sedt</rhyme>,</l>
        <l id="s1:l6" met="4" type="-+">Tot dat ick leg al u vyanden <rhyme label="c" type="m" corresp="#s1:l5">nedt</rhyme>,</l>
        <l id="s1:l7" met="3" type="-+">Tot een Schabbel van uwen <rhyme label="d" type="f">voeten</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="4" type="-+">De Heer die sal oock wt <rhyme label="a" type="m">Zyoon</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Uwe rijcks Septer u <rhyme label="b" type="f">seynden</rhyme>,</l>
        <l id="s2:l3" met="4" type="-+">U woort sal aldaer beghinnen <rhyme label="a" type="m" corresp="#s2:l1">schoon</rhyme>,</l>
        <l id="s2:l4" met="3" type="-+">En gaen tot aen swerelts <rhyme label="b" type="f" corresp="#s2:l2">eynde</rhyme>,</l>
        <l id="s2:l5" met="4" type="-+">Oock regneert ghy also <rhyme label="c" type="m">voorwaer</rhyme>,</l>
        <l id="s2:l6" met="4" type="-+">Ouer alle uwer vyanden <rhyme label="c" type="m" corresp="#s2:l5">schaer</rhyme>,</l>
        <l id="s2:l7" met="3" type="-+">Dat sy hun sonden moeten <rhyme label="d" type="f">boeten</rhyme>.</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="4" type="-+">Als ghy sult door het lijden <rhyme label="a" type="m">dijn</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">De sond, en den doet <rhyme label="b" type="f">bestrijden</rhyme>,</l>
        <l id="s3:l3" met="4" type="-+">So sal tvolck willich u offren <rhyme label="a" type="m" corresp="#s3:l1">fijn</rhyme>,</l>
        <l id="s3:l4" met="3" type="-+">Met heylighen ciraet en <rhyme label="b" type="f" corresp="#s3:l2">blijde</rhyme>:</l>
        <l id="s3:l5" met="4" type="-+">U kindren die worden u <rhyme label="c" type="m">gheboren</rhyme>,</l>
        <l id="s3:l6" met="4" type="-+">Ghelijck den coulen dauw <rhyme label="c" type="m" corresp="#s3:l5">wtvercoren</rhyme>,</l>
        <l id="s3:l7" met="3" type="-+">In den dagheraet smorghens <rhyme label="d" type="f">vroeghe</rhyme>.</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="4" type="-+">De Heer heeft ghesworen <rhyme label="a" type="m">certeynelijck</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">En ten sal hem niet <rhyme label="b" type="f">berouwen</rhyme>:</l>
        <l id="s4:l3" met="4" type="-+">Ghy zijt eenen Priester <rhyme label="a" type="m" corresp="#s4:l1">eewelijck</rhyme>,</l>
        <l id="s4:l4" met="3" type="-+">Om den dienst Godts nieu op te <rhyme label="b" type="f" corresp="#s4:l2">bouwen</rhyme>,</l>
        <l id="s4:l5" met="4" type="-+">Nae dordinancij van <rhyme label="c" type="m">Melchisedech</rhyme>:</l>
        <l id="s4:l6" met="4" type="-+">Doud offrand die moet dan gheheele <rhyme label="c" type="m" corresp="#s4:l5">wech</rhyme>,</l>
        <l id="s4:l7" met="3" type="-+">Als ghy u sult laten <rhyme label="d" type="f">dooden</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="4" type="-+">Doch sal Godt altijdts by u <rhyme label="a" type="m">zijn</rhyme>,</l>
        <l id="s5:l2" met="3" type="-+">En staen aen uwer <rhyme label="b" type="f">rechten</rhyme>:</l>
        <l id="s5:l3" met="4" type="-+">En straffen die met de eewighe <rhyme label="a" type="m" corresp="#s5:l1">pijn</rhyme>,</l>
        <l id="s5:l4" met="3" type="-+">Al die ghene die teghen u <rhyme label="b" type="f" corresp="#s5:l2">aenuechten</rhyme>:</l>
        <l id="s5:l5" met="4" type="-+">Als hy synen toornighen grim <rhyme label="c" type="m">aengrijpt</rhyme>,</l>
        <l id="s5:l6" met="4" type="-+">De Coninghen hy dan ontsucken <rhyme label="c" type="m" corresp="#s5:l5">smijdt</rhyme>,</l>
        <l id="s5:l7" met="3" type="-+">En hy neemtse gheel wech met <rhyme label="d" type="f">machten</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="4" type="-+">Hy sal oock heerlijck met grooter <rhyme label="a" type="m">macht</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Ouer alle de Heyden <rhyme label="b" type="f">ordeelen</rhyme>:</l>
        <l id="s6:l3" met="4" type="-+">Hy sal doen so menighen grooten <rhyme label="a" type="m" corresp="#s6:l1">slacht</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Teghen die so hem verachten <rhyme label="b" type="f" corresp="#s6:l2">gheele</rhyme>,</l>
        <l id="s6:l5" met="4" type="-+">Hy sal den Prince van deser <rhyme label="c" type="m">wereldt</rhyme>,</l>
        <l id="s6:l6" met="4" type="-+">Die hem altijdts tegehen hem <rhyme label="c" type="m" corresp="#s6:l5">steldt</rhyme>,</l>
        <l id="s6:l7" met="3" type="-+">Sijn helsche rijck gheheel <rhyme label="d" type="f">breken</rhyme>.</l>
      </lg>
      <lg id="s7" type="stanza">
        <l id="s7:l1" met="4" type="-+">In swackheyt, lijden, cruys, en <rhyme label="a" type="m">doodt</rhyme>,</l>
        <l id="s7:l2" met="3" type="-+">Sal hy hier tijtelijck <rhyme label="b" type="f">steruen</rhyme>:</l>
        <l id="s7:l3" met="4" type="-+">Maer sal ouerwinnen allen <rhyme label="a" type="m" corresp="#s7:l1">noodt</rhyme>,</l>
        <l id="s7:l4" met="3" type="-+">En alle Godts goeden <rhyme label="b" type="f" corresp="#s7:l2">eruen</rhyme>.</l>
        <l id="s7:l5" met="4" type="-+">Oock sal hy verrijsen op den derden <rhyme label="c" type="m">dach</rhyme>,</l>
        <l id="s7:l6" met="4" type="-+">Op dat hy eewlijck regieren <rhyme label="c" type="m" corresp="#s7:l5">mach</rhyme>,</l>
        <l id="s7:l7" met="3" type="-+">Een Coninck ouer Hemel, en <rhyme label="d" type="f">deerde</rhyme>.</l>
      </lg>
      <lg id="s8" type="stanza">
        <l id="s8:l1" met="4" type="-+">Daer voor wy hem segghen lof en <rhyme label="a" type="m">eer</rhyme>,</l>
        <l id="s8:l2" met="3" type="-+">Dat wy dien Salichmaker <rhyme label="b" type="f">haben</rhyme>,</l>
        <l id="s8:l3" met="4" type="-+">Singhen, en verblijden ons dies al <rhyme label="a" type="m" corresp="#s8:l1">seer</rhyme>,</l>
        <l id="s8:l4" met="3" type="-+">En dancken hem voor sulcke <rhyme label="b" type="f" corresp="#s8:l2">gauen</rhyme>:</l>
        <l id="s8:l5" met="4" type="-+">Oock ghelijck hijt ons door zijn woordt <rhyme label="c" type="m">bediet</rhyme>,</l>
        <l id="s8:l6" met="4" type="-+">Ist dat wijt gheloouen, ende twijflen <rhyme label="c" type="m" corresp="#s8:l5">niet</rhyme>,</l>
        <l id="s8:l7" met="3" type="-+">So sullen wy salich <rhyme label="d" type="f">worden</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
