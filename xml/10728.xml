<?xml version='1.0' encoding='utf-8' standalone='yes'?>
<song>
  <meta>
    <id>10728</id>
    <date>
      <year-start>1627</year-start>
      <year-end>1627</year-end>
    </date>
    <location>
      <orginal-name>rotterdam</orginal-name>
      <standard-name>Rotterdam, The Netherlands</standard-name>
      <coordinates>
        <lon>51.9228958</lon>
        <lat>4.4631727</lat>
      </coordinates>
    </location>
    <tags/>
    <melody>Schoon lief wil mij troost geven</melody>
  <author>Jacob Janszoon Colevelt</author></meta>
  <text>
    <lg type="song">
      <lg id="s1" type="stanza">
        <l id="s1:l1" met="3" type="-+">VOormaels de <rhyme label="a" type="f">Cupidoontjens</rhyme>,</l>
        <l id="s1:l2" met="3" type="-+">De wack're <rhyme label="a" type="f" corresp="#s1:l1">Minne-Good-jens</rhyme></l>
        <l id="s1:l3" met="3" type="-+">Beklommen met haer <rhyme label="b" type="m">al</rhyme>,</l>
        <l id="s1:l4" met="3" type="-+">Met hooghe leeren <rhyme label="c" type="f">vaerdigh</rhyme></l>
        <l id="s1:l5" met="3" type="-+">Iupijns Paleys <rhyme label="c" type="f" corresp="#s1:l4">eerwaerdigh</rhyme>,</l>
        <l id="s1:l6" met="3" type="-+">Met schrickelijck <rhyme label="b" type="m" corresp="#s1:l3">inval</rhyme>.</l>
      </lg>
      <lg id="s2" type="stanza">
        <l id="s2:l1" met="3" type="-+">Het gincker op een <rhyme label="a" type="f">buyten</rhyme>,</l>
        <l id="s2:l2" met="3" type="-+">Te rooven in te <rhyme label="a" type="f" corresp="#s2:l1">ruyten</rhyme></l>
        <l id="s2:l3" met="3" type="-+">Van 't reedste datter <rhyme label="b" type="m">was</rhyme>:</l>
        <l id="s2:l4" met="3" type="-+">En dat haer oogh <rhyme label="c" type="f">bekoorde</rhyme>,</l>
        <l id="s2:l5" met="3" type="-+">Van 't gheen de Goden <rhyme label="c" type="f" corresp="#s2:l4">hoorden</rhyme>,</l>
        <l id="s2:l6" met="3" type="-+">Het quam hun al te <rhyme label="b" type="m" corresp="#s2:l3">pas</rhyme>:</l>
      </lg>
      <lg id="s3" type="stanza">
        <l id="s3:l1" met="3" type="-+">Daer Phoebus heeft <rhyme label="a" type="f">verloren</rhyme>,</l>
        <l id="s3:l2" met="3" type="-+">Boogh, Pijl, en toe <rhyme label="a" type="f" corresp="#s3:l1">behooren</rhyme>,</l>
        <l id="s3:l3" met="3" type="-+">Soo als hy 't droegh ten <rhyme label="b" type="m">strijt</rhyme>:</l>
        <l id="s3:l4" met="3" type="-+">Iupiter oock sijn <rhyme label="c" type="f">donder</rhyme>,</l>
        <l id="s3:l5" met="3" type="-+">Daer hy soo menigh <rhyme label="c" type="f" corresp="#s3:l4">wonder</rhyme></l>
        <l id="s3:l6" met="3" type="-+">Mee dee, die wiert hy <rhyme label="b" type="m" corresp="#s3:l3">quijt</rhyme>:</l>
      </lg>
      <lg id="s4" type="stanza">
        <l id="s4:l1" met="3" type="-+">Sijn knodz' Alcides <rhyme label="a" type="f">mede</rhyme>,</l>
        <l id="s4:l2" met="3" type="-+">Neptunus heel <rhyme label="a" type="f" corresp="#s4:l1">t'onvrede</rhyme>,</l>
        <l id="s4:l3" met="3" type="-+">Sijn dry-tand gaffel <rhyme label="b" type="m">mee</rhyme>:</l>
        <l id="s4:l4" met="3" type="-+">Mars rings-kraegh en sijn <rhyme label="c" type="f">kolder</rhyme></l>
        <l id="s4:l5" met="3" type="-+">Die bleef in dat <rhyme label="c" type="f" corresp="#s4:l4">ghebolder</rhyme></l>
        <l id="s4:l6" met="3" type="-+">Diaen haer sackels <rhyme label="b" type="m" corresp="#s4:l3">ree</rhyme>.</l>
      </lg>
      <lg id="s5" type="stanza">
        <l id="s5:l1" met="3" type="-+">Mercurius de <rhyme label="a" type="f">Bode</rhyme></l>
        <l id="s5:l2" met="3" type="-+">Van al de hooghe <rhyme label="a" type="f" corresp="#s5:l1">Goden</rhyme>,</l>
        <l id="s5:l3" met="3" type="-+">Liet daer zijn Leerzen, <rhyme label="b" type="m">ach</rhyme>:</l>
        <l id="s5:l4" met="3" type="-+">En Bacchus <rhyme label="c" type="f">onberaden</rhyme></l>
        <l id="s5:l5" met="3" type="-+">Sijn Thyrsum schoon van <rhyme label="c" type="f" corresp="#s5:l4">bladen</rhyme>,</l>
        <l id="s5:l6" met="3" type="-+">Die noyt hy meer en <rhyme label="b" type="m" corresp="#s5:l3">sach</rhyme>.</l>
      </lg>
      <lg id="s6" type="stanza">
        <l id="s6:l1" met="3" type="-+">Dus isset dan gheen <rhyme label="a" type="f">wonder</rhyme>,</l>
        <l id="s6:l2" met="3" type="-+">Dat why oock moeder <rhyme label="a" type="f" corresp="#s6:l1">onder</rhyme></l>
        <l id="s6:l3" met="3" type="-+">Het minne-juck soo <rhyme label="b" type="m">wis</rhyme>,</l>
        <l id="s6:l4" met="3" type="-+">Na dien sy ons <rhyme label="c" type="f">betrapen</rhyme></l>
        <l id="s6:l5" met="3" type="-+">Met selfs der Goden <rhyme label="c" type="f" corresp="#s6:l4">Wapen</rhyme>,</l>
        <l id="s6:l6" met="3" type="-+">Die hiet t'ontwijcken <rhyme label="b" type="m" corresp="#s6:l3">is</rhyme>.</l>
      </lg>
    </lg>
  </text>
</song>
